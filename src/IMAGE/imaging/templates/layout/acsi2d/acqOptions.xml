<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqOptions" element="pages" type="acquisition" >
  <group size="800 260"
    style="Heading1"
    label="Water Suppr"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqOptions"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <button loc="440 130" size="120 20"
      style="Label4"
      label="Optimize - flip(C)"
      vc="ws_opt='c' ws_opt"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="440 180" size="120 20"
      style="Label4"
      label="Optimize - delay"
      vc="ws_opt='d' ws_opt('d1')"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="440 155" size="120 20"
      style="Label4"
      label="Optimize - flip(F)"
      vc="ws_opt='f' ws_opt('flipF')"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="440 40" size="120 20"
      style="Label4"
      label="Optimize"
      vq="ws_opt"
      vc="ws_opt='cf'  ws_opt"
      bg="darkGreen"
      decor1="no"
      />
    <group loc="220 0" size="210 230"
      style="Heading1"
      label="RF Pulse"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="RFPulse"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 195" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="95 20"
          style="PlainText"
          label="-start, end"
          justify="Left"
          />
        <entry loc="95 0" size="50 20"
          style="PlainText"
          vq="d1start"
          vc="d1start=$VALUE/1000.0 "
          set="$VALUE=d1start*1000.0"
          show="on('d1start'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="150 0" size="50 20"
          style="PlainText"
          vq="d1end"
          vc="d1end=$VALUE/1000.0 "
          set="$VALUE=d1end*1000.0"
          show="on('d1end'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <group loc="5 175" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="145 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <label loc="0 0" size="95 20"
          style="PlainText"
          label="d1 delay"
          justify="Left"
          />
        <entry loc="95 0" size="50 20"
          style="PlainText"
          vq="d1"
          vc="d1=$VALUE/1000.0 "
          set="$VALUE=d1*1000.0"
          show="on('d1'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <group loc="5 150" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="95 0" size="45 20"
          style="PlainText"
          label="54.00"
          vq="fliparray"
          set="write('line3',' %4.2f',fliparray[0]*flipws):$VALUE"
          digits="2"
          statpar="fliparray"
          />
        <textmessage loc="150 0" size="45 20"
          style="PlainText"
          label="180.00"
          vq="fliparray"
          set="$n=size('fliparray') write('line3',' %4.2f',fliparray[$n]*flipws):$VALUE"
          digits="2"
          statpar="fliparray"
          />
        <label loc="0 0" size="95 20"
          style="PlainText"
          label="-start, end"
          justify="Left"
          />
      </group>
      <group loc="5 130" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="95 0" size="45 20"
          style="PlainText"
          label="117.00"
          vq="flipws"
          set="write('line3',' %4.2f',flipws*wsflipf):$VALUE"
          digits="2"
          statpar="flipws"
          />
        <label loc="0 0" size="95 20"
          style="PlainText"
          label="Flip Angle"
          justify="Left"
          />
      </group>
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="70 0" size="50 20"
          style="PlainText"
          vq="wspat"
          vc="wspat='$VALUE' go('check')"
          set="$VALUE=wspat"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Shape"
          justify="Left"
          />
      </group>
      <group loc="5 80" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="70 0" size="50 20"
          style="PlainText"
          vq="pws"
          vc="pws=$VALUE*1000.0 calc_wspw(wspat,0,pws):wsbw go('check')"
          set="$VALUE=pws/1000.0"
          show="on('pws'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <label loc="120 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Duration"
          justify="Left"
          />
      </group>
      <group loc="5 40" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="70 0" size="50 20"
          style="PlainText"
          vq="flipws"
          vc="flipws=$VALUE"
          set="$VALUE=flipws"
          digits="2"
          disable="Grayed out"
          />
        <label loc="120 0" size="40 20"
          style="PlainText"
          label="deg x"
          justify="Left"
          />
        <entry loc="160 0" size="40 20"
          style="PlainText"
          vq="wsflipf"
          vc="wsflipf=$VALUE"
          set="$VALUE=wsflipf"
          digits="2"
          disable="Grayed out"
          />
        <label loc="0 0" size="70 20"
          style="PlainText"
          label="Flip Angle"
          justify="Left"
          />
      </group>
      <group loc="5 20" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="70 0" size="50 20"
          style="PlainText"
          vq="pws pwpat wsbw"
          vc="wsbw = $VALUE calc_wspw(wspat,wsbw):pws pws='n' go('check')"
          set="$VALUE = wsbw"
          show="on('pws'):$on $VALUE = ($on = 0)"
          digits="2"
          disable="Grayed out"
          />
        <label loc="120 0" size="30 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <label loc="0 0" size="75 20"
          style="PlainText"
          label="Bandwidth"
          justify="Left"
          />
        <button loc="145 0" size="55 20"
          style="PlainText"
          label="Cursors"
          vq="pws"
          vc="wsbw = delta calc_wspw(wspat,wsbw):pws pws=n go('check')"
          bg="transparent"
          decor1="no"
          />
      </group>
      <group loc="5 100" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="70 0" size="50 20"
          style="PlainText"
          label="38.96"
          vq="wstpwr wstwprf"
          set="calc_power(wstpwr,wstpwrf):$VALUE"
          digits="2"
          />
        <label loc="120 0" size="30 20"
          style="PlainText"
          label="dB"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Power"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 130" size="210 100"
      style="Heading1"
      label="Crushers"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Crushers"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 55" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="70 0" size="60 20"
          style="PlainText"
          vq="gcrushws"
          vc="gcrushws=$VALUE"
          set="$VALUE=gcrushws"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="135 0" size="60 20"
          style="PlainText"
          vq="tcrushws"
          vc="tcrushws=$VALUE/1000.0 go('check')"
          set="$VALUE=tcrushws*1000.0"
          digits="2"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="WS"
          justify="Left"
          />
      </group>
      <group loc="5 35" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="70 0" size="60 20"
          style="PlainText"
          vq="gcrush"
          vc="gcrush=$VALUE"
          set="$VALUE=gcrush"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="135 0" size="60 20"
          style="PlainText"
          vq="tcrush"
          vc="tcrush=$VALUE/1000.0 go('check')"
          set="$VALUE=tcrush*1000.0"
          digits="2"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="TE"
          justify="Left"
          />
      </group>
      <label loc="140 15" size="60 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="75 15" size="60 20"
        style="PlainText"
        label="G/cm"
        justify="Left"
        />
    </group>
    <button loc="440 15" size="120 20"
      style="Label4"
      label="Trial Spectrum"
      vc="im2Dcsi('prep','profile') au"
      bg="darkGreen"
      decor1="no"
      />
    <group loc="5 0" size="210 130"
      style="Heading1"
      label="Suppression Method"
      vq="ws"
      show="exists('ws','parameter'):$VALUE"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FatSaturation"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="10 70" size="195 20"
        vq="wss"
        show="$VALUE = (wss &lt;&gt; 'vapor')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="115 0" size="50 20"
          style="PlainText"
          vq="ws_delay"
          vc="ws_delay=$VALUE/1000.0 go('check')"
          set="$VALUE=ws_delay*1000"
          digits="2"
          disable="Grayed out"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <label loc="0 0" size="105 20"
          style="PlainText"
          label="Inter-pulse delay"
          justify="Left"
          />
      </group>
      <menu loc="20 45" size="105 20"
        style="PlainText"
        label="Supression Method"
        vq="wss"
        vc="wss = '$VALUE'"
        set="$VALUE = wss"
        editable="No"
        >
        <mlabel 
          label="DRY"
          chval="dry"
          />
        <mlabel 
          label="WET"
          chval="wet"
          />
        <mlabel 
          label="VAPOR"
          chval="vapor"
          />
        <mlabel 
          label="Single pulse"
          chval="1chess"
          />
      </menu>
      <check loc="5 20" size="155 20"
        style="PlainText"
        label="Suppression on"
        vq="ws"
        vc="ws='y'"
        vc2="ws='n'"
        set="$VALUE = (ws='y')"
        />
    </group>
  </group>
</template>
