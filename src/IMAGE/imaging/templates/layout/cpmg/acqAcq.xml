<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqAcq" element="pages" type="acquisition" >
  <group size="815 255"
    label="Acq"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqAcq"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="250 60" size="360 80"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="z"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="190 20" size="65 20"
        style="PlainText"
        label="46.8"
        vq="p1 p2"
        set="format(p2,0,1):$VALUE"
        />
      <label loc="190 0" size="150 20"
        style="Heading2"
        label="180 deg."
        justify="Left"
        />
      <entry loc="110 60" size="70 20"
        style="PlainText"
        vq="tpwr1"
        vc="tpwr1=$VALUE"
        set="$VALUE=tpwr1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 20" size="70 20"
        style="PlainText"
        vq="p1 p2"
        vc="p1=$VALUE p2=p1*2"
        set="$VALUE=p1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="110 0" size="70 20"
        style="Heading2"
        label="90 deg."
        justify="Left"
        />
      <entry loc="110 40" size="70 20"
        style="PlainText"
        vq="p1pat"
        vc="p1pat='$VALUE'"
        set="$VALUE=p1pat"
        show="on('p1pat'):$VALUE"
        disable="Grayed out"
        />
      <label loc="50 60" size="40 20"
        style="Label1"
        label="(dB)"
        justify="Center"
        />
      <label loc="50 20" size="40 20"
        style="Label1"
        label="(us)"
        justify="Center"
        />
      <label loc="0 20" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <label loc="0 60" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="0 40" size="50 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="0 0" size="90 20"
        style="Heading1"
        label="Pulses"
        justify="Left"
        />
    </group>
    <group loc="250 140" size="245 110"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="110 60" size="70 20"
        style="PlainText"
        vq="bt"
        vc="bt=$VALUE/1000.0"
        set="$VALUE=bt*1000.0"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="110 80" size="70 20"
        style="PlainText"
        vq="btarray"
        vc="btarray=$VALUE/1000.0"
        set="$VALUE=btarray*1000.0"
        digits="1"
        disable="Grayed out"
        />
      <label loc="60 60" size="50 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="0 60" size="60 20"
        style="Label1"
        label="bt"
        justify="Left"
        />
      <label loc="60 80" size="50 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="0 80" size="60 20"
        style="Label1"
        label="btarray"
        justify="Left"
        />
      <label loc="0 40" size="60 20"
        style="Label1"
        label="d2"
        justify="Left"
        />
      <check loc="180 20" size="55 20"
        style="PlainText"
        label="gating"
        vq="ticks"
        vc="ticks=1"
        vc2="ticks=0"
        set="$VALUE=(ticks&lt;&gt;0)"
        show="on('ticks'):$VALUE"
        />
      <label loc="0 0" size="90 20"
        style="Heading1"
        label="Delays"
        justify="Left"
        />
      <entry loc="110 40" size="70 20"
        style="PlainText"
        vq="d2"
        vc="d2=$VALUE/1000"
        set="$VALUE=d2*1000.0"
        show="on('d2'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="110 20" size="70 20"
        style="PlainText"
        vq="tr"
        vc="tr=$VALUE/1000.0"
        set="$VALUE=tr*1000.0"
        show="on('tr'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="60 40" size="50 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="60 20" size="50 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="0 20" size="60 20"
        style="Label1"
        label="tr"
        justify="Left"
        />
    </group>
    <group loc="520 155" size="240 80"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="0 20" size="110 20"
        style="Label1"
        label="Peak Threshold"
        justify="Left"
        />
      <label loc="0 0" size="90 20"
        style="Heading1"
        label="Misc."
        justify="Left"
        />
      <entry loc="110 20" size="70 20"
        style="PlainText"
        vq="th"
        vc="tth=$VALUE"
        set="$VALUE=th"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="0 40" size="240 210"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="spuls_acq"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="0 180" size="120 20"
        style="Label1"
        label="presig"
        justify="Left"
        />
      <menu loc="160 175" size="70 20"
        style="PlainText"
        vq="presig"
        vc="presig='$VALUE'"
        set="$VALUE=presig"
        show="on('presig'):$VALUE"
        >
        <mlabel 
          label="high"
          chval="h"
          />
        <mlabel 
          label="low"
          chval="l"
          />
      </menu>
      <entry loc="160 155" size="70 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=np/2"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 160" size="120 20"
        style="Label1"
        label="Complex points"
        justify="Left"
        />
      <label loc="0 140" size="120 20"
        style="Label1"
        label="Dummy scans"
        justify="Left"
        />
      <label loc="120 80" size="40 20"
        style="Label1"
        label="(ms)"
        justify="Center"
        />
      <entry loc="160 140" size="70 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="160 120" size="70 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 120" size="120 20"
        style="Label1"
        label="Averages"
        justify="Left"
        />
      <entry loc="160 100" size="70 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="160 80" size="70 20"
        style="PlainText"
        vq="at"
        vc="at=$VALUE/1000.0"
        set="$VALUE=at*1000.0"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="160 60" size="70 20"
        style="PlainText"
        vq="sw"
        vc="sw=$VALUE"
        set="$VALUE=sw"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="160 40" size="70 20"
        style="PlainText"
        vq="tof"
        vc="tof=$VALUE"
        set="$VALUE=tof"
        digits="2"
        disable="Grayed out"
        />
      <textmessage loc="160 20" size="70 20"
        style="PlainText"
        label="200.527"
        vq="sfrq"
        set="$VALUE=sfrq"
        digits="3"
        />
      <label loc="120 100" size="40 20"
        style="Label1"
        label="(dB)"
        justify="Center"
        />
      <label loc="120 60" size="40 20"
        style="Label1"
        label="(Hz)"
        justify="Center"
        />
      <label loc="120 40" size="40 20"
        style="Label1"
        label="(Hz)"
        justify="Center"
        />
      <menu loc="160 0" size="70 20"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        >
        <mlabel 
          label="Proton"
          chval="H1"
          />
        <mlabel 
          label="F19"
          chval="F19"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="Na23"
          chval="Na23"
          />
        <mlabel 
          label="Xe129"
          chval="Xe129"
          />
      </menu>
      <label loc="120 20" size="40 20"
        style="Label1"
        label="(MHz)"
        justify="Center"
        />
      <label loc="0 100" size="120 20"
        style="Label1"
        label="Receiver gain"
        justify="Left"
        />
      <label loc="0 80" size="120 20"
        style="Label1"
        label="Acquisition time"
        justify="Left"
        />
      <label loc="0 60" size="120 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
      <label loc="0 40" size="120 20"
        style="Label1"
        label="Observe offset"
        justify="Left"
        />
      <label loc="0 20" size="120 20"
        style="Label1"
        label="Observe frequency"
        justify="Left"
        />
      <label loc="0 0" size="110 20"
        style="Heading1"
        label="Acquisition"
        justify="Left"
        />
    </group>
    <button loc="610 10" size="150 20"
      style="Heading2"
      label="Acquire spectrum"
      vc="ldrf imprep  bt=btarray[1] wdone='' werr='' wexp='im1Dglobal(`proc`)' au"
      decor1="No"
      />
    <button loc="610 130" size="150 20"
      style="Heading2"
      label="Display T2 curve"
      vc="imcpmg('proc')"
      decor1="No"
      />
    <button loc="610 110" size="150 20"
      style="Heading2"
      label="Display spectra"
      vc="f full wft select(1) vsadj(40) dssh"
      decor1="No"
      />
    <button loc="610 90" size="150 20"
      style="Heading2"
      label="Acquire T2 data"
      vc="imcpmg('prep') wnt='' wexp='imcpmgt2(`proc`)'  werr='' au"
      decor1="No"
      />
    <button loc="610 70" size="150 20"
      style="Heading2"
      label="Display line width"
      vc="nl dres"
      decor1="No"
      />
    <button loc="610 50" size="150 20"
      style="Heading2"
      label="Set freq at cursor"
      vc="offset:H1offset tof=H1offset"
      decor1="No"
      />
    <button loc="610 30" size="150 20"
      style="Heading2"
      label="Set frequency"
      vc="prescan_freq('manual')"
      decor1="No"
      />
    <textmessage loc="695 230" size="120 20"
      style="Heading2"
      label="test"
      vq="rfcoil"
      set="$VALUE=rfcoil"
      show="on('rfcoil'):$VALUE"
      />
    <label loc="590 230" size="90 20"
      style="Heading2"
      label="RF coil"
      justify="Left"
      />
    <entry loc="350 20" size="250 20"
      style="PlainText"
      vq="comment"
      vc="comment='$VALUE' xm1('addcomment')"
      set="$VALUE=comment"
      digits="0"
      disable="Grayed out"
      />
    <label loc="250 20" size="100 20"
      style="Heading1"
      label="Comment:"
      justify="Left"
      />
    <textmessage loc="350 0" size="100 20"
      style="Heading2"
      label="cpmg"
      vq="seqfil"
      set="$VALUE=seqfil"
      />
    <label loc="250 0" size="100 20"
      style="Heading1"
      label="Sequence"
      justify="Left"
      />
    <textmessage loc="0 20" size="140 20"
      style="Heading2"
      label=" "
      vq="name"
      set="$VALUE=name"
      />
    <textmessage loc="0 0" size="140 20"
      style="Heading1"
      label="cpmg"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
