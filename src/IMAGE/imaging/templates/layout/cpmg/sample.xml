<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Start" element="panels" type="sample" >
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Subject Info"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="yes"
    reference="sampInfo"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    label="Prescan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="yes"
    reference="Prescan"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    label="Prescan Adv"
    vq="Prescan_ShowAdv panellevel"
    show="$VALUE = (Prescan_ShowAdv &gt; 0) AND (panellevel &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="yes"
    reference="PrescanAdv"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    label="Prescan List"
    vq="Prescan_ShowList panellevel"
    show="$VALUE = (Prescan_ShowList &gt; 0) AND (panellevel &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="yes"
    reference="PrescanList"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 295"
    label="Prescan Setup"
    vq="Prescan_ShowSetup panellevel"
    show="$VALUE = ((panellevel &gt; 0) AND (Prescan_ShowSetup &gt; 0))"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="yes"
    reference="PrescanSetup"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="810 220"
    style="PlainText"
    label="Manual Shim"
    vq="system"
    show="$VALUE = (system='spectrometer')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="yes"
    reference="sampShim"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
</template>
