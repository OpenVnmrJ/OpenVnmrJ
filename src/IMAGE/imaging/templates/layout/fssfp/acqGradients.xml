        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Amplitude"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          vq="gro"
          set="$VALUE = gro"
          digits="2"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
