          vc="tcrushro=$VALUE/1000"
          set="$VALUE=tcrushro*1000"
          show="on('tcrushro'):$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushro"
          vc="gcrushro=$VALUE go('check')"
          set="$VALUE=gcrushro"
          show="on('gcrushro'):$VALUE"
          disable="Grayed out"
