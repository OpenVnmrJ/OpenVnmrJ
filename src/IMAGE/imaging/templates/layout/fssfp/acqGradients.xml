<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqGradients" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="Gradients"
    vq="panellevel show_grad"
    show="$VALUE = (panellevel &gt; 0) AND (show_grad &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqGradients"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="435 0" size="280 200"
      style="Heading1"
      label="Crushers &amp; Spoilers"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="crusher_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 170" size="265 20"
        vq="spoilflag"
        show="exists('spoilflag','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="90 20"
          style="PlainText"
          label="Spoil"
          vq="spoilflag"
          vc="spoilflag='y' go('check')"
          vc2="spoilflag='n' go('check')"
          set="$VALUE=(spoilflag='y')"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tspoil spoilflag"
          vc="tspoil=$VALUE/1000.0 go('check')"
          set="$VALUE=tspoil*1000.0"
          show="if (spoilflag = 'y') then $VALUE = 1 else $VALUE = -1 endif"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gspoil spoilflag"
          vc="gspoil=$VALUE go('check')"
          set="$VALUE=gspoil"
          show="if (spoilflag = 'y') then $VALUE = 1 else $VALUE = -1 endif"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <group loc="5 60" size="265 20"
        vq="gcrushro"
        show="exists('gcrushro','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="15 0" size="75 20"
          style="Label1"
          label="Readout"
          />
