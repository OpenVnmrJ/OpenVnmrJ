<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqAcq" element="pages" type="acquisition" >
  <group size="800 270"
    label="Advanced"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqAcq"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="250 145" size="190 110"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="110 80" size="70 20"
        style="PlainText"
        vq="tcrush"
        vc="tcrush=$VALUE/1000.0"
        set="$VALUE=tcrush*1000.0"
        show="on('tcrush'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="0 80" size="60 20"
        style="Label1"
        label="tcrush"
        justify="Left"
        />
      <label loc="80 80" size="30 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="65 60" size="45 20"
        style="Label1"
        label="(G/cm)"
        justify="Left"
        />
      <label loc="0 60" size="60 20"
        style="Label1"
        label="gcrush "
        justify="Left"
        />
      <entry loc="110 60" size="70 20"
        style="PlainText"
        vq="gcrush"
        vc="gcrush=$VALUE"
        set="$VALUE=gcrush"
        show="on('gcrush'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="0 0" size="90 20"
        style="Heading1"
        label="Parameters"
        justify="Left"
        />
      <entry loc="110 40" size="70 20"
        style="PlainText"
        vq="te"
        vc="te=$VALUE/1000.0"
        set="$VALUE=te*1000.0"
        show="on('te'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="80 40" size="30 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="0 40" size="60 20"
        style="Label1"
        label="TE"
        justify="Left"
        />
      <label loc="0 20" size="60 20"
        style="Label1"
        label="TR"
        justify="Left"
        />
      <label loc="80 20" size="30 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <entry loc="110 20" size="70 20"
        style="PlainText"
        vq="tr"
        vc="tr=$VALUE/1000.0"
        set="$VALUE=tr*1000.0"
        show="on('tr'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="250 40" size="380 105"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="spuls_par"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="110 80" size="70 20"
        style="PlainText"
        vq="tpwr1 ws slab"
        vc="tpwr1=$VALUE"
        set="$VALUE=tpwr1"
        show="$VALUE = ( (ws='n') and (slab='n') )"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="200 80" size="70 20"
        style="PlainText"
        vq="tpwr2 ws slab"
        vc="tpwr2=$VALUE go('check')"
        set="$VALUE=tpwr2"
        show="$VALUE = ( (ws='y') or (slab='y') )"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="110 60" size="70 20"
        style="PlainText"
        vq="flip1 fliplist ws slab"
        vc="flip1=$VALUE fliplist[1]=flip1 go('check')"
        set="$VALUE=flip1"
        show="$VALUE = ( (ws='n') and (slab='n') )"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="200 60" size="70 20"
        style="PlainText"
        vq="flip2 fliplist ws slab"
        vc="flip2=$VALUE fliplist[2]=flip2 go('check')"
        set="$VALUE=flip2"
        show="$VALUE = ( (ws='y') or (slab='y') )"
        digits="1"
        disable="Grayed out"
        />
      <label loc="0 60" size="50 20"
        style="Label1"
        label="Flip"
        justify="Left"
        />
      <label loc="0 80" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="80 80" size="30 20"
        style="Label1"
        label="(dB)"
        justify="Center"
        />
      <entry loc="200 40" size="70 20"
        style="PlainText"
        vq="p2pat ws slab"
        vc="p2pat='$VALUE' go('check')"
        set="$VALUE=p2pat"
        show="$VALUE = ( (ws='y') or (slab='y') )"
        disable="Grayed out"
        />
      <entry loc="200 20" size="70 20"
        style="PlainText"
        vq="p2 ws slab"
        vc="p2=$VALUE go('check')"
        set="$VALUE=p2"
        show="$VALUE = ( (ws='y') or (slab='y') )"
        digits="1"
        disable="Grayed out"
        />
      <check loc="200 0" size="85 20"
        style="PlainText"
        label="Soft pulse"
        vq="ws"
        vc="ws='y' "
        vc2="ws='n' "
        set="$VALUE=(ws='y')"
        show="on('ws'):$VALUE"
        />
      <check loc="290 0" size="85 20"
        style="PlainText"
        label="slab select"
        vq="slab"
        vc="slab='y' "
        vc2="slab='n' "
        set="$VALUE=(slab='y')"
        show="on('slab'):$VALUE"
        />
      <label loc="80 20" size="30 20"
        style="Label1"
        label="(us)"
        justify="Center"
        />
      <entry loc="110 20" size="70 20"
        style="PlainText"
        vq="p1 ws slab"
        vc="p1=$VALUE go('check')"
        set="$VALUE=p1"
        show="$VALUE = ( (ws='n') and (slab='n') )"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="110 40" size="70 20"
        style="PlainText"
        vq="p1pat ws slab"
        vc="p1pat='$VALUE'"
        set="$VALUE=p1pat"
        show="$VALUE = ( (ws='n') and (slab='n') )"
        disable="Grayed out"
        />
      <label loc="0 20" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <label loc="0 40" size="50 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="0 0" size="90 20"
        style="Heading1"
        label="Pulses"
        justify="Left"
        />
    </group>
    <button loc="640 40" size="150 20"
      style="Label4"
      label="Acquire Profile"
      vc="Prescan_ShimMethod='ge3dshim'  wdone='' im3D('prep','profile') au"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="640 65" size="150 20"
      style="Label4"
      label="Acquire Test Image"
      vc="gs_testimg"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="640 110" size="150 20"
      style="Heading3"
      label="Scan time"
      vc="_nv prescan_control[1]=-1 settime"
      decor1="no"
      />
    <group loc="0 40" size="230 215"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="spuls_acq"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="160 180" size="70 20"
        style="PlainText"
        vq="presig"
        vc="presig='$VALUE'"
        set="$VALUE=presig"
        show="on('presig'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="high"
          chval="h"
          />
        <mlabel 
          label="low"
          chval="l"
          />
      </menu>
      <label loc="0 180" size="90 20"
        style="Label1"
        label="presig"
        justify="Left"
        />
      <entry loc="160 160" size="70 20"
        style="PlainText"
        vq="seqcon"
        vc="seqcon='$VALUE'"
        set="$VALUE=seqcon"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 160" size="120 20"
        style="Label1"
        label="seqcon"
        justify="Left"
        />
      <label loc="120 80" size="40 20"
        style="Label1"
        label="(ms)"
        justify="Center"
        />
      <entry loc="160 140" size="70 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="160 120" size="70 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 140" size="120 20"
        style="Label1"
        label="Dummy scans"
        justify="Left"
        />
      <label loc="0 120" size="120 20"
        style="Label1"
        label="Averages"
        justify="Left"
        />
      <entry loc="160 100" size="70 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="160 80" size="70 20"
        style="PlainText"
        vq="at"
        vc="at=$VALUE/1000.0"
        set="$VALUE=at*1000.0"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="160 60" size="70 20"
        style="PlainText"
        vq="sw"
        vc="sw=$VALUE"
        set="$VALUE=sw"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="160 40" size="70 20"
        style="PlainText"
        vq="resto"
        vc="resto=$VALUE"
        set="$VALUE=resto"
        digits="2"
        disable="Grayed out"
        />
      <textmessage loc="160 20" size="70 20"
        style="PlainText"
        label="399.595"
        vq="sfrq"
        set="$VALUE=sfrq"
        digits="3"
        />
      <label loc="120 100" size="40 20"
        style="Label1"
        label="(dB)"
        justify="Center"
        />
      <label loc="120 60" size="40 20"
        style="Label1"
        label="(Hz)"
        justify="Center"
        />
      <label loc="120 40" size="40 20"
        style="Label1"
        label="(Hz)"
        justify="Center"
        />
      <menu loc="160 0" size="70 20"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        editable="No"
        >
        <mlabel 
          label="Proton"
          chval="H1"
          />
        <mlabel 
          label="F19"
          chval="F19"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="Na23"
          chval="Na23"
          />
        <mlabel 
          label="Xe129"
          chval="Xe129"
          />
      </menu>
      <label loc="120 20" size="40 20"
        style="Label1"
        label="(MHz)"
        justify="Center"
        />
      <label loc="0 100" size="120 20"
        style="Label1"
        label="Receiver gain"
        justify="Left"
        />
      <label loc="0 80" size="120 20"
        style="Label1"
        label="Acquisition time"
        justify="Left"
        />
      <label loc="0 60" size="120 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
      <label loc="0 40" size="120 20"
        style="Label1"
        label="Observe offset"
        justify="Left"
        />
      <label loc="0 20" size="120 20"
        style="Label1"
        label="Observe frequency"
        justify="Left"
        />
      <label loc="0 0" size="110 20"
        style="Heading1"
        label="Acquisition"
        justify="Left"
        />
    </group>
    <textmessage loc="0 20" size="140 20"
      style="Heading2"
      label=" "
      vq="name"
      set="$VALUE=name"
      />
    <textmessage loc="0 0" size="140 20"
      style="Heading1"
      label="ge3dshim"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
