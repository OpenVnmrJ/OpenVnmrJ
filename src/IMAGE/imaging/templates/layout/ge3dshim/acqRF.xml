<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqRF" element="pages" type="acquisition" >
  <group size="820 270"
    style="Heading1"
    label="RF Pulses"
    vq="panellevel show_rf"
    show="$VALUE = (panellevel &gt; 0) AND (show_rf &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqRF"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="720 10" size="90 100"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="show"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <toggle loc="10 40" size="80 20"
        style="PlainText"
        label="Advanced"
        vq="show_adv"
        vc="show_adv = 1"
        vc2="show_adv = 0"
        set="$VALUE=show_adv"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="10 0" size="80 20"
        style="PlainText"
        label="Map"
        vq="show_map"
        vc="show_map = 1"
        vc2="show_map = 0"
        set="$VALUE=show_map"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="10 20" size="80 20"
        style="PlainText"
        label="Settings"
        vq="show_set"
        vc="show_set = 1"
        vc2="show_set = 0"
        set="$VALUE=show_set"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="10 60" size="80 20"
        style="PlainText"
        label="RF Pulses"
        vq="show_rf"
        vc="show_rf = 1"
        vc2="show_rf = 0"
        set="$VALUE=show_rf"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="10 80" size="80 20"
        style="PlainText"
        label="Gradients"
        vq="show_grad"
        vc="show_grad = 1"
        vc2="show_grad = 0"
        set="$VALUE=show_grad"
        radiobutton="no"
        subtype="no"
        />
    </group>
    <group loc="5 0" size="715 240"
      style="Heading1"
      label="RF pulses"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="RFpulses"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 60" size="395 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <menu loc="230 0" size="90 20"
          style="PlainText"
          vq="p2pat ws slab"
          vc="p2pat='$VALUE' go('check')"
          set="$VALUE=p2pat"
          show="if ((ws='y') or (slab='y') ) then $VALUE = 1 else $VALUE = 0 endif"
          bg="VJBackground"
          editable="Yes"
          >
          <mlabel 
            label="gauss"
            chval="gauss"
            />
          <mlabel 
            label="sinc"
            chval="sinc"
            />
        </menu>
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Soft Excite"
          vq="ws"
          vc="ws='y' go('check')"
          vc2="ws='n' go('check')"
          set="$VALUE=(ws='y')"
          show="on('ws'):$VALUE"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="p2 ws slab"
          vc="p2=$VALUE go('check')"
          set="$VALUE=p2"
          show="if ((ws='y') or (slab='y') ) then $VALUE = 1 else $VALUE = -1 endif"
          digits="1"
          disable="Grayed out"
          />
        <textmessage loc="335 0" size="50 20"
          style="PlainText"
          label="17.06"
          vq="tpwr2 tpwr2f"
          set="calc_power(tpwr2,tpwr2f):$VALUE"
          digits="2"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flip2 ws slab"
          vc="flip2=$VALUE go('check')"
          set="$VALUE=flip2"
          show="if ((ws='y') or (slab='y') ) then $VALUE = 1 else $VALUE = -1 endif"
          disable="Grayed out"
          />
      </group>
      <group loc="5 100" size="395 20"
        vq="sat"
        show="exists('sat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Sat Bands"
          vq="sat"
          vc="sat = 'y'"
          vc2="sat = 'n'"
          set="$VALUE = (sat = 'y')"
          />
        <textmessage loc="335 0" size="60 20"
          style="PlainText"
          label="49.52"
          vq="satpwr satpwrf"
          set="calc_power(satpwr,satpwrf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="satpat"
          vc="satpat='$VALUE' "
          set="$VALUE=satpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="psat"
          vc="psat=$VALUE "
          set="$VALUE=psat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipsat"
          vc="flipsat=$VALUE go('check')"
          set="$VALUE=flipsat"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 80" size="395 20"
        vq="fsat"
        show="exists('fsat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Fat Sat"
          vq="fsat"
          vc="fsat = 'y'"
          vc2="fsat = 'n'"
          set="$VALUE = (fsat = 'y')"
          />
        <textmessage loc="335 0" size="60 20"
          style="PlainText"
          label="39.98"
          vq="tpwrfsat tpwrfsatf"
          set="calc_power(tpwrfsat,tpwrfsatf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="fsatpat"
          vc="fsatpat='$VALUE' "
          set="$VALUE=fsatpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="pfsat fsat"
          vc="pfsat=$VALUE "
          set="$VALUE=pfsat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipfsat"
          vc="flipfsat=$VALUE go('check')"
          set="$VALUE=flipfsat"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="395 10" size="300 225"
        vq="advancedRF"
        show="$VALUE=(advancedRF='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <group loc="215 50" size="60 20"
          vq="pipat"
          show="$VALUE=((pipat='SGLgauss') OR (pipat='HS-AFP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[4]=$VALUE go('check')"
            set="$VALUE=pipars[4]"
            digits="1"
            />
        </group>
        <group loc="170 50" size="45 20"
          vq="pipat"
          show="$VALUE=((pipat='SGLsinc') OR (pipat='mao'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="10 0" size="25 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[3]=$VALUE go('check')"
            set="$VALUE=pipars[3]"
            digits="0"
            />
        </group>
        <group loc="105 50" size="60 20"
          vq="pipat"
          show="$VALUE=((pipat='HS-AFP') OR (pipat='HT-AHP') OR (pipat='mao') OR (pipat='SGLsinc') OR (pipat='SGLgauss'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[1]=$VALUE go('check')"
            set="$VALUE=pipars[1]"
            digits="1"
            />
        </group>
        <group loc="0 50" size="90 20"
          vq="pipat"
          show="$VALUE=((pipat='HS-AFP') OR (pipat='HT-AHP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="20 0" size="60 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[2]=$VALUE go('check')"
            set="$VALUE=pipars[2]"
            digits="1"
            />
        </group>
        <group loc="10 50" size="90 20"
          vq="pipat"
          show="$VALUE=((pipat='SGLsinc') OR (pipat='SGLgauss') OR (pipat='mao') )"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <textmessage loc="10 0" size="60 20"
            style="PlainText"
            vq="pipars"
            set="$VALUE=pipars[2]"
            digits="1"
            />
        </group>
        <group loc="215 30" size="60 20"
          vq="p1pat"
          show="$VALUE=((p1pat='SGLgauss') OR (p1pat='HS-AFP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[4]=$VALUE go('check')"
            set="$VALUE=p1pars[4]"
            digits="1"
            />
        </group>
        <label loc="220 10" size="60 20"
          style="Label1"
          label="Cutoff (%)"
          />
        <group loc="105 30" size="60 20"
          vq="p1pat"
          show="$VALUE=((p1pat='HS-AFP') OR (p1pat='HT-AHP') OR (p1pat='mao') OR (p1pat='SGLsinc') OR (p1pat='SGLgauss'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[1]=$VALUE go('check')"
            set="$VALUE=p1pars[1]"
            digits="1"
            />
        </group>
        <label loc="105 10" size="65 20"
          style="Label1"
          label="Res (usec)"
          />
        <group loc="170 30" size="45 20"
          vq="p1pat"
          show="$VALUE=((p1pat='SGLsinc') OR (p1pat='mao'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="10 0" size="25 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[3]=$VALUE go('check')"
            set="$VALUE=p1pars[3]"
            digits="0"
            />
        </group>
        <label loc="175 10" size="50 20"
          style="Label1"
          label="Lobes"
          />
        <group loc="0 30" size="90 20"
          vq="p1pat"
          show="$VALUE=((p1pat='HS-AFP') OR (p1pat='HT-AHP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <entry loc="20 0" size="60 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[2]=$VALUE go('check')"
            set="$VALUE=p1pars[2]"
            digits="1"
            />
        </group>
        <label loc="10 10" size="100 20"
          style="Label1"
          label="Bandwidth (Hz)"
          />
        <group loc="10 30" size="90 20"
          vq="p1pat"
          show="$VALUE=((p1pat='SGLsinc') OR (p1pat='SGLgauss') OR (p1pat='mao') )"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <textmessage loc="10 0" size="60 20"
            style="PlainText"
            vq="p1pars"
            set="$VALUE=p1pars[2]"
            digits="1"
            />
        </group>
      </group>
      <group loc="5 40" size="395 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="230 0" size="60 20"
          style="Label1"
          label="hard"
          />
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Hard Excite"
          vq="ws"
          vc="ws='n' go('check')"
          vc2="ws='y' go('check')"
          set="$VALUE=(ws='n')"
          show="on('ws'):$VALUE"
          />
        <textmessage loc="335 0" size="50 20"
          style="PlainText"
          label="55.76"
          vq="tpwr1 tpwr1f"
          set="calc_power(tpwr1,tpwr1f):$VALUE"
          digits="2"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="p1 ws slab"
          vc="p1=$VALUE go('check')"
          set="$VALUE=p1"
          show="if ((ws='y') or (slab='y') ) then $VALUE = -1 else $VALUE = 1 endif"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flip1 ws slab"
          vc="flip1=$VALUE go('check')"
          set="$VALUE=flip1"
          show="if ((ws='y') or (slab='y') ) then $VALUE = -1 else $VALUE = 1 endif"
          disable="Grayed out"
          />
      </group>
      <group loc="105 20" size="295 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="140 0" size="65 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
        <label loc="225 0" size="70 20"
          style="Label1"
          label="Power (dB)"
          justify="Left"
          />
        <label loc="65 0" size="65 20"
          style="Label1"
          label="Width (us)"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Flipangle"
          justify="Left"
          />
      </group>
    </group>
    <button loc="600 245" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_rf = 0"
      bg="transparent"
      decor1="no"
      />
  </group>
</template>
