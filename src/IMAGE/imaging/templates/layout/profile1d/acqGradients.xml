<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqGradients" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="Gradients"
    vq="panellevel show_adv"
    show="$VALUE = (panellevel &gt; 0) AND (show_adv &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqGradients"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="220 65" size="210 130"
      style="Heading1"
      label="Refocus &amp; Phase"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Refocus"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="---"
          vq="gper"
          set="$VALUE = gper"
          digits="2"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Phase"
          />
      </group>
      <group loc="5 40" size="200 20"
        style="Heading1"
        label="Readout Direction"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Gradients"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="5.81"
          vq="gror"
          set="$VALUE = gror"
          digits="2"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Readout"
          />
      </group>
      <group loc="5 20" size="200 20"
        style="Heading1"
        label="Readout Direction"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Gradients"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="11.88"
          vq="gssr"
          set="$VALUE = gssr"
          digits="2"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Slice"
          />
      </group>
      <group loc="5 85" size="195 40"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="---"
          vq="tror"
          set="format(tror*1e3,0,2):$VALUE"
          digits="2"
          />
        <textmessage loc="100 20" size="60 20"
          style="PlainText"
          label="---"
          vq="rgror"
          set="format(rgror*1e6,0,2):$VALUE"
          digits="2"
          />
        <label loc="160 20" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="ms"
          />
        <label loc="0 20" size="60 20"
          style="Label1"
          label="Ramp"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Duration"
          />
      </group>
    </group>
    <group loc="5 65" size="210 130"
      style="Heading1"
      label="Slice Select - Refocus"
      vq="spinecho"
      show="on('spinecho'):$VALUE"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Refocus"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="5 100" size="80 20"
        style="Label1"
        label="Ramp (to slice)"
        />
      <label loc="5 80" size="80 20"
        style="Label1"
        label="Ramp (to crush)"
        />
      <label loc="5 40" size="75 20"
        style="Label1"
        label="Crusher"
        />
      <label loc="5 60" size="100 20"
        style="Label1"
        label="Crusher Duration"
        />
      <textmessage loc="105 100" size="60 20"
        style="PlainText"
        label="---"
        vq="rgss2"
        set="format(rgss2*1e6,0,2):$VALUE"
        digits="2"
        />
      <textmessage loc="105 80" size="60 20"
        style="PlainText"
        label="---"
        vq="rgcrush"
        set="format(rcrush*1e6,0,2):$VALUE"
        digits="2"
        />
      <label loc="165 100" size="35 20"
        style="Label1"
        label="us"
        />
      <label loc="165 80" size="35 20"
        style="Label1"
        label="us"
        />
      <textmessage loc="105 20" size="60 20"
        style="PlainText"
        label="1.04"
        vq="gss2"
        set="$VALUE = gss2"
        digits="2"
        />
      <entry loc="105 60" size="60 20"
        style="PlainText"
        vc="tcrush = $VALUE/1000  go('check')"
        set="$VALUE = tcrush*1000"
        digits="2"
        />
      <entry loc="105 40" size="60 20"
        style="PlainText"
        vq="gcrush"
        vc="gcrush = $VALUE go('check')"
        set="$VALUE = gcrush"
        digits="2"
        />
      <label loc="165 40" size="35 20"
        style="Label1"
        label="G/cm"
        />
      <label loc="165 60" size="35 20"
        style="Label1"
        label="ms"
        />
      <label loc="5 20" size="60 20"
        style="Label1"
        label="Refocus"
        />
      <label loc="165 20" size="35 20"
        style="Label1"
        label="G/cm"
        />
    </group>
    <group loc="220 0" size="210 65"
      style="Heading1"
      label="Readout"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ReadoutDirection"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="200 40"
        style="Heading1"
        label="Readout Direction"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Gradients"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <textmessage loc="100 20" size="60 20"
          style="PlainText"
          label="---"
          vq="rgro"
          set="format(rgro*1e6,0,2):$VALUE"
          digits="2"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="0.86"
          vq="gro"
          set="$VALUE = gro"
          digits="2"
          />
        <label loc="160 20" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <label loc="0 20" size="60 20"
          style="Label1"
          label="Ramp"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Readout"
          />
      </group>
    </group>
    <button loc="475 175" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_adv = 0"
      decor1="No"
      />
    <group loc="5 0" size="210 65"
      style="Heading1"
      label="Slice Select - Excitation"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="200 40"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="100 20" size="60 20"
          style="PlainText"
          label="---"
          vq="rgss"
          set="format(rgss*1e6,0,2):$VALUE"
          digits="2"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="6.98"
          vq="gss"
          set="$VALUE = gss"
          digits="2"
          />
        <label loc="160 20" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <label loc="0 20" size="60 20"
          style="Label1"
          label="Ramp"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Excitation"
          />
      </group>
    </group>
  </group>
</template>
