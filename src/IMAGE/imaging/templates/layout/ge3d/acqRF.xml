<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqRF" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="RF Pulses"
    vq="panellevel show_rf"
    show="$VALUE = (panellevel &gt; 0) AND (show_rf &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqRF"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="5 0" size="715 240"
      style="Heading1"
      label="RF pulses"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="RFpulses"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 120" size="395 20"
        vq="mt"
        show="exists('mt','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="MT"
          vq="mt"
          vc="mt = 'y'"
          vc2="mt = 'n'"
          set="$VALUE = (mt = 'y')"
          />
        <textmessage loc="335 0" size="60 20"
          style="PlainText"
          label="62.55"
          vq="tpwrmt tpwrmtf"
          set="calc_power(tpwrmt,tpwrmtf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="mtpat"
          vc="mtpat='$VALUE' "
          set="$VALUE=mtpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="pmt"
          vc="pmt=$VALUE "
          set="$VALUE=pmt"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipmt"
          vc="flipmt=$VALUE go('check')"
          set="$VALUE=flipmt"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 100" size="395 20"
        vq="sat"
        show="exists('sat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Sat Bands"
          vq="sat"
          vc="sat = 'y'"
          vc2="sat = 'n'"
          set="$VALUE = (sat = 'y')"
          />
        <textmessage loc="335 0" size="60 20"
          style="PlainText"
          label="49.75"
          vq="satpwr satpwrf"
          set="calc_power(satpwr,satpwrf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="satpat"
          vc="satpat='$VALUE' "
          set="$VALUE=satpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="psat"
          vc="psat=$VALUE "
          set="$VALUE=psat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipsat"
          vc="flipsat=$VALUE go('check')"
          set="$VALUE=flipsat"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 80" size="395 20"
        vq="fsat"
        show="exists('fsat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Fat Sat"
          vq="fsat"
          vc="fsat = 'y'"
          vc2="fsat = 'n'"
          set="$VALUE = (fsat = 'y')"
          />
        <textmessage loc="335 0" size="60 20"
          style="PlainText"
          label="40.06"
          vq="tpwrfsat tpwrfsatf"
          set="calc_power(tpwrfsat,tpwrfsatf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="fsatpat"
          vc="fsatpat='$VALUE' "
          set="$VALUE=fsatpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="pfsat fsat"
          vc="pfsat=$VALUE "
          set="$VALUE=pfsat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipfsat"
          vc="flipfsat=$VALUE go('check')"
          set="$VALUE=flipfsat"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 60" size="395 20"
        vq="ir"
        show="exists('ir','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="230 0" size="90 20"
          style="PlainText"
          vq="pipat"
          vc="pipat='$VALUE' go('check')"
          set="$VALUE=pipat"
          editable="Yes"
          >
          <mlabel 
            label="gauss"
            chval="gauss"
            />
          <mlabel 
            label="sinc"
            chval="sinc"
            />
          <mlabel 
            label="HS-AFP"
            chval="HS-AFP"
            />
          <mlabel 
            label="mao"
            chval="mao"
            />
          <mlabel 
            label="SGLgauss"
            chval="SGLgauss"
            />
          <mlabel 
            label="SGLsinc"
            chval="SGLsinc"
            />
        </menu>
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="IR"
          vq="ir"
          vc="ir = 'y'"
          vc2="ir = 'n'"
          set="$VALUE = (ir = 'y')"
          />
        <textmessage loc="335 0" size="50 20"
          style="PlainText"
          label="59.71"
          vq="ir tpwri tpwrif"
          set="calc_power(tpwri,tpwrif):$VALUE"
          digits="2"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="pi ir"
          vc="pi=$VALUE go('check')"
          set="$VALUE=pi"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipir"
          vc="flipir=$VALUE go('check')"
          set="$VALUE=flipir"
          disable="Grayed out"
          />
      </group>
      <group loc="395 10" size="300 225"
        vq="advancedRF"
        show="$VALUE=(advancedRF='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <group loc="215 50" size="60 20"
          vq="pipat"
          show="$VALUE=((pipat='SGLgauss') OR (pipat='HS-AFP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[4]=$VALUE go('check')"
            set="$VALUE=pipars[4]"
            digits="1"
            />
        </group>
        <group loc="170 50" size="45 20"
          vq="pipat"
          show="$VALUE=((pipat='SGLsinc') OR (pipat='mao'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="10 0" size="25 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[3]=$VALUE go('check')"
            set="$VALUE=pipars[3]"
            digits="0"
            />
        </group>
        <group loc="105 50" size="60 20"
          vq="pipat"
          show="$VALUE=((pipat='HS-AFP') OR (pipat='HT-AHP') OR (pipat='mao') OR (pipat='SGLsinc') OR (pipat='SGLgauss'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[1]=$VALUE go('check')"
            set="$VALUE=pipars[1]"
            digits="1"
            />
        </group>
        <group loc="0 50" size="90 20"
          vq="pipat"
          show="$VALUE=((pipat='HS-AFP') OR (pipat='HT-AHP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="20 0" size="60 20"
            style="PlainText"
            vq="pipars"
            vc="pipars[2]=$VALUE go('check')"
            set="$VALUE=pipars[2]"
            digits="1"
            />
        </group>
        <group loc="10 50" size="90 20"
          vq="pipat"
          show="$VALUE=((pipat='SGLsinc') OR (pipat='SGLgauss') OR (pipat='mao') )"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <textmessage loc="10 0" size="60 20"
            style="PlainText"
            vq="pipars"
            set="$VALUE=pipars[2]"
            digits="1"
            />
        </group>
        <group loc="215 30" size="60 20"
          vq="p1pat"
          show="$VALUE=((p1pat='SGLgauss') OR (p1pat='HS-AFP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[4]=$VALUE go('check')"
            set="$VALUE=p1pars[4]"
            digits="1"
            />
        </group>
        <label loc="220 10" size="60 20"
          style="Label1"
          label="Cutoff (%)"
          />
        <group loc="105 30" size="60 20"
          vq="p1pat"
          show="$VALUE=((p1pat='HS-AFP') OR (p1pat='HT-AHP') OR (p1pat='mao') OR (p1pat='SGLsinc') OR (p1pat='SGLgauss'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="10 0" size="40 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[1]=$VALUE go('check')"
            set="$VALUE=p1pars[1]"
            digits="1"
            />
        </group>
        <label loc="105 10" size="65 20"
          style="Label1"
          label="Res (usec)"
          />
        <group loc="170 30" size="45 20"
          vq="p1pat"
          show="$VALUE=((p1pat='SGLsinc') OR (p1pat='mao'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="10 0" size="25 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[3]=$VALUE go('check')"
            set="$VALUE=p1pars[3]"
            digits="0"
            />
        </group>
        <label loc="175 10" size="50 20"
          style="Label1"
          label="Lobes"
          />
        <group loc="0 30" size="90 20"
          vq="p1pat"
          show="$VALUE=((p1pat='HS-AFP') OR (p1pat='HT-AHP'))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <entry loc="20 0" size="60 20"
            style="PlainText"
            vq="p1pars"
            vc="p1pars[2]=$VALUE go('check')"
            set="$VALUE=p1pars[2]"
            digits="1"
            />
        </group>
        <label loc="10 10" size="100 20"
          style="Label1"
          label="Bandwidth (Hz)"
          />
        <group loc="10 30" size="90 20"
          vq="p1pat"
          show="$VALUE=((p1pat='SGLsinc') OR (p1pat='SGLgauss') OR (p1pat='mao') )"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <textmessage loc="10 0" size="60 20"
            style="PlainText"
            vq="p1pars"
            set="$VALUE=p1pars[2]"
            digits="1"
            />
        </group>
      </group>
      <group loc="5 40" size="395 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="230 0" size="90 20"
          style="PlainText"
          vq="p1pat"
          vc="p1pat='$VALUE' go('check')"
          set="$VALUE=p1pat"
          editable="Yes"
          >
          <mlabel 
            label="gauss"
            chval="gauss"
            />
          <mlabel 
            label="sinc"
            chval="sinc"
            />
          <mlabel 
            label="SGLgauss"
            chval="SGLgauss"
            />
          <mlabel 
            label="SGLsinc"
            chval="SGLsinc"
            />
        </menu>
        <textmessage loc="335 0" size="50 20"
          style="PlainText"
          label="39.20"
          vq="tpwr1 tpwr1f"
          set="calc_power(tpwr1,tpwr1f):$VALUE"
          digits="2"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="p1"
          vc="p1=$VALUE go('check')"
          set="$VALUE=p1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flip1"
          vc="flip1=$VALUE go('check')"
          set="$VALUE=flip1"
          disable="Grayed out"
          />
        <label loc="16 0" size="85 20"
          style="Label1"
          label="Excitation"
          justify="Left"
          />
      </group>
      <group loc="105 20" size="295 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="140 0" size="65 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
        <label loc="225 0" size="70 20"
          style="Label1"
          label="Power (dB)"
          justify="Left"
          />
        <label loc="65 0" size="65 20"
          style="Label1"
          label="Width (us)"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Flipangle"
          justify="Left"
          />
      </group>
    </group>
    <button loc="600 245" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_rf = 0"
      bg="transparent"
      decor1="no"
      />
    <check loc="10 245" size="100 20"
      style="PlainText"
      label="Advanced"
      vq="advancedRF"
      vc="advancedRF='y'"
      vc2="advancedRF='n'"
      set="$VALUE=(advancedRF='y')"
      />
  </group>
</template>
