<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="900 280"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Inversion Recovery"
      vq="ir showOptions"
      show="if (showOptions=1) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="InversionRecovery"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Magnetization Transfer"
      vq="mt showOptions"
      show="if (showOptions=2) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MagTransfer"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Fat Saturation"
      vq="fsat showOptions"
      show="if (showOptions=3) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FatSaturation"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Sat Bands"
      vq="sat showOptions"
      show="if (showOptions=4) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Satbands"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Slab Plan"
      vq="showOptions"
      show="if (showOptions=5) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MoreFOVPlan3D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="435 0" size="210 230"
      style="Heading1"
      label="FOV &amp; Slab Plan"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FOVPlan3D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="220 0" size="210 230"
      style="Heading1"
      label="Scan Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ScanOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 180" size="200 20"
        vq="ticks ir"
        show="$VALUE=(ticks&gt;0 OR ir='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="blockslices"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="95 0" size="100 20"
          vq="blockslices"
          show="$VALUE=blockslices"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <label loc="20 0" size="45 20"
            style="Label1"
            label="slabs:"
            />
          <entry loc="55 0" size="45 20"
            style="PlainText"
            vq="nsblock"
            vc="nsblock=$VALUE prep go('check')"
            set="$VALUE=nsblock"
            />
        </group>
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Block Slabs"
          vq="blockslices"
          vc="blockslices=1 go('check')"
          vc2="blockslices=0"
          set="$VALUE=blockslices"
          />
      </group>
      <check loc="5 160" size="115 20"
        style="PlainText"
        label="External Trigger"
        vq="ticks"
        vc="exists('nsblock','parameter'):$ex if ($ex=0) then create('nsblock','integer') endif ticks = 1 go('check')"
        vc2="exists('nsblock','parameter'):$ex if ($ex=0) then create('nsblock','integer') endif ticks = 0 nsblock=1 go('check')"
        set="$VALUE=(ticks&gt;0)"
        />
      <check loc="5 140" size="100 20"
        style="PlainText"
        label="RF spoiling"
        vq="rfspoil"
        vc="rfspoil='y' go('check')"
        vc2="rfspoil='n' go('check')"
        set="$VALUE=(rfspoil='y')"
        />
      <check loc="5 120" size="110 20"
        style="PlainText"
        label="Gradient Spoil"
        vq="spoilflag"
        vc="spoilflag='y' go('check')"
        vc2="spoilflag='n' go('check')"
        set="$VALUE=(spoilflag='y')"
        />
      <check loc="5 100" size="100 20"
        style="PlainText"
        label="PE Rewind"
        vq="perewind"
        vc="perewind='y' go('check')"
        vc2="perewind='n' go('check')"
        set="$VALUE=(perewind='y')"
        />
      <toggle loc="155 80" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=4"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=4)"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 60" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=3"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=3)"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 40" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=2"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=2)"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 20" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=1"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=1)"
        radiobutton="no"
        subtype="no"
        />
      <check loc="5 80" size="135 20"
        style="PlainText"
        label="Spatial Satbands"
        vq="sat"
        vc="sat='y' go('check')"
        vc2="sat='n' go('check')"
        set="$VALUE=(sat='y')"
        />
      <check loc="5 60" size="110 20"
        style="PlainText"
        label="Fatsat"
        vq="fsat Prescan_FatOffset"
        vc="fsat='y'  fsatfrq=fsatfrq pfsat=pfsat if (autooffset='y') then fsatfrq=Prescan_FatOffset endif go('check')"
        vc2="fsat='n' unarray('fsatfrq','pfsat') go('check')"
        set="$VALUE=(fsat='y')"
        />
      <check loc="5 40" size="110 20"
        style="PlainText"
        label="MTC"
        vq="mt"
        vc="mt='y' pmt=pmt mtfrq=mtfrq go('check')"
        vc2="mt='n' unarray('pmt','mtfrq') go('check')"
        set="$VALUE=(mt='y')"
        />
      <check loc="5 20" size="135 20"
        style="PlainText"
        label="Inversion Recovery"
        vq="ir"
        vc="ir='y' ti=ti go('check')"
        vc2="ir='n' unarray('ti') go('check')"
        set="$VALUE=(ir='y')"
        />
    </group>
    <group loc="5 0" size="210 230"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <textmessage loc="135 210" size="65 20"
        style="PlainText"
        label=" "
        vq="nv2"
        set="if nv2&lt;&gt;16 and nv2&lt;&gt;32 and nv2&lt;&gt;64 and nv2&lt;&gt;128 and nv2&lt;&gt;256 and nv2&lt;&gt;512 and nv2&lt;&gt;1024 then $VALUE=nv2 else $VALUE='' endif"
        />
      <menu loc="135 190" size="65 20"
        style="PlainText"
        label="menu"
        vq="nv2"
        vc="nv2=$VALUE prep go('check')"
        set="$VALUE=nv2"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="16"
          chval="16"
          />
        <mlabel 
          label="32"
          chval="32"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="96"
          chval="96"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="192"
          chval="192"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
      </menu>
      <label loc="5 170" size="195 20"
        style="PlainText"
        label="Data Matrix (RO x PE x PE2)"
        justify="Left"
        />
      <label loc="175 100" size="30 20"
        style="PlainText"
        label="deg"
        justify="Left"
        />
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="flip1"
        vc="flip1=$VALUE"
        set="$VALUE=flip1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 100" size="90 20"
        style="PlainText"
        label="Flip Angle"
        justify="Left"
        />
      <entry loc="115 120" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE go('check')"
        set="$VALUE=nt"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 120" size="110 20"
        style="PlainText"
        label="Averages"
        justify="Left"
        />
      <label loc="5 140" size="110 20"
        style="PlainText"
        label="Dummy Scans"
        justify="Left"
        />
      <entry loc="115 140" size="60 20"
        style="PlainText"
        vq="ssc"
        vc="ssc=$VALUE go('check')"
        set="$VALUE=ssc"
        digits="0"
        disable="Grayed out"
        />
      <menu loc="70 190" size="65 20"
        style="PlainText"
        label="menu"
        vq="nv"
        vc="nv=$VALUE prep go('check')"
        set="$VALUE=nv"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="32"
          chval="32"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="96"
          chval="96"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="192"
          chval="192"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
      </menu>
      <menu loc="5 190" size="65 20"
        style="PlainText"
        label="menu"
        vq="np"
        vc="np=$VALUE*2 go('check')"
        set="$VALUE=np/2"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="32"
          chval="32"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="96"
          chval="96"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="192"
          chval="192"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
      </menu>
      <textmessage loc="70 210" size="65 20"
        style="PlainText"
        label=" "
        vq="nv"
        set="if nv&lt;&gt;32 and nv&lt;&gt;64 and nv&lt;&gt;128 and nv&lt;&gt;256 and nv&lt;&gt;512 and nv&lt;&gt;1024 then $VALUE=nv else $VALUE='' endif"
        />
      <textmessage loc="5 210" size="65 20"
        style="PlainText"
        label=" "
        vq="np"
        set="if np&lt;&gt;64 and np&lt;&gt;128 and np&lt;&gt;256 and np&lt;&gt;512 and np&lt;&gt;1024 and np&lt;&gt;2048 then $VALUE=np/2 else $VALUE='' endif"
        />
      <check loc="65 40" size="50 20"
        style="PlainText"
        label="Min"
        vq="minte te"
        vc="minte='y' go('check')"
        vc2="minte='n'  go('check')"
        set="$VALUE=(minte='y')"
        />
      <check loc="65 20" size="50 20"
        style="PlainText"
        label="Min"
        vq="tr mintr"
        vc="mintr='y' go('check')"
        vc2="mintr='n' go('check')"
        set="$VALUE=(mintr='y')"
        />
      <entry loc="115 40" size="60 20"
        style="PlainText"
        vq="te minte"
        vc="te=$VALUE/1000 go('check')"
        set="$VALUE=te*1000.0"
        show="$VALUE = (minte='n')"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="115 20" size="60 20"
        style="PlainText"
        vq="tr mintr"
        vc="tr=$VALUE/1000.0 go('check')"
        set="$VALUE=tr*1000.0"
        show="$VALUE = (mintr='n') "
        digits="2"
        disable="Grayed out"
        />
      <label loc="175 40" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="175 20" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="5 40" size="30 20"
        style="PlainText"
        label="TE"
        justify="Left"
        />
      <label loc="5 20" size="30 20"
        style="PlainText"
        label="TR"
        justify="Left"
        />
    </group>
    <group loc="5 235" size="855 40"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="InfoGroup"
      useref="yes"
      subtype="Untitled"
      expanded="no"
      >
    </group>
  </group>
</template>
