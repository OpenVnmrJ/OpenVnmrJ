<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqShim" element="pages" type="acquisition" >
  <group size="840 280"
    label="Shim"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqShim"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="365 20" size="260 120"
      style="Heading1"
      label="Shimfiles"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ShimCalibration"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="15 90" size="130 20"
        style="Heading3"
        label="Zero all shims"
        vc="rts('reg0') setallshims"
        decor1="No"
        />
      <entry loc="155 25" size="90 20"
        style="PlainText"
        vq="shims"
        vc="shimfile='$VALUE'"
        set="$VALUE=shimfile"
        disable="Grayed out"
        />
      <label loc="15 25" size="130 20"
        style="Heading3"
        label="Shim filename"
        justify="Left"
        />
      <button loc="15 45" size="130 20"
        style="Heading3"
        label="Load shimfile"
        vc="rts(shimfile) setallshims"
        decor1="No"
        />
      <button loc="15 65" size="130 20"
        style="Heading3"
        label="Save shimfile"
        vq="shims"
        vc="svs(shimfile,'force') "
        decor1="No"
        />
    </group>
    <group loc="100 20" size="255 125"
      style="Heading1"
      label="Autoshim"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ShimCalibration"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="25 100" size="145 20"
        style="Heading3"
        label="Save Prescan Params"
        vc="svpshimprescan"
        decor1="No"
        />
      <button loc="25 70" size="145 20"
        style="Heading3"
        label="Restore Shims"
        vc="rts('start0') setallshims"
        decor1="No"
        />
      <button loc="25 45" size="145 20"
        style="Heading3"
        label="Load Shims"
        vc="qkshimset"
        decor1="No"
        />
      <button loc="25 20" size="145 20"
        style="Label4"
        label="Shim"
        vc="Prescan_ShimMethod='quickshim' prescan_control[1]=0 cnt=1 wexp='' werr='' qkshim"
        bg="darkGreen"
        decor1="No"
        />
      <check loc="190 45" size="60 20"
        style="PlainText"
        vc="shimload=1"
        vc2="shimload=0"
        set="$VALUE=(shimload=1)"
        show="on('shimcalc'):$VALUE"
        />
    </group>
    <group loc="100 150" size="525 100"
      style="Heading1"
      label="Shim Parameters"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="20 20" size="140 20"
        style="Label1"
        label="Iterations    "
        justify="Left"
        />
      <label loc="20 51" size="140 20"
        style="Label1"
        label="Shim step    (DAC)    "
        justify="Left"
        />
      <entry loc="160 51" size="50 20"
        style="PlainText"
        vq="shdel[1]"
        vc="shdel[1]=$VALUE"
        set="$VALUE=shdel[1]"
        show="on('shdel'):$VALUE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="240 51" size="50 20"
        style="PlainText"
        vq="shdel[2]"
        vc="shdel[2]=$VALUE"
        set="$VALUE=shdel[2]"
        show="on('shdel'):$VALUE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="320 51" size="50 20"
        style="PlainText"
        vq="shdel"
        vc="shdel[3]=$VALUE"
        set="$VALUE=shdel[3]"
        show="on('shdel'):$VALUE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="400 51" size="50 20"
        style="PlainText"
        vq="shdel[4]"
        vc="shdel[4]=$VALUE"
        set="$VALUE=shdel[4]"
        show="on('shdel'):$VALUE"
        digits="0"
        disable="Grayed out"
        />
      <check loc="160 21" size="60 20"
        style="PlainText"
        vc="shcycle[1]=1"
        vc2="shcycle[1]=0"
        set="$VALUE=(shcycle[1]=1)"
        />
      <check loc="240 21" size="60 20"
        style="PlainText"
        vc="shcycle[2]=1"
        vc2="shcycle[2]=0"
        set="$VALUE=(shcycle[2]=1)"
        />
      <check loc="320 21" size="60 20"
        style="PlainText"
        vc="shcycle[3]=1"
        vc2="shcycle[3]=0"
        set="$VALUE=(shcycle[3]=1)"
        />
      <check loc="400 21" size="60 20"
        style="PlainText"
        vc="shcycle[4]=1"
        vc2="shcycle[4]=0"
        set="$VALUE=(shcycle[4]=1)"
        />
    </group>
    <textmessage loc="0 0" size="150 20"
      style="Heading1"
      label="quickshim"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
