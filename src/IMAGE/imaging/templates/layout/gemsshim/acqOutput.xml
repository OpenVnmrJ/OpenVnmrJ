<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqOutput" element="pages" type="acquisition" >
  <group size="840 330" style="PlainText" label="Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqOutput"
    useref="no"
    expanded="yes"
    >
    <textfile loc="10 10" size="370 310" style="Label4"
      vq="userdir"
      set="$fll=userdir+'/gshimdir/data/gshim.out' exists($fll,'file'):$e if $e&lt;0.5 then write('reset',$fll) endif $VALUE=$fll"
      editable="yes"
      wrap="yes"
      />
  </group>
</template>
