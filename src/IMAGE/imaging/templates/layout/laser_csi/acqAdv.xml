<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqAdv" element="pages" type="acquisition" >
  <group size="800 280"
    label="Advanced"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqAdv"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <button loc="620 15" size="130 20"
      style="Protocol"
      label="Show RF &amp; Grad"
      vq="show_adv"
      vc="exists('show_adv','parameter'):$e if ($e=0) then create('show_adv','integer') endif show_adv = 1"
      bg="transparent"
      decor1="no"
      />
    <group loc="5 0" size="210 270"
      style="Heading1"
      label="Acquisition"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Accquisition_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <textmessage loc="115 40" size="60 20"
        style="PlainText"
        label="399.415"
        vq="sfrq"
        set="$VALUE=sfrq"
        digits="3"
        />
      <group loc="5 230" size="200 30"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="isis1D"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="120 5" size="50 20"
          style="PlainText"
          label="csi"
          vq="csi"
          vc="csi='y' go('check')"
          vc2="csi='n' go('check')"
          set="$VALUE=(csi='y')"
          />
        <label loc="5 5" size="90 20"
          style="Label1"
          label="CSI on"
          />
      </group>
      <group loc="5 200" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="nt"
          vc="nt = $VALUE"
          set="$VALUE=nt"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Averages"
          />
      </group>
      <textmessage loc="115 60" size="60 20"
        style="PlainText"
        label="0.0"
        vq="resto restol"
        set="$VALUE=restol"
        digits="1"
        />
      <label loc="5 100" size="110 20"
        style="Label1"
        label="Complex points"
        justify="Left"
        />
      <label loc="175 120" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="175 140" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2 go('check')"
        set="$VALUE=np/2"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="115 120" size="60 20"
        style="PlainText"
        vq="at"
        vc="at=$VALUE/1000.0"
        set="$VALUE=at*1000.0"
        digits="2"
        disable="Grayed out"
        />
      <label loc="5 120" size="110 20"
        style="Label1"
        label="Acquisition Time"
        justify="Left"
        />
      <label loc="5 140" size="110 20"
        style="Label1"
        label="Receiver Gain"
        justify="Left"
        />
      <entry loc="115 140" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        digits="0"
        disable="Grayed out"
        />
      <menu loc="115 160" size="60 20"
        style="PlainText"
        label="menu"
        vc="presig = '$VALUE'"
        set="$VALUE = presig"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="high"
          chval="h"
          />
        <mlabel 
          label="low"
          chval="l"
          />
      </menu>
      <label loc="5 160" size="105 20"
        style="Label1"
        label="presig"
        justify="Left"
        />
      <group loc="5 180" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="170 0" size="25 20"
          style="Label1"
          label="%"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="glim"
          vc="glim = $VALUE"
          set="$VALUE=glim"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Glim"
          />
      </group>
      <entry loc="115 80" size="60 20"
        style="PlainText"
        vq="sw"
        vc="$np = np sw=$VALUE np=$np"
        set="$VALUE=sw"
        digits="2"
        disable="Grayed out"
        />
      <label loc="175 40" size="30 20"
        style="Label1"
        label="MHz"
        justify="Left"
        />
      <label loc="175 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 80" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <menu loc="115 20" size="75 20"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Proton"
          chval="H1"
          />
        <mlabel 
          label="F19"
          chval="F19"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="Na23"
          chval="Na23"
          />
        <mlabel 
          label="Xe129"
          chval="Xe129"
          />
      </menu>
      <label loc="5 20" size="110 20"
        style="Label1"
        label="Nucleus"
        justify="Left"
        />
      <label loc="6 80" size="110 20"
        style="Label1"
        label="Spectral Width"
        justify="Left"
        />
      <label loc="6 60" size="110 20"
        style="Label1"
        label="Local Offset"
        justify="Left"
        />
      <label loc="6 40" size="110 20"
        style="Label1"
        label="Obs Frequency"
        justify="Left"
        />
    </group>
    <group loc="215 225" size="575 45"
      style="Heading1"
      label="Block Processing"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BlockProcessing"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="5 20" size="110 20"
        style="Label1"
        label="Processing"
        justify="Left"
        />
      <entry loc="115 20" size="270 20"
        style="PlainText"
        vq="wbs bs nt"
        vc="wbs='$VALUE' wbs(wbs)"
        set="$VALUE=wbs"
        show="$VALUE = (nt &gt; 1) AND (bs &lt; nt)"
        digits="0"
        disable="Label"
        />
      <label loc="400 20" size="110 20"
        style="Label1"
        label="Block Size"
        justify="Left"
        />
      <entry loc="510 20" size="60 20"
        style="PlainText"
        vq="bs"
        vc="bs=$VALUE"
        set="$VALUE=bs"
        digits="0"
        disable="Grayed out"
        />
    </group>
    <group loc="215 0" size="215 125"
      style="Heading1"
      label="Profiles1D"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Profile"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <radio loc="5 55" size="55 20"
        style="PlainText"
        label="read"
        vq="rprof pprof sprof"
        vc="rprof=1 pprof=0 sprof=0"
        vc2="rprof=0"
        set="$VALUE=(rprof=1)"
        />
      <radio loc="5 70" size="65 20"
        style="PlainText"
        label="phase"
        vq="rprof pprof sprof"
        vc="pprof=1 rprof=0 sprof=0"
        vc2="pprof=0"
        set="$VALUE=(pprof=1)"
        />
      <radio loc="5 85" size="50 20"
        style="PlainText"
        label="slice"
        vq="rprof pprof sprof"
        vc="sprof=1 rprof=0 pprof=0"
        vc2="sprof=0"
        set="$VALUE=(sprof=1)"
        />
      <check loc="5 105" size="50 20"
        style="PlainText"
        label="OVS"
        vq="ovs"
        vc="ovs='y' sat='n' go('check')"
        vc2="ovs='n' go('check')"
        set="$VALUE=(ovs='y')"
        />
      <button loc="70 100" size="100 20"
        style="Label4"
        label="Combo"
        vq="profile_ovs profile_vox"
        vc="profile_ovs='y','n' testprofile1D axis='c' "
        bg="Ready"
        decor1="no"
        />
      <button loc="70 80" size="100 20"
        style="Label4"
        label="Profile"
        vq="profile_ovs"
        vc="profile_ovs='y' testprofile1D axis='c' "
        bg="Ready"
        decor1="no"
        />
      <button loc="70 60" size="100 20"
        style="Label4"
        label="Voxel Profile"
        vq="profile_vox profile_ovs"
        vc="profile_ovs='n' testprofile1D axis='c'"
        bg="Ready"
        decor1="no"
        />
      <label loc="70 15" size="50 20"
        style="Label1"
        label="FOV"
        />
      <label loc="120 15" size="50 20"
        style="Label1"
        label="Offset"
        />
      <entry loc="70 35" size="50 20"
        style="PlainText"
        vq="lro"
        vc="lro=$VALUE/10 go('check')"
        set="$VALUE=10*lro"
        digits="1"
        />
      <entry loc="120 35" size="50 20"
        style="PlainText"
        vq="pro"
        vc="pro=$VALUE/10.0"
        set="$VALUE=pro*10.0"
        digits="2"
        />
      <label loc="170 35" size="30 20"
        style="Label1"
        label="mm"
        />
    </group>
    <group loc="215 125" size="215 100"
      style="Heading1"
      label="OVS"
      vq="ovs"
      show="$VALUE=(ovs='y')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="zsetup"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="10 78" size="205 20"
        label="OVS"
        vq="ovs"
        show="$VALUE=(ovs='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="OVS"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <entry loc="135 0" size="30 20"
          style="PlainText"
          vq="ovsgap"
          vc="ovsgap=$VALUE/10"
          set="$VALUE=ovsgap*10"
          digits="1"
          />
        <label loc="100 0" size="35 20"
          style="Label1"
          label="Gap"
          />
        <entry loc="40 0" size="35 20"
          style="PlainText"
          vq="ovsthk"
          vc="ovsthk=$VALUE"
          set="$VALUE=ovsthk"
          digits="1"
          />
        <label loc="0 0" size="35 20"
          style="Label1"
          label="Thk"
          />
        <label loc="170 0" size="30 20"
          style="Label1"
          label="mm"
          />
      </group>
      <group loc="10 38" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="50 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
        <label loc="60 0" size="35 20"
          style="Label1"
          label="Pwr"
          justify="Left"
          />
        <label loc="95 0" size="40 20"
          style="Label1"
          label="Angle"
          justify="Left"
          />
      </group>
      <group loc="10 18" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="140 0" size="20 20"
          style="Label1"
          label="X"
          justify="Center"
          />
        <entry loc="0 0" size="60 20"
          style="PlainText"
          vq="satpat"
          vc="satpat=$VALUE"
          set="$VALUE=satpat"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="165 0" size="35 20"
          style="PlainText"
          vq="ovsflipf"
          vc="ovsflipf=$VALUE"
          set="$VALUE=ovsflipf"
          show="on('ovsflipf'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="60 0" size="35 20"
          style="PlainText"
          vq="satpwr"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          show="on('satpwr'):$VALUE"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="95 0" size="40 20"
          style="PlainText"
          vq="flipsat"
          vc="flipsat=$VALUE"
          set="$VALUE=flipsat"
          digits="0"
          disable="Grayed out"
          />
      </group>
    </group>
  </group>
</template>
