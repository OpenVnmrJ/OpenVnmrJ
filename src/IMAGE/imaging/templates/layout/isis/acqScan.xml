<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="900 280"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Setup"
      vq="showOptions"
      show="if (showOptions=1) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SetupP12isis"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Sat Bands"
      vq="sat showOptions"
      show="if (showOptions=4) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Satbands"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Plan &amp; Display"
      vq="showOptions"
      show="if (showOptions=5) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MoreFOVPlan1D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="435 0" size="210 230"
      style="Heading1"
      label="Voxel Plan"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FOVPlan1D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="220 0" size="210 230"
      style="Heading1"
      label="Scan Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="a_1ScanOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="10 120" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="130 20"
          style="PlainText"
          label="External Trigger"
          vc="ticks=1 go('check')"
          vc2="ticks=0 go('check')"
          set="$VALUE=(ticks &gt; 0)"
          tooltip="if 'y', phase cycle =nt*array, if &quot;no&quot; phase cycle=nt only"
          />
      </group>
      <group loc="10 100" size="195 20"
        label="OVS"
        vq="ovs"
        show="$VALUE=(ovs='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="OVS"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <entry loc="46 0" size="35 20"
          style="PlainText"
          vq="ovsthk"
          vc="ovsthk=$VALUE"
          set="$VALUE=ovsthk"
          digits="1"
          />
        <entry loc="126 0" size="35 20"
          style="PlainText"
          vq="ovsgap"
          vc="ovsgap=$VALUE/10"
          set="$VALUE=ovsgap*10"
          digits="1"
          />
        <label loc="166 0" size="30 20"
          style="Label1"
          label="mm"
          />
        <label loc="96 0" size="35 20"
          style="Label1"
          label="Gap"
          />
        <label loc="16 0" size="35 20"
          style="Label1"
          label="Thk"
          />
      </group>
      <check loc="10 80" size="190 20"
        style="PlainText"
        label="Outer Volume Suppression"
        vq="ovs"
        vc="ovs='y' sat='n' if (showOptions=4) then showOptions=0 endif"
        vc2="ovs='n'"
        set="$VALUE=(ovs='y')"
        />
      <toggle loc="145 60" size="35 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=4"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=4)"
        radiobutton="no"
        subtype="no"
        />
      <check loc="10 60" size="130 20"
        style="PlainText"
        label="Spatial Satbands"
        vq="sat"
        vc="sat='y' ovs='n' go('check')"
        vc2="sat='n' go('check')"
        set="$VALUE=(sat='y')"
        />
      <group loc="5 40" size="195 20"
        vq="wsrf"
        show="$VALUE=(wsrf='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="5 0" size="175 20"
          style="PlainText"
          label="Acquire Water Reference"
          vq="wref"
          vc="wref='y' go('check') wrefstatus='wref'"
          vc2="wref='n' go('check')"
          set="$VALUE = (wref='y')"
          show="exists('wsrf','parameter'):$VALUE"
          tooltip="Acquires and saves water reference"
          />
      </group>
      <check loc="10 20" size="115 20"
        style="PlainText"
        label="Water Sup On"
        vq="ws wsrf wref"
        vc="ws ='y' wsrf='y' go('check')"
        vc2="ws ='y' wsrf='n' go('check') wref='n'"
        set="$VALUE=(ws = 'y' and wsrf='y')"
        />
    </group>
    <group loc="5 0" size="210 230"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 185" size="120 20"
        style="PlainText"
        label="Water Sup On"
        vq="ws wsrf wref"
        vc="ws ='y' wsrf='y' go('check')"
        vc2="ws ='y' wsrf='n' go('check') wref='n'"
        set="$VALUE=(ws = 'y' and wsrf='y')"
        />
      <group loc="5 205" size="195 20"
        vq="wsrf"
        show="$VALUE=(wsrf='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="0 0" size="175 20"
          style="PlainText"
          label="Acquire Water Reference"
          vq="wref"
          vc="wref='y' go('check') wrefstatus='wref'"
          vc2="wref='n' go('check')"
          set="$VALUE = (wref='y')"
          show="exists('wsrf','parameter'):$VALUE"
          tooltip="Acquires and saves water reference"
          />
      </group>
      <group loc="5 160" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <label loc="0 0" size="100 20"
          style="PlainText"
          label="Spectral Width"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="sw"
          vc="$np = np sw = $VALUE np = $np go('check')"
          set="$VALUE=sw"
          digits="0"
          disable="Grayed out"
          />
      </group>
      <group loc="5 80" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="100 20"
          style="PlainText"
          label="Receiver Gain"
          justify="Left"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="dB"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="gain"
          vc="gain=$VALUE"
          set="$VALUE=gain"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <entry loc="115 140" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=np/2"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 140" size="110 20"
        style="PlainText"
        label="Complex Pairs"
        justify="Left"
        />
      <entry loc="115 120" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE go('check')"
        set="$VALUE=nt"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 120" size="85 20"
        style="PlainText"
        label="Averages"
        justify="Left"
        />
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="80 20"
          style="PlainText"
          label="TI delay"
          justify="Left"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="tidel"
          vc="tidel=$VALUE/1000 go('check')"
          set="$VALUE=tidel*1000.0"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <group loc="5 40" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="30 20"
          style="PlainText"
          label="TM"
          justify="Left"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="tm"
          vc="tm=$VALUE/1000 go('check')"
          set="$VALUE=tm*1000.0"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE go('check')"
        set="$VALUE=ss"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 100" size="100 20"
        style="PlainText"
        label="Dummy Scans"
        justify="Left"
        />
      <entry loc="115 20" size="60 20"
        style="PlainText"
        vq="tr mintr"
        vc="tr=$VALUE/1000.0 go('check')"
        set="$VALUE=tr*1000.0"
        digits="2"
        disable="Grayed out"
        />
      <label loc="175 20" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="5 20" size="30 20"
        style="PlainText"
        label="TR"
        justify="Left"
        />
    </group>
    <group loc="5 235" size="855 40"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="InfoGroup"
      useref="yes"
      subtype="Untitled"
      expanded="no"
      >
    </group>
  </group>
</template>
