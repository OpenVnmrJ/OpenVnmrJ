<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Process" element="panels" type="processing" >
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="ASL"
    show="$VALUE=(asl='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procASL"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Recon"
    vc="if (allvolumes = 'y') then aipXrecon('allvolumes') endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Drecon"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Weighting"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dweight"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Nifti"
    vq="niftipage"
    vc="aipXrecon('genpars') aipXrecon('nifti')"
    show="$VALUE=(niftipage='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procNifti"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="860 280"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
</template>
