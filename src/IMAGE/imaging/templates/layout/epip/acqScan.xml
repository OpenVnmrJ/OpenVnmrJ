        vq="show_grad"
        vc="show_grad = 1"
        vc2="show_grad = 0"
        set="$VALUE=show_grad"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <button loc="10 40" size="80 20"
        style="PlainText"
        label="Check"
        vc="im2Depi('prep','check')"
        bg="transparent"
        decor1="yes"
        />
    </group>
    <group loc="860 10" size="90 60"
      vq="showOptions"
      show="$VALUE=(showOptions&lt;&gt;0)"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <toggle loc="10 20" size="80 20"
        style="PlainText"
        label="show Grad"
        vq="show_grad"
        vc="show_grad = 1"
        vc2="show_grad = 0"
        set="$VALUE=show_grad"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="10 0" size="80 20"
        style="PlainText"
        label="show RF"
        vq="show_rf"
        vc="show_rf = 1"
        vc2="show_rf = 0"
        set="$VALUE=show_rf"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <button loc="10 40" size="80 20"
        style="PlainText"
        label="Check"
        vc="im2Depi('prep','check')"
        bg="transparent"
        decor1="yes"
        />
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Inversion Recovery"
      vq="ir showOptions"
      show="if (showOptions=1) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="InversionRecovery"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Magnetization Transfer"
