    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Slice Plan"
      vq="iplanType showOptions"
      show="if (showOptions=5) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MoreFOVPlan2D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Spin-Stimulated-echo"
      vq="iplanType showOptions"
      show="if (showOptions=7) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
