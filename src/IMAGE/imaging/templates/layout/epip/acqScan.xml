<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="950 280"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="220 0" size="210 230"
      style="Heading1"
      label="Scan Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ScanOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 140" size="200 20"
        show="$VALUE=1"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <toggle loc="150 0" size="45 20"
          style="PlainText"
          label="More"
          vq="showOptions"
          vc="showOptions=7"
          vc2="showOptions=0"
          set="$VALUE=(showOptions=7)"
          enable="yes"
          radiobutton="no"
          subtype="no"
          />
        <check loc="0 0" size="160 20"
          style="PlainText"
          label="Spin-Stimulated-echo"
          vq="spinecho spinstimecho"
          vc="spinstimecho='y' spinecho='n' teSave=te im2Depi('prep','check')"
          vc2="spinstimecho='n' te=teSave im2Depi('prep','check')"
          set="$VALUE=(spinstimecho='y')"
          />
      </group>
      <toggle loc="155 20" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=8"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=8)"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <check loc="5 120" size="95 20"
        style="PlainText"
        label="Diffusion"
        vq="diff"
        vc="diff='y' diffarray('set') im2Depi('prep','check')"
        vc2="diff='n' diffarray('save') zero_gf im2Depi('prep','check')"
        set="$VALUE=(diff='y')"
        />
      <check loc="5 100" size="160 20"
        style="PlainText"
        label="Spatial Sat"
        vq="sat"
        vc="sat='y' im2Depi('prep','check')"
        vc2="sat='n' im2Depi('prep','check')"
        set="$VALUE=(sat='y')"
        />
      <check loc="5 80" size="120 20"
        style="PlainText"
        label="Fatsat"
        vq="fsat Prescan_FatOffset"
        vc="fsat='y'  fsatfrq=fsatfrq pfsat=pfsat if (autooffset='y') then fsatfrq=Prescan_FatOffset endif im2Depi('prep','check')"
        vc2="fsat='n' unarray('fsatfrq','pfsat')   im2Depi('prep','check')"
        set="$VALUE=(fsat='y')"
        />
      <check loc="5 60" size="120 20"
        style="PlainText"
        label="MTC"
        vq="mt"
        vc="mt='y' pmt=pmt mtfrq=mtfrq im2Depi('prep','check')"
        vc2="mt='n' unarray('pmt','mtfrq') im2Depi('prep','check')"
        set="$VALUE=(mt='y')"
        />
      <toggle loc="155 120" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=6"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=6)"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 100" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=4"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=4)"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 80" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=3"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=3)"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 60" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=2"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=2)"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="155 40" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=1"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=1)"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <check loc="5 40" size="160 20"
        style="PlainText"
        label="Inversion Recovery"
        vq="ir"
        vc="ir='y' ti=ti im2Depi('prep','check')"
        vc2="ir='n' unarray('ti') im2Depi('prep','check')"
        set="$VALUE=(ir='y')"
        />
      <check loc="5 160" size="160 20"
        style="PlainText"
        label="Arterial Spin Labelling"
        vq="asl"
        vc="asl='y' aslset('on') im2Depi('prep','check')"
        vc2="asl='n' aslset('off') im2Depi('prep','check')"
        set="$VALUE=(asl='y')"
        />
      <check loc="5 180" size="180 20"
        style="PlainText"
        label="External Trigger"
        vq="ticks"
        vc="ticks = 1"
        vc2="ticks = 0"
        set="$VALUE=(ticks &gt; 0)"
        />
      <group loc="5 200" size="200 20"
        vq="ticks ir"
        show="$VALUE=(ticks&gt;0 OR ir='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <group loc="95 0" size="100 20"
          vq="blockslices"
          show="$VALUE=blockslices"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <entry loc="55 0" size="45 20"
            style="PlainText"
            vq="nsblock"
            vc="nsblock=$VALUE prep go('check')"
            set="$VALUE=nsblock"
            />
          <label loc="15 0" size="45 20"
            style="Label1"
            label="slices:"
            />
        </group>
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Block Slices"
          vq="blockslices"
          vc="blockslices=1 go('check')"
          vc2="blockslices=0"
          set="$VALUE=blockslices"
          />
      </group>
      <check loc="5 20" size="120 20"
        style="PlainText"
        label="Spin-echo"
        vq="spinecho spinstimecho"
        vc="spinecho='y' spinstimecho='n' im2Depi('prep','check')"
        vc2="spinecho='n' im2Depi('prep','check')"
        set="$VALUE=(spinecho='y')"
        />
    </group>
    <group loc="645 10" size="90 60"
      vq="showOptions"
      show="$VALUE=(showOptions=0)"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <toggle loc="10 0" size="80 20"
        style="PlainText"
        label="show RF"
        vq="show_rf"
        vc="show_rf = 1"
        vc2="show_rf = 0"
        set="$VALUE=show_rf"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="10 20" size="80 20"
        style="PlainText"
