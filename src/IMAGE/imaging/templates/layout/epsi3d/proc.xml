<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Process" element="panels" type="processing" >
  <group loc="220 165" size="305 120"
    style="Heading1"
    label="Phase and baseline corrections"
    vq="csiShowOpt specDisMode dmg"
    show="$VALUE=(csiShowOpt[3]&lt;1 and specDisMode=3 and dmg='ph')"
    bg="transparent"
    border="Etched"
    tab="no"
    enable="no"
    reference="Phaseandbaselinecorrections"
    useref="no"
    subtype="Titled"
    expanded="yes"
    >
    <menu loc="115 50" size="180 20"
      style="PlainText"
      label="baseline correction"
      vq="bcmode"
      vc="bcmode[1]=$VALUE csiCmd('bc','sel')"
      set="$VALUE=bcmode[1]"
      bg="VJBackground"
      editable="No"
      >
      <mlabel 
        label="none"
        chval="-1"
        />
      <mlabel 
        label="spline"
        chval="1"
        />
      <mlabel 
        label="polynomial"
        chval="2"
        />
      <mlabel 
        label="CWT/Whittaker smoother"
        chval="0"
        />
    </menu>
    <group loc="10 70" size="290 45"
      vq="bcmode"
      show="$SHOW=(bcmode[1]=0)"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <slider loc="50 1" size="240 25"
        style="PlainText"
        vq="bcmode"
        vc="bcmode[2]=$VALUE csiCmd('bc','dis')"
        set="$VALUE=bcmode[2]"
        digital="true"
        min="1"
        max="200"
        incr1="1"
        incr2="10"
        digits="7"
        statpar="bcmode"
        drag="500"
        tooltip="CWT scaling factor"
        />
      <label loc="0 21" size="50 20"
        style="Label1"
        label="smooth"
        />
      <label loc="0 1" size="50 20"
        style="Label1"
        label="scale"
        />
      <slider loc="50 21" size="240 20"
        style="PlainText"
        vq="bcmode"
        vc="bcmode[3]=$VALUE csiCmd('bc','dis')"
        set="$VALUE=bcmode[3]"
        digital="true"
        min="1"
        max="10000"
        incr1="5"
        incr2="50"
        digits="7"
        statpar="bcmode"
        drag="500"
        tooltip="Whittaker smooth factor"
        />
    </group>
    <check loc="30 25" size="70 20"
      style="PlainText"
      label="uniform"
      vq="blockph"
      vc="blockph=0"
      vc2="blockph=1"
      set="$VALUE=(blockph=0)"
      />
    <button loc="115 25" size="90 20"
      style="PlainText"
      label="auto phase"
      vc="csiCmd('phase')"
      bg="VJBackground"
      decor1="yes"
      />
    <group loc="10 70" size="290 40"
      vq="bcmode"
      show="$SHOW=(bcmode[1]=2)"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="0 5" size="45 20"
        style="PlainText"
        label="order:"
        vq="null bcmode"
        subtype="parameter"
        />
      <slider loc="50 5" size="230 25"
        style="PlainText"
        vq="bcmode"
        vc="bcmode[4]=$VALUE bcpopup('dis')"
        set="$VALUE=bcmode[4]"
        digital="true"
        min="1"
        max="20"
        incr1="1"
        incr2="2"
        digits="5"
        statpar="bcmode"
        drag="500"
        tooltip="polynomial order"
        />
    </group>
    <label loc="10 50" size="120 20"
      style="Label1"
      label="Select BC method:"
      />
  </group>
  <group loc="0 0" size="855 295"
    style="PlainText"
    label="Process"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procCSIrecon"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="605 250"
    style="PlainText"
    label="Process More"
    show="$VALUE=(jviewport=3)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procCSIrecon1"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="5 5" size="780 210"
    style="PlainText"
    label="CSI Display"
    vq="panellevel"
    show="csiCmd('getCSIPath',file):$p if($p='') then $SHOW=0 else $SHOW=1 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="CSIDisplay"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="860 280"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    >
  </group>
</template>
