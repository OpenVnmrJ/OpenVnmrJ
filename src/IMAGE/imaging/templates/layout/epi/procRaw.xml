<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="procRaw" element="pages" type="processing" >
  <group size="800 270"
    label="Raw data"
    bg="transparent"
    border="None"
    tab="yes"
    enable="no"
    reference="procRaw"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="0 0" size="490 140"
      style="Heading1"
      label="Select Data"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="selectimage"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 45" size="470 40"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="selectimage"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <label loc="0 20" size="115 20"
          style="Label1"
          label="Array element"
          justify="Left"
          />
        <slider loc="120 0" size="320 20"
          style="PlainText"
          vq="ns slice imgno"
          vc="slice = $VALUE imgno=(element-1)*ns+slice df2d('nf',imgno)"
          set="$VALUE = slice"
          digital="true"
          min="1"
          max="$VALUE = ns"
          digits="4"
          />
        <slider loc="120 20" size="320 20"
          style="PlainText"
          vq="arraydim element imgno"
          vc="element = $VALUE imgno=(element-1)*ns+slice df2d('nf',imgno)"
          set="$VALUE = element"
          digital="true"
          min="1"
          max="exists('flash_converted','parameter'):$ex if ($ex&gt;0) then $VALUE=arraydim/ns else $VALUE = arraydim endif"
          digits="4"
          />
        <label loc="0 0" size="75 20"
          style="Label1"
          label="Slice"
          justify="Left"
          />
      </group>
      <button loc="5 20" size="120 20"
        style="Heading3"
        label="Preprocess"
        vc="epirfall epiphms imgno=(element-1)*ns+slice df2d('nf',imgno)"
        decor1="No"
        />
      <button loc="5 115" size="120 20"
        style="Heading3"
        label="Refresh display"
        vc="f full imconi"
        decor1="No"
        />
      <button loc="5 90" size="120 20"
        style="Heading3"
        label="Display k-space"
        vc="f full imgno=(element-1)*ns+slice ph av1 df2d('nf',imgno)"
        decor1="No"
        />
    </group>
  </group>
</template>
