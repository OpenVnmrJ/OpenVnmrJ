<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Process" element="panels" type="processing" >
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Recon"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Drecon"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="430 270"
    style="PlainText"
    label="Weighting"
    vc="exists('gf_factor','parameter'):$e if ($e=0) then create('gf_factor') gf_factor=10 endif exists('sb_factor','parameter'):$e if ($e=0) then create('sb_factor') sb_factor=50 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dweight"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Profile"
    vq="profile"
    show="$str='' substr(profile,1,1):$str $VALUE =($str = 'y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procProfile"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="500 190"
    style="PlainText"
    label="Interactive"
    vq="recon"
    show="$VALUE=(recon='' OR recon='internal')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dinteract"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Nifti"
    vq="niftipage"
    vc="aipXrecon('genpars') aipXrecon('nifti')"
    show="$VALUE=(niftipage='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procNifti"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Mask"
    vc="aip2Dsmap('genpars')"
    vq="maskpage"
    show="$VALUE=(maskpage='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dmask"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Sensitivity Map"
    vc="aip2Dsmap('genpars')"
    vq="smappage"
    show="aipXrecon('nrcvrs'):$nr $VALUE=($nr>1) AND (smappage='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dsmap"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Sense"
    vc="aip2Dsmap('genpars') aip2Dsmap('refdir')"
    vq="sense"
    show="$VALUE=(sense='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dsense"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="726 222"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
</template>
