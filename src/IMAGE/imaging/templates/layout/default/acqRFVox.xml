<template name="acqRFVox" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="RF Pulses"
    vq="panellevel show_adv"
    show="$VALUE = (panellevel &gt; 0) AND (show_adv &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqRFVox"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <button loc="470 115" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_adv = 0"
      bg="transparent"
      decor1="no"
      />
    <group loc="5 0" size="415 135"
      style="Heading1"
      label="RF pulses"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="RFpulses"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 100" size="395 20"
        vq="sat"
        show="exists('sat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="295 0" size="60 20"
          style="PlainText"
          label="45.77"
          vq="satpwr satpwrf"
          set="calc_power(satpwr,satpwrf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="satpat"
          vc="satpat='$VALUE' "
          set="$VALUE=satpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="psat"
          vc="psat=$VALUE "
          set="$VALUE=psat"
          digits="1"
          disable="Grayed out"
          />
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Sat Bands"
          vq="sat"
          vc="sat = 'y'"
          vc2="sat = 'n'"
          set="$VALUE = (sat = 'y')"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipsat"
          vc="flipsat=$VALUE go('check')"
          set="$VALUE=flipsat"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 80" size="395 20"
        vq="sat"
        show="exists('sat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="OVS"
          vq="ovs"
          vc="ovs = 'y'"
          vc2="ovs = 'n'"
          set="$VALUE = (ovs = 'y')"
          />
        <textmessage loc="295 0" size="60 20"
          style="PlainText"
          label="45.77"
          vq="satpwr satpwrf"
          set="calc_power(satpwr,satpwrf):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="satpat"
          vc="satpat='$VALUE' "
          set="$VALUE=satpat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="psat"
          vc="psat=$VALUE "
          set="$VALUE=psat"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flipsat"
          vc="flipsat=$VALUE go('check')"
          set="$VALUE=flipsat"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 60" size="395 20"
        vq="spinecho"
        show="exists('spinecho','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="295 0" size="60 20"
          style="PlainText"
          label="42.02"
          vq="tpwr2 tpwr2f"
          set="calc_power(tpwr2,tpwr2f):$VALUE"
          digits="2"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="p2"
          vc="p2=$VALUE "
          set="$VALUE=p2"
          digits="1"
          disable="Grayed out"
          />
        <check loc="0 0" size="100 20"
          style="PlainText"
          label="Refocus"
          vq="spinecho"
          vc="spinecho = 'y'"
          vc2="spinecho = 'n'"
          set="$VALUE = (spinecho = 'y')"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flip2"
          vc="flip2=$VALUE go('check')"
          set="$VALUE=flip2"
          disable="Grayed out"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="p2pat"
          vc="p2pat='$VALUE'  "
          set="$VALUE=p2pat"
          disable="Grayed out"
          />
      </group>
      <group loc="105 20" size="295 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="195 0" size="85 20"
          style="Label1"
          label="Power (dB)"
          justify="Left"
          />
        <label loc="130 0" size="65 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
        <label loc="65 0" size="65 20"
          style="Label1"
          label="Width (us)"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Flipangle"
          justify="Left"
          />
      </group>
      <group loc="5 40" size="395 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="295 0" size="60 20"
          style="PlainText"
          label="36.00"
          vq="tpwr1 tpwr1f"
          set="calc_power(tpwr1,tpwr1f):$VALUE"
          digits="2"
          />
        <entry loc="230 0" size="60 20"
          style="PlainText"
          vq="p1pat"
          vc="p1pat='$VALUE'  "
          set="$VALUE=p1pat"
          disable="Grayed out"
          />
        <entry loc="165 0" size="60 20"
          style="PlainText"
          vq="p1"
          vc="p1=$VALUE"
          set="$VALUE=p1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="flip1"
          vc="flip1=$VALUE go('check')"
          set="$VALUE=flip1"
          disable="Grayed out"
          />
        <label loc="16 0" size="85 20"
          style="Label1"
          label="Excitation"
          justify="Left"
          />
      </group>
    </group>
  </group>
</template>
