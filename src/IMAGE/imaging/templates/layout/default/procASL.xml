<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="procASL" element="pages" type="processing" >
  <group size="800 270"
    style="PlainText"
    label="ASL"
    show="$VALUE=(asl='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procASL"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <check loc="5 95" size="210 20"
      style="PlainText"
      label="All difference images"
      vq="aslsub"
      vc="aslsub[2] = 'y'"
      vc2="aslsub[2] = 'n'"
      set="$VALUE = (aslsub[2] = 'y')"
      />
    <group loc="5 0" size="210 90"
      style="Heading1"
      label="Subtraction Scheme"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="junk"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <radio loc="5 65" size="120 20"
        style="PlainText"
        label="Surround"
        vq="aslsub"
        vc="aslsub[1] = 'surr'"
        set="$VALUE = (aslsub[1] = 'surr')"
        />
      <radio loc="5 45" size="120 20"
        style="PlainText"
        label="Adjacent"
        vq="aslsub"
        vc="aslsub[1] = 'adj'"
        set="$VALUE = (aslsub[1] = 'adj')"
        />
      <radio loc="5 25" size="120 20"
        style="PlainText"
        label="Pairwise"
        vq="aslsub"
        vc="aslsub[1] = 'pair'"
        set="$VALUE = (aslsub[1] = 'pair')"
        />
    </group>
    <button loc="235 10" size="170 20"
      style="Heading3"
      label="Subtract"
      vc="aslcalc"
      bg="transparent"
      decor1="yes"
      />
  </group>
</template>
