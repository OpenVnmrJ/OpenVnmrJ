<template name="acqAcq" element="pages" type="acquisition" >
  <group size="800 270"
    label="Acq"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqAcq"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="5 0" size="210 240"
      style="Heading1"
      label="Acquisition"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Accquisition_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 200" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="glim"
          vc="glim = $VALUE"
          set="$VALUE=glim"
          digits="2"
          disable="Grayed out"
          />
        <label loc="170 0" size="25 20"
          style="Label1"
          label="%"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Glim"
          />
      </group>
      <entry loc="115 180" size="60 20"
        style="PlainText"
        vq="sscprescan"
        vc="sscprescan=$VALUE"
        set="$VALUE=sscprescan"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 180" size="100 20"
        style="Label1"
        label="Prescan dummy"
        justify="Left"
        />
      <label loc="175 40" size="30 20"
        style="Label1"
        label="MHz"
        justify="Left"
        />
      <label loc="175 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 80" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 120" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="175 100" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <menu loc="115 20" size="75 20"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        editable="No"
        >
        <mlabel 
          label="Proton"
          chval="H1"
          />
        <mlabel 
          label="F19"
          chval="F19"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="Na23"
          chval="Na23"
          />
        <mlabel 
          label="Xe129"
          chval="Xe129"
          />
      </menu>
      <textmessage loc="115 40" size="60 20"
        style="PlainText"
        label="400.422"
        vq="sfrq"
        set="$VALUE=sfrq"
        digits="3"
        />
      <entry loc="115 60" size="60 20"
        style="PlainText"
        vq="resto"
        vc="resto=$VALUE"
        set="$VALUE=resto"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="115 80" size="60 20"
        style="PlainText"
        vq="sw"
        vc="sw=$VALUE"
        set="$VALUE=sw"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="at"
        vc="at=$VALUE/1000.0"
        set="$VALUE=at*1000.0"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="115 120" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        digits="0"
        disable="Grayed out"
        />
      <menu loc="115 140" size="60 20"
        style="PlainText"
        label="menu"
        vc="presig = '$VALUE'"
        set="$VALUE = presig"
        editable="No"
        >
        <mlabel 
          label="high"
          chval="h"
          />
        <mlabel 
          label="low"
          chval="l"
          />
      </menu>
      <entry loc="115 160" size="60 20"
        style="PlainText"
        vq="seqcon"
        vc="seqcon='$VALUE'"
        set="$VALUE=seqcon"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 160" size="100 20"
        style="Label1"
        label="seqcon"
        justify="Left"
        />
      <label loc="5 140" size="110 20"
        style="Label1"
        label="presig"
        justify="Left"
        />
      <label loc="5 20" size="110 20"
        style="Label1"
        label="Nucleus"
        justify="Left"
        />
      <label loc="6 120" size="110 20"
        style="Label1"
        label="Receiver Gain"
        justify="Left"
        />
      <label loc="6 100" size="110 20"
        style="Label1"
        label="Acquisition Time"
        justify="Left"
        />
      <label loc="6 80" size="110 20"
        style="Label1"
        label="Spectral Width"
        justify="Left"
        />
      <label loc="6 60" size="110 20"
        style="Label1"
        label="Obs Offset"
        justify="Left"
        />
      <label loc="6 40" size="110 20"
        style="Label1"
        label="Obs Frequency"
        justify="Left"
        />
    </group>
    <button loc="445 50" size="120 20"
      style="Heading3"
      label="Show RF pulses"
      vq="show_rf"
      vc="exists('show_rf','parameter'):$ex if ($ex = 0) then create('show_rf','integer') endif show_rf = 1 vnmrjcmd('setpage','Acquire','RF Pulses')"
      decor1="No"
      />
    <button loc="445 10" size="120 20"
      style="Heading2"
      label="Acquire Profile"
      vc="wdone='' im2D('prep','profile') au"
      bg="darkGreen"
      decor1="No"
      />
  </group>
</template>
