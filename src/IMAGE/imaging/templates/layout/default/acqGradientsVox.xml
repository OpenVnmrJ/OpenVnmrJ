<template name="acqVoxGradients" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="Gradients"
    vq="panellevel show_adv"
    show="$VALUE = (panellevel &gt; 0) AND (show_adv &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqVoxGradients"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <button loc="555 115" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_adv = 0"
      bg="transparent"
      decor1="no"
      />
    <group loc="220 0" size="280 135"
      style="Heading1"
      label="Crushers"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="crusher_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 100" size="265 20"
        vq="tm"
        show="on('tm'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="5 0" size="80 20"
          style="PlainText"
          label="TM"
          justify="Left"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrushsat"
          vc="tcrushsat=$VALUE/1000"
          set="$VALUE=tcrushsat*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushsat"
          vc="gcrushsat=$VALUE go('check')"
          set="$VALUE=gcrushsat"
          disable="Grayed out"
          />
      </group>
      <group loc="5 60" size="265 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushws"
          vc="gcrushws=$VALUE"
          set="$VALUE=gcrushws"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrushws"
          vc="tcrushws=$VALUE/1000.0 go('check')"
          set="$VALUE=tcrushws*1000.0"
          digits="2"
          disable="Grayed out"
          />
        <label loc="5 0" size="60 20"
          style="PlainText"
          label="WS"
          justify="Left"
          />
      </group>
      <group loc="5 80" size="265 20"
        vq="sat"
        show="exists('sat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="5 0" size="80 20"
          style="PlainText"
          label="Satbands"
          justify="Left"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrushsat"
          vc="tcrushsat=$VALUE/1000"
          set="$VALUE=tcrushsat*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushsat"
          vc="gcrushsat=$VALUE go('check')"
          set="$VALUE=gcrushsat"
          disable="Grayed out"
          />
      </group>
      <group loc="5 40" size="265 20"
        vq="spinecho"
        show="exists('spinecho','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrush"
          vc="gcrush=$VALUE go('check')"
          set="$VALUE=gcrush"
          disable="Grayed out"
          />
        <label loc="5 0" size="80 20"
          style="PlainText"
          label="Slice select"
          justify="Left"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrush"
          vc="tcrush=$VALUE/1000"
          set="$VALUE=tcrush*1000"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="100 20" size="175 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="80 20"
          style="Label1"
          label="Amp (G/cm)"
          justify="Left"
          />
        <label loc="80 0" size="90 20"
          style="Label1"
          label="Duration (ms)"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 0" size="210 135"
      style="Heading1"
      label="Voxel Select"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="5 105" size="90 20"
        style="Label1"
        label="Duration"
        />
      <label loc="5 85" size="80 20"
        style="Label1"
        label="Refocus"
        />
      <label loc="165 105" size="35 20"
        style="Label1"
        label="ms"
        />
      <textmessage loc="100 105" size="60 20"
        style="PlainText"
        label="0.21"
        vq="tvox1"
        set="$VALUE = tvox1*1000"
        digits="2"
        />
      <label loc="165 85" size="35 20"
        style="Label1"
        label="G/cm"
        />
      <textmessage loc="100 85" size="60 20"
        style="PlainText"
        label="7.82"
        vq="gvox1r"
        set="$VALUE = gvox1r"
        digits="2"
        />
      <group loc="5 20" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="1.40"
          vq="gvox1"
          set="$VALUE = gvox1"
          digits="2"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Vox 1"
          />
      </group>
      <group loc="5 40" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="1.04"
          vq="gvox2"
          set="$VALUE = gvox2"
          digits="2"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Vox 2"
          />
      </group>
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="1.04"
          vq="gvox3"
          set="$VALUE = gvox3"
          digits="2"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Vox 3"
          />
      </group>
    </group>
  </group>
</template>
