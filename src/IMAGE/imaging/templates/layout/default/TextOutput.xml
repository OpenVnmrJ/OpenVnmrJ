<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="TextOutput" element="pages" type="processing" >
  <group size="860 280"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="640 105" size="175 20"
      show="$VALUE=0"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="0 0" size="160 20"
        style="Heading3"
        label="Sequence Manual"
        vc="man(seqfil)"
        bg="transparent"
        decor1="no"
        />
    </group>
    <button loc="640 80" size="160 20"
      style="Heading1"
      label="Clear"
      vc="clear"
      bg="transparent"
      decor1="no"
      />
    <button loc="640 55" size="160 20"
      style="Heading3"
      label="Show Log"
      vc="xm1('vp'):$vp if ($vp='plan') then return elseif ($vp='current') then clear cat(curexp+'/acqfil/log') else clear cat(file+'/log') endif"
      bg="transparent"
      decor1="no"
      />
    <toggle loc="640 30" size="160 20"
      style="Heading3"
      label="Edit Text"
      vq="editText"
      vc="editText=1 clear if (file='exp') then cat(curexp+'/text') else cat(file+'/text') endif"
      vc2="editText=0 clear if (file='exp') then cat(curexp+'/text') else cat(file+'/text') endif"
      set="$VALUE=editText"
      enable="yes"
      radiobutton="no"
      subtype="no"
      />
    <button loc="640 5" size="160 20"
      style="Heading3"
      label="Show Text"
      vc="clear if (file='exp') then cat(curexp+'/text') else cat(file+'/text') endif"
      bg="transparent"
      decor1="no"
      />
    <group loc="0 0" size="640 275"
      vq="editText"
      show="$VALUE=(editText=1)"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textfile loc="0 0" size="630 270"
        style="StdPar"
        vq="curexp file"
        set="if (file='exp') then $VALUE=curexp+'/text' else $VALUE=file+'/text' endif"
        editable="yes"
        wrap="yes"
        units="char"
        />
    </group>
    <group loc="0 0" size="640 275"
      vq="editText"
      show="$VALUE=(editText=0)"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textfile loc="0 0" size="630 270"
        style="StdPar"
        vq="alphatext"
        set="alphatextname:$VALUE"
        editable="no"
        wrap="yes"
        units="char"
        />
    </group>
  </group>
</template>
