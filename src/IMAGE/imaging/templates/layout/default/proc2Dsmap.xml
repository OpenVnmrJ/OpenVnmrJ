<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="proc2Dsmap" element="pages" type="processing" >
  <group size="800 270"
    style="PlainText"
    label="Sensitivity Map"
    vq="smappage"
    vc="aip2Dsmap('genpars')"
    show="aipXrecon('nrcvrs'):$nr $VALUE=($nr&gt;1) AND (smappage='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Dsmap"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <button loc="655 10" size="80 20"
      style="PlainText"
      label="Close"
      vq="smappage"
      vc="smappage='n'"
      bg="transparent"
      decor1="yes"
      />
    <group loc="220 0" size="210 120"
      style="Heading1"
      label="Weighting"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Display"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="160 20" size="40 20"
        style="Label1"
        label="Phase"
        />
      <label loc="95 20" size="40 20"
        style="Label1"
        label="Read"
        />
      <entry loc="160 40" size="40 20"
        style="PlainText"
        vq="smapgf1"
        vc="smapgf1=$VALUE/100.0"
        set="$VALUE=smapgf1*100.0"
        show="on('smapgf1'):$VALUE"
        digits="1"
        />
      <entry loc="160 60" size="40 20"
        style="PlainText"
        vq="smapsb1"
        vc="smapsb1=$VALUE/100.0"
        set="$VALUE=smapsb1*100.0"
        show="on('smapsb1'):$VALUE"
        digits="1"
        />
      <check loc="145 40" size="20 20"
        style="PlainText"
        vq="smapgf1"
        vc="smapgf1='y'"
        vc2="smapgf1='n'"
        set="on('smapgf1'):$VALUE"
        />
      <check loc="145 60" size="20 20"
        style="PlainText"
        vq="smapsb1"
        vc="smapsb1='y'"
        vc2="smapsb1='n'"
        set="on('smapsb1'):$VALUE"
        />
      <entry loc="95 40" size="40 20"
        style="PlainText"
        vq="smapgf"
        vc="smapgf=$VALUE/100.0"
        set="$VALUE=smapgf*100.0"
        show="on('smapgf'):$VALUE"
        digits="1"
        />
      <check loc="80 40" size="20 20"
        style="PlainText"
        vq="smapgf"
        vc="smapgf='y'"
        vc2="smapgf='n'"
        set="on('smapgf'):$VALUE"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gaussian"
        />
      <entry loc="95 60" size="40 20"
        style="PlainText"
        vq="smapsb"
        vc="smapsb=$VALUE/100.0"
        set="$VALUE=smapsb*100.0"
        show="on('smapsb'):$VALUE"
        digits="1"
        />
      <check loc="80 60" size="20 20"
        style="PlainText"
        vq="smapsb"
        vc="smapsb='y'"
        vc2="smapsb='n'"
        set="on('smapsb'):$VALUE"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Sine Bell"
        />
      <entry loc="160 80" size="40 20"
        style="PlainText"
        vq="smaplb1"
        vc="smaplb1=$VALUE/100.0"
        set="$VALUE=smaplb1*100.0"
        show="on('smaplb1'):$VALUE"
        digits="1"
        />
      <check loc="145 80" size="20 20"
        style="PlainText"
        vq="smaplb1"
        vc="smaplb1='y'"
        vc2="smaplb1='n'"
        set="on('smaplb1'):$VALUE"
        />
      <entry loc="95 80" size="40 20"
        style="PlainText"
        vq="smaplb"
        vc="smaplb=$VALUE/100.0"
        set="$VALUE=smaplb*100.0"
        show="on('smaplb'):$VALUE"
        digits="1"
        />
      <check loc="80 80" size="20 20"
        style="PlainText"
        vq="smaplb"
        vc="smaplb='y'"
        vc2="smaplb='n'"
        set="on('smaplb'):$VALUE"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="Lorentzian"
        />
      <label loc="10 20" size="65 20"
        style="Label1"
        label="% of FOV"
        />
    </group>
    <group loc="435 70" size="210 50"
      style="Heading1"
      label="Equalize Noise (k-space)"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ReconType"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="25 20" size="175 20"
        vq="smapeqnoise"
        show="$VALUE=(smapeqnoise='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="105 0" size="60 20"
          style="Label1"
          label="% of FOV"
          />
        <entry loc="55 0" size="50 20"
          style="PlainText"
          vq="smapnoisefrac"
          vc="aip2Dsmap('genpars') smapnoisefrac=$VALUE/100.0"
          set="$VALUE=smapnoisefrac*100.0"
          digits="1"
          />
        <label loc="5 0" size="50 20"
          style="Label1"
          label="Sample"
          />
      </group>
      <check loc="10 20" size="30 20"
        style="PlainText"
        vq="smapeqnoise"
        vc="smapeqnoise='y'"
        vc2="smapeqnoise='n'"
        set="$VALUE=(smapeqnoise='y')"
        />
    </group>
    <group loc="5 160" size="210 55"
      style="Heading1"
      label="Volume Coil Reference"
      vq="smapref"
      show="$VALUE=(smapref='vcoil')"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="VolumeCoilReference"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <filemenu loc="10 25" size="190 20"
        style="PlainText"
        vq="vcoilrefselect"
        vc="aip2Dsmap('genpars') vcoilrefselect='$VALUE' vcoilref=vcoilrefdir+vcoilrefselect"
        set="$VALUE=vcoilrefselect"
        file="$VALUE=userdir aip2Dsmap('refdir'):$VALUE"
        type="directory"
        bg="VJBackground"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
    </group>
    <group loc="435 0" size="210 70"
      style="Heading1"
      label="Mask"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ReconType"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <radio loc="10 40" size="160 20"
        style="PlainText"
        label="Use Calculated Mask"
        vq="smapmask"
        vc="aip2Dsmap('genpars') smapmask='calculated'"
        vc2="aip2Dsmap('genpars') smapmask=''"
        set="$VALUE=(smapmask='calculated')"
        />
      <radio loc="10 20" size="160 20"
        style="PlainText"
        label="Use Mask Parameters"
        vq="smapmask"
        vc="aip2Dsmap('genpars') smapmask='parameters'"
        vc2="aip2Dsmap('genpars') smapmask='calculated'"
        set="$VALUE=(smapmask='parameters')"
        />
    </group>
    <group loc="220 120" size="210 95"
      style="Heading1"
      label="Map Reference"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ReconType"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <radio loc="10 60" size="160 20"
        style="PlainText"
        label="Volume Coil Data"
        vq="smapref"
        vc="aip2Dsmap('genpars') aip2Dsmap('refdir') smapref='vcoil'"
        vc2="aip2Dsmap('genpars') smapref=''"
        set="$VALUE=(smapref='vcoil')"
        />
      <radio loc="10 40" size="160 20"
        style="PlainText"
        label="SUPER"
        vq="smapref"
        vc="aip2Dsmap('genpars') smapref='super'"
        vc2="aip2Dsmap('genpars') smapref=''"
        set="$VALUE=(smapref='super')"
        />
      <radio loc="10 20" size="160 20"
        style="PlainText"
        label="SOS"
        vq="smapref"
        vc="aip2Dsmap('genpars') smapref='sos'"
        vc2="aip2Dsmap('genpars') smapref=''"
        set="$VALUE=(smapref='sos')"
        />
    </group>
    <group loc="5 0" size="210 160"
      style="Heading1"
      label="Output"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ReconType"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="10 60" size="100 20"
        style="PlainText"
        label="Magnitude"
        vq="imMG"
        vc="aip2Dmask('genpars') imMG='y'"
        vc2="aip2Dmask('genpars') imMG='n'"
        set="$VALUE=(imMG='y')"
        />
      <check loc="10 80" size="180 20"
        style="PlainText"
        label="Reverse Mask of Magnitude"
        vq="imRMK"
        vc="aip2Dmask('genpars') imRMK='y'"
        vc2="aip2Dmask('genpars') imRMK='n'"
        set="$VALUE=(imRMK='y')"
        />
      <button loc="10 105" size="90 20"
        style="PlainText"
        label="Generate"
        vc="aip2Dsmap('generate')"
        bg="transparent"
        decor1="yes"
        />
      <button loc="110 105" size="90 20"
        style="PlainText"
        label="Display"
        vc="aip2Dsmap('display')"
        bg="transparent"
        decor1="yes"
        />
      <button loc="110 130" size="90 20"
        style="PlainText"
        label="Clear"
        vc="aip2Dsmap('clear')"
        bg="transparent"
        decor1="yes"
        />
      <check loc="10 20" size="130 20"
        style="PlainText"
        label="Sensitivity Maps"
        vq="imSM"
        vc="aip2Dsmap('genpars') imSM='y'"
        vc2="aip2Dsmap('genpars') imSM='n'"
        set="$VALUE=(imSM='y')"
        />
      <check loc="10 40" size="80 20"
        style="PlainText"
        label="Mask"
        vq="imMK"
        vc="aip2Dmask('genpars') imMK='y'"
        vc2="aip2Dmask('genpars') imMK='n'"
        set="$VALUE=(imMK='y')"
        />
    </group>
  </group>
</template>
