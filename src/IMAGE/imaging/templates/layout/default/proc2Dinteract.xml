<template name="proc2Diwft" element="pages" type="processing" >
  <group size="792 264"
    style="PlainText"
    label="Interactive"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc2Diwft"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="5 0" size="210 190"
      style="Heading1"
      label="Interactive Weighting"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="InteractiveWeighting"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="15 145" size="180 25"
        style="Heading3"
        label="FF - Phase encode"
        vc="Mprocess_element"
        decor1="No"
        />
      <button loc="15 115" size="180 25"
        style="Heading3"
        label="Set Phase Weighting"
        vc="full wc2=wc2max-5 wc=.9*wcmax wtia"
        decor1="No"
        />
      <button loc="15 85" size="180 25"
        style="Heading3"
        label="FT - Readout"
        vc="Mprocess_element('1st')"
        decor1="No"
        />
      <button loc="15 55" size="180 25"
        style="Heading3"
        label="Set Readout Weighting"
        vc="full wc2=wc2max-5 wc=.9*wcmax wti"
        decor1="No"
        />
      <button loc="15 25" size="180 25"
        style="Heading3"
        label="Show Profile"
        vc="full ftproc[2]=trunc((arraydim+1)/2) exists('nv','parameter'):$e if ftproc[1]=0 then if $e&lt;0.5 then ft else ft(ftproc[2]) endif else if $e&lt;0.5 then wft else wft(ftproc[2]) endif endif select(ftproc[2]) select:ftproc[2] ds"
        decor1="No"
        />
    </group>
    <group loc="220 0" size="280 190"
      style="Heading1"
      label="Filtering"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="filter2D"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="235 35" size="25 20"
        style="PlainText"
        vq="gf1"
        vc="gf1='y'"
        vc2="gf1='n' gfs1='n'"
        set="on('gf1'):$VALUE"
        />
      <check loc="235 55" size="25 20"
        style="PlainText"
        vq="gfs1"
        vc="gfs1='y'"
        vc2="gfs1='n'"
        set="on('gfs1'):$VALUE"
