<template name="acqAdvVox" element="pages" type="acquisition" >
  <group size="800 270"
    label="Advanced"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqAdvVox"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="5 0" size="210 205"
      style="Heading1"
      label="Acquisition"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Accquisition_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="np"
        vc="$np = $VALUE*2"
        set="$VALUE=np/2"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 100" size="110 20"
        style="Label1"
        label="Complex Pairs"
        justify="Left"
        />
      <label loc="175 120" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="175 140" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="115 120" size="60 20"
        style="PlainText"
        vq="at"
        vc="at=$VALUE/1000.0"
        set="$VALUE=at*1000.0"
        digits="2"
        disable="Grayed out"
        />
      <label loc="5 120" size="110 20"
        style="Label1"
        label="Acquisition Time"
        justify="Left"
        />
      <entry loc="115 140" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 140" size="110 20"
        style="Label1"
        label="Receiver Gain"
        justify="Left"
        />
      <menu loc="115 160" size="60 20"
        style="PlainText"
        label="menu"
        vc="presig = '$VALUE'"
        set="$VALUE = presig"
        editable="No"
        >
        <mlabel 
          label="high"
          chval="h"
          />
        <mlabel 
          label="low"
          chval="l"
          />
      </menu>
      <label loc="5 160" size="105 20"
        style="Label1"
        label="presig"
        justify="Left"
        />
      <group loc="5 180" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="170 0" size="25 20"
          style="Label1"
          label="%"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="glim"
          vc="glim = $VALUE"
          set="$VALUE=glim"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Glim"
          />
      </group>
      <entry loc="115 80" size="60 20"
        style="PlainText"
        vq="sw"
        vc="$np = np sw=$VALUE np=$np"
        set="$VALUE=sw"
        digits="2"
        disable="Grayed out"
        />
      <label loc="175 40" size="30 20"
        style="Label1"
        label="MHz"
        justify="Left"
        />
      <label loc="175 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 80" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <menu loc="115 20" size="75 20"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        editable="No"
        >
        <mlabel 
          label="Proton"
          chval="H1"
          />
        <mlabel 
          label="F19"
          chval="F19"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="Na23"
          chval="Na23"
          />
        <mlabel 
          label="Xe129"
          chval="Xe129"
          />
      </menu>
      <textmessage loc="115 40" size="60 20"
        style="PlainText"
        label="400.420"
        vq="sfrq"
        set="$VALUE=sfrq"
        digits="3"
        />
      <entry loc="115 60" size="60 20"
        style="PlainText"
        vq="resto"
        vc="resto=$VALUE"
        set="$VALUE=resto"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 20" size="110 20"
        style="Label1"
        label="Nucleus"
        justify="Left"
        />
      <label loc="6 80" size="110 20"
        style="Label1"
        label="Spectral Width"
        justify="Left"
        />
      <label loc="6 60" size="110 20"
        style="Label1"
        label="Obs Offset"
        justify="Left"
        />
      <label loc="6 40" size="110 20"
        style="Label1"
        label="Obs Frequency"
        justify="Left"
        />
    </group>
    <group loc="5 205" size="425 65"
      style="Heading1"
      label="Block Processing"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BlockProcessing"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="115 40" size="300 20"
        style="PlainText"
        vq="wbs bs nt"
        vc="wbs='$VALUE' wbs(wbs)"
        set="$VALUE=wbs"
        show="$VALUE = (nt &gt; 1) AND (bs &lt; nt)"
        digits="0"
        disable="Label"
        />
      <label loc="5 40" size="110 20"
        style="Label1"
        label="Processing"
        justify="Left"
        />
      <entry loc="115 20" size="60 20"
        style="PlainText"
        vq="bs"
        vc="bs=$VALUE"
        set="$VALUE=bs"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 20" size="110 20"
        style="Label1"
        label="Block Size"
        justify="Left"
        />
    </group>
    <group loc="220 0" size="210 100"
      style="Heading1"
      label="Offsets"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MRSfreq"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <button loc="0 0" size="95 20"
          style="PlainText"
          label="Suppression"
          vc="offset:wsfrq ws_delta = wsfrq-resto"
          decor1="No"
          />
        <entry loc="95 0" size="60 20"
          style="PlainText"
          vq="wsfrq ws_delta"
          vc="ws_delta = $VALUE wsfrq = resto+ws_delta"
          set="$VALUE = ws_delta"
          digits="2"
          />
        <label loc="155 0" size="25 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
      </group>
      <group loc="5 40" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <button loc="0 0" size="95 20"
          style="PlainText"
          label="Receiver"
          vc="movetof tof_delta = tof-resto"
          decor1="No"
          />
        <entry loc="95 0" size="60 20"
          style="PlainText"
          vq="tof tof_delta"
          vc="tof_delta = $VALUE tof = resto+tof_delta"
          set="$VALUE = tof_delta"
          digits="2"
          />
        <label loc="155 0" size="25 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
      </group>
      <group loc="5 20" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="95 0" size="60 20"
          style="PlainText"
          vq="resto"
          vc="resto = $VALUE"
          set="$VALUE = resto"
          digits="2"
          />
        <button loc="0 0" size="95 20"
          style="PlainText"
          label="Transmitter"
          vc="offset:H1offset resto=H1offset "
          decor1="No"
          />
        <label loc="155 0" size="25 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
      </group>
    </group>
    <button loc="445 95" size="120 20"
      style="Heading3"
      label="Linewidth"
      vc="nl dres"
      decor1="No"
      />
    <button loc="445 75" size="120 20"
      style="Heading3"
      label="Freq at Cursor"
      vc="offset:H1offset resto=H1offset "
      decor1="No"
      />
    <button loc="445 120" size="120 20"
      style="Heading3"
      label="Show RF &amp; Grad"
      vq="show_adv"
      vc="exists('show_adv','parameter'):$e if ($e=0) then create('show_adv','integer') endif show_adv = 1"
      decor1="No"
      />
    <button loc="445 55" size="120 20"
      style="Heading3"
      label="Water Reference"
      vc="nl rl(4.7p) offset"
      decor1="No"
      />
    <button loc="445 35" size="120 20"
      style="Heading3"
      label="Display Spectrum"
      vc="im1D('proc')"
      decor1="No"
      />
    <button loc="445 10" size="120 20"
      style="Label4"
      label="Trial Spectrum"
      vc="im2Dcsi('prep','profile') au"
      bg="darkGreen"
      decor1="No"
      />
  </group>
</template>
