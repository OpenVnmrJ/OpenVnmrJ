<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Process" element="panels" type="processing" >
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Process"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc1Dprocess"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Display"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc1Ddisplay"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="220 0" size="210 155"
      style="Heading1"
      label="Arrayed Spectra"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Arrays"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="150 85" size="50 20"
        style="PlainText"
        vq="vo"
        vc="vo=$VALUE"
        set="$VALUE=vo"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 85" size="145 20"
        style="Label1"
        label="Vertical Offset"
        justify="Left"
        />
      <button loc="5 115" size="95 20"
        style="Label1"
        label="Add Label"
        vc="dssl"
        decor1="no"
        />
      <entry loc="150 65" size="50 20"
        style="PlainText"
        vq="ho"
        vc="ho=$VALUE"
        set="$VALUE=ho"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 65" size="140 20"
        style="Label1"
        label="Horizontal Offset"
        justify="Left"
        />
      <button loc="5 45" size="95 20"
        style="Label1"
        label="Custom"
        vc="dss"
        decor1="no"
        />
      <button loc="100 20" size="95 20"
        style="Label1"
        label="Vertical"
        vc="ho=0 vo=10 full dss"
        decor1="no"
        />
      <button loc="5 20" size="95 20"
        style="Label1"
        label="Horizontal"
        vc="vo=0 full dssh"
        decor1="no"
        />
    </group>
    <group loc="435 0" size="210 155"
      style="Heading1"
      label="Reference Line"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ReferenceLine"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="105 95" size="45 20"
        style="PlainText"
        vq="ref"
        vc="vnmrunits('set','ref',$VALUE) rl(ref) ds"
        set="vnmrunits('get','ref'):$VALUE"
        show="on('ref'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <menu loc="150 95" size="55 20"
        style="PlainText"
        label="menu"
        vq="ref"
        vc="parunits('set','ref','$VALUE')"
        set="parunits('get','ref'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <label loc="5 55" size="100 20"
        style="Label1"
        label="Cursor is on:"
        />
      <button loc="105 120" size="100 20"
        style="Label1"
        label="Cancel"
        vc="crl"
        decor1="no"
        />
      <button loc="105 75" size="100 20"
        style="Label1"
        label="Zero"
        vc="rl(0) ds"
        decor1="no"
        />
      <button loc="105 55" size="100 20"
        style="Label1"
        label="Water"
        vc="rl(4.7p) ds"
        decor1="no"
        />
      <button loc="105 20" size="100 20"
        style="Label1"
        label="Line Width"
        vc="dres"
        decor1="no"
        />
      <button loc="5 20" size="100 20"
        style="Label1"
        label="Nearest Line"
        vc="nl"
        decor1="no"
        />
    </group>
    <group loc="5 0" size="210 155"
      style="Heading1"
      label="Spectrum"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Display"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="105 110" size="100 20"
        style="Label1"
        label="Full Spectrum"
        vc="f ds"
        decor1="no"
        />
      <button loc="5 110" size="100 20"
        style="Label1"
        label="Full Screen"
        vc="full ds"
        decor1="no"
        />
      <button loc="105 85" size="100 20"
        style="Label1"
        label="SNR"
        vc="dsn"
        decor1="no"
        />
      <button loc="5 85" size="100 20"
        style="Label1"
        label="Autoscale"
        vc="vsadj ds"
        decor1="no"
        />
      <group loc="5 60" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="65 0" size="125 20"
          style="PlainText"
          label="menu"
          vq="aig"
          vc="aig = '$VALUE'"
          set="$VALUE = aig"
          editable="No"
          >
          <mlabel 
            label="Normalized"
            chval="nm"
            />
          <mlabel 
            label="Absolute"
            chval="ai"
            />
        </menu>
        <label loc="0 0" size="65 20"
          style="Label1"
          label="Scaling"
          />
      </group>
      <group loc="5 20" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="65 0" size="125 20"
          style="PlainText"
          label="menu"
          vq="dmg"
          vc="dmg = '$VALUE'"
          set="$VALUE = dmg"
          editable="No"
          >
          <mlabel 
            label="Phased"
            chval="ph"
            />
          <mlabel 
            label="Absolute Value"
            chval="av"
            />
          <mlabel 
            label="PowerMode"
            chval="pwr"
            />
        </menu>
        <label loc="0 0" size="65 20"
          style="Label1"
          label="Mode"
          />
      </group>
      <group loc="5 40" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="65 0" size="125 20"
          style="PlainText"
          label="menu"
          vq="axis"
          vc="axis='$VALUE' ds dscale"
          set="$VALUE = axis"
          editable="No"
          >
          <mlabel 
            label="cm"
            chval="c"
            />
          <mlabel 
            label="ppm"
            chval="p"
            />
          <mlabel 
            label="Hz"
            chval="h"
            />
          <mlabel 
            label="kHz"
            chval="k"
            />
        </menu>
        <label loc="0 0" size="65 20"
          style="Label1"
          label="Axis"
          />
      </group>
    </group>
    <group loc="5 155" size="210 75"
      style="Heading1"
      label="Peaks"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Peaks"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="10 45" size="120 20"
        style="Label1"
        label="Peak Frequencies"
        vc="dpf"
        decor1="no"
        />
      <button loc="10 20" size="120 20"
        style="Label1"
        label="Auto Threshold"
        vc="thadj ds ds('thresh')"
        decor1="no"
        />
    </group>
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Integration"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc1Dintegral"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Deconvolution"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Deconvolution"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Peaks"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc1Dlinelist"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="Heading2"
    label="Corrections"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="proc1D_Corrections"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="860 280"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
</template>
