<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="900 280"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Setup"
      vq="iplanType showOptions"
      show="$VALUE=(showOptions=1)"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SetupP123"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Sat Bands"
      vq="sat showOptions"
      show="if (showOptions=4) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Satbands"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="650 0" size="210 230"
      style="Heading1"
      label="Plan &amp; Display"
      vq="showOptions"
      show="if (showOptions=5) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MoreFOVPlan1D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="435 0" size="210 230"
      style="Heading1"
      label="Voxel Plan"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FOVPlan1D"
      useref="yes"
      subtype="Major"
      expanded="no"
      >
    </group>
    <group loc="220 0" size="210 230"
      style="Heading1"
      label="Scan Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ScanOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="10 170" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="130 20"
          style="PlainText"
          label="External Trigger"
          vc="ticks=1 go('check')"
          vc2="ticks=0 go('check')"
          set="$VALUE=(ticks &gt; 0)"
          tooltip="if 'y', phase cycle =nt*array, if &quot;no&quot; phase cycle=nt only"
          />
      </group>
      <check loc="10 25" size="115 20"
        style="PlainText"
        label="Water Sup On"
        vq="ws wsrf wref"
        vc="ws ='y' wsrf='y' go('check')"
        vc2="ws ='y' wsrf='n' go('check') wref='n'"
        set="$VALUE=(ws = 'y' and wsrf='y')"
        />
      <check loc="10 45" size="100 20"
        style="PlainText"
        label="Water Sup TM"
        vq="ws wsrf wref ws_tm"
        vc="ws_tm ='y' wsrf='y' go('check')"
        vc2="ws_tm ='n' wsrf='n' go('check') wref='n'"
        set="$VALUE=(ws_tm = 'y' and wsrf='y')"
        />
      <group loc="5 65" size="195 20"
        vq="wsrf"
        show="$VALUE=(wsrf='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="5 0" size="175 20"
          style="PlainText"
          label="Acquire Water Reference"
          vq="wref"
          vc="wref='y' go('check') wrefstatus='wref'"
          vc2="wref='n' go('check')"
          set="$VALUE = (wref='y')"
          show="exists('wsrf','parameter'):$VALUE"
          tooltip="Acquires and saves water reference"
          />
      </group>
      <group loc="10 85" size="190 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="185 20"
          style="PlainText"
          label="Phase Cycle Through Array"
          vc="autoph='y' go('check')"
          vc2="autoph ='n' go('check')"
          set="$VALUE=(autoph = 'y')"
          tooltip="if 'y', phase cycle =nt*array, if &quot;no&quot; phase cycle=nt only"
          />
      </group>
      <toggle loc="140 105" size="35 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=4"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=4)"
        radiobutton="no"
        subtype="no"
        />
      <check loc="10 105" size="130 20"
        style="PlainText"
        label="Spatial Satbands"
        vq="sat"
        vc="sat='y' ovs='n' go('check')"
        vc2="sat='n' go('check')"
        set="$VALUE=(sat='y')"
        />
      <check loc="10 125" size="190 20"
        style="PlainText"
        label="Outer Volume Suppression"
        vq="ovs"
        vc="ovs='y' sat='n' if (showOptions=4) then showOptions=0 endif"
        vc2="ovs='n'"
        set="$VALUE=(ovs='y')"
        />
      <group loc="10 145" size="196 20"
        label="OVS"
        vq="ovs"
        show="$VALUE=(ovs='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="OVS"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <entry loc="46 0" size="35 20"
          style="PlainText"
          vq="ovsthk"
          vc="ovsthk=$VALUE"
          set="$VALUE=ovsthk"
          digits="1"
          />
        <entry loc="126 0" size="35 20"
          style="PlainText"
          vq="ovsgap"
          vc="ovsgap=$VALUE/10"
          set="$VALUE=ovsgap*10"
          digits="1"
          />
        <label loc="166 0" size="30 20"
          style="Label1"
          label="mm"
          />
        <label loc="96 0" size="35 20"
          style="Label1"
          label="Gap"
          />
        <label loc="16 0" size="35 20"
          style="Label1"
          label="Thk"
          />
      </group>
    </group>
    <group loc="5 0" size="210 230"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff2"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 180" size="205 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="csd_ppm"
          vc="csd_ppm=$VALUE go('check')"
          set="$VALUE=csd_ppm"
          digits="1"
          disable="Grayed out"
          />
        <label loc="175 0" size="30 20"
          style="PlainText"
          label="ppm"
          justify="Left"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Chem shift offset"
          justify="Left"
          />
      </group>
      <group loc="5 160" size="205 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="gain"
          vc="gain=$VALUE"
          set="$VALUE=gain"
          digits="0"
          disable="Label"
          />
        <label loc="175 0" size="25 20"
          style="PlainText"
          label="dB"
          justify="Left"
          />
        <label loc="0 0" size="105 20"
          style="Label1"
          label="Receiver gain"
          />
      </group>
      <group loc="5 120" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="np"
          vc="np=$VALUE*2 go('check')"
          set="$VALUE=np/2"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="100 20"
          style="PlainText"
          label="Real Points"
          justify="Left"
          />
      </group>
      <group loc="5 140" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="sw"
          vc="$np = np sw = $VALUE np = $np go('check')"
          set="$VALUE=sw"
          digits="0"
          disable="Grayed out"
          />
        <label loc="175 0" size="25 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <label loc="0 0" size="105 20"
          style="PlainText"
          label="Spectral Width"
          justify="Left"
          />
      </group>
      <group loc="5 60" size="205 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="tm mintm"
          vc="tm=$VALUE/1000.0 go('check')"
          set="$VALUE=tm*1000.0"
          show="$VALUE = (mintm='n') "
          digits="2"
          disable="Grayed out"
          />
        <label loc="180 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <check loc="50 0" size="50 20"
          style="PlainText"
          label="Min"
          vq="mintm tm"
          vc="mintm = 'y' go('check') prep go('check')"
          vc2="mintm = 'n' go('check')"
          set="$VALUE = (mintm = 'y')"
          tooltip="Minimum TM time possible"
          />
        <label loc="0 0" size="40 20"
          style="Label1"
          label="TM"
          />
      </group>
      <group loc="5 80" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="nt"
          vc="nt=$VALUE go('check')"
          set="$VALUE=nt"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="70 20"
          style="PlainText"
          label="Averages"
          justify="Left"
          />
      </group>
      <group loc="5 100" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="ss"
          vc="ss=$VALUE go('check')"
          set="$VALUE=ss"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="100 20"
          style="PlainText"
          label="Dummy Scans"
          justify="Left"
          />
      </group>
      <group loc="5 20" size="205 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="tr mintr"
          vc="tr=$VALUE/1000.0 go('check')"
          set="$VALUE=tr*1000.0"
          show="$VALUE = (mintr='n') "
          digits="2"
          disable="Grayed out"
          />
        <check loc="50 0" size="50 20"
          style="PlainText"
          label="Min"
          vq="mintr tr"
          vc="mintr = 'y' go('check') prep go('check')"
          vc2="mintr = 'n' "
          set="$VALUE = (mintr = 'y')"
          />
        <label loc="0 0" size="30 20"
          style="PlainText"
          label="TR"
          justify="Left"
          />
      </group>
      <group loc="5 40" size="205 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="te minte"
          vc="te=$VALUE/1000.0 go('check')"
          set="$VALUE=te*1000.0"
          show="$VALUE = (minte='n') "
          digits="2"
          disable="Grayed out"
          />
        <label loc="180 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <check loc="50 0" size="50 20"
          style="PlainText"
          label="Min"
          vq="minte te"
          vc="minte = 'y' go('check') prep go('check')"
          vc2="minte = 'n' go('check')"
          set="$VALUE = (minte = 'y')"
          tooltip="Minimum TE time possible"
          />
        <label loc="0 0" size="40 20"
          style="Label1"
          label="TE"
          />
      </group>
      <label loc="185 20" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="5 235" size="855 40"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="InfoGroup"
      useref="yes"
      subtype="Untitled"
      expanded="no"
      >
    </group>
  </group>
</template>
