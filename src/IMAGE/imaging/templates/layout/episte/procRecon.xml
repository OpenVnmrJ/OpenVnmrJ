    <group loc="220 0" size="210 110"
      style="Heading1"
      label="Zero-filling"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="zerofilling"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <menu loc="70 40" size="60 20"
        style="PlainText"
        vq="fn1"
        vc="if $VALUE=0 then fn1='n' else fn1 = $VALUE*2 endif"
        set="on('fn1'):$on if ($on) then $VALUE = fn1/2 else $VALUE=0 endif"
        editable="No"
        >
        <mlabel 
          label="Off"
          chval="0"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
      </menu>
      <label loc="70 20" size="55 20"
        style="PlainText"
        label="Phase"
        justify="Left"
        />
      <label loc="5 20" size="55 20"
        style="PlainText"
        label="Readout"
        justify="Left"
        />
