      >
      <group loc="10 41" size="138 20"
        vq="recondisplay"
        show="$VALUE = recondisplay &gt; 0"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="61 0" size="75 17"
          style="PlainText"
          vq="recondisplay"
          vc="setrdisp($VALUE+1)"
          set="$VALUE=recondisplay-1"
          digits="0"
          />
        <label loc="0 0" size="65 20"
          style="Label1"
          label="Image skip"
          />
      </group>
      <check loc="5 20" size="170 20"
        style="PlainText"
        label="Display images"
        vq="recondisplay"
        vc="setrdisp(1)"
        vc2="setrdisp(0)"
        set="exists('recondisplay','parameter'):$ex if ($ex=0) then $VALUE=1 else $VALUE=(recondisplay&gt;0) endif"
        />
    </group>
    <button loc="10 240" size="120 20"
