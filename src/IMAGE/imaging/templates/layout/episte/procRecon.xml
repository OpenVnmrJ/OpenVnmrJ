      <radio loc="5 80" size="125 20"
        style="PlainText"
        label="Both"
        vq="raw"
        vc="raw = 'b'"
        set="$VALUE = (raw = 'b')"
        />
      <radio loc="5 60" size="125 20"
        style="PlainText"
        label="Phased"
        vq="raw"
        vc="raw = 'p'"
        set="$VALUE = (raw = 'p')"
        />
      <radio loc="5 40" size="125 20"
        style="PlainText"
        label="Magnitude"
        vq="raw"
        vc="raw = 'm'"
        set="$VALUE = (raw = 'm')"
        />
      <radio loc="5 20" size="125 20"
        style="PlainText"
        label="Off"
        vq="raw"
        vc="raw = 'n'"
        set="$VALUE = (raw = '') OR (raw = 'n')"
        />
    </group>
