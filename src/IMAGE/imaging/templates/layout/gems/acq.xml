<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Acquire" element="panels" type="acquisition" >
  <group loc="0 0" size="900 280"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    rows="1"
    columns="1"
    >
  </group>
  <group loc="0 0" size="800 270"
    label="Advanced"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqAdv"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    rows="1"
    columns="1"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="Heading1"
    label="Sense"
    vq="sense"
    show="$VALUE=(sense='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acq2Dsense"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    rows="1"
    columns="1"
    >
  </group>
  <group loc="0 0" size="810 270"
    style="Heading1"
    label="RF Pulses"
    vq="panellevel show_rf"
    show="$VALUE = (panellevel &gt; 0) AND (show_rf &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqRF"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    rows="1"
    columns="1"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="Heading1"
    label="Gradients"
    vq="panellevel show_rf"
    show="$VALUE = (panellevel &gt; 0) AND (show_rf &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqGradients"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    rows="1"
    columns="1"
    >
  </group>
</template>
