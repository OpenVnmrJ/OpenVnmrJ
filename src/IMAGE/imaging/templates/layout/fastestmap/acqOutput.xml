<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqOutput" element="pages" type="acquisition" >
  <group size="780 290"
    style="PlainText"
    label="Map"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqOutput"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <label loc="0 5" size="130 20"
      style="Heading1"
      label="Log information"
      />
    <button loc="515 250" size="165 20"
      style="Label4"
      label="Display Field Profiles"
      vc="fm_showmap"
      bg="darkGreen"
      decor1="no"
      />
    <group loc="510 135" size="235 100"
      style="Heading1"
      label="Shim Calibration Parameters"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tmp"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="20 30" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="z"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <entry loc="140 0" size="70 20"
          style="PlainText"
          vq="tau shimmaptau"
          vc="shimmaptau=$VALUE/1000.0 tau=shimmaptau"
          set="$VALUE=shimmaptau*1000.0"
          show="on('shimmaptau'):$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="0 0" size="55 20"
          style="PlainText"
          label="tau (ms)"
          justify="Left"
          />
      </group>
      <label loc="20 70" size="140 20"
        style="PlainText"
        label="Shim step2  %"
        justify="Left"
        />
      <entry loc="161 50" size="70 20"
        style="PlainText"
        vq="step1"
        vc="step1=$VALUE"
        set="$VALUE=step1"
        show="on('step1'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="21 50" size="140 20"
        style="PlainText"
        label="Shim step1 (x,y,z) %"
        justify="Left"
        />
      <entry loc="161 70" size="70 20"
        style="PlainText"
        vq="step2"
        vc="step2=$VALUE"
        set="$VALUE=step2"
        show="on('step2'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="510 25" size="235 95"
      style="Heading1"
      label="Shim Calibration"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ShimCalibration"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="5 50" size="165 20"
        style="Label4"
        label="Generate Shimmap"
        vc="fm_calib"
        bg="darkGreen"
        decor1="no"
        />
      <button loc="5 25" size="165 20"
        style="Label4"
        label="Acquire Shimmap"
        vc="wdone='' werr='' prescan_control[1]=0 prep tau=shimmaptau fastmap('init') fm_setmap(2) fm_setshimcalib(1,8)"
        bg="darkGreen"
        decor1="no"
        />
    </group>
    <textfile loc="2 25" size="500 265"
      style="Label4"
      vq="userdir"
      set="$fll=userdir+'/fastmap/fastmap.log' exists($fll,'file'):$e if $e&lt;0.5 then write('reset',$fll) endif $VALUE=$fll"
      editable="yes"
      wrap="yes"
      units="char"
      />
  </group>
</template>
