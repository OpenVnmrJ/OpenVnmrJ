<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="proc1Dallign" element="pages" type="processing" >
  <group size="800 270"
    style="Heading2"
    label="Corrections"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="proc1Dallign"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="355 5" size="210 185"
      style="Heading1"
      label="Add Scans"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="1dweight"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="30 155" size="145 20"
        style="Label1"
        label="Return To Review Port"
        vc="jexp3"
        bg="transparent"
        decor1="no"
        />
      <button loc="20 25" size="145 20"
        style="Label1"
        label="Apply Sum"
        vc="allign_initparams apply_sum"
        bg="transparent"
        decor1="no"
        />
      <entry loc="150 106" size="30 20"
        style="PlainText"
        vq="a_increment"
        vc="a_increment=$VALUE"
        set="$VALUE=a_increment"
        disable="Grayed out"
        />
      <label loc="20 106" size="120 20"
        style="PlainText"
        label="Increment"
        justify="Left"
        />
      <entry loc="150 86" size="30 20"
        style="PlainText"
        vq="a_fid_max"
        vc="a_fid_max=$VALUE"
        set="$VALUE=a_fid_max"
        disable="Grayed out"
        />
      <label loc="20 86" size="120 20"
        style="PlainText"
        label="Last Fid #"
        justify="Left"
        />
      <entry loc="150 66" size="30 20"
        style="PlainText"
        vq="a_fid_min"
        vc="a_fid_min=$VALUE"
        set="$VALUE=a_fid_min"
        disable="Grayed out"
        />
      <label loc="20 66" size="120 20"
        style="PlainText"
        label="First Fid #"
        justify="Left"
        />
    </group>
    <group loc="145 5" size="210 185"
      style="Heading1"
      label="Align Phase"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="1dallign"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="40 25" size="105 20"
        style="PlainText"
        label="Apply Phase"
        vc="allign_initparams allign_phase"
        bg="transparent"
        decor1="no"
        />
      <entry loc="170 140" size="30 20"
        style="PlainText"
        vq="a_fid_no"
        vc="a_fid_no=$VALUE"
        set="$VALUE=a_fid_no"
        disable="Grayed out"
        />
      <label loc="5 140" size="135 20"
        style="PlainText"
        label="Reference Fid #"
        justify="Left"
        />
      <entry loc="170 120" size="30 20"
        style="PlainText"
        vq="a_s2n"
        vc="a_s2n=$VALUE"
        set="$VALUE=a_s2n"
        disable="Grayed out"
        />
      <label loc="5 120" size="135 20"
        style="PlainText"
        label="SNR factor"
        justify="Left"
        />
      <entry loc="170 100" size="30 20"
        style="PlainText"
        vq="a_rmsd"
        vc="a_rmsd=$VALUE"
        set="$VALUE=a_rmsd"
        disable="Grayed out"
        />
      <label loc="5 100" size="145 20"
        style="PlainText"
        label="Noise points (end of fid)"
        justify="Left"
        />
      <entry loc="170 80" size="30 20"
        style="PlainText"
        vq="a_no_skip"
        vc="a_no_skip=$VALUE"
        set="$VALUE=a_no_skip"
        disable="Grayed out"
        />
      <label loc="5 80" size="160 20"
        style="PlainText"
        label="Skip points (start of fid)"
        justify="Left"
        />
      <label loc="5 60" size="120 20"
        style="PlainText"
        label="Analysis points"
        justify="Left"
        />
      <entry loc="170 60" size="30 20"
        style="PlainText"
        vq="a_no_points"
        vc="a_no_points=$VALUE"
        set="$VALUE=a_no_points"
        disable="Grayed out"
        />
    </group>
    <group loc="565 95" size="210 95"
      style="Heading1"
      label="Undo/Repeat"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="1dweight"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="35 35" size="130 20"
        style="PlainText"
        label="Restore Original"
        vc="allign_freq('restore')"
        bg="transparent"
        decor1="no"
        />
      <label loc="35 66" size="160 20"
        style="PlainText"
        label="Allow Repeat Processing"
        justify="Left"
        />
      <check loc="10 66" size="20 20"
        style="PlainText"
        vq="a_repeat"
        vc="allign_initparams a_repeat='y'"
        vc2="allign_initparams a_repeat='n'"
        set="$VALUE=(a_repeat='y')"
        />
    </group>
    <group loc="565 5" size="210 90"
      style="Heading1"
      label="Eddy Current Correction"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="1dweight"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="10 55" size="20 20"
        style="PlainText"
        vq="a_watref"
        vc="allign_initparams a_watref='y'"
        vc2="allign_initparams a_watref='n'"
        set="$VALUE=(a_watref='y')"
        toolTip="wrefname parameter stores the location of the reference fid"
        />
      <label loc="30 55" size="160 20"
        style="PlainText"
        label="Use Standard Reference"
        justify="Left"
        />
      <button loc="25 25" size="145 20"
        style="Label1"
        label="Correct EC"
        vc="allign_initparams apply_ecc"
        bg="transparent"
        decor1="no"
        toolTip="Only for non-arrayed experiment"
        />
    </group>
    <group loc="5 5" size="140 185"
      style="Heading1"
      label="Align Frequency"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="1dweight"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <menu loc="15 55" size="90 20"
        style="PlainText"
        label="menu"
        vq="dmg "
        vc="allign_initparams a_dmg_default='n' dmg='$VALUE'"
        set="$VALUE=dmg"
        editable="No"
        >
        <mlabel 
          label="Phased"
          chval="ph"
          />
        <mlabel 
          label="Magnitude"
          chval="av"
          />
      </menu>
      <button loc="15 25" size="90 20"
        style="PlainText"
        label="Apply Freq"
        vc="allign_initparams allign_freq "
        bg="transparent"
        decor1="no"
        />
    </group>
  </group>
</template>
