<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="TextOutput" element="pages" type="processing" >
  <group size="800 270"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <textfile loc="0 0" size="630 270"
      style="StdPar"
      vq="alphatext"
      set="alphatextname:$VALUE"
      editable="no"
      wrap="yes"
      units="char"
      />
    <button loc="635 55" size="160 20"
      style="Heading1"
      label="Clear"
      vc="clear"
      decor1="no"
      />
    <button loc="635 5" size="160 20"
      style="Heading3"
      label="Show Text"
      vc="clear text"
      decor1="no"
      />
    <button loc="635 30" size="160 20"
      style="Heading3"
      label="Sequence Manual"
      vc="man(seqfil)"
      decor1="no"
      />
  </group>
</template>
