<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqGradients" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="Gradients"
    vq="panellevel show_adv"
    show="$VALUE = (panellevel &gt; 0) AND (show_adv &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqGradients"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="435 0" size="210 125"
      style="Heading1"
      label="Voxel Select 3"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="200 100"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          vq="gvox3"
          set="$VALUE = gvox3"
          digits="2"
          />
        <label loc="0 20" size="75 20"
          style="Label1"
          label="Ramp time"
          />
        <textmessage loc="100 20" size="60 20"
          style="PlainText"
          vq="rgvox3"
          set="format(rgvox3*1e6,0,2):$VALUE"
          digits="2"
          />
        <label loc="160 20" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Amplitude"
          />
      </group>
    </group>
    <group loc="220 0" size="210 125"
      style="Heading1"
      label="Voxel Select 2"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="200 100"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="4.00"
          vq="gvox2"
          set="$VALUE = gvox2"
          digits="2"
          />
        <label loc="0 20" size="75 20"
          style="Label1"
          label="Ramp time"
          />
        <textmessage loc="100 20" size="60 20"
          style="PlainText"
          label="0.00"
          vq="rgvox2"
          set="format(rgvox2*1e6,0,2):$VALUE"
          digits="2"
          />
        <label loc="160 20" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Amplitude"
          />
      </group>
    </group>
    <group loc="5 0" size="210 125"
      style="Heading1"
      label="Voxel Select 1"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="200 100"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 60" size="35 20"
          style="Label1"
          label="ms"
          />
        <textmessage loc="100 80" size="60 20"
          style="PlainText"
          label="0.00"
          vq="rgvox1r"
          set="format(rgvox1r*1e6,0,2):$VALUE"
          digits="2"
          />
        <label loc="160 80" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="0 80" size="75 20"
          style="Label1"
          label="Ramp time"
          />
        <textmessage loc="100 60" size="60 20"
          style="PlainText"
          label="0.00"
          vq="tvox1"
          set="$VALUE = tvox1*1000"
          digits="2"
          />
        <label loc="0 60" size="75 20"
          style="Label1"
          label="Duration"
          />
        <label loc="160 40" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
        <textmessage loc="100 40" size="60 20"
          style="PlainText"
          label="0.00"
          vq="gvox1r"
          set="$VALUE = gvox1r"
          digits="2"
          />
        <textmessage loc="100 0" size="60 20"
          style="PlainText"
          label="1.40"
          vq="gvox1"
          set="$VALUE = gvox1"
          digits="2"
          />
        <label loc="0 40" size="60 20"
          style="Label1"
          label="Refocus"
          />
        <label loc="0 20" size="75 20"
          style="Label1"
          label="Ramp time"
          />
        <textmessage loc="100 20" size="60 20"
          style="PlainText"
          label="0.00"
          vq="rgvox1"
          set="format(rgvox1*1e6,0,2):$VALUE"
          digits="2"
          />
        <label loc="160 20" size="35 20"
          style="Label1"
          label="us"
          />
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Amplitude"
          />
      </group>
    </group>
    <button loc="650 205" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_adv = 0"
      decor1="No"
      />
  </group>
</template>
