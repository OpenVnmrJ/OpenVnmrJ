<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="800 270"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="220 0" size="210 220"
      style="Heading1"
      label="Scan Options"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ScanOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 100" size="160 20"
        style="PlainText"
        label="Spatial Sat"
        vq="sat"
        vc="sat='y' go('check')"
        vc2="sat='n' go('check')"
        set="$VALUE=(sat='y')"
        />
      <check loc="5 125" size="180 20"
        style="PlainText"
        label="External Trigger"
        vq="ticks"
        vc="ticks = 1"
        vc2="ticks = 0"
        set="$VALUE=(ticks &gt; 0)"
        />
      <check loc="5 40" size="120 20"
        style="PlainText"
        label="MTC"
        vq="mt"
        vc="mt='y' pmt=pmt mtfrq=mtfrq go('check')"
        vc2="mt='n' unarray('pmt','mtfrq') go('check')"
        set="$VALUE=(mt='y')"
        />
      <check loc="5 60" size="120 20"
        style="PlainText"
        label="Fatsat"
        vq="fsat Prescan_FatOffset"
        vc="fsat='y'  fsatfrq=fsatfrq pfsat=pfsat if (autooffset='y') then fsatfrq=Prescan_FatOffset endif go('check')"
        vc2="fsat='n' unarray('fsatfrq','pfsat')   go('check')"
        set="$VALUE=(fsat='y')"
        />
    </group>
    <group loc="435 0" size="210 220"
      style="Heading1"
      label="FOV &amp; Slice"
      vq="lro"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="2D_FOV"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <menu loc="75 20" size="90 20"
        style="PlainText"
        vq="orient"
        vc="gplan('setValue','orient','$VALUE') calcdim"
        set="$VALUE=orient"
        editable="No"
        >
        <mlabel 
          label="sagittal"
          chval="sag"
          />
        <mlabel 
          label="sagittal90"
          chval="sag90"
          />
        <mlabel 
          label="coronal"
          chval="cor"
          />
        <mlabel 
          label="coronal90"
          chval="cor90"
          />
        <mlabel 
          label="axial"
          chval="trans"
          />
        <mlabel 
          label="axial90"
          chval="trans90"
          />
        <mlabel 
          label="3 planes"
          chval="3orthogonal"
          />
        <mlabel 
          label="oblique"
          chval="oblique"
          />
      </menu>
      <label loc="5 150" size="70 20"
        style="PlainText"
        label="Position"
        justify="Left"
        />
      <label loc="5 110" size="70 20"
        style="PlainText"
        label="Slices"
        justify="Left"
        />
      <entry loc="75 130" size="60 20"
        style="PlainText"
        vq="thk"
        vc="gplan('setValue','thk',$VALUE) go('check')"
        set="$VALUE=thk"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="75 110" size="60 20"
        style="PlainText"
        vq="pss ns"
        vc="gplan('setValue','ns',$VALUE) ns=ns go('check') "
        set="$VALUE=size('pss')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 130" size="70 20"
        style="PlainText"
        label="Thickness"
        justify="Left"
        />
      <label loc="5 170" size="70 20"
        style="PlainText"
        label="Gap"
        justify="Left"
        />
      <entry loc="75 170" size="60 20"
        style="PlainText"
        vq="gap"
        vc="gplan('setValue','gap',$VALUE/10)"
        set="$VALUE=gap*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 170" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="135 150" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="75 150" size="60 20"
        style="PlainText"
        vq="pss0"
        vc="pss0=$VALUE/10.0 gplan('setValue','pss0',$VALUE/10)"
        set="$VALUE=pss0*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 130" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="5 85" size="70 20"
        style="PlainText"
        label="RO Offset"
        justify="Left"
        />
      <label loc="5 65" size="70 20"
        style="PlainText"
        label="Phase"
        justify="Left"
        />
      <entry loc="75 85" size="60 20"
        style="PlainText"
        vq="pro"
        vc="gplan('setValue','pro',$VALUE/10)"
        set="$VALUE=pro*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 85" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="135 65" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="75 65" size="60 20"
        style="PlainText"
        vq="lpe"
        vc="gplan('setValue','lpe',$VALUE/10)  go('check')"
        set="$VALUE=lpe*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 45" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="75 45" size="60 20"
        style="PlainText"
        vq="lro"
        vc="gplan('setValue','lro',$VALUE/10)  go('check')"
        set="$VALUE=lro*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 45" size="70 20"
        style="PlainText"
        label="Readout"
        justify="Left"
        />
      <label loc="5 20" size="70 20"
        style="PlainText"
        label="Orientation"
        justify="Left"
        />
    </group>
    <group loc="5 0" size="210 220"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 60" size="180 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="ne"
          vc="ne=$VALUE go('check')"
          set="$VALUE=ne"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Echoes"
          justify="Left"
          />
      </group>
      <group loc="5 90" size="180 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Averages"
          justify="Left"
          />
      </group>
      <group loc="5 110" size="180 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="ssc"
          vc="ssc=$VALUE"
          set="$VALUE=ssc"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Dummy Scans"
          justify="Left"
          />
      </group>
      <textmessage loc="5 175" size="65 20"
        style="PlainText"
        label=" "
        vq="np"
        set="if np&lt;&gt;64 and np&lt;&gt;128 and np&lt;&gt;256 and np&lt;&gt;512 and np&lt;&gt;1024 and np&lt;&gt;2048 then $VALUE=np/2 else $VALUE='' endif"
        />
      <textmessage loc="70 175" size="65 20"
        style="PlainText"
        label=" "
        vq="nv"
        set="if nv&lt;&gt;32 and nv&lt;&gt;64 and nv&lt;&gt;128 and nv&lt;&gt;256 and nv&lt;&gt;512 and nv&lt;&gt;1024 then $VALUE=nv else $VALUE='' endif"
        />
      <check loc="65 40" size="50 20"
        style="PlainText"
        label="Min"
        vq="minte te"
        vc="minte='y' go('check')"
        vc2="minte='n'  go('check')"
        set="$VALUE=(minte='y')"
        />
      <check loc="65 20" size="50 20"
        style="PlainText"
        label="Min"
        vq="tr mintr"
        vc="mintr='y' go('check')"
        vc2="mintr='n' go('check')"
        set="$VALUE=(mintr='y')"
        />
      <entry loc="115 40" size="60 20"
        style="PlainText"
        vq="te minte"
        vc="te=$VALUE/1000 go('check')"
        set="$VALUE=te*1000.0"
        show="$VALUE = (minte='n')"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="115 20" size="60 20"
        style="PlainText"
        vq="tr mintr"
        vc="tr=$VALUE/1000.0 go('check')"
        set="$VALUE=tr*1000.0"
        show="$VALUE = (mintr='n') "
        digits="2"
        disable="Grayed out"
        />
      <label loc="175 40" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="175 20" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <menu loc="70 155" size="65 20"
        style="PlainText"
        label="menu"
        vq="nv"
        vc="nv=$VALUE go('check')"
        set="$VALUE=nv"
        editable="No"
        >
        <mlabel 
          label="32"
          chval="32"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="96"
          chval="96"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="192"
          chval="192"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
      </menu>
      <menu loc="5 155" size="65 20"
        style="PlainText"
        label="menu"
        vq="np"
        vc="np=$VALUE*2 go('check')"
        set="$VALUE=np/2"
        editable="No"
        >
        <mlabel 
          label="32"
          chval="32"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="96"
          chval="96"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="192"
          chval="192"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
      </menu>
      <label loc="5 135" size="195 20"
        style="PlainText"
        label="Data Matrix (RO x PE)"
        justify="Left"
        />
      <label loc="5 40" size="30 20"
        style="PlainText"
        label="TE"
        justify="Left"
        />
      <label loc="5 20" size="30 20"
        style="PlainText"
        label="TR"
        justify="Left"
        />
    </group>
    <group loc="5 225" size="555 45"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <textmessage loc="300 20" size="130 20"
        style="PlainText"
        label="305_210S_Twin"
        vq="gcoil"
        set="$VALUE = gcoil"
        />
      <textmessage loc="300 0" size="130 20"
        style="PlainText"
        label="main"
        vq="rfcoil"
        set="$VALUE = rfcoil"
        />
      <label loc="220 20" size="80 20"
        style="Heading1"
        label="Gradient"
        justify="Left"
        />
      <label loc="220 0" size="80 20"
        style="Heading1"
        label="RF Coil"
        justify="Left"
        />
      <textmessage loc="80 20" size="130 20"
        style="PlainText"
        label="mems"
        vq="pslabel"
        set="$VALUE = pslabel"
        />
      <entry loc="80 0" size="130 20"
        style="PlainText"
        vq="comment"
        vc="comment='$VALUE' xm1('addcomment')"
        set="$VALUE=comment"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 20" size="80 20"
        style="Heading1"
        label="Protocol"
        justify="Left"
        />
      <label loc="0 0" size="80 20"
        style="Heading1"
        label="Comment"
        justify="Left"
        />
    </group>
  </group>
</template>
