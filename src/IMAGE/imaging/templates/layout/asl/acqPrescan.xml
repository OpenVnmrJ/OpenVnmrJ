<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqPrescan" element="pages" type="acquisition" >
  <group size="800 270"
    label="Prescan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqPrescan"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="5 0" size="350 215"
      style="Heading1"
      label="Tweakers"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="EPItweakers"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="175 140" size="120 20"
        style="Label4"
        label="Reference Scans"
        vc="imEPI('prep','refall') au"
        bg="darkGreen"
        decor1="no"
        />
      <button loc="175 120" size="120 20"
        style="Label4"
        label="Test Image"
        vc="imEPI('prep') wexp='imEPI(`proc`)' au"
        bg="darkGreen"
        decor1="no"
        />
      <group loc="15 120" size="130 80"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="episetup"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <entry loc="90 60" size="30 20"
          style="PlainText"
          vq="cf"
          vc="cf = $VALUE df2d('nf',$VALUE)"
          set="$VALUE = cf"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 60" size="90 20"
          style="Label1"
          label="Display k-space #"
          justify="Left"
          />
        <button loc="0 40" size="120 20"
          style="Heading3"
          label="Phase map"
          vc="epiphms"
          decor1="no"
          />
        <button loc="0 20" size="120 20"
          style="Label4"
          label="EPI setup manual"
          vc="imEPI('prep','ref') au"
          bg="darkGreen"
          decor1="no"
          />
        <button loc="0 0" size="120 20"
          style="Label4"
          label="EPI setup auto"
          vc="imEPI('prep','refauto') au"
          bg="darkGreen"
          decor1="no"
          />
      </group>
      <button loc="255 65" size="90 20"
        style="Label1"
        label="adjust shift"
        vc="wdone='' epishift"
        decor1="no"
        />
      <button loc="255 45" size="90 20"
        style="Label1"
        label="adjust tilt"
        vc="wdone='' epitilt"
        decor1="no"
        />
      <label loc="215 65" size="25 20"
        style="Label1"
        label="us"
        justify="Left"
        />
      <label loc="215 45" size="40 20"
        style="Label1"
        label="G/cm"
        justify="Left"
        />
      <label loc="215 25" size="40 20"
        style="Label1"
        label="G/cm"
        justify="Left"
        />
      <entry loc="155 85" size="60 20"
        style="PlainText"
        vq="ssepi"
        vc="ssepi=$VALUE"
        set="$VALUE=ssepi"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="155 65" size="60 20"
        style="PlainText"
        vq="tep"
        vc="tep = $VALUE"
        set="$VALUE=tep"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="155 45" size="60 20"
        style="PlainText"
        vq="groa"
        vc="groa=$VALUE"
        set="$VALUE=groa"
        show="on('groa'):$VALUE"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="155 25" size="60 20"
        style="PlainText"
        vq="grora"
        vc="grora=$VALUE"
        set="$VALUE=grora"
        show="on('grora'):$VALUE"
        digits="3"
        disable="Grayed out"
        />
      <label loc="5 25" size="150 20"
        style="Label1"
        label="Horizontal Shift (grora)"
        justify="Left"
        />
      <label loc="5 45" size="150 20"
        style="Label1"
        label="Horizontal tilt (groa)"
        justify="Left"
        />
      <label loc="5 65" size="150 20"
        style="Label1"
        label="Echo shift/zipper (tep)"
        justify="Left"
        />
      <label loc="5 85" size="150 20"
        style="Label1"
        label="Gradient Steady State"
        justify="Left"
        />
    </group>
  </group>
</template>
