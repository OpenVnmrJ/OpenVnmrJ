<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqGradients" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="Gradients"
    vq="panellevel show_adv"
    show="$VALUE = (panellevel &gt; 0) AND (show_adv &gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqGradients"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="220 0" size="280 185"
      style="Heading1"
      label="Crushers"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="crusher_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 40" size="265 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrush"
          vc="gcrush=$VALUE go('check')"
          set="$VALUE=gcrush"
          disable="Grayed out"
          />
        <label loc="5 0" size="80 20"
          style="PlainText"
          label="Slice select"
          justify="Left"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrush"
          vc="tcrush=$VALUE/1000"
          set="$VALUE=tcrush*1000"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="5 60" size="265 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushws"
          vc="gcrushws=$VALUE"
          set="$VALUE=gcrushws"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrushws"
          vc="tcrushws=$VALUE/1000.0 go('check')"
          set="$VALUE=tcrushws*1000.0"
          digits="2"
          disable="Grayed out"
          />
        <label loc="5 0" size="60 20"
          style="PlainText"
          label="WS"
          justify="Left"
          />
      </group>
      <group loc="5 80" size="265 20"
        vq="sat"
        show="exists('sat','parameter'):$VALUE"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushsat"
          vc="gcrushsat=$VALUE go('check')"
          set="$VALUE=gcrushsat"
          disable="Grayed out"
          />
        <label loc="5 0" size="80 20"
          style="PlainText"
          label="Satbands"
          justify="Left"
          />
        <entry loc="175 0" size="60 20"
          style="PlainText"
          vq="tcrushsat"
          vc="tcrushsat=$VALUE/1000"
          set="$VALUE=tcrushsat*1000"
          digits="1"
          disable="Grayed out"
          />
      </group>
      <group loc="100 20" size="175 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="80 20"
          style="Label1"
          label="Amp (G/cm)"
          justify="Left"
          />
        <label loc="80 0" size="90 20"
          style="Label1"
          label="Duration (ms)"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 0" size="210 185"
      style="Heading1"
      label="Phase Encoding"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 110" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="160 0" size="35 20"
          style="Label1"
          label="us"
          />
        <textmessage loc="115 0" size="45 20"
          style="PlainText"
          label="32.00"
          vq="tpe"
          set="$VALUE = tpe*1e6"
          digits="2"
          />
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Duration"
          />
      </group>
      <group loc="5 90" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="115 0" size="45 20"
          style="PlainText"
          label="7.340"
          vq="gpe3"
          set="$VALUE = gpe3"
          digits="3"
          />
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Maximum 3 (slice)"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
      </group>
      <group loc="5 70" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Maximum (circle)"
          />
        <textmessage loc="115 0" size="45 20"
          style="PlainText"
          label="10.28"
          vq="gpe"
          set="$VALUE = gpe"
          digits="2"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
      </group>
      <group loc="5 20" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Increment (circle)"
          />
        <textmessage loc="115 0" size="45 20"
          style="PlainText"
          label="3.43"
          vq="gpeincr"
          set="$VALUE = gpeincr"
          digits="2"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
      </group>
      <group loc="5 40" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="115 0" size="45 20"
          style="PlainText"
          label="3.670"
          vq="gpe3incr"
          set="$VALUE = gpe3incr"
          digits="3"
          />
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Increment (slice)"
          />
        <label loc="160 0" size="35 20"
          style="Label1"
          label="G/cm"
          />
      </group>
    </group>
    <button loc="45 205" size="120 20"
      style="Heading3"
      label="Close"
      vc="show_adv = 0"
      bg="transparent"
      decor1="no"
      />
  </group>
</template>
