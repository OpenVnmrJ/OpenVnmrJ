<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Process" element="panels" type="processing" >
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Recon"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc3Drecon"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="645 270"
    style="PlainText"
    label="Weighting"
    vc="exists('gf_factor','parameter'):$e if ($e=0) then create('gf_factor') gf_factor=10 endif exists('sb_factor','parameter'):$e if ($e=0) then create('sb_factor') sb_factor=50 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="proc3Dweight"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Profile"
    vq="profile"
    show="$VALUE =(profile = 'yy') OR (profile = 'y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procProfile"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="800 270"
    style="PlainText"
    label="Nifti"
    vq="niftipage"
    vc="aipXrecon('genpars') aipXrecon('nifti')"
    show="$VALUE=(niftipage='y')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="procNifti"
    useref="yes"
    subtype="Convenience"
    expanded="no"
    >
  </group>
  <group loc="0 0" size="699 270"
    style="Heading2"
    label="Text Output"
    bg="transparent"
    border="None"
    side="Top"
    justify="Right"
    tab="yes"
    enable="no"
    reference="TextOutput"
    useref="yes"
    subtype="Basic"
    expanded="no"
    >
  </group>
</template>
