<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqOptions" element="pages" type="acquisition" >
  <group size="800 290"
    style="Heading1"
    label="Options"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqOptions"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="480 0" size="210 225"
      style="Heading1"
      label="Tagging"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="cine_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 165" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="tcrushtag"
          vc="tcrushtag = $VALUE/1000 go('check')"
          set="$VALUE = tcrushtag*1000"
          />
        <label loc="0 0" size="100 20"
          style="Label1"
          label="crusher duration"
          />
      </group>
      <group loc="5 145" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="100 20"
          style="Label1"
          label="crusher gradient"
          />
        <label loc="160 0" size="40 20"
          style="Label1"
          label="G/cm"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="gcrushtag"
          vc="gcrushtag= $VALUE"
          set="$VALUE = gcrushtag"
          digits="2"
          />
      </group>
      <label loc="5 100" size="100 20"
        style="Label1"
        label="RF pulse width"
        />
      <entry loc="105 120" size="60 20"
        style="PlainText"
        vq="fliptag"
        vc="fliptag= $VALUE"
        set="$VALUE = fliptag"
        digits="2"
        />
      <entry loc="105 100" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw = $VALUE go('check')"
        set="$VALUE = pw"
        digits="2"
        />
      <label loc="165 60" size="30 20"
        style="Label1"
        label="mm"
        />
      <label loc="165 120" size="25 20"
        style="Label1"
        label="us"
        />
      <label loc="165 100" size="25 20"
        style="Label1"
        label="us"
        />
      <label loc="165 80" size="25 20"
        style="Label1"
        label="us"
        />
      <entry loc="105 80" size="60 20"
        style="PlainText"
        vq="ptag"
        vc="ptag = $VALUE go('check')"
        set="$VALUE = ptag"
        digits="1"
        />
      <entry loc="105 60" size="60 20"
        style="PlainText"
        vq="dtag"
        vc="dtag = $VALUE/10 go('check')"
        set="$VALUE = dtag*10"
        digits="2"
        />
      <entry loc="105 40" size="60 20"
        style="PlainText"
        vq="wtag"
        vc="wtag = $VALUE/10 go('check')"
        set="$VALUE = wtag*10"
        digits="2"
        />
      <label loc="165 40" size="30 20"
        style="Label1"
        label="mm"
        />
      <menu loc="95 20" size="105 20"
        style="PlainText"
        label="menu"
        vq="tag tagdir"
        vc="tagdir= $VALUE if (tagdir &gt; 0 ) then tag='y'  else tag='n' endif go('check')"
        set="$VALUE = tagdir"
        editable="No"
        >
        <mlabel 
          label="None"
          chval="0"
          />
        <mlabel 
          label="Readout"
          chval="1"
          />
        <mlabel 
          label="Phase"
          chval="2"
          />
        <mlabel 
          label="Both"
          chval="3"
          />
      </menu>
      <group loc="5 21" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Direction"
          />
      </group>
      <group loc="5 121" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="RF flip angle"
          />
      </group>
      <group loc="5 101" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
      </group>
      <group loc="5 81" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="RF train width"
          />
      </group>
      <group loc="5 61" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Separation"
          />
      </group>
      <group loc="5 41" size="195 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Width"
          />
      </group>
    </group>
    <group loc="265 0" size="210 225"
      style="Heading1"
      label="CINE"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="CINE"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 100" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="150 0" size="40 20"
          style="PlainText"
          label="b/min"
          justify="Left"
          />
        <entry loc="90 0" size="60 20"
          style="PlainText"
          vq="heartrate"
          vc="heartrate = $VALUE "
          set="$VALUE = heartrate"
          />
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Heart rate"
          />
      </group>
      <check loc="5 120" size="180 20"
        style="PlainText"
        label="Specify inter-phase delay"
        vq="user_idelay"
        vc="user_idelay=1 go('check')"
        vc2="user_idelay=0 go('check')"
        set="$VALUE = user_idelay=1"
        />
      <group loc="5 140" size="200 20"
        vq="user_idelay"
        show="$VALUE = (user_idelay = 1)"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="150 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="90 0" size="60 20"
          style="PlainText"
          vq="idelay"
          vc="idelay = $VALUE/1000 go('check')"
          set="$VALUE = idelay*1000"
          />
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Delay"
          />
      </group>
      <entry loc="95 175" size="90 20"
        style="PlainText"
        vq="fliptable"
        vc="fliptable = '$VALUE'"
        set="$VALUE = fliptable"
        />
      <label loc="5 175" size="90 20"
        style="Label1"
        label="Flip Table"
        />
      <check loc="5 20" size="85 20"
        style="PlainText"
        label="Triggered"
        vq="ticks"
        vc="ticks = 1"
        vc2="ticks = 0"
        set="$VALUE = (ticks &gt; 0)"
        />
      <group loc="5 40" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="90 0" size="60 20"
          style="PlainText"
          vq="ne"
          vc="ne = $VALUE go('check')"
          set="$VALUE = ne"
          />
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Frames"
          />
      </group>
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="150 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="90 0" size="60 20"
          style="PlainText"
          vq="rrdelay"
          vc="rrdelay = $VALUE/1000 go('check')"
          set="$VALUE = rrdelay*1000"
          />
        <label loc="0 0" size="90 20"
          style="Label1"
          label="R-R interval"
          />
      </group>
      <group loc="5 80" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="150 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="90 0" size="60 20"
          style="PlainText"
          vq="qrsdelay"
          vc="qrsdelay = $VALUE/1000 go('check')"
          set="$VALUE = qrsdelay*1000"
          />
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Delay after"
          />
      </group>
    </group>
    <group loc="5 155" size="255 70"
      style="Heading1"
      label="Spatial Sat"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SpatialSat"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="215 40" size="30 20"
        style="Label1"
        label="ms"
        />
      <entry loc="155 40" size="60 20"
        style="PlainText"
        vq="psat sat"
        vc="psat=$VALUE*1000.0 go('check')"
        set="$VALUE=psat/1000.0"
        show="if (sat = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        />
      <label loc="90 40" size="65 20"
        style="Label1"
        label="Duration"
        />
      <entry loc="155 20" size="60 20"
        style="PlainText"
        vq="flipsat sat"
        vc="flipsat=$VALUE"
        set="$VALUE=flipsat"
        show="if (sat = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        />
      <label loc="90 20" size="65 20"
        style="Label1"
        label="Flip Angle"
        />
      <label loc="215 20" size="30 20"
        style="Label1"
        label="deg"
        />
      <check loc="5 20" size="85 20"
        style="PlainText"
        label="Spatial Sat"
        vq="sat satthk nsat"
        vc="sat='y' nsat=size('satthk') go('check')"
        vc2="sat='n' nsat=size('satthk') go('check')"
        set="$VALUE=(sat='y')"
        show="on('sat'):$VALUE"
        />
    </group>
    <group loc="5 0" size="255 70"
      style="Heading1"
      label="Inversion Recovery"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SpatialSat"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="155 40" size="60 20"
        style="PlainText"
        vq="thkf"
        vc="thkf=$VALUE go('check')"
        set="$VALUE=thkf"
        digits="2"
        disable="Grayed out"
        />
      <label loc="90 40" size="65 20"
        style="PlainText"
        label="thk factor"
        justify="Left"
        />
      <check loc="5 40" size="50 20"
        style="PlainText"
        label="DIR"
        vq="dir ir"
        vc="dir='y'  ir='y' go('check')"
        vc2="dir='n' ir='n' unarray('ti') go('check')"
        set="$VALUE=(dir='y')"
        />
      <label loc="215 20" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="155 20" size="60 20"
        style="PlainText"
        vq="ti ir"
        vc="ti=$VALUE/1000.0 go('check')"
        set="$VALUE=ti*1000.0"
        show="if (ir = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="90 20" size="65 20"
        style="PlainText"
        label="TI"
        justify="Left"
        />
      <check loc="5 20" size="40 20"
        style="PlainText"
        label="IR"
        vq="ir"
        vc="ir='y' ti=ti go('check')"
        vc2="ir='n' unarray('ti') go('check')"
        set="$VALUE=(ir='y')"
        />
    </group>
    <group loc="5 70" size="255 85"
      style="Heading1"
      label="Fat Saturation"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FatSaturation"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 20" size="60 20"
        style="PlainText"
        label="Fatsat"
        vq="fsat Prescan_FatOffset"
        vc="fsat='y'  fsatfrq=fsatfrq pfsat=pfsat if (autooffset='y') then fsatfrq=Prescan_FatOffset endif go('check')"
        vc2="fsat='n' unarray('fsatfrq','pfsat')   go('check')"
        set="$VALUE=(fsat='y')"
        />
      <check loc="5 60" size="80 20"
        style="PlainText"
        label="Auto offset"
        vq="fsat fsatfrq autooffset"
        vc="autooffset='y' fsatfrq=Prescan_FatOffset"
        vc2="autooffset='n'"
        set="$VALUE=(autooffset='y')"
        show="if (fsat = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        />
      <label loc="90 20" size="65 20"
        style="PlainText"
        label="Flip Angle"
        justify="Left"
        />
      <label loc="90 40" size="65 20"
        style="PlainText"
        label="Duration"
        justify="Left"
        />
      <label loc="90 60" size="65 20"
        style="PlainText"
        label="Offset"
        justify="Left"
        />
      <entry loc="155 20" size="60 20"
        style="PlainText"
        vq="flipfsat fsat"
        vc="flipfsat=$VALUE"
        set="$VALUE=flipfsat"
        show="if (fsat = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="155 40" size="60 20"
        style="PlainText"
        vq="pfsat fsat"
        vc="pfsat=$VALUE*1000.0 go('check')"
        set="$VALUE=pfsat/1000.0"
        show="if (fsat = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="155 60" size="60 20"
        style="PlainText"
        vq="fsat fsatfrq"
        vc="fsatfrq=$VALUE autooffset=n"
        set="$VALUE=fsatfrq"
        show="if (fsat = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="215 20" size="30 20"
        style="PlainText"
        label="deg"
        justify="Left"
        />
      <label loc="215 40" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="215 60" size="30 20"
        style="PlainText"
        label="Hz"
        justify="Left"
        />
    </group>
  </group>
</template>
