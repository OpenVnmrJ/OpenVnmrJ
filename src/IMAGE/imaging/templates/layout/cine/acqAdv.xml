      subtype="Major"
      expanded="yes"
      >
      <check loc="5 60" size="125 20"
        style="PlainText"
        label="Gradient Spoil"
        vq="spoilflag"
        vc="spoilflag='y' go('check')"
        vc2="spoilflag='n' go('check')"
        set="$VALUE=(spoilflag='y')"
        />
      <radio loc="175 80" size="30 20"
        style="PlainText"
        label="3"
        vq="trigger"
        vc="trigger=3"
        set="$VALUE=(trigger=3)"
        />
      <radio loc="145 80" size="30 20"
        style="PlainText"
        label="2"
        vq="trigger"
        vc="trigger=2"
        set="$VALUE=(trigger=2)"
        />
      <radio loc="115 80" size="30 20"
        style="PlainText"
        label="1"
        vq="trigger"
