          />
        <mlabel 
          label="15"
          chval="15"
          />
        <mlabel 
          label="16"
          chval="16"
          />
      </menu>
      <label loc="5 120" size="90 20"
        style="PlainText"
        label="K-step"
        justify="Left"
        />
      <group loc="5 100" size="190 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="grocalc"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <menu loc="110 0" size="60 20"
          style="PlainText"
          label="menu"
          vc="grocalc = '$VALUE' prep go('check')"
          set="$VALUE = grocalc"
          bg="VJBackground"
          editable="No"
          >
          <mlabel 
            label="Auto"
            chval="auto"
            />
          <mlabel 
            label="SW"
            chval="sw"
            />
          <mlabel 
            label="GRO"
            chval="gro"
            />
        </menu>
        <label loc="0 0" size="110 20"
          style="Label1"
          label="GRO/SW Priority"
          justify="Left"
          />
      </group>
      <label loc="5 140" size="90 20"
        style="PlainText"
        label="PE Table"
        justify="Left"
        />
      <textmessage loc="115 140" size="90 20"
        style="PlainText"
        label="n"
        vq="petable"
        set="$VALUE=petable"
        />
      <label loc="5 160" size="105 20"
        style="PlainText"
        label="Single Phase ETL"
        justify="Left"
        />
      <entry loc="115 160" size="55 20"
        style="PlainText"
        vq="etl nv localizer"
        vc="etl=$VALUE go('check') locetl=etl"
        set="$VALUE=locetl"
        show="if (localizer='y') then $VALUE=1 else $VALUE=-1 endif"
        />
      <group loc="5 180" size="200 40"
        vq="coil_param"
        show="$VALUE=(coil_param[20]=3)"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="tmp"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="0 0" size="190 20"
          style="PlainText"
          label="Volume Coil Receive"
          vq="volumercv rcvrs"
          vc="create('local_volumercv','flag'):$e local_volumercv=y volumercv=y rcvrs=y"
          vc2="create('local_volumercv','flag'):$e local_volumercv=n  ldrf"
          set="$VALUE=(local_volumercv=y)"
          />
        <entry loc="85 20" size="80 20"
          style="PlainText"
          vq="rcvrs volumercv"
          vc="rcvrs='$VALUE'"
          set="$VALUE=rcvrs"
          />
        <label loc="18 20" size="70 20"
          style="Label1"
          label="Receivers"
          />
      </group>
      <check loc="5 40" size="185 20"
        style="PlainText"
        label="Reset CINE loop RF spoil"
        vq="rfspoilreset rfspoil"
        vc="rfspoilreset='y' go('check')"
        vc2="rfspoilreset='n' go('check')"
        set="$VALUE=(rfspoilreset='y')"
        show="if (rfspoil='y') then $VALUE=1 else $VALUE=-1 endif"
        />
      <check loc="5 20" size="105 20"
        style="PlainText"
        label="RF Spoiling"
        vq="rfspoil"
        vc="rfspoil='y' go('check')"
        vc2="rfspoil='n' go('check')"
        set="$VALUE=(rfspoil='y')"
        />
