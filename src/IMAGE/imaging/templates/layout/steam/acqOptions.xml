<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqOptions" element="pages" type="acquisition" >
  <group size="800 255"
    style="Heading1"
    label="Water Suppr"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqOptions"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <toggle loc="495 185" size="120 20"
      style="PlainText"
      label="More"
      vq="moreRF"
      vc="moreRF='y'"
      vc2="moreRF='n'"
      set="$VALUE=(moreRF='y')"
      enable="yes"
      radiobutton="no"
      subtype="no"
      />
    <group loc="220 125" size="225 110"
      style="Heading1"
      label="WS RF (TM)"
      vq="ws_tm"
      show="$VALUE=(ws_tm='y')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MultiplierAll"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="10 20" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="70 20"
          style="PlainText"
          label="Bandwidth"
          justify="Left"
          />
        <entry loc="70 0" size="45 20"
          style="PlainText"
          vq="p4 p4pat tmbw"
          vc="tmbw = $VALUE calc_wspw(p4pat,tmbw):p4 p4='n' go('check')"
          set="$VALUE = tmbw"
          show="on('p4'):$on $VALUE = ($on = 0)"
          digits="0"
          disable="Grayed out"
          />
        <label loc="120 0" size="30 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <button loc="155 0" size="50 20"
          style="PlainText"
          label="Cursors"
          vq="pwtm"
          vc="tmbw = delta calc_wspw(p4pat,tmbw):pwtm pwtm=n go('check')"
          bg="transparent"
          decor1="no"
          />
      </group>
      <group loc="10 40" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="70 0" size="135 20"
          style="PlainText"
          vq="p4pat p4"
          vc="p4pat='$VALUE' calc_wspw(p4pat,0,p4):tmbw go('check')"
          set="$VALUE=p4pat"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Shape"
          justify="Left"
          />
      </group>
      <group loc="10 60" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="70 0" size="45 20"
          style="PlainText"
          vq="p4"
          vc="p4=$VALUE*1000.0 calc_wspw(p4pat,0,p4):tmbw go('check')"
          set="$VALUE=p4/1000.0"
          show="on('p4'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <label loc="115 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Duration"
          justify="Left"
          />
      </group>
      <group loc="10 80" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="70 0" size="45 20"
          style="PlainText"
          vq="wsflipftm"
          vc="wsflipftm=$VALUE"
          set="$VALUE=wsflipftm"
          digits="2"
          disable="Grayed out"
          />
        <button loc="145 0" size="65 20"
          style="Label4"
          label="Optimize"
          vc="ws_opt='tm' ws_opt('tm')"
          bg="darkGreen"
          decor1="no"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="TM RF"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 125" size="210 110"
      style="Heading1"
      label="Water Reference"
      vq="wsrf"
      show="$VALUE=(wsrf='y')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FatSaturation"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="195 20"
        vq="wsrf"
        show="$VALUE=(wsrf='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="0 0" size="175 20"
          style="PlainText"
          label="Acquire Water Reference"
          vq="wref"
          vc="wref='y' go('check') wrefstatus='wref'"
          vc2="wref='n' go('check')"
          set="$VALUE = (wref='y')"
          show="exists('wsrf','parameter'):$VALUE"
          tooltip="Acquires and saves water reference"
          />
      </group>
      <group loc="5 40" size="195 20"
        vq="wref"
        show="$VALUE=(wref='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <entry loc="130 0" size="25 20"
          style="PlainText"
          vq="wrefgain"
          vc="wrefgain=$VALUE go('check')"
          set="$VALUE=wrefgain"
          digits="0"
          disable="Grayed out"
          />
        <label loc="160 0" size="25 20"
          style="PlainText"
          label="dB"
          justify="Left"
          />
        <label loc="5 0" size="105 20"
          style="PlainText"
          label="Water Ref Gain"
          justify="Left"
          />
      </group>
      <group loc="5 60" size="195 20"
        vq="wref"
        show="$VALUE=(wref='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="5 0" size="120 20"
          style="PlainText"
          label="Water Ref Averages"
          justify="Left"
          />
        <entry loc="130 0" size="25 20"
          style="PlainText"
          vq="wrefnt"
          vc="wrefnt=$VALUE go('check')"
          set="$VALUE=wrefnt"
          digits="0"
          disable="Grayed out"
          />
      </group>
      <group loc="5 80" size="195 20"
        vq="wref arraydim"
        show="$VALUE=(wref='y' and arraydim&gt;1)"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="WaterRefGain"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <check loc="0 0" size="175 20"
          style="PlainText"
          label="Array Water Reference"
          vq="wref wrefarray arraydim"
          vc="wrefarray='y' go('check') "
          vc2="wrefarray='n' go('check')"
          set="$VALUE = (wrefarray='y')"
          show="$VALUE=(arraydim&gt;1)"
          tooltip="If checked, water reference will use the same arrayed parameter"
          />
      </group>
    </group>
    <button loc="495 160" size="120 20"
      style="Label4"
      label="Test Scan"
      vc="im1D('prep') wexp='im1D(`proc`)' au"
      bg="darkGreen"
      decor1="no"
      />
    <group loc="450 0" size="225 125"
      style="Heading1"
      label="More Calibration"
      vq="moreRF"
      show="$VALUE=(moreRF='y')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="a_aMore"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="10 85" size="120 20"
        style="Label4"
        label="Optimize - flip(F)"
        vc="ws_opt='f' ws_opt('flipF')"
        bg="darkGreen"
        decor1="no"
        />
      <button loc="10 65" size="120 20"
        style="Label4"
        label="Optimize - flip(C)"
        vc="ws_opt='c' ws_opt"
        bg="darkGreen"
        decor1="no"
        />
      <button loc="10 45" size="120 20"
        style="Label4"
        label="Optimize All"
        vq="ws_opt"
        vc="ws_opt='cf'  ws_opt"
        bg="darkGreen"
        decor1="no"
        />
      <entry loc="175 20" size="40 20"
        style="PlainText"
        vq="wsflipf"
        vc="wsflipf=$VALUE"
        set="$VALUE=wsflipf"
        digits="2"
        disable="Grayed out"
        />
      <label loc="10 20" size="160 20"
        style="PlainText"
        label="Flip Angle Mult For All WS"
        justify="Left"
        />
    </group>
    <group loc="5 0" size="210 125"
      style="Heading1"
      label="Suppression Options"
      vq="ws"
      show="exists('ws','parameter'):$VALUE"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="a_aSupTM"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="110 20" size="95 20"
        vq="tm"
        show="on('tm'):r1 $VALUE=(r1=1)"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="85 20"
          style="PlainText"
          label="Sup On TM"
          vq="ws_tm ws wsrf"
          vc="ws_tm='y' go('check') wsrf='y'"
          vc2="ws_tm='n' go('check') wref='n'"
          set="$VALUE = (ws_tm='y')"
          />
      </group>
      <group loc="10 40" size="95 20"
        vq="wsrf"
        show="$VALUE = (wsrf= 'n')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 0" size="70 20"
          style="PlainText"
          label="None"
          vq="ws wsrf"
          vc="ws='n' go('check')"
          vc2="ws='y' wsrf='n' go('check')"
          set="$VALUE = (ws='n')"
          tooltip="no ws unit is played "
          />
      </group>
      <menu loc="10 65" size="105 20"
        style="PlainText"
        label="Supression Method"
        vq="wss "
        vc="wss = '$VALUE'"
        set="$VALUE = wss"
        show="$VALUE=(ws='y')"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="DRY"
          chval="dry"
          />
        <mlabel 
          label="WET"
          chval="wet"
          />
        <mlabel 
          label="VAPOR"
          chval="vapor"
          />
        <mlabel 
          label="Single pulse"
          chval="1chess"
          />
      </menu>
      <group loc="10 85" size="195 20"
        vq="wss ws"
        show="$VALUE = (wss &lt;&gt; 'vapor' and ws='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="115 0" size="50 20"
          style="PlainText"
          vq="ws_delay"
          vc="ws_delay=$VALUE/1000.0 go('check')"
          set="$VALUE=ws_delay*1000"
          digits="2"
          disable="Grayed out"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <label loc="0 0" size="105 20"
          style="PlainText"
          label="Inter-pulse delay"
          justify="Left"
          />
      </group>
      <check loc="10 20" size="75 20"
        style="PlainText"
        label="Sup On"
        vq="ws wsrf ws_tm wref"
        vc="ws='y' wsrf='y' go('check')"
        vc2="ws='y' wsrf='n' go('check') ws_tm='n' wref='n'"
        set="$VALUE = (ws='y' and wsrf='y')"
        />
    </group>
    <group loc="220 0" size="225 125"
      style="Heading1"
      label="WS RF"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="RFPulse"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="125 0" size="30 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <entry loc="75 0" size="40 20"
          style="PlainText"
          vq="pws pwpat wsbw"
          vc="wsbw = $VALUE calc_wspw(wspat,wsbw):pws pws='n' go('check')"
          set="$VALUE = wsbw"
          show="on('pws'):$on $VALUE = ($on = 0)"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="70 20"
          style="PlainText"
          label="Bandwidth"
          justify="Left"
          />
        <button loc="160 0" size="50 20"
          style="PlainText"
          label="Cursors"
          vq="pws"
          vc="wsbw = delta calc_wspw(wspat,wsbw):pws pws=n go('check')"
          bg="transparent"
          decor1="no"
          />
      </group>
      <group loc="5 60" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="125 0" size="30 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="75 0" size="45 20"
          style="PlainText"
          vq="pws"
          vc="pws=$VALUE*1000.0 calc_wspw(wspat,0,pws):wsbw go('check')"
          set="$VALUE=pws/1000.0"
          show="on('pws'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Duration"
          justify="Left"
          />
      </group>
      <group loc="5 40" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="75 0" size="135 20"
          style="PlainText"
          vq="wspat"
          vc="wspat='$VALUE' go('check') calc_wspw(wspat,0,pws):wsbw"
          set="$VALUE=wspat"
          disable="Grayed out"
          />
        <label loc="0 0" size="70 20"
          style="PlainText"
          label="Shape"
          justify="Left"
          />
      </group>
      <group loc="5 100" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="120 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="75 0" size="45 20"
          style="PlainText"
          vq="d1"
          vc="d1=$VALUE/1000.0 "
          set="$VALUE=d1*1000.0"
          show="on('d1'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <button loc="145 0" size="65 20"
          style="Label4"
          label="Optimize"
          vc="ws_opt='d' ws_opt('d1')"
          bg="darkGreen"
          decor1="no"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Delay"
          justify="Left"
          />
      </group>
      <group loc="5 80" size="210 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="75 0" size="45 20"
          style="PlainText"
          vq="wsflipf_last"
          vc="wsflipf_last=$VALUE"
          set="$VALUE=wsflipf_last"
          digits="2"
          disable="Grayed out"
          />
        <button loc="145 0" size="65 20"
          style="Label4"
          label="Optimize"
          vc="ws_opt='rf' ws_opt('rf')"
          bg="darkGreen"
          decor1="no"
          tooltip="Flip Angle Mult for the last RF in WS"
          />
        <label loc="0 0" size="60 20"
          style="PlainText"
          label="Last RF"
          justify="Left"
          />
      </group>
    </group>
  </group>
</template>
