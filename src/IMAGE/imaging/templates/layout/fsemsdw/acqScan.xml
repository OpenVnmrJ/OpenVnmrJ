<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="800 270"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="435 0" size="210 225"
      style="Heading1"
      label="FOV &amp; Slice"
      vq="lro"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="2D_FOV"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <menu loc="75 20" size="100 20"
        style="PlainText"
        vq="orient"
        vc="gplan('setValue','orient','$VALUE') calcdim"
        set="$VALUE=orient"
        editable="No"
        >
        <mlabel
          label="Axial"
          chval="trans"
          />
        <mlabel
          label="Coronal"
          chval="cor"
          />
        <mlabel
          label="Sagittal"
          chval="sag"
          />
        <mlabel
          label="3 Planes"
          chval="3orthogonal"
          />
        <mlabel
          label="Axial 90"
          chval="trans90"
          />
        <mlabel
          label="Coronal 90"
          chval="cor90"
          />
        <mlabel
          label="Sagittal 90"
          chval="sag90"
          />
        <mlabel
          label="Oblique"
          chval="oblique"
          />
      </menu>
      <label loc="5 150" size="70 20"
        style="PlainText"
        label="Position"
        justify="Left"
        />
      <label loc="5 110" size="70 20"
        style="PlainText"
        label="Slices"
        justify="Left"
        />
      <entry loc="75 130" size="60 20"
        style="PlainText"
        vq="thk"
        vc="gplan('setValue','thk',$VALUE) prep go('check')"
        set="$VALUE=thk"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="75 110" size="60 20"
        style="PlainText"
        vq="pss ns"
        vc="gplan('setValue','ns',$VALUE) ns=ns go('check') "
        set="$VALUE=size('pss')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="5 130" size="70 20"
        style="PlainText"
        label="Thickness"
        justify="Left"
        />
      <label loc="5 170" size="70 20"
        style="PlainText"
        label="Gap"
        justify="Left"
        />
      <entry loc="75 170" size="60 20"
        style="PlainText"
        vq="gap"
        vc="gplan('setValue','gap',$VALUE/10)"
        set="$VALUE=gap*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 170" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="135 150" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="75 150" size="60 20"
        style="PlainText"
        vq="pss0"
        vc="pss0=$VALUE/10.0 gplan('setValue','pss0',$VALUE/10)"
        set="$VALUE=pss0*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 130" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="5 85" size="70 20"
        style="PlainText"
        label="RO Offset"
        justify="Left"
        />
      <label loc="5 65" size="70 20"
        style="PlainText"
        label="Phase"
        justify="Left"
        />
      <entry loc="75 85" size="60 20"
        style="PlainText"
        vq="pro"
        vc="gplan('setValue','pro',$VALUE/10)"
        set="$VALUE=pro*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 85" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="135 65" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="75 65" size="60 20"
        style="PlainText"
        vq="lpe"
        vc="gplan('setValue','lpe',$VALUE/10) prep go('check')"
        set="$VALUE=lpe*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="135 45" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="75 45" size="60 20"
        style="PlainText"
        vq="lro"
        vc="gplan('setValue','lro',$VALUE/10) prep go('check')"
        set="$VALUE=lro*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 45" size="70 20"
        style="PlainText"
        label="Readout"
        justify="Left"
        />
      <label loc="5 20" size="70 20"
        style="PlainText"
        label="Orientation"
        justify="Left"
        />
    </group>
    <group loc="220 0" size="210 225"
      style="Heading1"
      label="Scan Options"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ScanOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 130" size="180 20"
        style="PlainText"
        label="External Trigger"
        vq="ticks"
        vc="ticks = 1"
        vc2="ticks = 0"
        set="$VALUE=(ticks &gt; 0)"
        />
      <check loc="5 100" size="160 20"
        style="PlainText"
        label="Spatial Sat"
        vq="sat"
        vc="sat='y' go('check')"
        vc2="sat='n' go('check')"
        set="$VALUE=(sat='y')"
        />
      <check loc="5 40" size="120 20"
        style="PlainText"
        label="Diffusion"
        vq="diff"
        vc="diff='y' prep go('check')"
        vc2="diff='n' prep go('check')"
        set="$VALUE=(diff='y')"
        />
      <check loc="5 60" size="120 20"
        style="PlainText"
        label="MTC"
        vq="mt"
        vc="mt='y' pmt=pmt mtfrq=mtfrq go('check')"
        vc2="mt='n' unarray('pmt','mtfrq') go('check')"
        set="$VALUE=(mt='y')"
        />
      <check loc="5 80" size="120 20"
        style="PlainText"
        label="Fatsat"
        vq="fsat Prescan_FatOffset"
        vc="fsat='y'  fsatfrq=fsatfrq pfsat=pfsat if (autooffset='y') then fsatfrq=Prescan_FatOffset endif go('check')"
        vc2="fsat='n' unarray('fsatfrq','pfsat')   go('check')"
        set="$VALUE=(fsat='y')"
        />
      <check loc="5 20" size="120 20"
        style="PlainText"
        label="Navigator"
        vq="navigator"
        vc="navigator='y' prep go('check')"
        vc2="navigator='n' prep go('check')"
        set="$VALUE=(navigator='y')"
        />
    </group>
    <group loc="5 0" size="210 225"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 60" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="105 20"
          style="PlainText"
          label="TE (First echo)"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="te1"
          vc="te1=$VALUE/1000 go('check')"
          set="$VALUE=te1*1000.0"
          digits="2"
          disable="Grayed out"
          />
      </group>
      <group loc="5 80" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="55 20"
          style="PlainText"
          label="ESP"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="esp minesp"
          vc="esp=$VALUE/1000 go('check')"
          set="$VALUE=esp*1000.0"
          show="$VALUE = (minesp='n')"
          digits="2"
          disable="Grayed out"
          />
        <check loc="60 0" size="50 20"
          style="PlainText"
          label="Min"
          vq="minesp esp"
          vc="minesp='y' go('check') imprep go('check')"
          vc2="minesp='n' go('check')"
          set="$VALUE=(minesp='y')"
          />
      </group>
      <group loc="5 100" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="110 0" size="60 20"
          style="PlainText"
          label="21.65"
          vq="te"
          set="$VALUE = '' format(te*1000,0,2):$VALUE"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Effective TE"
          justify="Left"
          />
      </group>
      <group loc="5 120" size="185 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="nt"
          vc="nt=$VALUE go('check')"
          set="$VALUE=nt"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Averages"
          justify="Left"
          />
      </group>
      <group loc="5 140" size="185 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="ssc"
          vc="ssc=$VALUE go('check')"
          set="$VALUE=ssc"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="110 20"
          style="PlainText"
          label="Dummy Scans"
          justify="Left"
          />
      </group>
      <group loc="5 160" size="195 60"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="65 40" size="65 20"
          style="PlainText"
          label=" "
          vq="nv"
          set="if nv&lt;&gt;32 and nv&lt;&gt;64 and nv&lt;&gt;128 and nv&lt;&gt;256 and nv&lt;&gt;512 and nv&lt;&gt;1024 then $VALUE=nv else $VALUE='' endif"
          />
        <textmessage loc="0 40" size="65 20"
          style="PlainText"
          label=" "
          vq="np"
          set="if np&lt;&gt;64 and np&lt;&gt;128 and np&lt;&gt;256 and np&lt;&gt;512 and np&lt;&gt;1024 and np&lt;&gt;2048 then $VALUE=np/2 else $VALUE='' endif"
          />
        <menu loc="65 20" size="65 20"
          style="PlainText"
          label="menu"
          vq="nv"
          vc="nv=$VALUE prep go('check')"
          set="$VALUE=nv"
          editable="No"
          >
          <mlabel 
            label="0"
            chval="1024"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="96"
            chval="96"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="192"
            chval="192"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <menu loc="0 20" size="65 20"
          style="PlainText"
          label="menu"
          vq="np"
          vc="np=$VALUE*2 go('check')"
          set="$VALUE=np/2"
          editable="No"
          >
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="96"
            chval="96"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="192"
            chval="192"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="32"
            chval="1024"
            />
        </menu>
        <label loc="0 0" size="195 20"
          style="PlainText"
          label="Data Matrix (RO x PE)"
          justify="Left"
          />
      </group>
      <group loc="5 40" size="170 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="0 0" size="55 20"
          style="PlainText"
          label="ETL"
          justify="Left"
          />
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="etl"
          vc="etl=$VALUE prep go('check')"
          set="$VALUE=etl"
          digits="0"
          disable="Grayed out"
          />
      </group>
      <group loc="5 20" size="200 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="110 0" size="60 20"
          style="PlainText"
          vq="tr mintr"
          vc="tr=$VALUE/1000.0 go('check')"
          set="$VALUE=tr*1000.0"
          show="$VALUE = (mintr='n') "
          digits="2"
          disable="Grayed out"
          />
        <label loc="170 0" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <check loc="60 0" size="50 20"
          style="PlainText"
          label="Min"
          vq="tr mintr fsat sat mt"
          vc="mintr='y' go('check')"
          vc2="mintr='n' go('check')"
          set="$VALUE=(mintr='y')"
          />
        <label loc="0 0" size="30 20"
          style="PlainText"
          label="TR"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 230" size="725 40"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="Protocol"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="430 0" size="215 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="85 0" size="130 20"
          style="PlainText"
          label="1.0s"
          vq="scantime"
          set="$VALUE = scantime"
          />
        <label loc="0 0" size="80 20"
          style="Heading1"
          label="Scan Time"
          justify="Left"
          />
      </group>
      <textmessage loc="300 20" size="130 20"
        style="PlainText"
        label="205_120_HD"
        vq="gcoil"
        set="$VALUE = gcoil"
        />
      <textmessage loc="300 0" size="130 20"
        style="PlainText"
        label="main"
        vq="rfcoil"
        set="$VALUE = rfcoil"
        />
      <label loc="220 20" size="80 20"
        style="Heading1"
        label="Gradient"
        justify="Left"
        />
      <label loc="220 0" size="80 20"
        style="Heading1"
        label="RF Coil"
        justify="Left"
        />
      <textmessage loc="80 20" size="130 20"
        style="PlainText"
        label="fsemsdw_nav"
        vq="pslabel"
        set="$VALUE = pslabel"
        />
      <entry loc="80 0" size="130 20"
        style="PlainText"
        vq="comment"
        vc="comment='$VALUE' xm1('addcomment')"
        set="$VALUE=comment"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 20" size="80 20"
        style="Heading1"
        label="Protocol"
        justify="Left"
        />
      <label loc="0 0" size="80 20"
        style="Heading1"
        label="Comment"
        justify="Left"
        />
    </group>
  </group>
</template>
