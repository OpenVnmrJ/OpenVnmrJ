<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="800 270"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <button loc="240 20" size="110 20"
      style="Label4"
      label="Go"
      vc="go"
      bg="Active"
      decor1="no"
      />
    <check loc="240 45" size="115 20"
      style="PlainText"
      label="External Trigger"
      vq="ticks"
      vc="exists('nsblock','parameter'):$ex if ($ex=0) then create('nsblock','integer') endif ticks = 1 go('check')"
      vc2="exists('nsblock','parameter'):$ex if ($ex=0) then create('nsblock','integer') endif ticks = 0 nsblock=1 go('check')"
      set="$VALUE=(ticks&gt;0)"
      />
    <group loc="5 0" size="210 220"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="5 175" size="70 20"
        style="PlainText"
        label="Axis"
        justify="Left"
        />
      <menu loc="115 175" size="60 20"
        style="PlainText"
        vq="orient"
        vc="gplan('setValue','orient','$VALUE')"
        set="$VALUE=orient"
        editable="No"
        >
        <mlabel 
          label="X"
          chval="trans90"
          />
        <mlabel 
          label="Y"
          chval="trans"
          />
        <mlabel 
          label="Z"
          chval="sag"
          />
      </menu>
      <label loc="5 150" size="85 20"
        style="PlainText"
        label="Averages"
        justify="Left"
        />
      <entry loc="115 150" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE go('check')"
        set="$VALUE=nt"
        digits="0"
        disable="Grayed out"
        />
      <label loc="175 130" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="5 130" size="30 20"
        style="PlainText"
        label="TR"
        justify="Left"
        />
      <entry loc="115 130" size="60 20"
        style="PlainText"
        vq="tr mintr"
        vc="tr=$VALUE/1000.0 go('check')"
        set="$VALUE=tr*1000.0"
        show="$VALUE = (mintr='n') "
        digits="0"
        disable="Grayed out"
        />
      <label loc="175 60" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="5 60" size="105 20"
        style="PlainText"
        label="Intergrad Delay"
        justify="Left"
        />
      <entry loc="115 60" size="60 20"
        style="PlainText"
        vq="d2"
        vc="d2=$VALUE/1000"
        set="$VALUE=d2*1000.0"
        digits="1"
        disable="Grayed out"
        />
      <label loc="175 40" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="5 40" size="105 20"
        style="PlainText"
        label="Grad Duration"
        justify="Left"
        />
      <entry loc="115 40" size="60 20"
        style="PlainText"
        vq="tspoil"
        vc="tspoil=$VALUE/1000"
        set="$VALUE=tspoil*1000.0"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 20" size="105 20"
        style="PlainText"
        label="Grad Amplitude"
        justify="Left"
        />
      <entry loc="115 20" size="60 20"
        style="PlainText"
        vq="gspoil"
        vc="gspoil=$VALUE"
        set="$VALUE=gspoil"
        digits="1"
        disable="Grayed out"
        />
      <label loc="5 85" size="90 20"
        style="PlainText"
        label="Grad Mult 1"
        justify="Left"
        />
      <label loc="5 105" size="90 20"
        style="PlainText"
        label="Grad Mult 2"
        justify="Left"
        />
      <entry loc="115 85" size="60 20"
        style="PlainText"
        vq="sign1"
        vc="sign1=$VALUE"
        set="$VALUE=sign1"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="115 105" size="60 20"
        style="PlainText"
        vq="sign2"
        vc="sign2=$VALUE"
        set="$VALUE=sign2"
        digits="2"
        disable="Grayed out"
        />
    </group>
    <group loc="5 230" size="725 40"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="Protocol"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="430 0" size="215 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="80 0" size="130 20"
          style="PlainText"
          label="---"
          vq="scantime"
          set="$VALUE = scantime"
          />
        <label loc="0 0" size="80 20"
          style="Heading1"
          label="Scan Time"
          justify="Left"
          />
      </group>
      <textmessage loc="300 20" size="130 20"
        style="PlainText"
        label="205_120S_Single"
        vq="gcoil"
        set="$VALUE = gcoil"
        />
      <textmessage loc="300 0" size="130 20"
        style="PlainText"
        label="quad63"
        vq="rfcoil"
        set="$VALUE = rfcoil"
        />
      <label loc="220 20" size="80 20"
        style="Heading1"
        label="Gradient"
        justify="Left"
        />
      <label loc="220 0" size="80 20"
        style="Heading1"
        label="RF Coil"
        justify="Left"
        />
      <textmessage loc="80 20" size="130 20"
        style="PlainText"
        label="gtest"
        vq="pslabel"
        set="$VALUE = pslabel"
        />
      <entry loc="80 0" size="130 20"
        style="PlainText"
        vq="comment"
        vc="comment='$VALUE' xm1('addcomment')"
        set="$VALUE=comment"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 20" size="80 20"
        style="Heading1"
        label="Protocol"
        justify="Left"
        />
      <label loc="0 0" size="80 20"
        style="Heading1"
        label="Comment"
        justify="Left"
        />
    </group>
  </group>
</template>
