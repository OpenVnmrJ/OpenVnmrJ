<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="acqScan" element="pages" type="acquisition" >
  <group size="870 280"
    label="Scan"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="acqScan"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="435 0" size="210 230"
      style="Heading1"
      label="RF Pulse"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="spuls_par"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="80 170" size="20 20"
        style="Label1"
        label="to"
        justify="Left"
        />
      <entry loc="5 170" size="60 20"
        style="PlainText"
        vq="pw_start"
        vc="pw_start=$VALUE"
        set="$VALUE=pw_start"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 170" size="60 20"
        style="PlainText"
        vq="pw_end"
        vc="pw_end=$VALUE"
        set="$VALUE=pw_end"
        digits="0"
        disable="Grayed out"
        />
      <button loc="110 195" size="95 20"
        style="Label4"
        label="Calibrate 180"
        vc="ldrf findpw(pw_start,pw_end,180)"
        bg="darkGreen"
        decor1="no"
        />
      <button loc="5 195" size="95 20"
        style="Label4"
        label="Calibrate 90"
        vc="ldrf findpw(pw_start,pw_end,90)"
        bg="darkGreen"
        decor1="no"
        />
      <label loc="5 150" size="155 20"
        style="Label1"
        label="Pulse width array"
        />
      <check loc="15 100" size="95 20"
        style="PlainText"
        label="Calc. Power"
        vq="imprep"
        vc="imprep = 's'"
        vc2="imprep = 'n'"
        set="$VALUE = (imprep = 's')"
        />
      <label loc="170 170" size="35 20"
        style="Label1"
        label="usec"
        justify="Left"
        />
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="fliplist imprep"
        vc="fliplist=$VALUE imprep = 's'"
        set="$VALUE=fliplist"
        show="$VALUE = (imprep = 's')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="tpwr imprep"
        vc="tpwr=$VALUE imprep='n'"
        set="$VALUE=tpwr"
        show="$VALUE = (imprep = 'n')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 40" size="60 20"
        style="PlainText"
        vq="pwpat"
        vc="pwpat='$VALUE'"
        set="$VALUE=pwpat"
        disable="Grayed out"
        />
      <entry loc="110 20" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE"
        set="$VALUE=pw"
        digits="1"
        disable="Grayed out"
        />
      <label loc="170 20" size="35 20"
        style="Label1"
        label="usec"
        justify="Left"
        />
      <label loc="170 80" size="35 20"
        style="Label1"
        label="deg"
        justify="Left"
        />
      <label loc="170 60" size="35 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="5 130" size="155 20"
        style="Heading1"
        label="Calibrate RF"
        />
      <label loc="5 80" size="70 20"
        style="Label1"
        label="Flip angle"
        justify="Left"
        />
      <label loc="5 60" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="5 40" size="50 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="5 20" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
    </group>
    <group loc="220 0" size="210 230"
      style="Heading1"
      label="Acquisition"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Acquisition_group"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="sw"
        vc="sw=$VALUE"
        set="$VALUE=sw"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="110 100" size="60 20"
        style="PlainText"
        vq="at"
        vc="at=$VALUE/1000.0"
        set="$VALUE=at*1000.0"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="110 120" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        digits="0"
        disable="Grayed out"
        />
      <menu loc="110 140" size="70 20"
        style="PlainText"
        vq="presig"
        vc="presig='$VALUE'"
        set="$VALUE=presig"
        show="on('presig'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="high"
          chval="h"
          />
        <mlabel 
          label="low"
          chval="l"
          />
      </menu>
      <label loc="170 120" size="35 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="170 100" size="35 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="170 80" size="35 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <menu loc="110 20" size="75 20"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="H1"
          chval="H1"
          />
        <mlabel 
          label="F19"
          chval="F19"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="Na23"
          chval="Na23"
          />
        <mlabel 
          label="Xe129"
          chval="Xe129"
          />
        <mlabel 
          label="H2"
          chval="H2"
          />
      </menu>
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="tof"
        vc="tof=$VALUE"
        set="$VALUE=tof"
        digits="1"
        disable="Grayed out"
        />
      <label loc="170 60" size="35 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <textmessage loc="110 40" size="60 20"
        style="PlainText"
        label="399.287"
        vq="sfrq"
        set="$VALUE=sfrq"
        digits="3"
        />
      <label loc="170 40" size="35 20"
        style="Label1"
        label="MHz"
        justify="Left"
        />
      <label loc="5 120" size="100 20"
        style="Label1"
        label="Receiver Gain"
        justify="Left"
        />
      <label loc="5 20" size="100 20"
        style="Label1"
        label="Nucleus"
        justify="Left"
        />
      <label loc="5 40" size="100 20"
        style="Label1"
        label="Obs Frequency"
        justify="Left"
        />
      <label loc="5 60" size="100 20"
        style="Label1"
        label="Obs Offset"
        justify="Left"
        />
      <label loc="5 80" size="100 20"
        style="Label1"
        label="Spectral Width"
        justify="Left"
        />
      <label loc="5 100" size="100 20"
        style="Label1"
        label="Acquisition Time"
        justify="Left"
        />
      <label loc="5 140" size="95 20"
        style="Label1"
        label="presig"
        justify="Left"
        />
    </group>
    <group loc="5 0" size="210 230"
      style="Heading1"
      label="Scan Parameters"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="trstuff"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="110 100" size="60 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE go('check')"
        set="$VALUE=ss"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="bs"
        vc="bs=$VALUE"
        set="$VALUE=bs"
        show="on('bs'):$ENABLE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=np/2"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 40" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE go('check')"
        set="$VALUE=nt"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 20" size="60 20"
        style="PlainText"
        vq="tr"
        vc="tr=$VALUE/1000.0"
        set="$VALUE=tr*1000.0"
        show="on('tr'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="170 20" size="35 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="5 20" size="100 20"
        style="Label1"
        label="TR"
        justify="Left"
        />
      <check loc="5 120" size="160 20"
        style="PlainText"
        label="External Trigger"
        vq="ticks"
        vc="ticks=1"
        vc2="ticks=0"
        set="$VALUE=(ticks&lt;&gt;0)"
        show="on('ticks'):$VALUE"
        />
      <label loc="5 80" size="100 20"
        style="Label1"
        label="Block size"
        justify="Left"
        />
      <label loc="5 40" size="100 20"
        style="Label1"
        label="Averages"
        justify="Left"
        />
      <label loc="5 60" size="100 20"
        style="Label1"
        label="Complex points"
        justify="Left"
        />
      <label loc="5 100" size="100 20"
        style="Label1"
        label="Dummy scans"
        justify="Left"
        />
    </group>
    <button loc="650 35" size="120 20"
      style="Label4"
      label="Freq Prescan"
      vc="prescan_freq('manual') rfl=sw/2 rfp=0"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="650 125" size="120 20"
      style="Heading3"
      label="Line Width"
      vc="nl dres"
      bg="transparent"
      decor1="no"
      />
    <button loc="650 60" size="120 20"
      style="Heading3"
      label="Freq at Cursor"
      vc="offset:$f tof=$f ldrf($f) rfl=sw/2 rfp=0"
      bg="transparent"
      decor1="no"
      />
    <button loc="650 10" size="120 20"
      style="Label4"
      label="Test Spectrum"
      vc="im1Dglobal('prep') wdone='' werr='' wexp='im1Dglobal(`proc`)' au"
      bg="darkGreen"
      decor1="no"
      />
    <button loc="650 100" size="120 20"
      style="Heading3"
      label="Process"
      vc="im1Dglobal('proc')"
      bg="transparent"
      decor1="no"
      />
    <group loc="5 235" size="855 40"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="InfoGroup"
      useref="yes"
      subtype="Untitled"
      expanded="no"
      >
    </group>
  </group>
</template>
