<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Averages" element="groups" type="acquisition" >
  <group size="200 20"
    bg="transparent"
    border="None"
    tab="no"
    enable="no"
    reference="Averages"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <label loc="0 0" size="70 20"
      style="PlainText"
      label="Averages"
      justify="Left"
      />
    <entry loc="120 0" size="55 20"
      style="PlainText"
      vq="nt"
      vc="nt=$VALUE go('check')"
      set="$VALUE=nt"
      digits="0"
      disable="Grayed out"
      />
  </group>
</template>
