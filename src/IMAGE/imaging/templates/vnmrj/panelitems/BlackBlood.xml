<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="BlackBlood" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="Black Blood"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="BlackBlood"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <check loc="5 20" size="195 20"
      style="PlainText"
      label="Black Blood (DIR)"
      vq="dir"
      vc="dir='y' go('check')"
      vc2="dir='n' go('check')"
      set="$VALUE=(dir='y')"
      />
    <group loc="0 65" size="210 20"
      style="Heading1"
      vq="thkirfact"
      show="exists('thkirfact','parameter'):$VALUE"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BlackBlood"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="175 0" size="35 20"
        style="Label1"
        label="x thk"
        />
      <entry loc="115 0" size="55 20"
        style="PlainText"
        vq="thkirfact dir"
        vc="thkirfact=$VALUE go('check')"
        set="$VALUE=thkirfact"
        show="if (dir = 'y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="2"
        />
      <label loc="5 0" size="80 20"
        style="Label1"
        label="Thickness"
        />
    </group>
    <entry loc="115 45" size="55 20"
      style="PlainText"
      vq="ti dir"
      vc="minti='n' ti=$VALUE/1000.0 go('check')"
      set="$VALUE=ti*1000.0"
      show="if (dir = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      digits="2"
      disable="Grayed out"
      />
    <label loc="175 45" size="30 20"
      style="PlainText"
      label="ms"
      justify="Left"
      />
    <check loc="65 45" size="45 20"
      style="PlainText"
      label="min"
      vq="minti ti dir"
      vc="minti='y' go('check')"
      vc2="minti='n' go('check')"
      set="$VALUE=(minti='y')"
      show="if (dir = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <label loc="5 45" size="30 20"
      style="PlainText"
      label="TI"
      justify="Left"
      />
  </group>
</template>
