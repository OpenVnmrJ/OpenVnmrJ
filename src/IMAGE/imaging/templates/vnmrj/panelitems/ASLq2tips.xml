<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ASLq2tips" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="Q2TIPS"
    vq="q2tips"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="ASLq2tips"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="85 160" size="100 20"
      vq="asltype q2tips q2plan aslplan"
      show="aslset('showq2control'):$VALUE"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="slice"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="55 0" size="20 20"
        style="PlainText"
        label="2"
        vq="sl"
        set="$VALUE = sl"
        digits="0"
        statpar="sl"
        />
      <label loc="25 0" size="35 20"
        style="Label1"
        label="slice"
        />
      <button loc="5 10" size="15 10"
        style="PlainText"
        vc="sl=sl-1 aslset('showq2',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="down"
        />
      <button loc="5 0" size="15 10"
        style="PlainText"
        vc="sl=sl+1 aslset('showq2',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="up"
        />
    </group>
    <group loc="85 180" size="100 20"
      vq="asltype q2tips q2plan aslplan starctrl"
      show="aslset('showctrlq2control'):$VALUE"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="slice"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="55 0" size="20 20"
        style="PlainText"
        label="2"
        vq="sl"
        set="$VALUE = sl"
        digits="0"
        statpar="sl"
        />
      <label loc="25 0" size="35 20"
        style="Label1"
        label="slice"
        />
      <button loc="5 10" size="15 10"
        style="PlainText"
        vc="sl=sl-1 aslset('showctrlq2',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="down"
        />
      <button loc="5 0" size="15 10"
        style="PlainText"
        vc="sl=sl+1 aslset('showctrlq2',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="up"
        />
    </group>
    <button loc="10 200" size="75 20"
      style="PlainText"
      label="Clear"
      vq="q2tips asltype"
      vc="aslset('clearq2')"
      show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <button loc="10 180" size="75 20"
      style="PlainText"
      label="Control"
      vq="asltype q2tips"
      vc="aslset('showctrlq2',sl)"
      show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <button loc="10 160" size="75 20"
      style="PlainText"
      label="Tag"
      vq="asltype q2tips"
      vc="aslset('showq2',sl)"
      show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <check loc="10 140" size="60 20"
      style="PlainText"
      label="Plan"
      vq="q2plan q2tips asltype"
      vc="q2plan='y' aslset('showq2',sl)"
      vc2="q2plan='n' aslset('showq2',sl)"
      set="$VALUE=(q2plan='y')"
      show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      />
    <group loc="5 40" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="q2ti minq2ti q2tips asltype"
        vc="minq2ti='n' q2ti=$VALUE/1000.0 go('check')"
        set="$VALUE=q2ti*1000.0"
        show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="2"
        disable="Grayed out"
        />
      <check loc="65 0" size="45 20"
        style="PlainText"
        label="Min"
        vq="minq2ti q2ti q2tips asltype"
        vc="minq2ti='y' go('check')"
        vc2="minq2ti='n' go('check')"
        set="$VALUE=(minq2ti='y')"
        show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        />
      <label loc="160 0" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="0 0" size="70 20"
        style="PlainText"
        label="Start time"
        justify="Left"
        />
    </group>
    <group loc="5 100" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="0 0" size="100 20"
        style="Label1"
        label="Duration"
        justify="Left"
        />
      <textmessage loc="110 0" size="50 20"
        style="PlainText"
        label="12.20"
        vq="q2time"
        set="$VALUE=q2time*1000.0"
        digits="2"
        statpar="q2time"
        />
      <label loc="160 0" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="5 80" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="160 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="q2thk q2tips asltype"
        vc="q2thk=$VALUE aslset('showq2',sl)"
        set="$VALUE=q2thk"
        show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="0 0" size="60 20"
        style="PlainText"
        label="Sat width"
        justify="Left"
        />
    </group>
    <group loc="5 60" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="nq2 q2tips asltype"
        vc="nq2=$VALUE go('check')"
        set="$VALUE=nq2"
        show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 0" size="80 20"
        style="PlainText"
        label="Sat pulses"
        justify="Left"
        />
    </group>
    <check loc="5 20" size="190 20"
      style="PlainText"
      label="Q2TIPS"
      vq="q2tips asltype"
      vc="q2tips='y' go('check')"
      vc2="q2tips='n' aslset('clearq2') go('check')"
      set="$VALUE=(q2tips='y')"
      show="if (asltype &lt;&gt; 'CASL') then $VALUE = 1 else $VALUE = -1 endif"
      />
  </group>
</template>
