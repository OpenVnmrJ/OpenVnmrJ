<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="FOVPlan1D" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="Voxel Plan"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="FOVPlan1D"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <toggle loc="155 165" size="50 20"
      style="PlainText"
      label="Setup"
      vq="showOptions"
      vc="showOptions=1"
      vc2="showOptions=0"
      set="$VALUE=(showOptions=1)"
      enabled="true"
      radiobutton="no"
      subtype="no"
      />
    <button loc="85 165" size="70 20"
      style="Label4"
      label="Test Scan"
      vc="checkprofile im1D('prep',`ntopt`) wexp='im1D(`proc`,`ntopt`)' wdone='' au"
      bg="Children"
      decor1="no"
      />
    <group loc="5 100" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="a_aShimVoxel"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <button loc="100 0" size="45 20"
        style="Label1"
        label="Save"
        vc="vox_save_load('saveshim')"
        bg="transparent"
        decor1="no"
        />
      <button loc="150 0" size="45 20"
        style="Label1"
        label="Load"
        vc="vox_save_load('loadshim') go('check')"
        bg="transparent"
        decor1="no"
        />
      <label loc="0 0" size="75 20"
        style="PlainText"
        label="Shim Voxel"
        justify="Left"
        />
    </group>
    <group loc="5 70" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="0 0" size="35 20"
        style="PlainText"
        label="Pos"
        justify="Left"
        />
      <entry loc="50 0" size="40 20"
        style="PlainText"
        vq="pos1"
        vc="gplan('setValue','pos1',$VALUE/10)"
        set="$VALUE=pos1*10"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="40 20"
        style="PlainText"
        vq="pos2"
        vc="gplan('setValue','pos2',$VALUE/10)"
        set="$VALUE=pos2*10"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="130 0" size="40 20"
        style="PlainText"
        vq="pos3"
        vc="gplan('setValue','pos3',$VALUE/10)"
        set="$VALUE=pos3*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="170 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
    </group>
    <group loc="5 50" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="0 0" size="40 20"
        style="PlainText"
        label="Size"
        justify="Left"
        />
      <entry loc="50 0" size="40 20"
        style="PlainText"
        vq="vox1"
        vc="gplan('setValue','vox1',$VALUE) go('check')"
        set="$VALUE=vox1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="170 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <entry loc="130 0" size="40 20"
        style="PlainText"
        vq="vox3"
        vc="gplan('setValue','vox3',$VALUE) go('check')"
        set="$VALUE=vox3"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="40 20"
        style="PlainText"
        vq="vox2"
        vc="gplan('setValue','vox2',$VALUE) go('check')"
        set="$VALUE=vox2"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="0 20" size="205 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="a_aOrient"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <menu loc="95 0" size="100 20"
        style="PlainText"
        vq="vorient orient"
        vc="orient='$VALUE' gplan('setDefaultType', 3) setgplan('vorient','$VALUE') calcdim  "
        set="$VALUE=vorient"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Axial"
          chval="trans"
          />
        <mlabel 
          label="Coronal"
          chval="cor"
          />
        <mlabel 
          label="Sagittal"
          chval="sag"
          />
        <mlabel 
          label="Axial 90"
          chval="trans90"
          />
        <mlabel 
          label="Coronal 90"
          chval="cor90"
          />
        <mlabel 
          label="Sagittal 90"
          chval="sag90"
          />
        <mlabel 
          label="Oblique"
          chval="oblique"
          />
      </menu>
      <label loc="5 0" size="70 20"
        style="PlainText"
        label="Orientation"
        justify="Left"
        />
    </group>
    <group loc="0 195" size="210 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="Milestone"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <button loc="55 0" size="45 20"
        style="PlainText"
        label="Clear"
        vq="$VALUE = planValue"
        vc="gplan('clearStacks')"
        bg="transparent"
        decor1="yes"
        />
      <button loc="10 0" size="40 20"
        style="PlainText"
        label="Show"
        vq="iplanType planValue"
        vc="gplan('setDefaultType', 3) gplan('getCurrentStacks')"
        bg="transparent"
        decor1="yes"
        />
      <button loc="105 0" size="45 20"
        style="Label1"
        label="Mark"
        vc="gplan('addVoxel') "
        bg="transparent"
        decor1="no"
        />
      <toggle loc="155 0" size="45 20"
        style="PlainText"
        label="More"
        vq="showOptions"
        vc="showOptions=5"
        vc2="showOptions=0"
        set="$VALUE=(showOptions=5)"
        enabled="false"
        radiobutton="no"
        subtype="no"
        />
    </group>
  </group>
</template>
