<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ASLps" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="Presaturation"
    vq="ps"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="ASLps"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="85 180" size="100 20"
      vq="asltype ps psplan"
      show="aslset('showpscontrol'):$VALUE"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="slice"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="55 0" size="20 20"
        style="PlainText"
        label="1"
        vq="sl"
        set="$VALUE = sl"
        digits="0"
        statpar="sl"
        />
      <label loc="25 0" size="35 20"
        style="Label1"
        label="slice"
        />
      <button loc="5 10" size="15 10"
        style="PlainText"
        vc="sl=sl-1 aslset('showps',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="down"
        />
      <button loc="5 0" size="15 10"
        style="PlainText"
        vc="sl=sl+1 aslset('showps',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="up"
        />
    </group>
    <button loc="10 200" size="75 20"
      style="PlainText"
      label="Clear"
      vq="asltype ps"
      vc="aslset('clearps')"
      show="if (asltype='CASL' OR ps&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <button loc="10 180" size="75 20"
      style="PlainText"
      label="Show"
      vq="asltype ps"
      vc="aslset('showps',sl)"
      show="if (asltype='CASL' OR ps&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <check loc="10 160" size="60 20"
      style="PlainText"
      label="Plan"
      vq="psplan ps asltype"
      vc="psplan='y' aslset('showps',sl)"
      vc2="psplan='n' aslset('showps',sl)"
      set="$VALUE=(psplan='y')"
      show="if (asltype='CASL' OR ps&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      />
    <check loc="5 100" size="190 20"
      style="PlainText"
      label="WET presaturation"
      vq="wetps ps asltype"
      vc="wetps='y' nps=4 go('check')"
      vc2="wetps='n'  go('check')"
      set="$VALUE=(wetps='y')"
      show="if (asltype='CASL' OR ps&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      />
    <check loc="5 20" size="190 20"
      style="PlainText"
      label="Presaturation"
      vq="ps asltype"
      vc="ps='y' go('check')"
      vc2="ps='n' aslset('clearps') go('check')"
      set="$VALUE=(ps='y')"
      show="if (asltype='CASL') then $VALUE = -1 else $VALUE = 1 endif"
      />
    <group loc="5 80" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textmessage loc="110 0" size="50 20"
        style="PlainText"
        label="2.00"
        vq="psthk"
        set="$VALUE=psthk"
        digits="2"
        statpar="psthk"
        />
      <label loc="160 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="0 0" size="105 20"
        style="Label1"
        label="Presat width"
        justify="Left"
        />
    </group>
    <group loc="5 60" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="psaddthk ps asltype psplan"
        vc="psaddthk=$VALUE aslset('showps',sl)"
        set="$VALUE=psaddthk"
        show="if (asltype='CASL' OR ps&lt;&gt;'y' OR psplan='y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="160 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="0 0" size="105 20"
        style="PlainText"
        label="Additional width"
        justify="Left"
        />
    </group>
    <group loc="5 40" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="nps wetps ps asltype"
        vc="nps=$VALUE go('check')"
        set="$VALUE=nps"
        show="if (asltype='CASL' OR ps&lt;&gt;'y' OR wetps='y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 0" size="90 20"
        style="PlainText"
        label="Presat pulses"
        justify="Left"
        />
    </group>
  </group>
</template>
