<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="SpinechoPrep" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="Spin-echo Preparation"
    vq="spinecho"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="SpinechoPrep"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <check loc="5 95" size="140 20"
      style="PlainText"
      label="Autoset crushers"
      vq="autocrush spinecho"
      vc="autocrush='y' go('check')"
      vc2="autocrush='n' go('check')"
      set="$VALUE=(autocrush = 'y')"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <label loc="175 145" size="30 20"
      style="Label1"
      label="ms"
      />
    <label loc="175 125" size="30 20"
      style="Label1"
      label="ms"
      />
    <entry loc="115 165" size="55 20"
      style="PlainText"
      vq="nte spinecho"
      vc="nte=$VALUE"
      set="$VALUE=nte"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <entry loc="115 145" size="55 20"
      style="PlainText"
      vq="approxt2 spinecho"
      vc="approxt2=$VALUE/1000.0"
      set="$VALUE=approxt2*1000.0"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      digits="1"
      />
    <entry loc="115 125" size="55 20"
      style="PlainText"
      vq="startte spinecho"
      vc="startte=$VALUE/1000.0"
      set="$VALUE=startte*1000.0"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      digits="2"
      />
    <button loc="5 195" size="55 20"
      style="PlainText"
      label="array"
      vq="spinecho"
      vc="minte='n' seprep('arrayte')"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      bg="transparent"
      decor1="no"
      />
    <label loc="5 165" size="110 20"
      style="Label1"
      label="Number of TEs"
      />
    <label loc="5 145" size="110 20"
      style="Label1"
      label="Approximate T2"
      />
    <label loc="5 125" size="80 20"
      style="Label1"
      label="Start TE"
      />
    <label loc="175 65" size="35 20"
      style="Label1"
      label="x thk"
      />
    <textmessage loc="115 65" size="55 20"
      style="PlainText"
      label="0.99"
      vq="thk3fact"
      set="$VALUE=thk3fact"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      digits="2"
      />
    <label loc="5 65" size="80 20"
      style="Label1"
      label="Thickness"
      />
    <check loc="65 44" size="50 20"
      style="PlainText"
      label="min"
      vq="minte te"
      vc="minte='y' go('check') seprep('sette1')"
      vc2="minte='n' go('check')"
      set="$VALUE=(minte='y')"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <entry loc="115 45" size="55 20"
      style="PlainText"
      vq="te spinecho"
      vc="minte='n' te1=$VALUE/1000 seprep('sette') go('check')"
      set="$VALUE=te1*1000"
      show="if (spinecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      digits="2"
      />
    <label loc="175 45" size="30 20"
      style="Label1"
      label="ms"
      />
    <label loc="5 45" size="30 20"
      style="Label1"
      label="TE"
      />
    <check loc="5 20" size="160 20"
      style="PlainText"
      label="Spin-echo Preparation"
      vq="spinecho"
      vc="spinecho='y' seprep('sette') go('check')"
      vc2="spinecho='n' seprep('savete') go('check')"
      set="$VALUE=(spinecho='y')"
      />
  </group>
</template>
