<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="SpinStimEcho" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="Spin-Stimulated-echo"
    vq="spinstimecho"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="SpinStimEcho"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <label loc="5 165" size="60 20"
      style="Label1"
      label="Power"
      />
    <label loc="5 145" size="90 20"
      style="Label1"
      label="Flip Multiplier"
      />
    <label loc="175 165" size="30 20"
      style="PlainText"
      label="dB"
      justify="Left"
      />
    <check loc="5 125" size="205 20"
      style="PlainText"
      label="Adjust SE Refocusing RF Power"
      vq="spinstimecho p2flipadjust"
      vc="p2flipadjust='y' go('check')"
      vc2="p2flipadjust='n' go('check')"
      set="$VALUE=(p2flipadjust='y')"
      show="if (spinstimecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <textmessage loc="115 165" size="55 20"
      style="PlainText"
      vq="tpwr2 tpwr2f"
      set="calc_power(tpwr2,tpwr2f):$VALUE"
      digits="2"
      />
    <entry loc="115 145" size="55 20"
      style="PlainText"
      vq="p2flipmult spinstimecho p2flipadjust"
      vc="p2flipmult=$VALUE go('check')"
      set="$VALUE=p2flipmult"
      show="if (spinstimecho = 'y' AND p2flipadjust='y') then $VALUE = 1 else $VALUE = -1 endif"
      digits="2"
      />
    <check loc="5 105" size="165 20"
      style="PlainText"
      label="Excitation RF on"
      vq="exrf spinstimecho"
      vc="exrf='y' go('check')"
      vc2="exrf='n' go('check')"
      set="$VALUE=(exrf='y')"
      show="if (spinstimecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <entry loc="115 45" size="55 20"
      style="PlainText"
      vq="sete spinstimecho minsete"
      vc="minsete='n' sete=$VALUE/1000 go('check')"
      set="$VALUE=sete*1000"
      show="if (spinstimecho = 'n') then $VALUE = -1 elseif (minsete='y') then $VALUE=0 else $VALUE = 1 endif"
      digits="2"
      />
    <label loc="175 85" size="30 20"
      style="Label1"
      label="ms"
      />
    <entry loc="115 85" size="55 20"
      style="PlainText"
      vq="tm spinstimecho mintm"
      vc="mintm='n' tm=$VALUE/1000 go('check')"
      set="$VALUE=tm*1000"
      show="if (spinstimecho = 'n') then $VALUE = -1 elseif (mintm='y') then $VALUE=0 else $VALUE = 1 endif"
      digits="2"
      />
    <check loc="65 85" size="50 20"
      style="PlainText"
      label="min"
      vq="mintm tm spinstimecho"
      vc="mintm='y' go('check')"
      vc2="mintm='n' go('check')"
      set="$VALUE=(mintm='y')"
      show="if (spinstimecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <label loc="5 85" size="55 20"
      style="Label1"
      label="TM"
      />
    <label loc="175 65" size="30 20"
      style="Label1"
      label="ms"
      />
    <entry loc="115 65" size="55 20"
      style="PlainText"
      vq="stete spinstimecho minstete"
      vc="minstete='n' stete=$VALUE/1000 go('check')"
      set="$VALUE=stete*1000"
      show="if (spinstimecho = 'n') then $VALUE = -1 elseif (minstete='y') then $VALUE=0 else $VALUE = 1 endif"
      digits="2"
      />
    <check loc="65 65" size="50 20"
      style="PlainText"
      label="min"
      vq="minstete stete spinstimecho"
      vc="minstete='y' go('check')"
      vc2="minstete='n' go('check')"
      set="$VALUE=(minstete='y')"
      show="if (spinstimecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <label loc="5 65" size="55 20"
      style="Label1"
      label="STE TE"
      />
    <label loc="5 45" size="50 20"
      style="Label1"
      label="SE TE"
      />
    <check loc="65 44" size="50 20"
      style="PlainText"
      label="min"
      vq="minsete sete spinstimecho"
      vc="minsete='y' go('check')"
      vc2="minsete='n' go('check')"
      set="$VALUE=(minsete='y')"
      show="if (spinstimecho = 'y') then $VALUE = 1 else $VALUE = -1 endif"
      />
    <label loc="175 45" size="30 20"
      style="Label1"
      label="ms"
      />
    <check loc="5 20" size="160 20"
      style="PlainText"
      label="Spin-Stimulated-echo"
      vq="spinstimecho"
      vc="spinstimecho='y' spinecho='n' go('check')"
      vc2="spinstimecho='n' go('check')"
      set="$VALUE=(spinstimecho='y')"
      />
  </group>
</template>
