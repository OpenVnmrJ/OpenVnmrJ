<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ASLgrad" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="ASL Spoilers"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="ASLgrad"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="5 120" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="tspoilmir mir"
        vc="tspoilmir=$VALUE/1000"
        set="$VALUE=tspoilmir*1000"
        show="if (mir='y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="50 20"
        style="PlainText"
        vq="gspoilmir mir"
        vc="gspoilmir=$VALUE go('check')"
        set="$VALUE=gspoilmir"
        show="if (mir='y') then $VALUE = 1 else $VALUE = -1 endif"
        disable="Grayed out"
        />
      <label loc="0 0" size="75 20"
        style="Label1"
        label="Multiple IR"
        />
    </group>
    <group loc="5 100" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="tspoilips ips"
        vc="tspoilips=$VALUE/1000 go('check')"
        set="$VALUE=tspoilips*1000"
        show="if (ips='y') then $VALUE = 1 else $VALUE = -1 endif"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="50 20"
        style="PlainText"
        vq="gspoilips ips"
        vc="gspoilips=$VALUE go('check')"
        set="$VALUE=gspoilips"
        show="if (ips='y') then $VALUE = 1 else $VALUE = -1 endif"
        disable="Grayed out"
        />
      <label loc="0 0" size="75 20"
        style="Label1"
        label="In Plane Sat"
        />
    </group>
    <group loc="5 140" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="tspoilq2 asltype q2tips"
        vc="tspoilq2=$VALUE/1000 go('check')"
        set="$VALUE=tspoilq2*1000"
        show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="50 20"
        style="PlainText"
        vq="gspoilq2 asltype q2tips"
        vc="gspoilq2=$VALUE go('check')"
        set="$VALUE=gspoilq2"
        show="if (asltype='CASL' OR q2tips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        disable="Grayed out"
        />
      <label loc="0 0" size="75 20"
        style="Label1"
        label="Q2TIPS"
        />
    </group>
    <group loc="5 80" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="tspoilps asltype ps"
        vc="tspoilps=$VALUE/1000 go('check')"
        set="$VALUE=tspoilps*1000"
        show="if (asltype='CASL' OR ps&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="50 20"
        style="PlainText"
        vq="gspoilps asltype ps"
        vc="gspoilps=$VALUE go('check')"
        set="$VALUE=gspoilps"
        show="if (asltype='CASL' OR ps&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
        disable="Grayed out"
        />
      <label loc="0 0" size="85 20"
        style="Label1"
        label="Presaturation"
        />
    </group>
    <group loc="95 20" size="115 40"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="55 20" size="40 20"
        style="Label1"
        label="(ms)"
        justify="Left"
        />
      <label loc="55 0" size="60 20"
        style="Label1"
        label="Duration"
        justify="Left"
        />
      <label loc="0 20" size="50 20"
        style="Label1"
        label="(G/cm)"
        justify="Left"
        />
      <label loc="0 0" size="50 20"
        style="Label1"
        label="Amp"
        justify="Left"
        />
    </group>
    <group loc="5 60" size="200 20"
      vq="spinecho"
      show="exists('spinecho','parameter'):$VALUE"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="tspoilasl"
        vc="tspoilasl=$VALUE/1000 go('check')"
        set="$VALUE=tspoilasl*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="90 0" size="50 20"
        style="PlainText"
        vq="gspoilasl"
        vc="gspoilasl=$VALUE go('check')"
        set="$VALUE=gspoilasl"
        disable="Grayed out"
        />
      <label loc="0 0" size="75 20"
        style="Label1"
        label="Tag"
        />
    </group>
  </group>
</template>
