<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ShimPlanFOV" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="FOV &amp; Slab Plan"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="ShimPlanFOV"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="5 20" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="60 0" size="70 20"
        style="Label1"
        label="Sagittal"
        justify="Left"
        />
      <label loc="0 0" size="50 20"
        style="PlainText"
        label="Orient"
        justify="Left"
        />
    </group>
    <label loc="5 145" size="70 20"
      style="PlainText"
      label="Slab Fract"
      justify="Left"
      />
    <check loc="65 165" size="110 20"
      style="PlainText"
      label="Center on FOV"
      vq="slabctr"
      vc="slabctr = 'y' gplan('setValue','pss0',ppe2)"
      vc2="slabctr = 'n'"
      set="$VALUE = (slabctr='y')"
      />
    <entry loc="65 145" size="50 20"
      style="PlainText"
      vq="thk lpe2 slabfract"
      vc="slabfract=$VALUE thk=lpe2*slabfract*10"
      set="$VALUE=slabfract"
      digits="2"
      disable="Grayed out"
      />
    <group loc="5 40" size="200 80"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="3DFOV"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="165 60" size="30 20"
        style="Label1"
        label="mm"
        justify="Left"
        />
      <entry loc="115 60" size="50 20"
        style="PlainText"
        vq="ppe2"
        vc="gplan('setValue','ppe2',$VALUE/10.0)"
        set="$VALUE=ppe2*10.0"
        show="gplan('getstatus','ppe2'):$v if($v=0) then $VALUE = 0 else $VALUE = 1 endif"
        digits="1"
        disable="Label"
        />
      <entry loc="60 60" size="50 20"
        style="PlainText"
        vq="lpe2"
        vc="gplan('setValue','lpe2',$VALUE/10.0) thk=lpe2*10*slabfract go('check')"
        set="$VALUE=lpe2*10.0"
        digits="1"
        disable="Grayed out"
        />
      <label loc="0 60" size="60 20"
        style="Label1"
        label="Phase 2"
        justify="Left"
        />
      <label loc="165 40" size="30 20"
        style="Label1"
        label="mm"
        justify="Left"
        />
      <label loc="165 20" size="30 20"
        style="Label1"
        label="mm"
        justify="Left"
        />
      <label loc="70 0" size="30 20"
        style="Label1"
        label="FOV"
        justify="Left"
        />
      <label loc="120 0" size="40 20"
        style="Label1"
        label="Offset"
        justify="Left"
        />
      <entry loc="60 20" size="50 20"
        style="PlainText"
        vq="lro"
        vc="gplan('setValue','lro',$VALUE/10.0) go('check')"
        set="$VALUE=lro*10.0"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="60 40" size="50 20"
        style="PlainText"
        vq="lpe"
        vc="gplan('setValue','lpe',$VALUE/10.0) go('check')"
        set="$VALUE=lpe*10.0"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="115 40" size="50 20"
        style="PlainText"
        vq="ppe"
        vc="gplan('setValue','ppe',$VALUE/10.0)"
        set="$VALUE=ppe*10.0"
        show="gplan('getstatus','ppe'):$v if($v=0) then $VALUE = 0 else $VALUE = 1 endif"
        digits="1"
        disable="Label"
        />
      <entry loc="115 20" size="50 20"
        style="PlainText"
        vq="pro"
        vc="gplan('setValue','pro',$VALUE/10.0)"
        set="$VALUE=pro*10.0"
        digits="1"
        disable="Grayed out"
        />
      <label loc="0 40" size="50 20"
        style="Label1"
        label="Phase"
        justify="Left"
        />
      <label loc="0 20" size="50 20"
        style="Label1"
        label="Read"
        justify="Left"
        />
    </group>
    <group loc="5 125" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="60 0" size="50 20"
        style="PlainText"
        vq="thk slabfract lpe2"
        vc="gplan('setValue','thk',$VALUE) slabfract=thk/10/lpe2 prep go('check')"
        set="$VALUE=thk"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="115 0" size="50 20"
        style="PlainText"
        vq="pss0"
        vc="pss0=$VALUE/10.0 gplan('setValue','pss0',$VALUE/10)"
        set="$VALUE=pss0*10"
        digits="1"
        disable="Grayed out"
        />
      <label loc="165 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="0 0" size="60 20"
        style="PlainText"
        label="Slab Thk"
        justify="Left"
        />
    </group>
    <group loc="10 195" size="190 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="0 0" size="60 20"
        style="PlainText"
        label="Show"
        vq="iplanType planValue"
        vc="gplan('setDefaultType', 2) gplan('notuse','ppe') gplan('notuse','ppe2') gplan('getCurrentStacks')"
        bg="transparent"
        decor1="yes"
        />
      <button loc="65 0" size="60 20"
        style="PlainText"
        label="Clear"
        vc="gplan('clearStacks')"
        bg="transparent"
        decor1="yes"
        />
    </group>
  </group>
</template>
