<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="SumEchoes" element="groups" type="acquisition" >
  <group size="330 160"
    style="Heading1"
    label="Sum Echoes"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="SumEchoes"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="5 65" size="320 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textmessage loc="115 0" size="90 20"
        style="PlainText"
        label="1"
        vq="ne"
        set="$VALUE=ne"
        />
      <label loc="5 0" size="100 20"
        style="Label1"
        label="Total Echoes"
        />
    </group>
    <group loc="5 45" size="320 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="115 0" size="90 20"
        style="PlainText"
        vq="aipEndecho"
        vc="aipEndecho=$VALUE imagemath('checkecho')"
        set="$VALUE=aipEndecho"
        />
      <label loc="5 0" size="100 20"
        style="Label1"
        label="End Echo"
        />
    </group>
    <group loc="5 25" size="320 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="5 0" size="100 20"
        style="Label1"
        label="Start Echo"
        />
      <entry loc="115 0" size="90 20"
        style="PlainText"
        vq="aipStartecho"
        vc="aipStartecho=$VALUE imagemath('checkecho')"
        set="$VALUE=aipStartecho"
        />
    </group>
  </group>
</template>
