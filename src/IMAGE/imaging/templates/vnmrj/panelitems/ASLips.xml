<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ASLips" element="groups" type="acquisition" >
  <group size="210 230"
    style="Heading1"
    label="In Plane Saturation"
    vq="ips"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="ASLips"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="5 80" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textmessage loc="110 0" size="50 20"
        style="PlainText"
        label="5.00"
        vq="ipsthk"
        set="$VALUE=ipsthk"
        digits="2"
        statpar="ipsthk"
        />
      <label loc="160 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
      <label loc="0 0" size="105 20"
        style="Label1"
        label="Sat width"
        justify="Left"
        />
    </group>
    <check loc="5 120" size="190 20"
      style="PlainText"
      label="WET in plane saturation"
      vq="wetips ips"
      vc="wetips='y' nips=4 go('check')"
      vc2="wetips='n'  go('check')"
      set="$VALUE=(wetips='y')"
      show="if (ips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      />
    <button loc="10 200" size="75 20"
      style="PlainText"
      label="Clear"
      vq="ips"
      vc="aslset('clearips')"
      show="if (ips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <check loc="10 160" size="60 20"
      style="PlainText"
      label="Plan"
      vq="ipsplan ips asltype"
      vc="ipsplan='y' aslset('showips',sl)"
      vc2="ipsplan='n' aslset('showips',sl)"
      set="$VALUE=(ipsplan='y')"
      show="if (ips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      />
    <button loc="10 180" size="75 20"
      style="PlainText"
      label="Show"
      vq="ips"
      vc="aslset('showips',sl)"
      show="if (ips&lt;&gt;'y') then $VALUE = -1 else $VALUE = 1 endif"
      bg="transparent"
      decor1="yes"
      />
    <group loc="85 180" size="100 20"
      vq="asltype ips ipsplan"
      show="aslset('showipscontrol'):$VALUE"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textmessage loc="55 0" size="20 20"
        style="PlainText"
        label="1"
        vq="sl"
        set="$VALUE = sl"
        digits="0"
        statpar="sl"
        />
      <label loc="25 0" size="35 20"
        style="Label1"
        label="slice"
        />
      <button loc="5 10" size="15 10"
        style="PlainText"
        vc="sl=sl-1 aslset('showips',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="down"
        />
      <button loc="5 0" size="15 10"
        style="PlainText"
        vc="sl=sl+1 aslset('showips',sl)"
        bg="VJBackground"
        decor1="yes"
        icon="up"
        />
    </group>
    <check loc="5 20" size="190 20"
      style="PlainText"
      label="In plane Saturation"
      vq="ips"
      vc="ips='y' go('check')"
      vc2="ips='n' aslset('clearips') go('check')"
      set="$VALUE=(ips='y')"
      />
    <group loc="5 100" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="0 0" size="100 20"
        style="Label1"
        label="Duration"
        justify="Left"
        />
      <textmessage loc="110 0" size="50 20"
        style="PlainText"
        label="16.10"
        vq="ipstime"
        set="$VALUE=ipstime*1000.0"
        digits="2"
        statpar="ipstime"
        />
      <label loc="160 0" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="5 60" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="ipsaddthk ips ipsplan"
        vc="ipsaddthk=$VALUE aslset('showips',sl)"
        set="$VALUE=ipsaddthk"
        show="if (ips&lt;&gt;'y' OR ipsplan='y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="0 0" size="105 20"
        style="PlainText"
        label="Additional width"
        justify="Left"
        />
      <label loc="160 0" size="30 20"
        style="PlainText"
        label="mm"
        justify="Left"
        />
    </group>
    <group loc="5 40" size="200 20"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="110 0" size="50 20"
        style="PlainText"
        vq="nips ips wetips"
        vc="nips=$VALUE go('check')"
        set="$VALUE=nips"
        show="if (ips&lt;&gt;'y' OR wetips='y') then $VALUE = -1 else $VALUE = 1 endif"
        digits="0"
        disable="Grayed out"
        />
      <label loc="0 0" size="90 20"
        style="PlainText"
        label="Sat pulses"
        justify="Left"
        />
    </group>
  </group>
</template>
