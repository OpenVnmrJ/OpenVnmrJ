<template>
    <splitpane loc="0 0" size="750 200" style="PlainText"
      label="Top Level Split Pane"
      vq="lcGraphSplit"
      vc="lcGraphSplit='$VALUE'"
      set="$VALUE=lcGraphSplit"
      bg="VJBackground"
      tab="no"
      reference="SplitPane"
      useref="no"
      expanded="yes"
      >
     <splitpane loc="0 0" size="750 200" style="PlainText"
	label="Second Level Split Pane"
	vq="lcGraphSplit2"
	vc="lcGraphSplit2='$VALUE'"
	set="$VALUE=lcGraphSplit2"
	bg="VJBackground"
	tab="no"
	reference="SplitPane"
	useref="no"
	expanded="yes"
	>
        <pdaplot loc="5 8" size="900 300" style="GraphText"
	  bg="VJBackground"
	  jointpts="Yes"
	  pointsize="None"
	  graphbgcolor="GraphBackground"
	  graphfgcolor="GraphForeground"
	  xaxisshow="Yes"
	  yaxisshow="Yes"
	  logyaxis="No"
	  showgrid="No"
	  gridcolor="GraphGrid"
	  tickcolor="GraphTick"
	  />
	<msplot loc="5 8" size="900 300" style="GraphText"
	  bg="VJBackground"
	  jointpts="Yes"
	  pointsize="None"
	  graphbgcolor="GraphBackground"
	  graphfgcolor="GraphForeground"
	  xaxisshow="Yes"
	  yaxisshow="Yes"
	  logyaxis="No"
	  showgrid="No"
	  gridcolor="GraphGrid"
	  tickcolor="GraphTick"
	  />
      </splitpane>
      <lcplot loc="5 8" size="900 300" style="GraphText"
	bg="VJBackground"
	vq="lcCr1 lcCr2"
	jointpts="Yes"
	fillhistgm="Yes"
	pointsize="None"
	lcset="$VALUE=lcCr1"
	lccmd="lcCursor(1,$VALUE)"
	lcshow="$VALUE=1"
	lccolor="LeftCursor"
	rcset="$VALUE=lcCr2"
	rccmd="lcCursor(2,$VALUE)"
	rcshow="$VALUE=1"
	rccolor="RightCursor"
	graphbgcolor="GraphBackground"
	graphbgcolor2="GraphBackground2"
	graphfgcolor="GraphForeground"
	graphfgcolor2="GraphForeground2"
	graphfgcolor3="GraphForeground3"
	color4="GraphForeground4"
	color5="GraphForeground5"
	xaxisshow="Yes"
	yaxisshow="Yes"
	logyaxis="No"
	showgrid="Yes"
	gridcolor="GraphGrid"
	tickcolor="GraphGrid"
	/>
    </splitpane>
</template>
