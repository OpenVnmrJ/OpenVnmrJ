<template name="saveprocess" element="pages" type="processing" >
    <group loc="0 0" size="520 100" style="Plain"
      point="8"
      fg="blue"
      tab="false"
      >
      <label loc="8 40" size="500 24" font="Dialog"
        style="Heading2"
        point="Heading2"
        fg="blue"
        value="Move current text to experiment number: "
        />
      <entry loc="320 40" size="50 24" font="Dialog"
        style="Heading2"
        point="Heading2"
        fg="black"
        vc="$v='$VALUE' if $v&lt;&gt;'' then format($v,1,0):$p mt($p) endif"
        />
    </group>
</template>
