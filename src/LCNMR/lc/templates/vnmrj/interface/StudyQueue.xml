<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="cpstudyQ" element="groups" type="acquisition" >
  <group size="300 100"
    bg="transparent"
    border="None"
    tab="no"
    enable="no"
    reference="cpstudyQ"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <group loc="0 0" size="300 100"
      bg="transparent"
      border="None"
      vq="jviewport jviewports"
      show="$SHOW=1"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      trackViewport="yes"
      >
      <!-- If in submit mode: Choose Day or Night submission. -->
      <!-- LC NMR stopflow / stopflowexp / analyte option -->
      <group loc="5 5" size="290 90"
        style="Plain"
        vq="jviewport seqfil cppauseflg customflag traymax system"
        show="$SHOW=((seqfil&lt;&gt;'Submitq') and (cppauseflg&lt;&gt;'y') and (customflag&lt;&gt;'y') and (traymax=1))"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        trackViewport="yes"
        >
        <button loc="10 5" size="270 35"
          style="Heading2"
          label="Define Experiments/Peaks"
          vq="lcexp seqfil samplename autodir lcRunActive"
          show="$ENABLE=(((lcexp='stop') or (lcexp='enter') or (lcexp='enterA')) and (lcRunActive=0))*2-1"
          vc="lcenter"
          fg="darkRed"
          decor1="Yes"
          trackViewport="yes"
          />
        <label loc="5 45" size="120 20"
          style="Heading2"
	  label="LC Method:"
          trackViewport="yes"
          />
        <textmessage loc="130 45" size="160 20"
          style="Label1"
          vq="lcMethodFile seqfil pslabel autodir"
          set="$VALUE=lcMethodFile"
          trackViewport="yes"
          />
        <label loc="5 65" size="120 20"
          style="Heading2"
	  label="Method Type:"
          trackViewport="yes"
          />
        <textmessage loc="130 65" size="160 20"
          style="Label1"
          vq="lcexp seqfil pslabel autodir"
          set="cplcsq('MethodType'):$VALUE"
          trackViewport="yes"
          />
      </group>
      <!-- Submit mode for lcnmr  -->
      <group loc="5 5" size="290 90"
        style="Plain"
        vq="operator jviewport seqfil traymax cppauseflg customflag wqinit clonemod"
        show="$SHOW=((traymax=1) and ((seqfil='Submitq') or (cppauseflg='y') or (customflag='y')) and (clonemod&lt;&gt;'yes'))"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        trackViewport="yes"
        >
        <button loc="5 5" size="135 30"
          style="Heading3"
          label="Submit"
          vq="jviewport seqfil replaceQ resumeQ"
          vc="walkupQ_lcsubmit"
	  bg="green"
          decor1="Yes"
          trackViewport="yes"
          />
        <button loc="150 5" size="135 30"
          style="Heading3"
          label="New Peak"
          vq="jviewport lcexp"
	  show="$ENABLE=((lcexp='enter') or (lcexp='enterA'))*2-1"
          vc="addPeakNode"
	  bg="yellow"
          decor1="Yes"
          trackViewport="yes"
          />
        <button loc="5 40" size="280 20"
          style="Label4"
          fg="navy"
          label="Duplicate last Peak"
          vq="jviewport lcexp"
          show="$ENABLE=((lcexp='enter') or (lcexp='enterA'))*2-1"
          vc="copyPeakNode"
          decor1="Yes"
          trackViewport="yes"
          />
        <button loc="5 65" size="280 20"
          style="Label4"
          fg="navy"
          label="Clear All Peaks from Queue"
          vq="jviewport"
          vc="xmhaha('clearSQ')"
          decor1="Yes"
          trackViewport="yes"
          />
      </group>
    </group>
  </group>
</template>
