        set="$VALUE=lcThreshold[3]"
        show="if (lcEnableC='y') then if (lcPeakDetect[3]='y') then $VALUE=1 else $VALUE=0 endif else $VALUE=-1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="155 0" size="65 20"
