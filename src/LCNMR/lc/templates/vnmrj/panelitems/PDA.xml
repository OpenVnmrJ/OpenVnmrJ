<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PDA" element="groups" type="preparation" >
  <group size="320 80" bg="transparent" tab="no"
    enable="no"
    reference="PDA"
    useref="no"
    expanded="yes"
    >
    <textmessage loc="210 20" size="110 20" style="PlainText"
      statpar="PdaLampStatus"
      />
    <textmessage loc="210 0" size="110 20" style="PlainText"
      statpar="PdaStatus"
      />
    <button loc="5 60" size="90 20" style="PlainText"
      label="Load Method"
      vc="lccmd('pdaLoadMethod')"
      statpar="PdaStatus"
      statshow="Ready"
      />
    <button loc="110 60" size="90 20" style="PlainText"
      label="Lamp Off"
      vc="lccmd('pdaLampOff')"
      statpar="PdaStatus"
      statshow="Ready"
      />
    <button loc="110 40" size="90 20" style="PlainText"
      label="Lamp On"
      vc="lccmd('pdaLampOn')"
      statpar="PdaLampStatus"
      statshow="Off"
      />
    <button loc="110 20" size="90 20" style="PlainText"
      label="Baseline"
      vc="lccmd('pdaBaseline')"
      statpar="PdaStatus"
      statshow="Ready"
      />
    <button loc="110 0" size="90 20" style="PlainText"
      label="Reset"
      vc="lccmd('pdaReset')"
      statpar="PdaStatus"
      statshow="Ready Running Baseline"
      />
    <label loc="10 0" size="40 20" style="Menu1"
      label="PDA"
      justify="Left"
      />
  </group>
</template>
