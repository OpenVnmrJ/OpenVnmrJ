<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standardacq" element="pages" type="sample" >
  <group size="840 305"
    style="PlainText"
    label="Standard"
    vq="customflag wqinit"
    show="$VALUE=(wqinit='acq')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standardacq"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="415 150" size="285 50"
      vq="lcexp"
      show="$SHOW=(lcexp='enter')"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="rettime"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <button loc="5 25" size="275 25"
        style="Heading4"
        label="Define expected Peak retention times"
        vq="lcexp"
        vc="EstRetentionTime"
        show="$pk='' cplcsq('getNumPeaks'):$pk $ENABLE=((lcexp='enter') and ($pk&lt;&gt;'') and ($pk&lt;&gt;'0'))*2-1"
        bg="transparent"
        decor1="yes"
        />
      <label loc="240 0" size="25 20"
        style="Label1"
        label="sec"
        justify="Left"
        />
      <entry loc="195 0" size="45 20"
        style="Label1"
        vq="rettimeRange"
        vc="on('rettimeRange','global'):$on if ($on&gt;0) then rettimeRange=$VALUE endif"
        set="$VALUE=rettimeRange"
        show="on('rettimeRange','global'):$on $ENABLE=($on&gt;0)"
        digits="0"
        disable="Grayed out"
        />
      <check loc="5 0" size="190 20"
        style="PlainText"
        label="validate retention time  (+/-)"
        vq="rettimeRange"
        vc="on('rettimeRange','global')"
        vc2="off('rettimeRange','global')"
        set="on('rettimeRange','global'):$on $VALUE=($on&gt;0)"
        show="on('rettimeRange','global'):$on $ENABLE=($on&gt;-1)*2-1"
        />
    </group>
    <group loc="380 105" size="445 30"
      vq="wshim"
      show="$SHOW=(wshim&lt;&gt;'n')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <filemenu loc="130 5" size="295 20"
        style="PlainText"
        vq="Gmap Gmapname operator"
        vc="Gmaplist('set','$VALUE')"
        set="if (Gmapname='') then $VALUE=Gmap else $VALUE='custom_'+Gmapname endif"
        file="$VALUE=userdir+'/persistence/'+operator+'_Gmaplist'"
        type="file"
        bg="VJBackground"
        editable="No"
        vq2="Gmap Gmapname"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 5" size="115 20"
        style="Label1"
        label="Select shimmap:"
        />
    </group>
    <group loc="380 30" size="210 70"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <menu loc="50 25" size="140 20"
        style="Label4"
        label="menu"
        vq="alock"
        vc="$alock='$VALUE' if (Console='mercury') and ($alock='u') then $alock='n' endif alock=$alock"
        set="if (alock&lt;&gt;'n') and (alock&lt;&gt;'u') then $VALUE='s' else $VALUE=alock endif"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Yes (alock&lt;&gt;n)"
          chval="s"
          />
        <mlabel 
          label="No (alock=n)"
          chval="n"
          />
        <mlabel 
          label="Unlocked (alock=u)"
          chval="u"
          />
      </menu>
      <label loc="5 25" size="45 20"
        style="Label1"
        label="Lock?"
        justify="Left"
        />
      <label loc="25 5" size="160 20"
        style="Label1"
        label="Before First Exp"
        justify="Center"
        />
      <check loc="30 46" size="60 20"
        style="Label4"
        label="Shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="$VALUE=(wshim&lt;&gt;'n')"
        />
      <check loc="120 46" size="60 20"
        style="Label4"
        label="Tune"
        vq="ptuneatsample wtune atune tunemethod"
        vc="strstr(ptuneatsample,'auto'):$wk if ($wk) then ptuneatsample='autowkup' else ptuneatsample='wkup' endif"
        vc2="strstr(ptuneatsample,'auto'):$wk if ($wk) then ptuneatsample='auto' else ptuneatsample='no' endif"
        set="strstr(ptuneatsample,'wkup'):$ato $VALUE=($ato&gt;0)"
        show="$ok='no' checkprotune('global'):$ok $ENABLE=($ok='yes')*2-1"
        />
    </group>
    <group loc="595 30" size="210 50"
      style="PlainText"
      vq="system"
      show="is_spectrometer('value'):$SHOW"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampstat"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 25" size="195 20"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <button loc="0 0" size="65 20"
          style="Heading2"
          label="Temp"
          vc="su"
          bg="transparent"
          decor1="yes"
          />
        <label loc="70 0" size="20 20"
          style="Label1"
          label="at"
          justify="Center"
          />
        <entry loc="95 0" size="35 20"
          style="StdPar"
          vq="temp tin"
          vc="temp=$VALUE tin='y'"
          set="$VALUE=temp"
          show="on('temp'):$ENABLE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="135 0" size="10 20"
          style="Label1"
          label="C"
          justify="Left"
          />
        <check loc="160 0" size="20 20"
          style="PlainText"
          vq="temp tin"
          vc="on('temp') tin='y'"
          vc2="off('temp') tin='n'"
          set="on('temp'):$VALUE"
          />
      </group>
      <label loc="60 5" size="85 20"
        style="Heading2"
        label="Sample"
        justify="Center"
        />
    </group>
    <textmessage loc="35 5" size="200 20"
      style="Heading3"
      label="Operator : lcmsusr"
      vq="operator"
      set="$VALUE='     Operator : '+operator"
      />
    <group loc="5 30" size="370 215"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampinfo"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="100 65" size="110 20"
        style="StdPar"
        vq="lot"
        vc="lot='$VALUE'"
        set="$VALUE=lot"
        disable="Grayed out"
        />
      <entry loc="100 45" size="110 20"
        style="StdPar"
        vq=" notebook"
        vc=" notebook='$VALUE'"
        set="$VALUE=notebook"
        disable="Grayed out"
        />
      <label loc="10 65" size="90 20"
        style="Label1"
        label="Lot Number:"
        justify="Left"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="Notebook:"
        justify="Left"
        />
      <filemenu loc="85 85" size="125 20"
        style="Heading2"
        vq="solvent"
        vc="solvent='$VALUE' setref"
        set="format(solvent,'lower'):$VALUE"
        file="/vnmr/solventlist"
        type="file"
        bg="VJBackground"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <textfile loc="5 145" size="360 65"
        style="PlainText"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <entry loc="255 65" size="105 20"
        style="PlainText"
        vq="emailaddr "
        vc="emailaddr='$VALUE'"
        set="$VALUE=emailaddr"
        disable="Grayed out"
        />
      <label loc="215 65" size="45 20"
        style="Label1"
        label="Eaddr"
        justify="Left"
        />
      <entry loc="255 45" size="105 20"
        style="StdPar"
        vq=" page"
        vc=" page='$VALUE'"
        set="$VALUE=page"
        disable="Grayed out"
        />
      <label loc="215 45" size="40 20"
        style="Label1"
        label="Page:"
        justify="Left"
        />
      <entry loc="100 5" size="130 20"
        style="StdPar"
        vq=" samplename"
        vc=" samplename='$VALUE' setsamplename"
        set="$VALUE=samplename"
        disable="Grayed out"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="SampleDir:"
        justify="Left"
        />
      <textmessage loc="100 25" size="260 20"
        style="PlainText"
        label="NMR"
        vq="sample sdirtmplt adirtmplt samplename"
        set="if (sample='') then $as='' $s='' Svfname(adirtmplt+'/'+sdirtmplt,''):$as shell('basename '+$as):$s $VALUE='('+$s+')' else $VALUE=sample endif"
        />
      <toggle loc="295 105" size="70 20"
        style="Heading2"
        label="Other"
        vq="solvent"
        vc="solvent=solvent"
        set="format(solvent,'lower'):$solv $VALUE=(($solv&lt;&gt;'d2o') and ($solv&lt;&gt;'dmso') and ($solv&lt;&gt;'cdcl3'))"
        radiobutton="yes"
        subtype="no"
        />
      <toggle loc="225 105" size="70 20"
        style="Heading2"
        label="D2O"
        vq="solvent"
        vc="solvent='d2o' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='d2o')"
        radiobutton="yes"
        subtype="no"
        />
      <toggle loc="155 105" size="70 20"
        style="Heading2"
        label="CDCl3"
        vq="solvent"
        vc="solvent='cdcl3' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='cdcl3')"
        radiobutton="yes"
        subtype="no"
        />
      <toggle loc="85 105" size="70 20"
        style="Heading2"
        label="DMSO"
        vq="solvent"
        vc="solvent='dmso' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='dmso')"
        radiobutton="yes"
        subtype="no"
        />
      <label loc="5 95" size="80 20"
        style="Heading2"
        label="Solvent"
        justify="Left"
        />
      <label loc="5 125" size="100 20"
        style="Heading1"
        label="Comment"
        justify="Left"
        />
      <label loc="10 5" size="90 20"
        style="Label1"
        label="Sample Name:"
        justify="Left"
        />
    </group>
  </group>
</template>
