<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="LcControl" element="pages" type="preparation" >
  <group size="935 275"
    style="Heading3"
    label="Test"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="LcControl"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 20" size="920 250"
      style="Heading3"
      label="LC Control"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="yes"
      enable="no"
      reference="LCControl"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="0 140" size="320 80"
        bg="transparent"
        tab="no"
        enable="no"
        reference="PDA"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <button loc="60 0" size="130 20"
          style="PlainText"
          label="Show 2D Spectrum"
          vc="lccmd('pdagetimage', lcDataFile)"
          decor1="No"
          />
        <label loc="10 0" size="40 20"
          style="Menu1"
          label="PDA"
          justify="Left"
          />
      </group>
      <button loc="350 180" size="80 20"
        style="PlainText"
        label="Button"
        decor1="No"
        icon="cycle"
        />
      <group loc="485 0" size="135 65"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="5 0" size="120 20"
          style="Menu1"
          label="Cursor Locations:"
          justify="Left"
          />
        <textmessage loc="5 25" size="65 20"
          style="PlainText"
          label="3.41106"
          vq="lcCr1"
          set="$VALUE=lcCr1"
          digits="5"
          />
        <textmessage loc="70 25" size="65 20"
          style="PlainText"
          label="6.61123"
          vq="lcCr2"
          set="$VALUE=lcCr2"
          digits="5"
          />
        <textmessage loc="5 45" size="120 20"
          style="PlainText"
          label="Delta: 192.010 s"
          vq="lcCr1 lcCr2"
          set="$V='' format((lcCr2-lcCr1)*60,0,3):$V $VALUE='Delta: '+$V+' s'"
          digits="3"
          />
      </group>
      <group loc="475 135" size="160 105"
        style="Menu1"
        label="Output"
        bg="transparent"
        border="Etched"
        side="Top"
        tab="no"
        enable="no"
        reference="Plotting"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <button loc="20 25" size="120 20"
          style="PlainText"
          label="Print Run Log"
          vc="lccmd('printHtml', lcRunLogFile, 'p')"
          bg="0xF2E0BB"
          decor1="No"
          />
        <button loc="20 50" size="120 20"
          style="PlainText"
          label="Print Chromatogram"
          vc="lccmd('printLcGraph', svfdir+'/'+lcid+'/chromatogram.ps', 36, 756, 540, 0)"
          bg="0xF2E0BB"
          decor1="No"
          />
        <button loc="20 75" size="120 20"
          style="PlainText"
          label="Print Report"
          vc="lccmd('printHtml', lcRunLogFile, 'nopopup') lccmd('printLcGraph', svfdir+'/'+lcid+'/chromatogram.ps', 36, 756, 270, 0)"
          bg="0xF2E0BB"
          decor1="No"
          />
      </group>
      <group loc="235 10" size="180 50"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Split"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <button loc="90 25" size="80 20"
          style="PlainText"
          label="Hide LC"
          vc="lccmd('hideLcGraph')"
          decor1="No"
          />
        <button loc="10 25" size="80 20"
          style="PlainText"
          label="Split2"
          vc="substr(lcGraphSplit2,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit2,1,1,$str):lcGraphSplit2"
          decor1="No"
          icon="rot_split.png"
          />
        <button loc="10 5" size="80 20"
          style="PlainText"
          label="Split"
          vc="substr(lcGraphSplit,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit,1,1,$str):lcGraphSplit"
          decor1="No"
          icon="rot_split.png"
          />
        <button loc="90 5" size="80 20"
          style="PlainText"
          label="Show LC"
          vc="lccmd('showLcGraph')"
          decor1="No"
          />
      </group>
      <button loc="770 85" size="85 20"
        style="PlainText"
        label="Start Method"
        vc="lccmd('downloadMethod')"
        bg="0xCCCCFF"
        decor1="No"
        />
      <button loc="680 0" size="160 20"
        style="PlainText"
        label="Show Current MS Scan"
        vc="lccmd('showMsGraph', -1)"
        decor1="No"
        />
      <label loc="680 25" size="190 20"
        style="Label1"
        label="Send PML Command to MS:"
        justify="Left"
        />
      <entry loc="680 45" size="215 20"
        style="PlainText"
        vc="pmlcmd('$VALUE')"
        disable="Grayed out"
        />
      <button loc="245 75" size="80 20"
        style="PlainText"
        label="Enable Trig"
        vc="lccmd('enableTrigger')"
        decor1="No"
        />
      <check loc="10 110" size="130 20"
        style="PlainText"
        label="To NMR"
        vq="lcNmrValve"
        vc="lccmd('toNmr')"
        vc2="lccmd('toWaste')"
        set="$VALUE=lcNmrValve"
        />
      <check loc="10 90" size="130 20"
        style="PlainText"
        label="Through Column"
        vq="lcColumnValve"
        vc="lccmd('toColumn')"
        vc2="lccmd('toCollector')"
        set="$VALUE=lcColumnValve"
        />
      <button loc="680 85" size="80 20"
        style="PlainText"
        label="Edit Method"
        vc="lccmd('editMethod', lcMethodFile)"
        bg="0xCCCCFF"
        decor1="No"
        />
      <textfile loc="680 130" size="230 110"
        style="PlainText"
        vq="lcMethodFile"
        set="$VALUE='lc/lcmethods.txt/'+lcMethodFile+'.text'"
        editable="no"
        wrap="yes"
        />
      <filemenu loc="680 110" size="230 20"
        style="Label1"
        vq="lcMethodFile"
        vc="lcMethodFile='$VALUE'  lcMethod('read',lcMethodFile)"
        set="$VALUE=lcMethodFile"
        file="$VALUE='lc/lcmethods'"
        type="directory"
        bg="default"
        editable="No"
        vq2="lcMethodFile"
        />
      <button loc="245 105" size="80 20"
        style="PlainText"
        label="Read Data:"
        vc="readLcData"
        decor1="No"
        />
      <entry loc="325 105" size="345 20"
        style="PlainText"
        vq="lcDataFile"
        vc="lcDataFile='$VALUE' readLcData"
        set="$VALUE=lcDataFile"
        bg="0xeeeeee"
        disable="Grayed out"
        />
      <check loc="485 75" size="140 20"
        style="PlainText"
        label="Send Heartbeat"
        vc="lccmd('enableHeartbeat')"
        vc2="lccmd('disableHeartbeat')"
        />
      <button loc="100 65" size="60 20"
        style="PlainText"
        label="Stop"
        vc="lccmd('stopRun')"
        bg="0xF2D5D5"
        decor1="Yes"
        />
      <button loc="160 45" size="70 20"
        style="PlainText"
        label="Restart"
        vc="lccmd('restartRun')"
        bg="0xC9F2C9"
        decor1="Yes"
        />
      <button loc="100 45" size="60 20"
        style="PlainText"
        label="Pause"
        vc="lccmd('pauseRun')"
        bg="0xD3D3D3"
        decor1="Yes"
        />
      <button loc="15 45" size="85 20"
        style="PlainText"
        label="Start Run"
        vc="lcStartRun"
        bg="0xC9F2CA"
        decor1="Yes"
        />
      <button loc="124 12" size="24 24"
        style="Heading3"
        vc="lccmd('step')"
        decor1="Yes"
        icon="up11x17"
        />
      <textmessage loc="100 12" size="24 24"
        style="Menu1"
        label="0"
        vq="lcLoopIndex"
        set="$VALUE=lcLoopIndex"
        />
      <label loc="16 12" size="84 24"
        style="Menu1"
        label="Loop #"
        justify="Right"
        />
    </group>
    <label loc="8 0" size="112 24"
      style="Heading3"
      label="LC Test"
      justify="Left"
      />
  </group>
</template>
