      style="Menu1"
      label="AS 430 Comm"
      vq="vjDebugString"
      vc="vnmrjcmd('DEBUG add 430comm')"
      vc2="vnmrjcmd('DEBUG remove 430comm')"
      set="strstr(vjDebugString,',430comm,'):$VALUE"
      />
