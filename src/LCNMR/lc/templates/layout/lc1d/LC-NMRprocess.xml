<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="LC-NMRprocess" element="pages" type="processing" >
  <group size="840 290"
    style="Heading3"
    label="LC-NMR Process"
    bg="transparent"
    border="etched"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="LC-NMRprocess"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="665 0" size="175 290"
      style="Label1"
      label="Plotting"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Plotting"
      useref="no"
      subtype="Minor"
      expanded="yes"
      >
      <button loc="20 200" size="130 40"
        style="Label1"
        label="Save Glued Data"
        vc="svf(svfdir+'/'+lcid+'/NMR/glued')"
        show="if (lcexp='iso') or (lcexp='scout') then $SHOW=1 else $SHOW=-1 endif"
        bg="transparent"
        decor1="no"
        />
      <button loc="15 20" size="140 30"
        style="Label1"
        label="Plot LCNMR(MS) 2D"
        vc="lcplot"
        bg="white"
        decor1="no"
        />
      <button loc="15 125" size="140 20"
        style="Label1"
        label="Print Mass Spectrum"
        vc="lccmd('printMsGraph', svfdir+'/'+lcid+'/massSpectrum.ps', 36, 756, 540)"
        show="if(msSelection='0') then $SHOW=-1 else $SHOW=1 endif"
        decor1="no"
        />
      <button loc="15 105" size="140 20"
        style="Label1"
        label="Print UV Spectrum"
        vc="lccmd('printPdaGraph', 'uvSpectrum.eps', 36, 756, 540)"
        decor1="no"
        />
      <button loc="15 85" size="140 20"
        style="Label1"
        label="Print Chromatogram"
        vc="lccmd('printLcGraph', svfdir+'/'+lcid+'/chromatogram.ps', 36, 756, 540)"
        decor1="no"
        />
      <button loc="15 65" size="140 20"
        style="Label1"
        label="Print Run Log"
        vc="lccmd('printHtml', lcRunLogFile, lcPrinter[1])"
        decor1="no"
        />
    </group>
    <button loc="565 20" size="75 25"
