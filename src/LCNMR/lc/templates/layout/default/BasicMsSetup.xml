<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="BasicMsSetup" element="pages" type="preparation" >
  <group size="840 260"
    style="PlainText"
    label="MS Setup"
    vq="msSelection"
    show="$SHOW = (msSelection &lt;&gt; '0')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="BasicMsSetup"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="0 160" size="275 95"
      style="Heading3"
      label="MS Synchronization"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="Configuration"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="200 25" size="25 20"
        style="Label1"
        label=" s"
        />
      <entry loc="145 25" size="55 20"
        style="PlainText"
        vq="msSyncDelay"
        vc="msSyncDelay=$VALUE"
        set="$VALUE=msSyncDelay"
        />
      <label loc="5 25" size="70 20"
        style="Label1"
        label="Start Delay"
        />
    </group>
    <group loc="560 85" size="280 70"
      style="Heading3"
      label="Tune File"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="TuneFile:"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="220 45" size="55 20"
        style="PlainText"
        label="Restore"
        vc="msTune('restore', msTuneFile)"
        decor1="No"
        />
      <button loc="160 45" size="55 20"
        style="PlainText"
        label="Save"
        vc="msTune('save', msTuneFile)"
        decor1="No"
        />
      <entry loc="5 20" size="270 20"
        style="PlainText"
        vq="msTuneFile"
        vc="msTuneFile='$VALUE'"
        set="$VALUE=msTuneFile"
        />
    </group>
    <group loc="560 25" size="280 60"
      style="Heading3"
      label="Display"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MSDisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="150 25" size="60 20"
        style="Label1"
        label="Off"
        vc="lccmd('updateMsGraph') msShowOldData='n'"
        decor1="No"
        />
      <button loc="90 25" size="60 20"
        style="Label1"
        label="Historical"
        vc="lccmd('updateMsGraph') msShowOldData='y' lccmd('showLcGraph')"
        decor1="No"
        />
      <button loc="30 25" size="60 20"
        style="Label1"
        label="Current"
        vc="lccmd('updateMsGraph 500') msShowOldData='n' lccmd('showLcGraph')"
        decor1="No"
        />
    </group>
    <group loc="280 25" size="275 130"
      style="Heading3"
      label="Instrument Settings"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="InstrumentSettings"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="5 25" size="235 100"
        bg="transparent"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="140 50" size="55 20"
          style="PlainText"
          vc="pmlcmd('corona=', $VALUE)"
          statpar="MSneedleAmpsSetting"
          />
        <label loc="195 25" size="25 20"
          style="Label1"
          label=" V"
          />
        <entry loc="140 25" size="55 20"
          style="PlainText"
          vc="pmlcmd('needle=', $VALUE)"
          statpar="MSneedleVoltsSetting"
          />
        <label loc="0 25" size="140 20"
          style="Label1"
          label="Needle Voltage (ESI)"
          />
        <label loc="0 50" size="140 20"
          style="Label1"
          label="Needle Current (APCI)"
          />
        <label loc="195 50" size="25 20"
          style="Label1"
          label=" ÂµA"
          />
        <label loc="0 75" size="140 20"
          style="Label1"
          label="Shield Voltage"
          />
        <label loc="195 75" size="25 20"
          style="Label1"
          label=" V"
          />
        <entry loc="140 75" size="55 20"
          style="PlainText"
          vc="pmlcmd('shield=', $VALUE)"
          statpar="MSshieldVoltsSetting"
          />
        <button loc="175 0" size="35 20"
          style="PlainText"
          label="Off"
          vc="pmlcmd('off:api_off') pmlcmd('send_to_nmr:&quot;|MSdetOn|&quot;:?on + 1:cr')"
          statpar="MSdetOn"
          statshow="On 2"
          decor1="Yes"
          />
        <button loc="140 0" size="35 20"
          style="PlainText"
          label="On"
          vc="pmlcmd('on: api_on') pmlcmd('send_to_nmr:&quot;|MSdetOn|&quot;:?on + 1:cr')"
          statpar="MSdetOn"
          statshow="Off 1"
          decor1="Yes"
          />
        <textmessage loc="50 0" size="55 20"
          style="PlainText"
          statpar="MSdetOn"
          />
        <label loc="0 0" size="50 20"
          style="Label1"
          label="Status: "
          justify="Left"
          />
      </group>
    </group>
    <group loc="0 25" size="275 130"
      style="Heading3"
      label="Flow Settings"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MakeupPump"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="0 25" size="270 90"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <textmessage loc="80 70" size="90 20"
          style="PlainText"
          statpar="makeupPressure"
          />
        <textmessage loc="80 50" size="90 20"
          style="PlainText"
          statpar="makeupFlow"
          />
        <label loc="10 70" size="60 20"
          style="Label1"
          label="Pressure:"
          justify="Left"
          />
        <label loc="10 50" size="50 20"
          style="Label1"
          label="Flow:"
          justify="Left"
          />
        <label loc="10 20" size="70 20"
          style="Label1"
          label="MS Pump: "
          justify="Left"
          />
        <label loc="10 0" size="70 20"
          style="Label1"
          label="MS Valve: "
          justify="Left"
          />
        <button loc="80 0" size="40 20"
          style="Label1"
          label="Load"
          vc="pmlcmd('lc_valve=0')"
          statpar="MSvalveStatus"
          statshow="inject 2"
          decor1="Yes"
          />
        <button loc="120 0" size="40 20"
          style="Label1"
          label="Inject"
          vc="pmlcmd('lc_valve=1')"
          statpar="MSvalveStatus"
          statshow="load 1"
          decor1="Yes"
          />
        <button loc="80 20" size="40 20"
          style="Label1"
          label="Off"
          vc="lccmd('makeupPumpOff')"
          statpar="makeupStatus"
          statshow="on"
          decor1="Yes"
          />
        <button loc="120 20" size="40 20"
          style="Label1"
          label="On"
          vc="lccmd('makeupPumpOn', msPumpFlow)"
          statpar="makeupStatus"
          statshow="off"
          decor1="Yes"
          />
        <label loc="160 20" size="20 20"
          style="Label1"
          label="at"
          justify="Center"
          />
        <entry loc="180 20" size="40 20"
          style="PlainText"
          vq="msPumpFlow"
          vc="msPumpFlow=$VALUE"
          set="$VALUE=msPumpFlow"
          disable="Grayed out"
          />
        <label loc="220 20" size="50 20"
          style="Label1"
          label=" mL/min"
          justify="Left"
          />
      </group>
    </group>
    <label loc="0 0" size="160 20"
      style="Heading1"
      label="MS Setup"
      />
  </group>
</template>
