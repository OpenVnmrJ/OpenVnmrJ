<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standard" element="pages" type="sample" >
  <group size="845 320"
    style="PlainText"
    label="Study"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standard"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="320 10" size="300 240"
      style="Heading2"
      label="LC Method"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="LCMethod"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="15 75" size="265 20"
        style="PlainText"
        label="Auto Process when LC run completes"
        vq="scoutap"
        vc="scoutap='y'"
        vc2="scoutap='n'"
        set="$VALUE=(scoutap='y')"
        />
      <group loc="20 90" size="220 30"
        vq="lcexp"
        show="$SHOW=(lcexp='enterA')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="11 5" size="205 20"
          style="Heading2"
          label="Define Exp for each peak"
          />
      </group>
      <group loc="15 95" size="210 45"
        vq="lcexp"
        show="$SHOW=(lcexp='stop' or lcexp='enter')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <radio loc="0 0" size="205 20"
          style="Heading2"
          label="Same Exp on all peaks"
          vq="lcexp lcHoldType lcid seqfil autodir"
          vc="lcexp='stop'"
          vc2="lcexp='enter'"
          set="$VALUE=(lcexp='stop')"
          show="lcsq('checkrun'):$ENABLE"
          />
        <radio loc="0 20" size="205 20"
          style="Heading2"
          label="Define Exp for each peak"
          vq="lcexp lcHoldType lcid seqfil autodir"
          vc="lcexp='enter'"
          vc2="lcexp='stop'"
          set="$VALUE=(lcexp='enter')"
          show="lcsq('checkrun'):$ENABLE"
          />
      </group>
      <group loc="5 95" size="250 20"
        vq="lcexp"
        show="$SHOW=(lcexp='iso' or lcexp='scout')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="10 0" size="150 20"
          style="Heading2"
          label="Isocratic On Flow"
          vq="lcexp lcHoldType lcid seqfil autodir"
          vc="lcexp='iso'"
          vc2="lcexp='scout'"
          set="$VALUE=(lcexp='iso')"
          show="lcsq('checkrun'):$ENABLE"
          />
      </group>
      <group loc="0 165" size="295 75"
        vq="lcexp"
        show="$SHOW=(lcexp='enter' or lcexp='enterA')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <group loc="5 20" size="285 50"
          vq="lcexp"
          show="$SHOW=(lcexp='enter')"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          reference="rettime"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <button loc="5 25" size="275 25"
            style="Heading4"
            label="Define expected Peak retention times"
            vq="lcexp"
            vc="EstRetentionTime"
            show="$pk='' cplcsq('getNumPeaks'):$pk $ENABLE=((lcexp='enter') and ($pk&lt;&gt;'') and ($pk&lt;&gt;'0'))*2-1"
            bg="transparent"
            decor1="yes"
            />
          <label loc="240 0" size="25 20"
            style="Label1"
            label="sec"
            justify="Left"
            />
          <entry loc="195 0" size="45 20"
            style="Label1"
            vq="rettimeRange"
            vc="on('rettimeRange','global'):$on if ($on&gt;0) then rettimeRange=$VALUE endif"
            set="$VALUE=rettimeRange"
            show="on('rettimeRange','global'):$on $ENABLE=($on&gt;0)"
            digits="0"
            disable="Grayed out"
            />
          <check loc="5 0" size="190 20"
            style="Label1"
            label="validate retention time  (+/-)"
            vq="rettimeRange"
            vc="on('rettimeRange','global')"
            vc2="off('rettimeRange','global')"
            set="on('rettimeRange','global'):$on $VALUE=($on&gt;0)"
            show="on('rettimeRange','global'):$on $ENABLE=($on&gt;-1)*2-1"
            />
        </group>
        <textmessage loc="15 0" size="155 20"
          style="Heading2"
          label="2 peaks defined"
          vq="lcexp"
          set="$VALUE='' cplcsq('getNumPeaks'):$VALUE if ($VALUE='') then $VALUE='0' endif $VALUE=$VALUE+' peaks defined'"
          />
      </group>
      <textmessage loc="15 145" size="230 20"
        style="Label1"
        label="Use StudyQueue to define exp/peak"
        vq="lcexp"
        set="if (lcexp='stop') or (lcexp='enter') or (lcexp='enterA') then $VALUE='Use StudyQueue to define exp/peak' else $VALUE='' endif"
        />
      <button loc="150 45" size="130 25"
        style="Label3"
        label="Create New Method"
        vc="lccmd('editMethod', 'NewMethod')"
        bg="transparent"
        decor1="no"
        />
      <button loc="10 45" size="130 25"
        style="Label3"
        label="Edit Current Method"
        vc="lccmd('editMethod', lcMethodFile)"
        bg="transparent"
        decor1="no"
        />
      <cascadingMenu loc="10 20" size="270 20"
        style="Label1"
        label="Select LC method to use"
        vq="lcMethodFile"
        vc="lccmd('loadMethod', '\&quot;$VALUE\&quot;')"
        set="$VALUE=lcMethodFile"
        file="lc/lcmethods"
        bg="VJBackground"
        statshow="no"
        />
    </group>
    <group loc="625 105" size="210 120"
      style="Heading2"
      label="NMR Actions"
      vq="atune wtune lcexp"
      show="$SHOW=(lcexp='stop' or lcexp='enter' or lcexp='enterA')"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <menu loc="30 80" size="110 20"
        style="PlainText"
        vq="Gmap"
        vc="$v='$VALUE' gmapshim('findprobemap',$v):$e if $e&gt;0.5 then Gmap=$v else Gmap='lk' endif"
        set="$VALUE=Gmap"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="2H gradient"
          chval="lk"
          />
        <mlabel 
          label="1H gradient"
          chval="H1"
          />
        <mlabel 
          label="2H homospoil"
          chval="hs"
          />
        <mlabel 
          label="1H homospoil"
          chval="H1hs"
          />
      </menu>
      <check loc="10 20" size="85 20"
        style="PlainText"
        label="Auto Lock"
        vq="alock"
        vc="alock='a' getparam('alock','LCoption'):alock"
        vc2="alock='n'"
        set="$VALUE=(alock&lt;&gt;'n')"
        />
      <check loc="10 40" size="90 20"
        style="PlainText"
        label="Tune"
        vq="wtune atune"
        vc="wtune='s' getparam('wtune','Probe'):wtune"
        vc2="wtune='n'"
        set="$VALUE=(wtune&lt;&gt;'n')"
        show="$ENABLE=(atune='y')*2-1"
        />
      <check loc="10 60" size="105 20"
        style="PlainText"
        label="Gradient Shim"
        vq="wshim"
        vc="wshim='g' getparam('wshim','LCoption'):wshim"
        vc2="wshim='n'"
        set="$VALUE=(wshim&lt;&gt;'n')"
        />
      <check loc="10 100" size="130 20"
        style="PlainText"
        label="Plot NMR data"
        vq="pltopt pltmod"
        vc="pltopt='y' pltmod='full'"
        vc2="pltopt='n' pltmod='off'"
        set="$VALUE=(pltopt='y')"
        />
    </group>
    <group loc="625 10" size="210 90"
      style="Heading2"
      label="NMR Hold Time"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="NmrFramedHoldTime"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <radio loc="10 40" size="170 20"
        style="PlainText"
        label="Until NMR Finishes"
        vq="lcHoldType lcexp lcid seqfil autodir"
        vc="lcHoldType='nmr'"
        set="$VALUE=(lcHoldType='nmr')"
        show="lcsq('checkrun'):$ENABLE"
        />
      <radio loc="10 20" size="170 20"
        style="PlainText"
        label="Indefinitely"
        vq="lcHoldType lcexp lcid seqfil autodir"
        vc="lcHoldType='indefinite'"
        set="$VALUE=(lcHoldType='indefinite')"
        show="lcsq('checkrun'):$ENABLE"
        />
      <group loc="110 60" size="80 20"
        vq="lcHoldType lcexp"
        show="$SHOW=(lcHoldType='fixed')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="50 0" size="25 20"
          style="Label4"
          label="min"
          justify="Left"
          />
        <entry loc="0 0" size="50 20"
          style="PlainText"
          vq="lcHoldTime lcHoldType lcexp lcid seqfil autodir"
          vc="lcHoldTime='$VALUE'"
          set="$VALUE=lcHoldTime"
          show="lcsq('checkrun'):$ENABLE"
          digits="2"
          disable="Label"
          />
      </group>
      <radio loc="10 60" size="100 20"
        style="PlainText"
        label="Fixed Time:"
        vq="lcHoldType lcexp lcid seqfil autodir"
        vc="lcHoldType='fixed'"
        set="$VALUE=(lcHoldType='fixed')"
        show="lcsq('checkrun'):$ENABLE"
        />
    </group>
    <group loc="5 5" size="310 245"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Major"
      >
      <check loc="75 220" size="100 20"
        style="PlainText"
        label="after peak"
        vq="emailflag lcexp"
        vc="strstr(emailwhen,'peak'):$ok if ($ok=0) then emailwhen=emailwhen+'peak' endif"
        vc2="$s1='' $s2='' strstr(emailwhen,'peak'):$ok,$s1,$s2 if ($ok) then emailwhen=$s1+$s2 endif"
        set="$VALUE=0 if (emailwhen='') then $VALUE=1 else strstr(emailwhen,'peak'):$VALUE endif"
        show="$ENABLE=((emailflag='y') and ((lcexp='stop') or (lcexp='enter') or (lcexp='enterA')))*2-1"
        />
      <check loc="190 220" size="115 20"
        style="PlainText"
        label="after LC run"
        vq="emailflag"
        vc="strstr(emailwhen,'lcrun'):$ok if ($ok=0) then emailwhen=emailwhen+'lcrun' endif"
        vc2="$s1='' $s2='' strstr(emailwhen,'lcrun'):$ok,$s1,$s2 if ($ok) then emailwhen=$s1+$s2 endif"
        set="$VALUE=0 if (emailwhen='') then $VALUE=1 else strstr(emailwhen,'lcrun'):$VALUE endif"
        show="$ENABLE=(emailflag='y')*2-1"
        />
      <entry loc="75 200" size="230 20"
        style="PlainText"
        vq="emailaddr"
        vc="emailaddr='$VALUE'"
        set="$VALUE=emailaddr"
        show="$ENABLE=(emailflag='y')*2-1"
        disable="Grayed out"
        />
      <check loc="5 200" size="90 20"
        style="PlainText"
        label="Email to:"
        vq="emailflag"
        vc="emailflag='y'"
        vc2="emailflag='n'"
        set="$VALUE=(emailflag='y')"
        />
      <textfile loc="5 135" size="300 65"
        style="PlainText"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <label loc="5 115" size="100 20"
        style="Heading2"
        label="Comment"
        justify="Left"
        />
      <textmessage loc="90 90" size="180 20"
        style="PlainText"
        label="D2O"
        vq="solvent"
        set="$VALUE=solvent"
        />
      <label loc="5 90" size="70 20"
        style="Label1"
        label="Solvent"
        justify="Left"
        />
      <entry loc="225 50" size="80 20"
        style="Label1"
        vq="page"
        vc="page='$VALUE'"
        set="$VALUE=page"
        disable="Grayed out"
        />
      <entry loc="75 50" size="90 20"
        style="Label1"
        vq="notebook"
        vc="notebook='$VALUE'"
        set="$VALUE=notebook"
        disable="Grayed out"
        />
      <label loc="5 50" size="60 20"
        style="Label1"
        label="Notebook"
        justify="Left"
        />
      <label loc="5 30" size="70 20"
        style="Label1"
        label="Sample"
        justify="Left"
        />
      <entry loc="75 30" size="230 20"
        style="Label1"
        vq="samplename"
        vc="samplename='$VALUE'"
        set="$VALUE=samplename"
        disable="Grayed out"
        />
      <textmessage loc="85 5" size="220 25"
        style="Heading1"
        label="lcmsusr"
        vq="operator"
        set="$VALUE=operator"
        />
      <label loc="5 5" size="70 25"
        style="Heading2"
        label="Operator"
        />
      <group loc="0 70" size="300 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="5 0" size="90 20"
          style="Label1"
          label="Temperature"
          />
        <check loc="110 0" size="20 20"
          style="PlainText"
          vq="temp tin"
          vc="on('temp') tin='y'"
          vc2="off('temp') tin='n'"
          set="on('temp'):$VALUE"
          show="vnmrinfo('get','tempExpControl'):$tc $ENABLE=$tc*2-1"
          />
        <entry loc="130 0" size="70 20"
          style="PlainText"
          vq="temp tin"
          vc="temp=$VALUE tin='y'"
          set="$VALUE=temp"
          show="vnmrinfo('get','tempExpControl'):$tc if $tc=0 then $ENABLE=-1 else on('temp'):$ENABLE endif"
          digits="1"
          disable="Label"
          />
        <label loc="200 0" size="20 20"
          style="Label1"
          label="C"
          justify="Left"
          />
      </group>
      <label loc="180 50" size="40 20"
        style="Label1"
        label="Page"
        justify="Left"
        />
    </group>
    <textmessage loc="180 295" size="600 20"
      style="PlainText"
      label="/home/lcmsusr/vnmrsys/data/testX013"
      vq="svfdir lcid"
      set="getsvfdirpath:$svfdir filepath($svfdir, lcid):$VALUE"
      />
    <textmessage loc="180 255" size="600 20"
      style="PlainText"
      label="/home/lcmsusr/vnmrsys/data/"
      vq="svfdir"
      set="getsvfdirpath:$VALUE"
      />
    <entry loc="180 275" size="405 20"
      style="PlainText"
      vq="lcid"
      vc="lcid='$VALUE'"
      set="$VALUE=lcid"
      disable="Label"
      />
    <label loc="10 295" size="165 20"
      style="Label1"
      label="LC-NMR/MS Data directory:"
      justify="Left"
      />
    <label loc="10 275" size="165 20"
      style="Label1"
      label="LC-NMR/MS Subdirectory"
      justify="Left"
      />
    <label loc="10 255" size="145 20"
      style="Label1"
      label="Data Storage Area:"
      justify="Left"
      />
  </group>
</template>
