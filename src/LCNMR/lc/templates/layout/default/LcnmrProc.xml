<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="LcnmrProc" element="pages" type="processing" >
  <group size="764 304"
    style="PlainText"
    label="LC-NMR Proc."
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="LcnmrProc"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <button loc="600 260" size="130 40"
      style="Heading3"
      label="Save Glued Data"
      vc="svf(svfdir+'/'+lcid+'/NMR/glued')"
      bg="white"
      decor1="No"
      />
    <button loc="600 130" size="140 30"
      style="Heading3"
      label="Plot "
      vc="lcplot"
      bg="white"
      decor1="No"
      />
    <button loc="600 230" size="140 20"
      style="PlainText"
      label="Print Mass Spectrum"
      vc="lccmd('printMsGraph', svfdir+'/'+lcid+'/massSpectrum.ps', 36, 756, 540)"
      show="if(msSelection='0') then $SHOW=-1 else $SHOW=1 endif"
      decor1="No"
      />
    <button loc="600 210" size="140 20"
      style="PlainText"
      label="Print UV Spectrum"
      vc="lccmd('printPdaGraph', 'uvSpectrum.eps', 36, 756, 540)"
      decor1="No"
      />
    <button loc="600 190" size="140 20"
      style="PlainText"
      label="Print Chromatogram"
      vc="lccmd('printLcGraph', svfdir+'/'+lcid+'/chromatogram.ps', 36, 756, 540)"
      decor1="No"
      />
    <button loc="600 170" size="140 20"
      style="PlainText"
      label="Print Run Log"
      vc="lccmd('printHtml', lcRunLogFile, 'popup')"
      decor1="No"
      />
    <button loc="430 10" size="130 50"
      style="Heading4"
      label="Display LC-NMR"
      vc="dLCNMR"
      bg="white"
      decor1="Yes"
      />
    <button loc="10 10" size="170 50"
      style="Heading3"
      label="Auto-Process LC-NMR"
      vc="wftlc"
      decor1="Yes"
      />
    <check loc="570 15" size="75 45"
      style="Label3"
      label="Contour"
      vq="dconi"
      vc="dconi='dpcon'"
      vc2="dconi=''"
      />
    <group loc="195 5" size="225 65"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="128 33" size="93 29"
        vq="fn"
        show="on('fn'):$on if $on then $SHOW=1 else $SHOW=-1 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <scroll loc="2 2" size="89 24"
          style="PlainText"
          label="value1"
          vq="fn"
          vc="fn=$VALUE"
          set="$VALUE=fn"
          values="8k,16k,32k,64k"
          />
      </group>
      <toggle loc="3 35" size="125 22"
        style="Label1"
        label="Fourier Number"
        vq="fn"
        vc="on('fn')"
        vc2="fn='n'"
        radiobutton="false"
        />
      <toggle loc="2 6" size="112 25"
        style="Label1"
        label="Line Broadening"
        vq="lb"
        vc="on('lb')"
        vc2="lb='n'"
        radiobutton="false"
        />
      <group loc="116 3" size="49 28"
        vq="lb"
        show="on('lb'):$on if $on then $SHOW=1 else $SHOW=-1 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="1 2" size="40 24"
          style="PlainText"
          vq="lb"
          vc="lb=$VALUE"
          set="$VALUE=lb"
          digits="3"
          disable="Grayed out"
          />
      </group>
    </group>
    <group loc="0 200" size="595 90"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <button loc="345 25" size="80 25"
        style="Label1"
        label="DC Correct"
        vq="dc"
        vc="dc"
        decor1="No"
        />
      <group loc="435 0" size="150 85"
        style="Label1"
        label="NMR Shift Range"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="40 24" size="64 24"
          style="PlainText"
          vq="sp sfrq startshift wp"
          vc="sp=$VALUE*sfrq startshift=$VALUE wp=endshift*sfrq-sp"
          set="$VALUE=sp/sfrq"
          digits="3"
          disable="Grayed out"
          />
        <entry loc="40 56" size="64 24"
          style="PlainText"
          vq="wp sp sfrq endshift startshift"
          vc="wp=$VALUE*sfrq-sp endshift=$VALUE sp=startshift*sfrq"
          set="$VALUE=(sp + wp) / sfrq"
          digits="3"
          disable="Grayed out"
          />
        <label loc="104 56" size="40 24"
          style="Label1"
          label="(ppm)"
          justify="Left"
          />
        <label loc="104 24" size="40 24"
          style="Label1"
          label="(ppm)"
          justify="Left"
          />
        <label loc="8 56" size="32 24"
          style="Label1"
          label="End"
          justify="Left"
          />
        <label loc="8 24" size="32 24"
          style="Label1"
          label="Start"
          justify="Left"
          />
      </group>
      <group loc="5 5" size="335 80"
        style="Label1"
        label="NMR Intensity"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <button loc="115 50" size="105 25"
          style="Label1"
          label="Normalize NMR"
          vc="vsadjlc"
          decor1="No"
          />
        <button loc="245 20" size="70 25"
          style="Label1"
          label="down 20%"
          vq="lcnmrvsd20"
          vc="lcnmrvsd20"
          decor1="No"
          />
        <button loc="175 20" size="70 25"
          style="Label1"
          label="up 20%"
          vq="lcnmrvsu20"
          vc="lcnmrvsu20"
          decor1="No"
          />
        <button loc="130 20" size="40 25"
          style="Label1"
          label="/ 2"
          vq="lcnmrvsd2"
          vc="lcnmrvsd2"
          decor1="No"
          />
        <button loc="90 20" size="40 25"
          style="Label1"
          label="x 2"
          vq="lcnmrvsu2"
          vc="lcnmrvsu2"
          decor1="No"
          />
        <label loc="5 20" size="75 25"
          style="Label1"
          label="NMR Scale"
          justify="Left"
          />
      </group>
    </group>
    <group loc="0 80" size="595 120"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="440 5" size="150 110"
        style="Label1"
        label="LC Time Range"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="100 55" size="30 20"
          style="Label1"
          label="(min)"
          justify="Left"
          />
        <label loc="100 25" size="30 20"
          style="Label1"
          label="(min)"
          justify="Left"
          />
        <label loc="10 55" size="30 20"
          style="Label1"
          label="End"
          justify="Left"
          />
        <label loc="10 25" size="30 20"
          style="Label1"
          label="Start"
          justify="Left"
          />
        <entry loc="40 25" size="60 20"
          style="PlainText"
          vq="sf1 endlctime"
          vc="sf1=$VALUE wf1=endlctime - sf1"
          set="$VALUE=sf1"
          digits="3"
          disable="Grayed out"
          />
        <entry loc="40 55" size="60 20"
          style="PlainText"
          vq="endlctime wf1 sf1"
          vc="wf1=$VALUE-sf1 endlctime=$VALUE"
          set="$VALUE=sf1 + wf1"
          digits="3"
          disable="Grayed out"
          />
      </group>
      <group loc="5 5" size="435 110"
        style="Label1"
        label="LC Intensity"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="LcIntensity"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <button loc="110 80" size="85 25"
          style="Label1"
          label="Show LC/MS"
          vc="lccmd('updateMsGraph') msShowOldData='y' lccmd('showLcGraph')"
          decor1="No"
          />
        <button loc="345 80" size="85 25"
          style="Label1"
          label="Show PDA"
          vc="lccmd('pdaGetImage', svfdir + lcid)"
          decor1="No"
          />
        <button loc="5 80" size="105 25"
          style="Label1"
          label="Normalize LC"
          vc="maxlctime"
          decor1="No"
          />
        <button loc="195 80" size="80 25"
          style="Label1"
          label="Hide LC/MS"
          vc="lccmd('hideLcGraph')"
          decor1="No"
          />
        <group loc="5 60" size="425 20"
          vq="lcEnableA"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <button loc="345 0" size="40 20"
            style="Label1"
            label="-20%"
            vq="vslc"
            vc="vslc[3]=vslc[3]/1.2"
            show="if (size('vslc') &gt;= 3) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="305 0" size="40 20"
            style="Label1"
            label="+20%"
            vq="vslc"
            vc="vslc[3]=vslc[3]*1.2"
            show="if (size('vslc') &gt;= 3) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="270 0" size="30 20"
            style="Label1"
            label="/ 2"
            vq="vslc"
            vc="vslc[3]=vslc[3]/2"
            show="if (size('vslc') &gt;= 3) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="235 0" size="35 20"
            style="Label1"
            label="x 2"
            vq="vslc"
            vc="vslc[3]=vslc[3]*2"
            show="if (size('vslc') &gt;= 3) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <entry loc="190 0" size="45 20"
            style="PlainText"
            vq="lcEnableC lcPeakDetect vslc"
            vc="vslc[3]=$VALUE"
            set="$VALUE=vslc[3]"
            show="if (size('vslc') &gt;= 3) then $VALUE=1 else $VALUE= -1 endif"
            digits="0"
            disable="Grayed out"
            />
          <label loc="150 0" size="40 20"
            style="Label1"
            label="Scale"
            justify="Right"
            />
          <label loc="0 0" size="20 20"
            style="Heading3"
            label=" C"
            justify="Left"
            />
          <group loc="25 5" size="10 10"
            bg="GraphForeground3"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
          </group>
          <textmessage loc="45 0" size="105 20"
            style="Label1"
            label="none"
            vq="lcTrace3"
            set="lcExtractLabel(lcTrace[3]):$VALUE"
            />
        </group>
        <group loc="5 40" size="425 20"
          vq="lcEnableA"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <button loc="345 0" size="40 20"
            style="Label1"
            label="-20%"
            vq="vslc"
            vc="vslc[2]=vslc[2]/1.2"
            show="if (size('vslc') &gt;= 2) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="305 0" size="40 20"
            style="Label1"
            label="+20%"
            vq="vslc"
            vc="vslc[2]=vslc[2]*1.2"
            show="if (size('vslc') &gt;= 2) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="270 0" size="30 20"
            style="Label1"
            label="/ 2"
            vq="vslc"
            vc="vslc[2]=vslc[2]/2"
            show="if (size('vslc') &gt;= 2) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="235 0" size="35 20"
            style="Label1"
            label="x 2"
            vq="vslc"
            vc="vslc[2]=vslc[2]*2"
            show="if (size('vslc') &gt;= 2) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <entry loc="190 0" size="45 20"
            style="PlainText"
            vq=" lcEnableB lcPeakDetect vslc"
            vc="vslc[2]=$VALUE"
            set="$VALUE=vslc[2]"
            show="if (size('vslc') &gt;= 2) then $VALUE=1 else $VALUE= -1 endif"
            digits="0"
            disable="Grayed out"
            />
          <label loc="150 0" size="40 20"
            style="Label1"
            label="Scale"
            justify="Right"
            />
          <group loc="25 5" size="10 10"
            bg="GraphForeground2"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
          </group>
          <textmessage loc="45 0" size="105 20"
            style="Label1"
            label="PDA 335 Out-2"
            vq="lcTrace2"
            set="lcExtractLabel(lcTrace[2]):$VALUE"
            />
          <label loc="0 0" size="20 20"
            style="Heading3"
            label=" B"
            justify="Left"
            />
        </group>
        <group loc="5 20" size="425 20"
          vq="lcEnableA"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <button loc="345 0" size="40 20"
            style="Label1"
            label="-20%"
            vq="vslc"
            vc="vslc[1]=vslc[1]/1.2"
            show="if (size('vslc') &gt;= 1) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="305 0" size="40 20"
            style="Label1"
            label="+20%"
            vq="vslc"
            vc="vslc[1]=vslc[1]*1.2"
            show="if (size('vslc') &gt;= 1) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <entry loc="190 0" size="45 20"
            style="PlainText"
            vq="vslc lcPeakDetect[1] lcEnableA"
            vc="vslc[1]=$VALUE"
            set="$VALUE=vslc[1]"
            show="if (size('vslc') &gt;= 1) then $VALUE=1 else $VALUE= -1 endif"
            digits="0"
            disable="Grayed out"
            />
          <button loc="270 0" size="30 20"
            style="Label1"
            label="/ 2"
            vq="vslc"
            vc="vslc[1]=vslc[1]/2"
            show="if (size('vslc') &gt;= 1) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <button loc="235 0" size="35 20"
            style="Label1"
            label="x 2"
            vq="vslc"
            vc="vslc[1]=vslc[1]*2"
            show="if (size('vslc') &gt;= 1) then $VALUE=1 else $VALUE= -1 endif"
            decor1="No"
            />
          <label loc="150 0" size="40 20"
            style="Label1"
            label="Scale"
            justify="Right"
            />
          <group loc="25 5" size="10 10"
            bg="GraphForeground"
