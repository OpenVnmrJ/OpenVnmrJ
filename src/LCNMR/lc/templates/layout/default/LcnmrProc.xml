            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
          </group>
          <textmessage loc="45 0" size="105 20"
            style="Label1"
            label="PDA 335 Out-1"
            vq="lcTrace1"
            set="lcExtractLabel(lcTrace[1]):$VALUE"
            />
          <label loc="0 0" size="20 20"
            style="Heading3"
            label=" A"
            justify="Left"
            />
        </group>
      </group>
    </group>
  </group>
</template>
