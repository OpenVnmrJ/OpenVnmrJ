            set="lcXferDelay('get', 'time', 'loop', lcXferDelayType):$VALUE"
            show="if (lcFlowRate = '0') then $SHOW = -1 elseif (lcXferDelayType = 1) then $SHOW = 0 else $SHOW = 1 endif"
