<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="StopFlowInteract" element="pages" type="preparation" >
  <group size="850 260"
    style="Heading3"
    label="Stop Flow Interact"
    vq="lcexp"
    show="$VALUE = (lcexp = 'stop' or lcexp = 'enter')"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="StopFlowInteract"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="330 10" size="220 90"
      style="Label1"
      label="NMR Hold Time"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="NmrFramedHoldTime"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <radio loc="10 40" size="170 20"
        style="PlainText"
        label="Until NMR Finishes"
        vq="lcHoldType lcexp lcid seqfil autodir"
        vc="lcHoldType='nmr'"
        set="$VALUE=(lcHoldType='nmr')"
        show="lcsq('checkrun'):$ENABLE"
        />
      <radio loc="10 20" size="170 20"
        style="PlainText"
        label="Indefinitely"
        vq="lcHoldType lcexp lcid seqfil autodir"
        vc="lcHoldType='indefinite'"
        set="$VALUE=(lcHoldType='indefinite')"
        show="lcsq('checkrun'):$ENABLE"
        />
      <group loc="110 60" size="80 20"
        vq="lcHoldType lcexp"
        show="$SHOW=(lcHoldType='fixed')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="50 0" size="25 20"
          style="Label1"
          label="min"
          justify="Left"
          />
        <entry loc="0 0" size="50 20"
          style="PlainText"
          vq="lcHoldTime lcHoldType lcexp lcid seqfil autodir"
          vc="lcHoldTime='$VALUE'"
          set="$VALUE=lcHoldTime"
          show="lcsq('checkrun'):$ENABLE"
          digits="2"
          disable="Label"
          />
      </group>
      <radio loc="10 60" size="100 20"
        style="PlainText"
        label="Fixed Time:"
        vq="lcHoldType lcexp lcid seqfil autodir"
        vc="lcHoldType='fixed'"
        set="$VALUE=(lcHoldType='fixed')"
        show="lcsq('checkrun'):$ENABLE"
        />
    </group>
    <group loc="590 90" size="170 35"
      bg="PausedBackground"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <label loc="14 7" size="70 21"
        style="Label1"
        label="Time Held: "
        justify="Left"
        />
      <textmessage loc="91 7" size="70 21"
        style="Menu4"
        label="0.00"
        vq="lcHoldCount"
        set="$VALUE=lcHoldCount"
        digits="2"
        />
    </group>
    <group loc="10 190" size="300 60"
      style="Label1"
      label="Time Slice"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="10 25" size="120 20"
        style="Label1"
        label="Time Slice Control"
        justify="Left"
        />
      <button loc="130 25" size="65 20"
        style="PlainText"
        label="Start"
        vc="lccmd('timeSliceNmr','init')"
        decor1="yes"
        />
      <button loc="195 25" size="65 20"
        style="PlainText"
        label="End"
        vc="lccmd('timeSliceNmr','stop')"
        decor1="yes"
        />
    </group>
    <group loc="330 120" size="220 130"
      style="Label3"
      label="Processing"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="lbfn"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="35 91" size="147 28"
        style="PlainText"
        label="Process"
        vc="setssfilter wft"
        decor1="no"
        />
      <label loc="175 60" size="42 21"
        style="Label1"
        label=" ppm"
        justify="Left"
        />
      <entry loc="121 60" size="56 21"
        style="PlainText"
        vq="refpos rfp sfrq rfl sw"
        vc="refpos='n' rfp=$VALUE*sfrq rfl=sw/2"
        set="$VALUE=rfp/sfrq"
        digits="3"
        disable="Grayed out"
        />
      <label loc="9 60" size="98 21"
        style="Label1"
        label="Reference Freq"
        justify="Left"
        />
      <entry loc="121 18" size="56 21"
        style="PlainText"
        vq="lb"
        vc="lb=$VALUE"
        set="$VALUE=lb"
        show="on('lb'):$VALUE $VALUE=(2*$VALUE)-1"
        digits="3"
        disable="Grayed out"
        />
      <scroll loc="121 39" size="91 21"
        style="PlainText"
        label="value1"
        vq="fn"
        vc="fn=$VALUE"
        set="$VALUE=fn"
        show="on('fn'):$VALUE $VALUE=(2*$VALUE)-1"
        values="8k,16k,32k,64k"
        />
      <check loc="9 39" size="112 21"
        style="Label1"
        label="Fourier Number"
        vq="fn"
        vc="on('fn')"
        vc2="fn='n'"
        set="on('fn'):$VALUE"
        />
      <check loc="9 18" size="112 21"
        style="Label1"
        label="Line Broadening"
        vq="lb"
        vc="on('lb')"
        vc2="lb='n'"
        set="on('lb'):$VALUE"
        />
    </group>
    <group loc="580 140" size="200 110"
      style="Label1"
      label="LC Display"
      bg="transparent"
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Split"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="5 80" size="190 20"
        style="Label1"
        vq="lcDetector"
        show="$SHOW=(lcDetector='335')"
        label="Show 2D PDA Spectrum"
        vc="lccmd('pdagetimage', lcDataFile)"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="5 60" size="190 20"
        style="Label1"
        vq="lcDetector msSelection"
        label="Mass Spectrum Position"
        vc="substr(lcGraphSplit2,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit2,1,1,$str):lcGraphSplit2"
        show="$SHOW=(msSelection&lt;&gt;'0' and lcDetector='335')"
        bg="VJBackground"
        decor1="no"
        icon="rot_split.png"
        />
      <button loc="5 40" size="190 20"
        style="Label1"
        vq="lcDetector msSelection"
        show="$SHOW=(lcDetector='335' or msSelection='1200')"
        label="Chromatogram Position"
        vc="substr(lcGraphSplit,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit,1,1,$str):lcGraphSplit"
        bg="VJBackground"
        decor1="no"
        icon="rot_split.png"
        />
      <button loc="105 20" size="90 20"
        style="Label1"
        label="Hide LC"
        vc="lccmd('hideLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="5 20" size="90 20"
        style="Label1"
        label="Show LC"
        vc="lccmd('showLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
    </group>
    <group loc="610 5" size="130 80"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <button loc="7 42" size="119 35"
        style="Heading3"
        label="Resume Run"
        vc="lccmd('restartRun')"
        bg="0xCCFFCC"
        decor1="yes"
        />
      <button loc="7 7" size="119 35"
        style="Heading3"
        label="HOLD NOW"
        vc="lccmd('holdNow')"
        bg="0xFFCCCC"
        decor1="yes"
        />
    </group>
    <group loc="10 10" size="300 130"
      style="Label1"
      label="Peak Detection"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="PeakDetection_5"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 60" size="290 20"
        vq="lcTrace"
        show="$SHOW = (lcTrace[3] &lt;&gt; 'none')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <textmessage loc="20 0" size="105 20"
          style="Label1"
          label="1200 MS Out-2"
          vq="lcTrace"
          set="lcExtractLabel(lcTrace[3]):$VALUE"
          />
        <group loc="270 5" size="10 10"
          bg="GraphForeground3"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <entry loc="220 0" size="45 20"
          style="PlainText"
          vq="lcThreshold lcPeakDetect"
          vc="lccmd('setThresh 3 $VALUE')"
          set="$VALUE=lcThreshold[3]"
          show="$VALUE=(lcPeakDetect[3]='y')"
          digits="2"
          disable="Grayed out"
          />
        <label loc="155 0" size="65 20"
          style="Label1"
          label="Threshold "
          justify="Right"
          />
        <check loc="0 0" size="20 20"
          style="Label1"
          vq="lcPeakDetect"
          vc="lccmd('setPeakDet 3 y')"
          vc2="lccmd('setPeakDet 3 n')"
          set="$VALUE=(lcPeakDetect[3]='y')"
          />
      </group>
      <group loc="10 20" size="290 20"
        vq="lcTrace"
        show="$SHOW=(lcTrace[1] &lt;&gt; 'none')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <group loc="270 5" size="10 10"
          bg="GraphForeground"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <entry loc="220 0" size="45 20"
          style="PlainText"
          vq="lcThreshold lcPeakDetect"
          vc="lccmd('setThresh 1 $VALUE')"
          set="$VALUE=lcThreshold[1]"
          show="$VALUE=(lcPeakDetect[1]='y')"
          digits="2"
          disable="Grayed out"
          />
        <label loc="155 0" size="65 20"
          style="Label1"
          label="Threshold "
          justify="Right"
          />
        <check loc="0 0" size="20 20"
          style="Label1"
          vq="lcPeakDetect"
          vc="lccmd('setPeakDet 1 y')"
          vc2="lccmd('setPeakDet 1 n')"
          set="$VALUE=(lcPeakDetect[1]='y')"
          />
        <textmessage loc="20 0" size="105 20"
          style="Label1"
          label="PDA 335 Out-1"
          vq="lcTrace"
          set="lcExtractLabel(lcTrace[1]):$VALUE"
          />
      </group>
      <group loc="10 100" size="290 20"
        vq="lcTrace"
        show="$SHOW = (lcTrace[5] &lt;&gt; 'none')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <textmessage loc="20 0" size="105 20"
          style="Label1"
          vq="lcTrace"
          set="lcExtractLabel(lcTrace[5]):$VALUE"
          />
        <group loc="270 5" size="10 10"
          bg="GraphForeground5"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <entry loc="220 0" size="45 20"
          style="PlainText"
          vq="lcThreshold lcPeakDetect"
          vc="lccmd('setThresh 5 $VALUE')"
          set="$VALUE=lcThreshold[5]"
          show="$VALUE=(lcPeakDetect[5]='y')"
          digits="2"
          disable="Grayed out"
          />
        <label loc="155 0" size="65 20"
          style="Label1"
          label="Threshold "
          justify="Right"
          />
        <check loc="0 0" size="20 20"
          style="Label1"
          vq="lcPeakDetect"
          vc="lccmd('setPeakDet 5 y')"
          vc2="lccmd('setPeakDet 5 n')"
          set="$VALUE=(lcPeakDetect[5]='y')"
          />
      </group>
      <group loc="10 80" size="290 20"
        vq="lcTrace"
        show="$SHOW = (lcTrace[4] &lt;&gt; 'none')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <textmessage loc="20 0" size="105 20"
          style="Label1"
          vq="lcTrace"
          set="lcExtractLabel(lcTrace[4]):$VALUE"
          />
        <group loc="270 5" size="10 10"
          bg="GraphForeground4"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <entry loc="220 0" size="45 20"
          style="PlainText"
          vq="lcThreshold lcPeakDetect"
          vc="lccmd('setThresh 4 $VALUE')"
          set="$VALUE=lcThreshold[4]"
          show="$VALUE=(lcPeakDetect[4]='y')"
          digits="2"
          disable="Grayed out"
          />
        <label loc="155 0" size="65 20"
          style="Label1"
          label="Threshold "
          justify="Right"
          />
        <check loc="0 0" size="20 20"
          style="Label1"
          vq="lcPeakDetect"
          vc="lccmd('setPeakDet 4 y')"
          vc2="lccmd('setPeakDet 4 n')"
          set="$VALUE=(lcPeakDetect[4]='y')"
          />
      </group>
      <group loc="10 40" size="290 20"
        vq="lcTrace"
        show="$SHOW = (lcTrace[2] &lt;&gt; 'none')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <group loc="270 5" size="10 10"
          bg="GraphForeground2"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <entry loc="220 0" size="45 20"
          style="PlainText"
          vq="lcThreshold lcPeakDetect"
          vc="lccmd('setThresh 2 $VALUE')"
          set="$VALUE=lcThreshold[2]"
          show="$VALUE=(lcPeakDetect[2]='y')"
          digits="2"
          disable="Grayed out"
          />
        <label loc="155 0" size="65 20"
          style="Label1"
          label="Threshold "
          justify="Right"
          />
        <check loc="0 0" size="20 20"
          style="Label1"
          vq="lcPeakDetect"
          vc="lccmd('setPeakDet 2 y')"
          vc2="lccmd('setPeakDet 2 n')"
          set="$VALUE=(lcPeakDetect[2]='y')"
          />
        <textmessage loc="20 0" size="105 20"
          style="Label1"
          label="1200 MS Out-1"
          vq="lcTrace"
          set="lcExtractLabel(lcTrace[2]):$VALUE"
          />
      </group>
    </group>
  </group>
</template>
