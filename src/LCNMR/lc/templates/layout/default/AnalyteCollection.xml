<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="AnalyteCollection" element="pages" type="preparation" >
  <group size="850 260"
    style="Heading3"
    label="Analyte Collection"
    vq="lcexp lcAnalyteCollector"
    show="$SHOW = (lcexp = 'analyte' and lcAnalyteCollector &lt;&gt; 0)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="AnalyteCollection"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="650 130" size="200 110"
      style="Heading3"
      label="LC Display"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Split"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="5 80" size="190 20"
        style="Label1"
        vq="lcDetector"
        show="$SHOW=(lcDetector='335')"
        label="Show 2D PDA Spectrum"
        vc="lccmd('pdagetimage', lcDataFile)"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="5 60" size="190 20"
        style="Label1"
        vq="lcDetector msSelection"
        label="Mass Spectrum Position"
        vc="substr(lcGraphSplit2,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit2,1,1,$str):lcGraphSplit2"
        show="$SHOW=(msSelection&lt;&gt;'0' and lcDetector='335')"
        bg="VJBackground"
        decor1="no"
        icon="rot_split.png"
        />
      <button loc="5 40" size="190 20"
        style="Label1"
        vq="lcDetector msSelection"
        show="$SHOW=(lcDetector='335' or msSelection='1200')"
        label="Chromatogram Position"
        vc="substr(lcGraphSplit,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit,1,1,$str):lcGraphSplit"
        bg="VJBackground"
        decor1="no"
        icon="rot_split.png"
        />
      <button loc="105 20" size="90 20"
        style="Label1"
        label="Hide LC"
        vc="lccmd('hideLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="5 20" size="90 20"
        style="Label1"
        label="Show LC"
        vc="lccmd('showLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
    </group>
    <button loc="680 50" size="130 30"
      style="Label1"
      label="Edit Current Method"
      vc="lccmd('editMethod', lcMethodFile)"
      bg="0xB6CCE3"
      decor1="yes"
      />
    <textmessage loc="10 220" size="460 20"
      style="PlainText"
      label="/usr25/chrisp/vnmrsys/data/bogus011"
      vq="svfdir lcid"
      set="filepath(svfdir, lcid):$VALUE"
      />
    <label loc="10 200" size="100 20"
      style="Label1"
      label="Data Directory"
      justify="Left"
      />
    <group loc="10 0" size="480 190"
      label="analyte collection"
      vq="lcexp"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Analytecollection"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <button loc="320 50" size="120 30"
        style="Heading3"
        label="STEP NOW"
        vc="lccmd('collectLoop event') lccmd('collectLoop')"
        bg="VJBackground"
        decor1="yes"
        />
      <group loc="0 0" size="300 130"
        style="Label1"
        label="Peak Detection"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="PeakDetection"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <group loc="10 60" size="290 20"
          vq="lcTrace"
          show="$SHOW = (lcTrace[3] &lt;&gt; 'none')"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <textmessage loc="20 0" size="105 20"
            style="Label1"
            vq="lcTrace"
            set="lcExtractLabel(lcTrace[3]):$VALUE"
            />
          <group loc="270 5" size="10 10"
            bg="GraphForeground3"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
          </group>
          <entry loc="220 0" size="45 20"
            style="PlainText"
            vq="lcThreshold lcPeakDetect"
            vc="lccmd('setThresh 3 $VALUE')"
            set="$VALUE=lcThreshold[3]"
            show="$VALUE=(lcPeakDetect[3]='y')"
            digits="2"
            disable="Grayed out"
            />
          <label loc="155 0" size="65 20"
            style="Label1"
            label="Threshold"
            justify="Right"
            />
          <check loc="0 0" size="20 20"
            style="Label1"
            vq="lcPeakDetect"
            vc="lccmd('setPeakDet 3 y')"
            vc2="lccmd('setPeakDet 3 n')"
            set="$VALUE=(lcPeakDetect[3]='y')"
            />
        </group>
        <group loc="10 20" size="290 20"
          vq="lcTrace"
          show="$SHOW=(lcTrace[1] &lt;&gt; 'none')"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <group loc="270 5" size="10 10"
            bg="GraphForeground"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
          </group>
          <entry loc="220 0" size="45 20"
            style="PlainText"
            vq="lcThreshold lcPeakDetect"
            vc="lccmd('setThresh 1 $VALUE')"
            set="$VALUE=lcThreshold[1]"
            show="$VALUE=(lcPeakDetect[1]='y')"
            digits="2"
            disable="Grayed out"
            />
          <label loc="155 0" size="65 20"
            style="Label1"
            label="Threshold"
            justify="Right"
            />
          <check loc="0 0" size="20 20"
            style="Label1"
            vq="lcPeakDetect"
            vc="lccmd('setPeakDet 1 y')"
            vc2="lccmd('setPeakDet 1 n')"
            set="$VALUE=(lcPeakDetect[1]='y')"
            />
          <textmessage loc="20 0" size="105 20"
            style="Label1"
            label="310/9050 UV-Vis"
            vq="lcTrace"
            set="lcExtractLabel(lcTrace[1]):$VALUE"
            />
        </group>
        <group loc="10 100" size="290 20"
          vq="lcTrace"
          show="$SHOW = (lcTrace[5] &lt;&gt; 'none')"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <textmessage loc="20 0" size="105 20"
            style="Label1"
            vq="lcTrace"
            set="lcExtractLabel(lcTrace[5]):$VALUE"
            />
          <group loc="270 5" size="10 10"
            bg="GraphForeground5"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
          </group>
          <entry loc="220 0" size="45 20"
            style="PlainText"
            vq="lcThreshold lcPeakDetect"
            vc="lccmd('setThresh 5 $VALUE')"
            set="$VALUE=lcThreshold[5]"
            show="$VALUE=(lcPeakDetect[5]='y')"
            digits="2"
            disable="Grayed out"
            />
          <label loc="155 0" size="65 20"
            style="Label1"
            label="Threshold"
            justify="Right"
            />
          <check loc="0 0" size="20 20"
            style="Label1"
            vq="lcPeakDetect"
            vc="lccmd('setPeakDet 5 y')"
            vc2="lccmd('setPeakDet 5 n')"
            set="$VALUE=(lcPeakDetect[5]='y')"
            />
        </group>
        <group loc="10 80" size="290 20"
          vq="lcTrace"
          show="$SHOW = (lcTrace[4] &lt;&gt; 'none')"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <textmessage loc="20 0" size="105 20"
            style="Label1"
            vq="lcTrace"
            set="lcExtractLabel(lcTrace[4]):$VALUE"
            />
          <group loc="270 5" size="10 10"
            bg="GraphForeground4"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
          </group>
          <entry loc="220 0" size="45 20"
            style="PlainText"
            vq="lcThreshold lcPeakDetect"
            vc="lccmd('setThresh 4 $VALUE')"
            set="$VALUE=lcThreshold[4]"
            show="$VALUE=(lcPeakDetect[4]='y')"
            digits="2"
            disable="Grayed out"
            />
          <label loc="155 0" size="65 20"
            style="Label1"
            label="Threshold"
            justify="Right"
            />
          <check loc="0 0" size="20 20"
            style="Label1"
            vq="lcPeakDetect"
            vc="lccmd('setPeakDet 4 y')"
            vc2="lccmd('setPeakDet 4 n')"
            set="$VALUE=(lcPeakDetect[4]='y')"
            />
        </group>
        <group loc="10 40" size="290 20"
          vq="lcTrace"
          show="$SHOW = (lcTrace[2] &lt;&gt; 'none')"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <group loc="270 5" size="10 10"
            bg="GraphForeground2"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
          </group>
          <entry loc="220 0" size="45 20"
            style="PlainText"
            vq="lcThreshold lcPeakDetect"
            vc="lccmd('setThresh 2 $VALUE')"
            set="$VALUE=lcThreshold[2]"
            show="$VALUE=(lcPeakDetect[2]='y')"
            digits="2"
            disable="Grayed out"
            />
          <label loc="155 0" size="65 20"
            style="Label1"
            label="Threshold"
            justify="Right"
            />
          <check loc="0 0" size="20 20"
            style="Label1"
            vq="lcPeakDetect"
            vc="lccmd('setPeakDet 2 y')"
            vc2="lccmd('setPeakDet 2 n')"
            set="$VALUE=(lcPeakDetect[2]='y')"
            />
          <textmessage loc="20 0" size="105 20"
            style="Label1"
            label="1200 MS Out-1"
            vq="lcTrace"
            set="lcExtractLabel(lcTrace[2]):$VALUE"
            />
        </group>
      </group>
      <group loc="0 140" size="450 50"
        vq="lcexp"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="CurrentLoop#"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <button loc="320 0" size="120 25"
          style="Label1"
          label="Initialize Loop"
          vc="lccmd('gotoLoop', startloop)"
          bg="VJBackground"
          decor1="yes"
          tooltip="Set AC to the first loop"
          />
        <label loc="155 25" size="115 20"
          style="Heading3"
          label="Current Loop #"
          justify="Left"
          />
        <textmessage loc="275 25" size="30 20"
          style="Heading2"
          label="0"
          vq="lcLoopIndex"
          set="$VALUE=lcLoopIndex"
          />
        <label loc="155 0" size="115 20"
          style="Heading3"
          label="Start at Loop #"
          justify="Left"
          />
        <entry loc="270 0" size="35 20"
          style="Heading2"
          vq="startloop"
          vc="startloop=$VALUE lccmd('gotoLoop $VALUE')"
          set="$VALUE=startloop"
          disable="Grayed out"
          />
        <group loc="0 0" size="150 50"
          vq="startloop lcexp"
          show="if startloop&lt;&gt;lcBypassLoop or lcexp='lcalone' then $SHOW=-1 else $SHOW=1 endif"
          bg="ErrorBkg"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <label loc="0 20" size="130 20"
            style="Error"
            label="is the bypass loop"
            justify="Center"
            />
          <textmessage loc="120 0" size="30 20"
            style="Error"
            vq="lcBypassLoop"
            set="$VALUE=lcBypassLoop"
            />
          <label loc="0 0" size="120 20"
            style="Error"
            label="WARNING: Loop #"
            justify="Left"
            />
        </group>
      </group>
    </group>
  </group>
</template>
