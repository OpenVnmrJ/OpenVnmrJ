<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="BasicLcSetup" element="pages" type="preparation" >
  <group size="845 290"
    style="PlainText"
    label="LC Setup"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="BasicLcSetup"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="210 0" size="300 180"
      style="Heading3"
      label="LC Pump"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="LCPump"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <textmessage loc="5 20" size="140 20"
        style="Heading3"
        statpar="pumpId"
        />
      <label loc="5 120" size="165 20"
        style="Label1"
        label="Solvent Concentrations:"
        />
      <button loc="215 80" size="80 20"
        style="Label2"
        label="Stop"
        vc="lccmd('gpib ' + lcPump + ' stop')"
        bg="VJBackground"
        decor1="yes"
        />
      <button loc="215 60" size="80 20"
        style="Label1"
        label="Start"
        vc="lccmd('gpib ' + lcPump + ' start')"
        bg="VJBackground"
        decor1="yes"
        />
      <button loc="215 40" size="80 20"
        style="Label1"
        label="Download"
        vc="lcdownloadMethod('pump')"
        bg="VJBackground"
        decor1="yes"
        />
      <button loc="215 15" size="80 20"
        style="Label1"
        label="Reset"
        vc="lccmd('gpib ' + lcPump + ' reset')"
        bg="VJBackground"
        decor1="yes"
        />
      <textmessage loc="100 140" size="50 20"
        style="OutputTextPar"
        statpar="pumpPctB"
        />
      <textmessage loc="200 140" size="50 20"
        style="OutputTextPar"
        statpar="pumpPctC"
        />
      <textmessage loc="5 160" size="95 20"
        style="Label1"
        label="D2O"
        vq="lcConfigSolvA"
        set="translate(lcConfigSolvA, $VJFILE:'INTERFACE/LcInvSolvents.txt'):$VALUE"
        />
      <textmessage loc="5 140" size="50 20"
        style="OutputTextPar"
        statpar="pumpPctA"
        />
      <label loc="5 100" size="65 20"
        style="Label1"
        label="Pressure"
        justify="Left"
        />
      <label loc="5 80" size="65 20"
        style="Label1"
        label="Flow"
        justify="Left"
        />
      <label loc="5 60" size="65 20"
        style="Label1"
        label="Status"
        justify="Left"
        />
      <label loc="5 40" size="65 20"
        style="Label1"
        label="Pump"
        justify="Left"
        />
      <textmessage loc="200 160" size="100 20"
        style="Label1"
        label="ACN"
        vq="lcConfigSolvC"
        set="translate(lcConfigSolvC, $VJFILE:'INTERFACE/LcInvSolvents.txt'):$VALUE"
        />
      <textmessage loc="100 160" size="100 20"
        style="Label1"
        label="H2O"
        vq="lcConfigSolvB"
        set="translate(lcConfigSolvB, $VJFILE:'INTERFACE/LcInvSolvents.txt'):$VALUE"
        />
      <button loc="110 40" size="40 20"
        style="Label1"
        label="Off"
        vc="lccmd('gpib ' + lcPump + ' stop')"
        bg="VJBackground"
        statpar="pumpStatus"
        statshow="Ready"
        decor1="yes"
        />
      <button loc="70 40" size="40 20"
        style="Label1"
        label="On"
        vc="lccmd('gpib ' + lcPump + ' pump')"
        bg="VJBackground"
        statpar="pumpStatus"
        statshow="Pump_Off"
        decor1="yes"
        />
      <textmessage loc="70 60" size="120 20"
        style="OutputTextPar"
        statpar="pumpStatus"
        />
      <textmessage loc="70 80" size="120 20"
        style="OutputTextPar"
        statpar="pumpFlow"
        />
      <textmessage loc="70 100" size="120 20"
        style="OutputTextPar"
        statpar="pumpPress"
        />
    </group>
    <group loc="515 180" size="315 110"
      style="Heading3"
      label="LC Method"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <cascadingMenu loc="10 30" size="290 20"
        style="Label1"
        label="Select LC method to use"
        vq="lcMethodFile"
        vc="lccmd('loadMethod', '\&quot;$VALUE\&quot;')"
        set="$VALUE=lcMethodFile"
        file="lc/lcmethods"
        bg="VJBackground"
        statshow="no"
        />
      <button loc="160 70" size="140 30"
        style="Label1"
        label="Create New Method"
        vc="lccmd('editMethod', 'NewMethod')"
        bg="0xB6CCE3"
        decor1="no"
        />
      <button loc="10 70" size="140 30"
        style="Label1"
        label="Edit Current Method"
        vc="lccmd('editMethod', lcMethodFile)"
        bg="0xB6CCE3"
        decor1="no"
        />
    </group>
    <group loc="515 0" size="315 180"
      style="Heading3"
      label="UV Detector"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="UVDetector"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <textmessage loc="5 20" size="135 20"
        style="Heading3"
        statpar="uvId"
        />
      <group loc="5 80" size="220 20"
        show="$VALUE=1"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="LampStatus:"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Lamp Status"
          justify="Left"
          />
        <textmessage loc="100 0" size="100 20"
          style="OutputTextPar"
          statpar="uvLampStatus"
          />
      </group>
      <group loc="5 60" size="220 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="100 20"
          style="Label1"
          label="Detector Status"
          justify="Left"
          />
        <textmessage loc="99 0" size="115 20"
          style="OutputTextPar"
          statpar="uvStatus"
          />
      </group>
      <button loc="230 80" size="80 20"
        style="Label2"
        label="Stop"
        vc="lccmd('pdaStop')"
        bg="VJBackground"
        statpar="uvStopOk"
        statshow="true"
        decor1="yes"
        />
      <button loc="230 60" size="80 20"
        style="Label1"
        label="Start"
        vc="lccmd('pdaStart')"
        bg="VJBackground"
        statpar="uvStartOk"
        statshow="true"
        decor1="yes"
        />
      <button loc="230 40" size="80 20"
        style="Label1"
        label="Download"
        vc="lcdownloadMethod('uv')"
        bg="VJBackground"
        statpar="uvDownloadOk"
        statshow="true"
        decor1="yes"
        />
      <button loc="230 15" size="80 20"
        style="Label1"
        label="Reset"
        vc="lccmd('pdaReset')"
        bg="VJBackground"
        statpar="uvResetOk"
        statshow="true"
        decor1="yes"
        />
      <button loc="160 15" size="70 20"
        style="Label1"
        label="AutoZero"
        vc="lccmd('pdaAutozero')"
        bg="VJBackground"
        statpar="uvAutoZeroOk"
        statshow="true"
        decor1="yes"
        />
      <group loc="0 135" size="310 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="5 0" size="80 20"
          style="Label1"
          label="Absorption:"
          justify="Left"
          />
        <textmessage loc="210 0" size="90 20"
          style="OutputTextPar"
          statpar="uvLambda"
          />
        <textmessage loc="100 0" size="80 20"
          style="OutputTextPar"
          statpar="uvAttn"
          />
        <label loc="180 0" size="30 20"
          style="Label1"
          label="at"
          justify="Right"
          />
      </group>
      <group loc="20 155" size="290 20"
        vq="lcDetector"
        show="$SHOW=(lcDetector='335')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="190 0" size="90 20"
          style="OutputTextPar"
          statpar="uvLambda2"
          />
        <label loc="160 0" size="30 20"
          style="Label1"
          label="at"
          justify="Right"
          />
        <textmessage loc="80 0" size="80 20"
          style="OutputTextPar"
          statpar="uvAttn2"
          />
      </group>
      <group loc="5 40" size="220 20"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="50 20"
          style="Label1"
          label="Lamp"
          justify="Left"
          />
        <button loc="50 0" size="40 20"
          style="Label1"
          label="On"
          vc="lccmd('pdaLampOn')"
          bg="VJBackground"
          statpar="uvLampOnOk"
          statshow="true"
          decor1="yes"
          />
        <button loc="90 0" size="40 20"
          style="Label1"
          label="Off"
          vc="lccmd('pdaLampOff')"
          bg="VJBackground"
          statpar="uvLampOffOk"
          statshow="true"
          decor1="yes"
          />
      </group>
    </group>
    <group loc="5 0" size="200 180"
      style="Heading3"
      label="Solvent Flow Path"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventFlowPath"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 80" size="195 95"
        vq="lcAnalyteCollector"
        show="$SHOW=lcAnalyteCollector"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <button loc="130 60" size="20 20"
          style="Label1"
          vq="startloop"
          vc="lccmd('step')"
          bg="VJBackground"
          decor1="yes"
          icon="up11x17"
          />
        <textmessage loc="100 60" size="30 20"
          style="OutputTextPar"
          vq="lcLoopIndex"
          set="$VALUE=lcLoopIndex"
          />
        <label loc="0 60" size="100 20"
          style="Label1"
          label="Current Loop #"
          justify="Left"
          />
        <group loc="0 0" size="180 50"
          style="Label1"
          label="Column Flow"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <radio loc="90 20" size="80 20"
            style="Label1"
            label="Bypass"
            vq="lcColumnValve"
            vc="lccmd('toCollector')"
            vc2="lccmd('toColumn')"
            set="$VALUE=(lcColumnValve=0)"
            />
          <radio loc="10 20" size="80 20"
            style="Label1"
            label="Column"
            vq="lcColumnValve"
            vc="lccmd('toColumn')"
            vc2="lccmd('toCollector')"
            set="$VALUE=lcColumnValve"
            />
          <label loc="0 0" size="110 20"
            style="Label1"
            label="Column Flow:"
            />
        </group>
      </group>
      <group loc="5 25" size="180 50"
        style="Label1"
        label="NMR Flow"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="0 0" size="110 20"
          style="Label1"
          label="NMR Flow:"
          />
        <radio loc="90 20" size="80 20"
          style="Label1"
          label="To Waste"
          vq="lcNmrValve"
          vc="lccmd('toWaste')"
          vc2="lccmd('toNmr')"
          set="$VALUE=(lcNmrValve=0)"
          />
        <radio loc="10 20" size="80 20"
          style="Label1"
          label="To NMR"
          vq="lcNmrValve"
          vc="lccmd('toNmr')"
          vc2="lccmd('toWaste')"
          set="$VALUE=lcNmrValve"
          />
      </group>
    </group>
    <group loc="5 180" size="200 110"
      style="Heading3"
      label="LC Display"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Split"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="5 80" size="190 20"
        style="Label1"
        vq="lcDetector"
        show="$SHOW=(lcDetector='335')"
        label="Show 2D PDA Spectrum"
        vc="lccmd('pdagetimage', lcDataFile)"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="5 60" size="190 20"
        style="Label1"
        vq="lcDetector msSelection"
        label="Mass Spectrum Position"
        vc="substr(lcGraphSplit2,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit2,1,1,$str):lcGraphSplit2"
        show="$SHOW=(msSelection&lt;&gt;'0' and lcDetector='335')"
        bg="VJBackground"
        decor1="no"
        icon="rot_split.png"
        />
      <button loc="5 40" size="190 20"
        style="Label1"
        vq="lcDetector msSelection"
        show="$SHOW=(lcDetector='335' or msSelection='1200')"
        label="Chromatogram Position"
        vc="substr(lcGraphSplit,1,1):$str if ($str='v') then $str='h' else $str='v' endif substr(lcGraphSplit,1,1,$str):lcGraphSplit"
        bg="VJBackground"
        decor1="no"
        icon="rot_split.png"
        />
      <button loc="105 20" size="90 20"
        style="Label1"
        label="Hide LC"
        vc="lccmd('hideLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="5 20" size="90 20"
        style="Label1"
        label="Show LC"
        vc="lccmd('showLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
    </group>
    <group loc="210 180" size="300 110"
      style="Heading3"
      label="Mass Spectrometer"
      vq="msSelection"
      show="$SHOW = (msSelection = '1200')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MakeupPump"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="70 30" size="70 20"
        style="Label1"
        label="Current"
        vc="lccmd('updateMsGraph 500') msShowOldData='n' lccmd('showLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="140 30" size="70 20"
        style="Label1"
        label="Historical"
        vc="lccmd('updateMsGraph') msShowOldData='y' lccmd('showLcGraph')"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="210 30" size="60 20"
        style="Label1"
        label="Off"
        vc="lccmd('updateMsGraph') msShowOldData='n'"
        bg="VJBackground"
        decor1="no"
        />
      <check loc="70 60" size="120 20"
        style="Label1"
        label="Status Updating"
        vq="msStatusUpdates"
        vc="msStatusUpdates = 'y' lccmd('msStatus','on')"
        vc2="msStatusUpdates = 'n' lccmd('msStatus','off')"
        set="$VALUE = (msStatusUpdates = 'y')"
        />
      <label loc="10 30" size="60 20"
        style="Label1"
        label="Display:"
        justify="Left"
        />
    </group>
  </group>
</template>
