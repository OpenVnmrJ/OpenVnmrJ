<jsmoothproject>
<skeletonName>Console Wrapper</skeletonName>
<javaProperties>
<name>userdir</name>
<value>%HOMEDRIVE%\%HOMEPATH%\vnmrsys</value>
</javaProperties>
<javaProperties>
<name>sysdir</name>
<value>%vnmrsystem_win%</value>
</javaProperties>
<bundledJVMPath>..\jre</bundledJVMPath>
<executableName>protune.exe</executableName>
<embeddedJar>true</embeddedJar>
<jarLocation>apt.jar</jarLocation>
<maximumMemoryHeap>-1</maximumMemoryHeap>
<skeletonProperties>
<key>Message</key>
<value>This program needs Java to run.
Please download it at http://www.java.com</value>
</skeletonProperties>
<skeletonProperties>
<key>PressKey</key>
<value>0</value>
</skeletonProperties>
<skeletonProperties>
<key>Debug</key>
<value>1</value>
</skeletonProperties>
<minimumVersion>1.4.2</minimumVersion>
<maximumVersion></maximumVersion>
<initialMemoryHeap>-1</initialMemoryHeap>
<mainClassName>vnmr.apt.ProbeTune</mainClassName>
<JVMSearchPath>registry</JVMSearchPath>
<JVMSearchPath>javahome</JVMSearchPath>
<JVMSearchPath>jrepath</JVMSearchPath>
<JVMSearchPath>jdkpath</JVMSearchPath>
<JVMSearchPath>exepath</JVMSearchPath>
<JVMSearchPath>jview</JVMSearchPath>
</jsmoothproject>
