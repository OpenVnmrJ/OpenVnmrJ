        style="Label1"
        label="Delay"
        justify="Center"
        />
      <label loc="85 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Center"
        />
      <entry loc="55 40" size="30 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
