        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="10 65" size="165 20"
        style="Label1"
        label="Optimize transfer delay for:"
        />
      <menu loc="175 65" size="80 20"
        style="PlainText"
        vq="xhn"
        vc="xhn=$VALUE"
        set="$VALUE=xhn"
        editable="No"
        >
        <mlabel 
          label="XH"
          chval="1"
          />
        <mlabel 
          label="XH2/All"
          chval="2"
          />
        <mlabel 
          label="XH3"
          chval="3"
          />
      </menu>
      <entry loc="205 45" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        digits="1"
        />
      <entry loc="220 20" size="35 20"
        style="PlainText"
        vq="pwxlvl"
        vc="pwxlvl=$VALUE"
        set="$VALUE=pwxlvl"
        digits="0"
        />
      <entry loc="110 20" size="40 20"
        style="PlainText"
        vq="pwx"
        vc="pwx=$VALUE"
        set="$VALUE=pwx"
        digits="2"
        />
