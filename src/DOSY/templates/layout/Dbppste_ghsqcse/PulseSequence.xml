        style="Label1"
        label="Ratio"
        justify="Left"
        />
      <label loc="35 150" size="40 20"
        style="Label1"
        label="Level"
        justify="Left"
        />
      <entry loc="75 150" size="55 20"
        style="PlainText"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
