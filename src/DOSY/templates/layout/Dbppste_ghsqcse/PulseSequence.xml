<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="780 280"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <button loc="655 240" size="110 25"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      bg=""
      decor1=""
      />
    <button loc="545 240" size="105 25"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      bg=""
      decor1=""
      icon="Clear Subexperiment"
      />
    <group loc="255 200" size="285 65"
      style="Heading1"
      label="Probe"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="235 40"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="5 20" size="80 20"
          style="Heading1"
          label="Probe used:"
          justify="Left"
          />
        <textmessage loc="85 20" size="145 20"
          style="PlainText"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="125 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
