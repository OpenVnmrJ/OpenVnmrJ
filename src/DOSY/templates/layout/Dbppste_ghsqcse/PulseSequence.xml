          style="PlainText"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="255 0" size="285 200"
      style="Heading1"
      label="Sequence Parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="145 150" size="60 20"
        style="Heading1"
        label="Duration"
        justify="Left"
        />
