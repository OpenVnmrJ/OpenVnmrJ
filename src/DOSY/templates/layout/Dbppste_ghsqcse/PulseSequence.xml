        style="Label1"
        label="Start from 1/2 dwell time"
        vq="f1180"
        vc="f1180='y'"
        vc2="f1180='n'"
        set="strstr(f1180,'y'):$VALUE"
        />
      <check loc="10 85" size="140 20"
        style="Label1"
        label="Multiplicity editing"
        vq="edit"
        vc="edit='y'"
        vc2="edit='n'"
        set="strstr(edit,'y'):$VALUE"
        />
      <label loc="260 45" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="10 45" size="175 20"
        style="Label1"
        label="One-bond XH coupling"
        />
      <label loc="260 20" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="180 20" size="45 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="90 deg. X pulse"
        />
    </group>
    <group loc="545 150" size="220 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="195 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <group loc="545 65" size="220 85"
      style="Heading1"
      label="Solvent presaturation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
