        style="Label1"
        label="Frequency"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="satfrq"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        />
      <entry loc="150 40" size="40 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="2"
        />
