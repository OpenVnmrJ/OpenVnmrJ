          vq="convcomp"
          vc="convcomp='y' dps"
          vc2="convcomp='n' dps"
          set="$VALUE=(convcomp='y' )"
          />
        <label loc="10 40" size="155 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="10 20" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
