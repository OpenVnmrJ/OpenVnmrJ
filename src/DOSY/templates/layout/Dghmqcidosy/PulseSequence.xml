        style="PlainText"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="145 100" size="60 20"
        style="Label1"
        label="Duration"
        justify="Left"
        />
      <entry loc="65 120" size="55 20"
        style="PlainText"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="3"
        disable="Grayed out"
        />
