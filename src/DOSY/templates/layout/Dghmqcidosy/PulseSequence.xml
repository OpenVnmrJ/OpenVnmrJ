<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="770 305"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="550 195" size="205 95"
      style="Heading1"
      label="Steady State"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="175 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="100 60" size="70 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="100 40" size="70 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y' dps"
        vc2="sspul='n' dps"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
    </group>
    <group loc="550 95" size="205 100"
      style="Heading1"
      label="Solvent presaturation"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <menu loc="100 25" size="90 20"
        style="PlainText"
        vq="satmode"
        vc="satmode='$VALUE'"
        set="$VALUE=satmode"
        editable="No"
        >
        <mlabel 
          label="never"
          chval="nn"
          />
        <mlabel 
          label="d1 only"
          chval="yn"
          />
        <mlabel 
          label="del only"
          chval="ny"
          />
        <mlabel 
          label="both"
          chval="yy"
          />
      </menu>
      <label loc="175 65" size="25 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <label loc="175 45" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="100 65" size="70 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="3"
        />
      <entry loc="100 45" size="70 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
