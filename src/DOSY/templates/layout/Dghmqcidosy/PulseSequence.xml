        justify="Left"
        />
      <label loc="10 80" size="180 20"
        style="Label1"
        label="Encoding/Decoding gradient:"
        justify="Left"
        />
      <label loc="155 20" size="20 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <label loc="260 20" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="175 20" size="45 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="220 20" size="35 20"
        style="PlainText"
        vq="pwxlvl"
        vc="pwxlvl=$VALUE"
        set="$VALUE=pwxlvl"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 20" size="40 20"
        style="PlainText"
        vq="pwx"
        vc="pwx=$VALUE"
        set="$VALUE=pwx"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="90 deg. X pulse"
        justify="Left"
        />
    </group>
    <group loc="550 20" size="205 75"
      style="Heading1"
      label="DOSY Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 25" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <check loc="10 45" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
    </group>
    <button loc="410 260" size="125 30"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      halignment="Center"
      valignment="Center"
      />
    <button loc="270 260" size="125 30"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      icon="Clear Subexperiment"
      halignment="Center"
      valignment="Center"
      />
    <group loc="5 165" size="250 135"
