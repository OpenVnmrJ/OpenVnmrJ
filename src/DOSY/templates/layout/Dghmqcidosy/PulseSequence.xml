        <entry loc="165 20" size="55 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
      </group>
    </group>
    <textmessage loc="10 0" size="220 20"
      style="PlainText"
      label="Dghmqcidosy"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
