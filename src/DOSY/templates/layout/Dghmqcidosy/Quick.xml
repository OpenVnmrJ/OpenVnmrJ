<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="775 260"
    style="Heading1"
    label="Defaults"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="300 35" size="250 205"
      style="Heading1"
      label="DOSY Setup"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 20" size="240 180"
        style="Heading1"
        label="DOSY Setup"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYSetup"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="230 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max)  vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
        <button loc="0 90" size="230 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
        <label loc="215 155" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 135" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="10 155" size="145 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <entry loc="160 155" size="50 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1000"
          set="$VALUE=del*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="160 115" size="50 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
        <entry loc="160 135" size="50 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="160 65" size="50 20"
          style="PlainText"
          vq="gzlvl1_max kappa"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <entry loc="160 45" size="50 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="160 25" size="50 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <label loc="10 135" size="150 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="10 115" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
        <label loc="10 25" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <label loc="10 45" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="10 65" size="140 20"
          style="Label1"
          label="Highest gradient value"
          />
      </group>
    </group>
    <group loc="5 35" size="290 205"
      style="Heading1"
      label="Acquisition Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="0 25" size="290 155"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="gHSQCacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <label loc="10 100" size="165 20"
          style="Label1"
          label="Long-range coupling (jnxh)"
          justify="Left"
          />
        <group loc="10 5" size="255 30"
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="F1Nucleus:"
          useref="no"
          subtype="Titled"
          expanded="yes"
          rows="1"
          columns="1"
          >
          <menu loc="90 5" size="95 20"
            style="PlainText"
            label="menu"
            vq="dn"
            vc="$dn='$VALUE' if ($dn = 'other') then dn='P31' else dn=$dn endif setdecpars('update') reff1:$ret"
            set="$VALUE=dn"
            editable="No"
            >
            <mlabel 
              label="C13"
              chval="C13"
              />
            <mlabel 
              label="N15"
              chval="N15"
              />
            <mlabel 
              label="Other"
              chval="other"
              />
          </menu>
          <group loc="205 0" size="50 25"
            vq="dn"
            show="if (dn='C13') or (dn='N15') then $SHOW=0 else $SHOW=1 endif"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            rows="1"
            columns="1"
            >
            <entry loc="0 5" size="45 20"
              style="PlainText"
              vq="dn"
              vc="dn='$VALUE' setdecpars('update') reff1:$ret"
              set="$VALUE=dn"
              disable="Grayed out"
              />
          </group>
          <label loc="5 5" size="75 20"
            style="Label1"
            label="F1 Nucleus"
            justify="Left"
            />
        </group>
        <label loc="265 100" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <entry loc="170 100" size="90 20"
          style="PlainText"
          vq="jnxh"
          vc="jnxh=$VALUE"
          set="$VALUE=jnxh"
          digits="1"
          disable="Grayed out"
          />
        <menu loc="170 80" size="90 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1 setF1wt"
          set="$VALUE=ni"
          editable="No"
          >
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <menu loc="170 60" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <group loc="150 40" size="120 20"
          vq="dn"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <entry loc="65 0" size="45 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$VALUE,$f2)"
            set="invsetsw1:$f1,$f2 $VALUE=$f1"
            digits="0"
            disable="Grayed out"
            />
          <label loc="45 0" size="20 20"
            style="Label1"
            label="to"
            justify="Center"
            />
          <entry loc="0 0" size="45 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$f1,$VALUE)"
            set="invsetsw1:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
        </group>
        <label loc="10 40" size="110 20"
          style="Label1"
          label="F1 Spectral Width"
          justify="Left"
          />
        <group loc="10 125" size="120 20"
          vq="dn"
          show="$SHOW=(dn='N15')"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          rows="1"
          columns="1"
          >
          <textmessage loc="5 0" size="110 20"
            style="PlainText"
            label="[CH3NO2 (0 ppm)]"
            vq="dn"
            set="$ref='' N15ref:$ref if ($ref='') then $ref='CH3NO2' endif $VALUE='['+$ref+' (0 ppm)]'"
            />
        </group>
        <label loc="10 80" size="110 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="10 60" size="140 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 5" size="770 25"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="445 0" size="240 25"
        style="PlainText"
        label="Observe: H1       H1-C13 Correlation"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
      <textmessage loc="10 0" size="435 25"
        style="PlainText"
        label="Experiment: Dghmqcidosy   Solvent:"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
    </group>
    <group loc="555 35" size="220 150"
      vq="customflag"
      show="$SHOW=(customflag='y')"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="customizepar2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="10 105" size="200 25"
        style="PlainText"
        label=" "
        vq="samplename"
        set="$VALUE=samplename"
        />
      <textmessage loc="5 25" size="130 20"
        style="PlainText"
        label="After Dghmqcidosy acq:"
        vq="pslabel"
        set="$VALUE='After '+pslabel+' acq:'"
        />
      <textmessage loc="5 5" size="130 20"
        style="PlainText"
        label="Before Dghmqcidosy acq:"
        vq="pslabel"
        set="$VALUE='Before '+pslabel+' acq:'"
        />
      <check loc="140 25" size="75 20"
        style="Label1"
        label="Autoplot"
        vq="pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <check loc="140 5" size="75 20"
        style="Label1"
        label="Re-shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
        />
      <textmessage loc="140 125" size="65 20"
        style="PlainText"
        label="---"
        vq="SQalock SQwshim"
        set="$VALUE=SQalock+' / '+SQwshim"
        />
      <label loc="5 85" size="125 20"
        style="Label1"
        label="Samplename:"
        />
      <label loc="5 125" size="130 20"
        style="Label1"
        label="start of Q lock/shim?"
        />
      <label loc="5 45" size="85 20"
        style="Label1"
        label="Starting with:"
        />
      <textmessage loc="10 65" size="200 20"
        style="PlainText"
        label="wet1D PRESAT PROTON"
        vq="strtpar REQexp chreqexp"
        set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
        />
    </group>
  </group>
</template>
