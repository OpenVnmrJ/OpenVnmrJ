<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="775 270"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <textmessage loc="10 0" size="220 25"
      style="PlainText"
      label="Dbppste_wg"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
    <group loc="530 85" size="220 90"
      style="Heading1"
      label="Solvent presaturation"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="10 60" size="45 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
      <label loc="190 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Center"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="2"
        />
      <label loc="195 60" size="25 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <menu loc="105 20" size="85 20"
        style="PlainText"
        vq="satmode"
        vc="satmode='$VALUE'"
        set="$VALUE=satmode"
        editable="No"
        >
        <mlabel 
          label="never"
          chval="nn"
          />
        <mlabel 
          label="d1 only"
          chval="yn"
          />
        <mlabel 
          label="del only"
          chval="ny"
          />
        <mlabel 
          label="both"
          chval="yy"
          />
      </menu>
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 20" size="90 20"
        style="Label1"
        label="Do saturation:"
        />
    </group>
    <button loc="410 215" size="110 30"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="285 215" size="105 30"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      decor1="yes"
      icon="Clear Subexperiment"
      halignment="Center"
      valignment="Center"
      />
    <group loc="280 135" size="245 70"
      style="Heading1"
      label="Probe"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 20" size="235 45"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <textmessage loc="85 25" size="145 20"
          style="PlainText"
          label="---"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="125 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <textmessage loc="45 0" size="70 20"
          style="PlainText"
          label="0.005000"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
        <label loc="5 25" size="80 20"
          style="Label1"
          label="Probe used:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="530 175" size="220 90"
      style="Heading1"
      label="Steady State"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="195 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y' dps "
        vc2="sspul='n' dps"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <group loc="5 135" size="270 130"
      style="Heading1"
      label="3-9-19 WATERGATE"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="245 105" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="245 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="245 40" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="245 20" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <entry loc="190 105" size="50 20"
        style="PlainText"
        vc="prgtime=$VALUE/1000"
        set="$VALUE=prgtime*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="130 105" size="55 20"
        style="Label1"
        label="Duration"
        justify="Right"
        />
      <entry loc="80 105" size="50 20"
        style="PlainText"
        vc="prgpwr=$VALUE"
        set="$VALUE=prgpwr"
        digits="0"
        disable="Grayed out"
        />
      <check loc="10 85" size="145 20"
        style="Label1"
        label="Purge TRIM pulse"
        vq="prg_flg"
        vc="prg_flg='y' dps"
        vc2="prg_flg='n' dps"
        set="$VALUE=(prg_flg='y' )"
        />
      <label loc="25 105" size="40 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="135 60" size="50 20"
        style="Label1"
        label="G-time"
        justify="Right"
        />
      <entry loc="190 40" size="50 20"
        style="PlainText"
        vq="ex_max"
        vc="ex_max=$VALUE d3=1/(2.0*ex_max)"
        set="$VALUE=ex_max"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="190 20" size="50 20"
        style="PlainText"
        vq="d3"
        vc="d3=$VALUE/1e6 ex_max=1/(2.0*d3)"
        set="$VALUE=d3*1e6"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="190 60" size="50 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="80 60" size="50 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="G-strength"
        justify="Left"
        />
      <label loc="10 40" size="175 20"
        style="Label1"
        label="Excitation maximum from tof"
        justify="Left"
        />
      <label loc="10 20" size="75 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
    </group>
    <group loc="530 0" size="220 85"
      style="Heading1"
      label="DOSY Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 40" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <check loc="10 60" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <check loc="10 20" size="140 20"
        style="Label1"
        label="DOSY on/off"
        vq="delflag"
        vc="delflag='y' ddrpm='e' setrc"
        vc2="delflag='n' ddrpm='p' setrc"
        set="$VALUE=(delflag='y')"
        />
    </group>
    <group loc="280 0" size="245 135"
      style="Heading1"
      label="DOSY Setup"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 0" size="235 130"
        style="Heading1"
        label="DOSY Setup"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="155 85" size="55 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <entry loc="155 65" size="55 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <label loc="5 45" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <label loc="5 65" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="5 85" size="140 20"
          style="Label1"
          label="Highest gradient value"
          />
        <entry loc="155 45" size="55 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <button loc="5 110" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 20" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
      </group>
    </group>
    <group loc="5 25" size="270 110"
      style="Heading1"
      label="DOSY Parameters"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 5" size="265 95"
        style="Heading1"
        label="DOSY Parameters"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <label loc="240 75" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="240 55" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="240 35" size="25 20"
          style="Label1"
