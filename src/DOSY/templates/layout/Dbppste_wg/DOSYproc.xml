<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="DOSYproc" element="pages" type="processing" >
  <group size="820 305"
    style="Heading1"
    label="DOSY Process"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="DOSYproc"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="565 5" size="225 295"
      style="Heading1"
      label="Misc"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Data"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="0 10" size="225 265"
        label="Display"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <button loc="100 195" size="115 30"
          style="Heading2"
          label="Preview DOSY"
          vc="pageview('auto') resetplotter"
          bg=""
          decor1=""
          />
        <button loc="10 195" size="90 30"
          style="Heading2"
          label="Plot DOSY"
          vc="plot resetplotter"
          bg=""
          decor1=""
          />
        <label loc="90 15" size="80 20"
          style="Label1"
          label="Fit multiplier"
          justify="Right"
          />
        <entry loc="175 15" size="40 20"
          style="PlainText"
          vq="dosy_peakmult"
          vc="dosy_peakmult=$VALUE"
          set="$VALUE=dosy_peakmult"
          digits="1"
          />
        <button loc="10 240" size="205 20"
          style="Heading2"
          label="Clear Subexperiment"
          vc="cleardosy"
          bg=""
          decor1=""
          />
        <button loc="10 165" size="205 20"
          style="Heading2"
          label="Recall Original NMR Spectra"
          vc="undosy"
          bg=""
          decor1=""
          />
        <button loc="10 140" size="205 20"
          style="Heading2"
          label="Recall DOSY Display"
          vc="redosy "
          bg=""
          decor1=""
          />
        <button loc="10 115" size="205 20"
          style="Heading2"
          label="Show Diffusion Projection"
          vc="sdp"
          bg=""
          decor1=""
          />
        <button loc="10 90" size="205 20"
          style="Heading2"
          label="Show Residual for Peak # Above"
          vc="if (dosy_peaknum&gt;0)and(dosy_peakmult&gt;0) then showdosyresidual(dosy_peaknum,dosy_peakmult) else banner('Please set peak number  
 and fit multiplier') endif if dosy_autoincr='y' then dosy_peaknum=dosy_peaknum+1 endif"
          bg=""
          decor1=""
          />
        <button loc="10 65" size="205 20"
          style="Heading2"
          label="Show Fit for Peak # Above"
          vc="if (dosy_peaknum&gt;0)and(dosy_peakmult&lt;&gt;0) then showdosyfit(dosy_peaknum,dosy_peakmult) else banner('Please set peak number  
 and fit multiplier') endif if dosy_autoincr='y' then dosy_peaknum=dosy_peaknum+1 endif"
          bg=""
          decor1=""
          />
        <check loc="35 40" size="165 20"
          style="Label1"
          label="Auto increment peak #"
          vc="dosy_autoincr='y'"
          vc2="dosy_autoincr='n'"
          />
        <label loc="10 15" size="45 20"
          style="Label1"
          label="Peak #"
          justify="Left"
          />
        <entry loc="55 15" size="30 20"
          style="PlainText"
          vq="dosy_peaknum"
          vc="dosy_peaknum=$VALUE"
          set="$VALUE=dosy_peaknum"
          digits="0"
          />
      </group>
    </group>
    <group loc="270 5" size="290 295"
      style="Heading1"
      label="DOSY"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Data"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 10" size="280 275"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="95 250" size="180 20"
          style="PlainText"
          label=" "
          vq="probe_"
          set="$VALUE=probe_"
          />
        <label loc="205 220" size="70 20"
          style="Label1"
          label="10⁻¹⁰ m²/s"
          justify="Left"
          />
        <button loc="5 195" size="185 20"
          style="Heading2"
          label="Calculate Partial DOSY"
          vq="lowlim uplim"
          vc="dosy(lowlim,uplim)"
          bg=""
          decor1=""
          />
        <button loc="190 195" size="85 20"
          style="Heading2"
          label="W/ Dialogue"
          vq="lowlim uplim"
          vc="iscmdopen:$r if $r=0 then write('error','Please open command line for this option') else dosy('prune',lowlim,uplim) endif"
          bg=""
          decor1=""
          />
        <button loc="190 160" size="85 30"
          style="Heading2"
          label="W/ Dialogue"
          vc="iscmdopen:$r if $r=0 then write('error','Please open command line for this option') else dosy('prune') endif "
          bg=""
          decor1=""
          />
        <button loc="5 160" size="185 30"
          style="Heading2"
          label="Calculate Full DOSY"
          vc="dosy"
          bg=""
          decor1=""
          />
        <menu loc="180 30" size="95 20"
          style="PlainText"
          vq="dosyproc"
          vc="dosyproc='$VALUE'"
          set="$VALUE=dosyproc"
          editable="No"
          >
          <mlabel 
            label="Discrete"
            chval="discrete"
            />
          <mlabel 
            label="Continuous"
            chval="continuous"
            />
        </menu>
        <menu loc="220 10" size="55 20"
          style="PlainText"
          label="menu"
          vq="ncomp"
          vc="ncomp=$VALUE"
          set="$VALUE=ncomp"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
        </menu>
        <menu loc="220 105" size="55 20"
          style="PlainText"
          label="menu"
          vq="fn1"
          vc="fn1=$VALUE"
          set="$VALUE=fn1"
          editable="No"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1k"
            chval="1024"
            />
        </menu>
        <label loc="30 10" size="150 20"
          style="Label1"
          label="Number of components:"
          justify="Left"
          />
        <label loc="30 30" size="140 20"
          style="Label1"
          label="Processing type:"
          justify="Left"
          />
        <label loc="5 105" size="215 20"
          style="Label1"
          label="Fourier number in the diffusion dim."
          justify="Left"
          />
        <label loc="5 250" size="80 20"
          style="Label1"
          label="Probe used:"
          justify="Left"
          />
        <label loc="135 220" size="20 20"
          style="Label1"
          label="-"
          justify="Center"
          />
        <entry loc="155 220" size="45 20"
          style="PlainText"
          vq="uplim"
          vc="uplim=$VALUE"
          set="$VALUE=uplim"
          digits="1"
          />
        <entry loc="95 220" size="40 20"
          style="PlainText"
          vq="lowlim"
          vc="lowlim=$VALUE"
          set="$VALUE=lowlim"
          digits="1"
          />
        <label loc="5 220" size="90 20"
          style="Label1"
          label="D value range:"
          justify="Left"
          />
        <radio loc="150 135" size="130 20"
          style="Label1"
          label="Use integral values"
          vq="int_flg"
          vc="int_flg='y'"
          set="if (int_flg='y') then $VALUE=1 else $VALUE=0 endif"
          />
        <radio loc="15 135" size="120 20"
          style="Label1"
          label="Use peak heights"
          vq="int_flg"
          vc="int_flg='n'"
          set="if (int_flg='n') then $VALUE=1 else $VALUE=0 endif"
          />
        <check loc="30 75" size="225 20"
          style="Label1"
          label="Perform point-by-point analysis"
          vq="dosybypoints"
          vc="dosybypoints='y'"
          vc2="dosybypoints='n'"
          set="$VALUE=(dosybypoints='y')"
          />
        <check loc="30 55" size="220 20"
          style="Label1"
          label="Correct for non-uniform gradients"
          vq="nugflag"
          vc="nugflag='y'"
          vc2="nugflag='n'"
          set="$VALUE=(nugflag='y')"
          />
      </group>
    </group>
    <group loc="5 5" size="260 295"
      style="Heading1"
      label="Data"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Data"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="255 270"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <group loc="150 65" size="105 110"
          style="Heading1"
          bg=""
          border="None"
          side="BelowTop"
          justify="Left"
          tab="no"
          enable="no"
          reference="offset"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <label loc="5 80" size="40 20"
            style="Heading1"
            label="Offset"
            justify="Left"
            />
          <label loc="5 60" size="40 20"
            style="Label1"
            label="Polyn."
            justify="Left"
            />
          <check loc="5 0" size="100 20"
            style="Label1"
            label="Solvent Subtr."
            vq="ssfilter"
            vc="parfidss ssfilter='y'"
            vc2="ssfilter='n'"
            set="exists('ssfilter','parameter'):$v1 if $v1&lt;0.5 then $VALUE=0 else on('ssfilter'):$VALUE endif"
            />
          <label loc="5 40" size="40 20"
            style="Label1"
            label="Coefs."
            justify="Left"
            />
          <label loc="5 20" size="40 20"
            style="Label1"
            label="Width"
            justify="Left"
            />
          <entry loc="45 20" size="40 20"
            style="PlainText"
            vq="ssfilter"
            vc="ssfilter=$VALUE"
            set="$VALUE=ssfilter"
            show="on('ssfilter'):$VALUE"
            disable="Grayed out"
            />
          <entry loc="45 40" size="40 20"
            style="PlainText"
            vq="ssntaps ssfilter"
            vc="ssntaps=$VALUE"
            set="$VALUE=ssntaps"
            show="on('ssfilter'):$VALUE"
            disable="Grayed out"
            />
          <entry loc="45 60" size="40 20"
            style="PlainText"
            vq="ssorder ssfilter"
            vc="ssorder=$VALUE"
            set="$VALUE=ssorder"
            show="onparams('ssorder','ssfilter'):$VALUE"
            disable="Grayed out"
            />
          <check loc="85 60" size="20 20"
            style="Label1"
            vq="ssorder ssfilter"
            vc="ssorder='y'"
            vc2="ssorder='n'"
            set="on('ssorder'):$os on('ssfilter'):$of $VALUE=($os=1 and $of=1)"
            show="exists('ssorder','parameter'):$en $ENABLE=$en*2-1"
            />
          <check loc="85 80" size="20 20"
            style="Label1"
            vq="sslsfrq ssfilter"
            vc="sslsfrq='y'"
            vc2="sslsfrq='n'"
            set="on('sslsfrq'):$os on('ssfilter'):$of $VALUE=($os=1 and $of=1)"
            show="exists('sslsfrq','parameter'):$en $ENABLE=$en*2-1"
            />
          <entry loc="45 80" size="40 20"
            style="PlainText"
            vq="sslsfrq ssfilter"
            vc="sslsfrq=$VALUE"
            set="$VALUE=sslsfrq"
            show="onparams('sslsfrq','ssfilter'):$VALUE"
            disable="Grayed out"
            />
        </group>
        <menu loc="165 0" size="70 20"
          style="PlainText"
          label="menu"
          vq="fn"
          vc="fn=$VALUE*2"
          set="$VALUE=fn/2"
          show="on('fn'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="4k"
            chval="4096"
            />
          <mlabel 
            label="8k"
            chval="8192"
            />
          <mlabel 
            label="16k"
            chval="16384"
            />
          <mlabel 
            label="32k"
            chval="32768"
            />
          <mlabel 
            label="64k"
            chval="65536"
            />
          <mlabel 
            label="128k"
            chval="131072"
            />
          <mlabel 
            label="256k"
            chval="262144"
            />
          <mlabel 
            label="512k"
            chval="524288"
            />
        </menu>
        <check loc="5 0" size="130 20"
          style="Label1"
          label="1D FT Size (fn/2)"
          vq="fn"
          vc="on('fn')"
          vc2="off('fn')"
          set="on('fn'):$VALUE"
          />
        <textmessage loc="165 20" size="70 20"
          style="PlainText"
          label="16,384"
          vq="np"
          set="$VALUE=np/2"
          digits="0"
          />
        <label loc="20 20" size="140 20"
          style="Label1"
          label="Acquired Points (np/2)"
          justify="Left"
          />
        <group loc="0 60" size="150 110"
          style="Heading1"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="shift"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <label loc="5 5" size="70 20"
            style="Heading1"
            label="Line broad."
            justify="Left"
            />
          <check loc="125 85" size="20 20"
            style="Label1"
            vq="gfs"
            vc="gfs='y'"
            vc2="gfs='n'"
            set="on('gfs'):$VALUE"
            />
          <label loc="15 85" size="55 20"
            style="Label1"
            label="shift"
            justify="Left"
            />
          <label loc="5 65" size="65 20"
            style="Label1"
            label="Gaussian"
            justify="Left"
            />
          <label loc="15 45" size="55 20"
            style="Label1"
            label="shift"
            justify="Left"
            />
          <label loc="5 25" size="65 20"
            style="Label1"
            label="Sinebell"
            justify="Left"
            />
          <entry loc="75 85" size="50 20"
            style="PlainText"
            vq="gf gfs"
            vc="gfs=$VALUE"
            set="$VALUE=gfs"
            show="onparams('gf','gfs'):$VALUE"
            disable="Grayed out"
            />
          <check loc="125 65" size="20 20"
            style="Label1"
            vq="gf gfs"
            vc="gf='y'"
            vc2="gf='n'"
            set="on('gf'):$VALUE"
            />
          <entry loc="75 65" size="50 20"
            style="PlainText"
            vq="gf"
            vc="gf=$VALUE"
            set="$VALUE=gf"
            show="on('gf'):$VALUE"
            disable="Grayed out"
            />
          <check loc="125 45" size="20 20"
            style="Label1"
            vq="sbs"
            vc="sbs='y'"
            vc2="sbs='n'"
            set="on('sbs'):$VALUE"
            />
          <entry loc="75 45" size="50 20"
            style="PlainText"
            vq="sb sbs"
            vc="sbs=$VALUE"
            set="$VALUE=sbs"
            show="onparams('sb','sbs'):$VALUE"
            disable="Grayed out"
            />
          <check loc="125 25" size="20 20"
            style="Label1"
            vq="sb sbs"
            vc="sb='y'"
            vc2="sb='n'"
            set="on('sb'):$VALUE"
            />
          <entry loc="75 25" size="50 20"
            style="PlainText"
            vq="sb"
            vc="sb=$VALUE"
            set="$VALUE=sb"
            show="on('sb'):$VALUE"
            disable="Grayed out"
            />
          <check loc="125 5" size="20 20"
            style="Label1"
            vq="lb"
            vc="lb='y'"
            vc2="lb='n'"
            set="on('lb'):$VALUE"
            />
          <entry loc="75 5" size="50 20"
            style="PlainText"
            vq="lb"
            vc="lb=$VALUE"
            set="$VALUE=lb"
            show="on('lb'):$VALUE"
            digits="1"
            disable="Grayed out"
            />
        </group>
        <button loc="5 170" size="230 20"
          style="Heading2"
          label="Process all Spectra"
          vc="wft"
          bg=""
          decor1=""
          />
        <button loc="5 195" size="230 20"
          style="Heading2"
          label="Baseline Correct all Spectra"
          vc="fbc"
          bg=""
          decor1=""
          />
        <check loc="5 245" size="215 20"
          style="Label1"
          label="Write out corrected FID data"
          vq="fiddle_writeflg"
          vc="fiddle_writeflg='y'"
          vc2="fiddle_writeflg='n'"
          set="$VALUE=(fiddle_writeflg='y')"
          />
        <button loc="125 220" size="110 20"
          style="Heading2"
          label="FIDDLE (no TMS)"
          vc="dofiddle"
          bg=""
          decor1=""
          />
        <button loc="5 220" size="110 20"
          style="Heading2"
          label="FIDDLE (TMS)"
          vc="dofiddle('satellites','TMS')"
          bg=""
          decor1=""
          />
        <button loc="5 40" size="230 20"
          style="Heading2"
          label="Interactive Weighting"
          vc="wti"
          bg=""
          decor1=""
          />
      </group>
    </group>
  </group>
</template>
