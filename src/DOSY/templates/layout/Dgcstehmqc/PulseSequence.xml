      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="0 10" size="250 90"
        style="Heading1"
        label="DOSY Parameters"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
