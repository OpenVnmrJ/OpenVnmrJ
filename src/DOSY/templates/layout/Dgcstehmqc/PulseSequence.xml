<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="785 275"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="260 0" size="285 195"
      style="Heading1"
      label="Sequence parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="260 100" size="25 20"
        style="Heading1"
        label="Hz"
        justify="Left"
        />
      <label loc="260 75" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="200 75" size="55 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="220 100" size="35 20"
        style="PlainText"
        vq="jnxh mbond"
        vc="jnxh=$VALUE"
        set="$VALUE=jnxh"
        show="$ENABLE=(mbond='y')"
        digits="1"
        disable="Grayed out"
        />
      <check loc="10 100" size="205 20"
        style="Label1"
        label="Select HMBC and optimize it for"
        vq="mbond"
        vc="mbond='y' dm='nnn' dps"
        vc2="mbond='n' dm='nny' dps"
        set="$VALUE=(mbond='y' )"
        />
      <label loc="10 75" size="150 20"
        style="Label1"
        label="One-bond X-H coupling"
        justify="Left"
        />
      <check loc="10 50" size="190 20"
        style="Label1"
        label="Composite 180 deg. X pulse"
        vq="c180"
        vc="c180='y' dps"
        vc2="c180='n' dps"
        set="$VALUE=(c180='y' )"
        />
      <label loc="260 145" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
