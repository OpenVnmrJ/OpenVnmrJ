        style="Label1"
        label="Delay"
        justify="Center"
        />
      <label loc="85 45" size="25 20"
        style="Label1"
        label="dB"
        justify="Center"
        />
      <entry loc="55 45" size="30 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="Do saturation:"
        />
      <label loc="10 45" size="45 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 65" size="80 20"
        style="Label1"
        label="Frequency"
        />
    </group>
    <group loc="550 0" size="220 65"
      style="Heading1"
      label="DOSY Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 40" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <check loc="10 20" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
    </group>
    <group loc="5 125" size="250 135"
      style="Heading1"
      label="DOSY Setup"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 0" size="240 130"
        style="Heading1"
        label="DOSY Setup"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <button loc="5 20" size="230 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
        <button loc="5 110" size="230 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
        <entry loc="160 85" size="55 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <entry loc="160 65" size="55 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
