    <group loc="260 195" size="285 65"
      style="Heading1"
      label="Probe"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="235 40"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="5 20" size="80 20"
          style="Heading1"
          label="Probe used:"
          justify="Left"
          />
        <textmessage loc="85 20" size="145 20"
          style="PlainText"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="125 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <textmessage loc="45 0" size="70 20"
          style="PlainText"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
      </group>
    </group>
    <button loc="550 240" size="105 20"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      bg=""
      decor1=""
      icon="Clear Subexperiment"
      />
    <button loc="665 240" size="105 20"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      bg=""
      decor1=""
      />
    <group loc="550 150" size="220 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="195 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <group loc="550 65" size="220 85"
      style="Heading1"
      label="Solvent presaturation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="195 45" size="25 20"
        style="Heading1"
        label="s"
