        style="PlainText"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="65 145" size="55 20"
        style="PlainText"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 125" size="180 20"
        style="Label1"
        label="Encoding/Decoding gradient:"
        justify="Left"
        />
      <label loc="260 25" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="220 25" size="35 20"
        style="PlainText"
        vq="pwxlvl"
        vc="pwxlvl=$VALUE"
        set="$VALUE=pwxlvl"
        digits="0"
        disable="Grayed out"
        />
      <label loc="180 25" size="45 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="155 25" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <entry loc="110 25" size="40 20"
        style="PlainText"
        vq="pwx"
        vc="pwx=$VALUE"
        set="$VALUE=pwx"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 25" size="100 20"
        style="Label1"
        label="90 deg. X pulse"
        justify="Left"
        />
