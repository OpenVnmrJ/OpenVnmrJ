<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="760 250"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="520 65" size="220 90"
      style="Heading1"
      label="Solvent presaturation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="10 60" size="45 20"
        style="Heading1"
        label="Delay"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
      <label loc="190 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Center"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="2"
        />
      <label loc="195 60" size="25 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <menu loc="105 20" size="85 20"
        style="PlainText"
        vq="satmode"
        vc="satmode='$VALUE'"
        set="$VALUE=satmode"
        bg=""
        editable="No"
        >
        <mlabel 
          label="never"
          chval="nn"
          />
        <mlabel 
          label="d1 only"
          chval="yn"
          />
        <mlabel 
          label="del only"
          chval="ny"
          />
        <mlabel 
          label="both"
          chval="yy"
          />
      </menu>
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 20" size="90 20"
        style="Label1"
        label="Do saturation:"
        />
    </group>
    <button loc="405 205" size="100 30"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      bg=""
      decor1=""
      />
    <button loc="280 205" size="100 30"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      bg=""
      decor1=""
      icon="Clear Subexperiment"
      />
    <group loc="270 135" size="245 65"
      style="Heading1"
      label="Probe"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="235 40"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="5 20" size="80 20"
          style="Heading1"
          label="Probe used:"
          justify="Left"
          />
        <textmessage loc="85 20" size="145 20"
          style="PlainText"
          label="HCN"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="125 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <textmessage loc="45 0" size="70 20"
          style="PlainText"
          label="0.001774"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 20" size="260 150"
      style="Heading1"
      label="DOSY Parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="0 10" size="260 135"
        style="Heading1"
        label="DOSY Parameters"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="235 90" size="25 20"
          style="Heading1"
          label="ms"
          justify="Left"
          />
        <label loc="235 70" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="235 50" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="235 30" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <check loc="10 115" size="215 20"
          style="Label1"
          label="Select convection compensation"
          vq="convcomp"
          vc="convcomp='y'"
          vc2="convcomp='n'"
          set="$VALUE=(convcomp='y')"
          />
        <label loc="10 10" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
        <label loc="10 30" size="145 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="10 50" size="145 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <label loc="10 70" size="145 20"
          style="Label1"
          label="Off-center delay (del2)"
          justify="Left"
          />
        <label loc="10 90" size="145 20"
          style="Label1"
          label="Grad. stab. delay"
          justify="Left"
          />
        <entry loc="175 90" size="55 20"
          style="PlainText"
          vq="gstab"
          vc="gstab=$VALUE/1000"
          set="$VALUE=gstab*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="175 70" size="55 20"
          style="PlainText"
          vq="del2"
          vc="del2=$VALUE/1000"
          set="$VALUE=del2*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="175 50" size="55 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1000"
          set="$VALUE=del*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="175 30" size="55 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="175 10" size="55 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
      </group>
    </group>
    <group loc="5 170" size="260 65"
      style="Heading1"
      label="Sequence Parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="235 40" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="10 20" size="195 20"
        style="Label1"
        label="Coherence selection gradient:"
        justify="Left"
        />
      <entry loc="175 40" size="55 20"
        style="PlainText"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="110 40" size="60 20"
        style="Label1"
        label="Duration"
        justify="Right"
        />
      <entry loc="55 40" size="55 20"
        style="PlainText"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 40" size="35 20"
        style="Label1"
        label="Level"
        justify="Left"
        />
    </group>
    <group loc="520 150" size="220 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="195 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <group loc="520 0" size="220 65"
      style="Heading1"
      label="DOSY Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 40" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <check loc="10 20" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
    </group>
    <group loc="270 0" size="245 135"
      style="Heading1"
      label="DOSY Setup"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 0" size="235 130"
        style="Heading1"
        label="DOSY Setup"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="5 85" size="140 20"
          style="Heading1"
          label="Highest gradient value"
          />
        <label loc="5 65" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="5 45" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <entry loc="170 45" size="60 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <entry loc="170 65" size="60 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="170 85" size="60 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <button loc="5 110" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
        <button loc="5 20" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
      </group>
    </group>
    <textmessage loc="10 0" size="220 20"
      style="PlainText"
      label="Dhom2djidosy"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
