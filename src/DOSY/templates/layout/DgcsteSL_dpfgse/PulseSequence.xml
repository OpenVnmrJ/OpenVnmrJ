<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="775 280"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="550 0" size="200 100"
      style="Heading1"
      label="DOSY Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 45" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <check loc="10 70" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <check loc="10 20" size="140 20"
        style="Label1"
        label="DOSY on/off"
        vq="delflag"
        vc="delflag='y' ddrpm='e' setrc"
        vc2="delflag='n' ddrpm='p' setrc"
        set="$VALUE=(delflag='y')"
        />
    </group>
    <group loc="550 100" size="200 90"
      style="Heading1"
      label="Solvent presaturation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="175 60" size="25 20"
        style="Heading1"
        label="s"
        justify="Left"
        />
      <label loc="170 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Center"
        />
      <entry loc="100 60" size="70 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="2"
        />
      <entry loc="100 40" size="70 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
      <menu loc="100 20" size="85 20"
        style="PlainText"
        vq="satmode"
        vc="satmode='$VALUE'"
        set="$VALUE=satmode"
        bg=""
        editable="No"
        >
        <mlabel 
          label="never"
          chval="nn"
          />
        <mlabel 
          label="d1 only"
          chval="yn"
          />
        <mlabel 
          label="del only"
          chval="ny"
          />
        <mlabel 
          label="both"
          chval="yy"
          />
      </menu>
      <label loc="10 60" size="45 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 20" size="90 20"
        style="Label1"
        label="Do saturation:"
        />
    </group>
    <group loc="550 190" size="200 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="175 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="100 60" size="70 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="100 40" size="70 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <button loc="420 255" size="120 25"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      bg=""
      decor1=""
      />
    <button loc="260 255" size="120 25"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      bg=""
      decor1=""
      icon="Clear Subexperiment"
      />
    <group loc="255 180" size="290 70"
      style="Heading1"
      label="Probe"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="280 40"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="85 20" size="190 20"
          style="PlainText"
          label="HCN"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="150 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <label loc="5 20" size="80 20"
          style="Label1"
          label="Probe used:"
          justify="Left"
          />
        <textmessage loc="45 0" size="70 20"
          style="PlainText"
          label="0.001774"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="255 0" size="290 180"
      style="Heading1"
      label="Sequence parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="dpfgsegroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="135 40" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="265 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="265 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="155 150" size="55 20"
        style="Label1"
        label="Duration"
        justify="Center"
        />
      <label loc="135 150" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="265 150" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="30 60" size="50 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="30 40" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <entry loc="210 150" size="50 20"
        style="PlainText"
        vq="prgtime prg_flg"
        vc="prgtime=$VALUE/1000"
        set="$VALUE=prgtime*1000"
        show="$ENABLE=(prg_flg='y')"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="80 150" size="50 20"
        style="PlainText"
        vq="prgpwr prg_flg"
        vc="prgpwr=$VALUE"
        set="$VALUE=prgpwr"
        show="$ENABLE=(prg_flg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="25 150" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <check loc="10 130" size="175 20"
        style="Label1"
        label="Purge TRIM pulse"
        vq="prg_flg"
        vc="prg_flg='y' prgpwr=tpwr-8"
        vc2="prg_flg='n'"
        set="$VALUE=(prg_flg='y' )"
        />
      <button loc="10 105" size="250 20"
        style="Heading2"
        label="Recreate water refocusing shape"
        vc="update_wrefshape"
        bg=""
        decor1=""
        />
      <entry loc="210 80" size="50 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="150 80" size="55 20"
        style="Label1"
        label="G-time"
        justify="Right"
        />
      <entry loc="80 80" size="50 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="G-strength"
        justify="Left"
        />
      <entry loc="80 60" size="70 20"
        style="PlainText"
        vq="wrefshape"
        vc="wrefshape='$VALUE'"
        set="$VALUE=wrefshape"
        disable="Grayed out"
        />
      <label loc="150 60" size="55 20"
        style="Label1"
        label="Fine pwr"
        justify="Right"
        />
      <entry loc="210 60" size="50 20"
        style="PlainText"
        vq="wrefpwrf"
        vc="wrefpwrf=$VALUE"
        set="$VALUE=wrefpwrf"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 20" size="140 20"
        style="Label1"
        label="Solvent Selective Pulse:"
        justify="Left"
        />
      <label loc="160 40" size="45 20"
        style="Label1"
        label="Power"
        justify="Right"
        />
      <entry loc="210 40" size="50 20"
        style="PlainText"
        vq="wrefpwr"
        vc="wrefpwr=$VALUE"
        set="$VALUE=wrefpwr"
        disable="Grayed out"
        />
      <entry loc="80 40" size="50 20"
        style="PlainText"
        vq="wrefpw"
        vc="wrefpw=$VALUE*1000"
        set="$VALUE=wrefpw/1000"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="5 145" size="245 135"
      style="Heading1"
      label="DOSY Setup"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 0" size="235 130"
        style="Heading1"
        label="DOSY Setup"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="170 45" size="60 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <entry loc="170 65" size="60 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="170 85" size="60 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <label loc="5 85" size="140 20"
          style="Label1"
          label="Highest gradient value"
          />
        <label loc="5 65" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="5 45" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <button loc="5 110" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
        <button loc="5 20" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
      </group>
    </group>
    <group loc="5 20" size="245 125"
      style="Heading1"
      label="DOSY Parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 0" size="240 120"
        style="Heading1"
        label="DOSY Parameters"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="215 100" size="25 20"
          style="Heading1"
          label="ms"
          justify="Left"
          />
        <label loc="215 60" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 40" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <entry loc="155 100" size="55 20"
          style="PlainText"
          vq="gstab"
          vc="gstab=$VALUE/1000"
          set="$VALUE=gstab*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="155 80" size="55 20"
          style="PlainText"
          vq="tweek"
          vc="tweek=$VALUE/1000"
          set="$VALUE=tweek"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="155 60" size="55 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1e3"
          set="$VALUE=del*1e3"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="155 20" size="55 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
        <entry loc="155 40" size="55 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 80" size="150 20"
          style="Label1"
          label="Tuning factor (0-1)"
          justify="Left"
          />
        <label loc="5 100" size="150 20"
          style="Label1"
          label="Grad. stabilization delay"
          justify="Left"
          />
        <label loc="5 60" size="150 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <label loc="5 40" size="150 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="5 20" size="150 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
      </group>
    </group>
    <textmessage loc="10 0" size="220 20"
      style="PlainText"
      label="DgcsteSL_dpfgse"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
