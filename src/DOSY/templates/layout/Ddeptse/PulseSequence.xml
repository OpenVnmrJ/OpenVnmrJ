<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="735 275"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <textmessage loc="10 0" size="220 25"
      style="PlainText"
      label="Ddeptse"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
    <button loc="610 205" size="105 30"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      bg="transparent"
      halignment="Center"
      valignment="Center"
      />
    <button loc="515 205" size="95 30"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      bg="transparent"
      icon="Clear Subexperiment"
      halignment="Center"
      valignment="Center"
      />
    <group loc="515 100" size="200 85"
      style="Heading1"
      label="Steady State"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y' dps"
        vc2="sspul='n' dps"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="175 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="100 60" size="70 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="100 40" size="70 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <group loc="515 10" size="200 90"
      style="Heading1"
      label="DOSY Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 40" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <check loc="10 60" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <check loc="10 20" size="140 20"
        style="Label1"
        label="DOSY on/off"
        vq="delflag"
        vc="delflag='y' ddrpm='e' setrc"
        vc2="delflag='n' ddrpm='p' setrc"
        set="$VALUE=(delflag='y')"
        />
    </group>
    <group loc="255 10" size="255 175"
      style="Heading1"
      label="Sequence parameters"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_4"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 90" size="230 20"
        style="Label1"
        label="GRD-90-GRD option on X channel"
        vq="sspulX"
        vc="sspulX='y' dps"
        vc2="sspulX='n' dps"
        set="strstr(sspulX,'y'):$VALUE"
        />
      <label loc="230 20" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <label loc="230 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="230 65" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <menu loc="25 135" size="200 20"
        style="PlainText"
        vq="mult"
        vc="mult=$VALUE"
        set="$VALUE=mult"
        editable="No"
        >
        <mlabel 
          label="XH only"
          chval="1"
          />
        <mlabel 
          label="XH2 down, XH, XH3 up"
          chval="1.5"
          />
        <mlabel 
          label="All protonated carbons"
          chval="0.5"
          />
      </menu>
      <label loc="10 115" size="125 20"
        style="Label1"
        label="Multiplicity selection:"
        />
      <entry loc="165 65" size="60 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        digits="1"
        />
      <label loc="10 65" size="155 20"
        style="Label1"
        label="One-bond XH coupling"
        />
      <label loc="10 40" size="155 20"
        style="Label1"
        label="Power level for 1H pulses"
        />
      <entry loc="165 40" size="60 20"
        style="PlainText"
        vq="pplvl"
        vc="pplvl=$VALUE"
        set="$VALUE=pplvl"
        digits="0"
        />
      <entry loc="165 20" size="60 20"
        style="PlainText"
        vq="pp"
        vc="pp=$VALUE"
        set="$VALUE=pp"
        digits="2"
        />
      <label loc="10 20" size="155 20"
        style="Label1"
        label="90 deg. 1H pulse"
        />
    </group>
    <group loc="255 185" size="255 80"
      style="Heading1"
      label="Probe"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 20" size="235 40"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="5 20" size="80 20"
          style="Label1"
          label="Probe used:"
          justify="Left"
          />
        <textmessage loc="85 20" size="145 20"
          style="PlainText"
          label="---"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="125 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <textmessage loc="45 0" size="70 20"
          style="PlainText"
          label="0.005000"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 130" size="245 135"
      style="Heading1"
      label="DOSY Setup"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 0" size="235 130"
        style="Heading1"
        label="DOSY Setup"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="155 85" size="55 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <entry loc="155 65" size="55 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="155 45" size="55 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <label loc="5 85" size="140 20"
          style="Label1"
          label="Highest gradient value"
          />
        <label loc="5 65" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="5 45" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <button loc="5 110" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 20" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
      </group>
    </group>
    <group loc="5 25" size="245 105"
      style="Heading1"
      label="DOSY Parameters"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 5" size="240 95"
        style="Heading1"
        label="DOSY Parameters"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <label loc="215 75" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 55" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 35" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="5 75" size="150 20"
          style="Label1"
          label="Grad. stabilization delay"
          justify="Left"
          />
        <label loc="5 55" size="145 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <entry loc="155 75" size="55 20"
          style="PlainText"
          vq="gstab"
          vc="gstab=$VALUE/1000"
          set="$VALUE=gstab*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="155 55" size="55 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1000"
          set="$VALUE=del*1000"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 35" size="150 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <entry loc="155 35" size="55 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="155 15" size="55 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
        <label loc="5 15" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
      </group>
    </group>
  </group>
</template>
