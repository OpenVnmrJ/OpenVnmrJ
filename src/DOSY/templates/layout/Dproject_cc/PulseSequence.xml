<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="745 260"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <button loc="10 225" size="100 30"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      icon="Clear Subexperiment"
      halignment="Center"
      valignment="Center"
      />
    <button loc="150 225" size="100 30"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      halignment="Center"
      valignment="Center"
      />
    <group loc="260 185" size="255 70"
      style="Heading1"
      label="Probe"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 20" size="235 40"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <textmessage loc="45 0" size="75 20"
          style="PlainText"
          label="0.001970"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 20" size="80 20"
          style="Label1"
          label="Probe used:"
          justify="Left"
          />
        <textmessage loc="85 20" size="145 20"
          style="PlainText"
          label="---"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="125 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 25" size="250 190"
      style="Heading1"
      label="DOSY Parameters"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="0 15" size="250 170"
        style="Heading1"
        label="DOSY Parameters"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <entry loc="165 45" size="55 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1000"
          set="$VALUE=del*1000"
          digits="1"
          disable="Grayed out"
          />
        <label loc="224 90" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <entry loc="165 90" size="55 20"
          style="PlainText"
          vq="cycle_time"
          vc="cycle_time=$VALUE/1000"
          set="$VALUE=cycle_time*1000"
          digits="1"
          disable="Grayed out"
          />
        <label loc="225 70" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="225 130" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="225 45" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="225 25" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="10 70" size="145 20"
          style="Label1"
          label="Grad. stab. delay"
          justify="Left"
          />
        <label loc="10 90" size="145 20"
          style="Label1"
          label="Cycle time"
          justify="Left"
          />
        <label loc="10 110" size="145 20"
          style="Label1"
          label="HS gradient level"
          justify="Left"
          />
        <label loc="10 130" size="145 20"
          style="Label1"
          label="HS gradient duration"
          justify="Left"
          />
        <entry loc="165 70" size="55 20"
          style="PlainText"
          vq="gstab"
          vc="gstab=$VALUE/1000"
          set="$VALUE=gstab*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="165 110" size="55 20"
          style="PlainText"
          vc="gzlvl3=$VALUE"
          set="$VALUE=gzlvl3"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="165 130" size="55 20"
          style="PlainText"
          vc="gt3=$VALUE/1000"
          set="$VALUE=gt3*1000"
          digits="2"
          disable="Grayed out"
          />
        <entry loc="165 150" size="55 20"
          style="PlainText"
          vc="cycles=$VALUE"
          set="$VALUE=cycles"
          digits="0"
          disable="Grayed out"
          />
        <label loc="10 150" size="145 20"
          style="Label1"
          label="No. cycles"
          justify="Left"
          />
        <entry loc="165 5" size="55 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
        <entry loc="165 25" size="55 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <label loc="10 45" size="145 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <label loc="10 25" size="145 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="10 5" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
      </group>
    </group>
    <group loc="520 170" size="220 85"
      style="Heading1"
      label="Steady State"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="195 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="105 40" size="85 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <group loc="520 0" size="220 85"
      style="Heading1"
      label="DOSY Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 40" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <check loc="10 60" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <check loc="10 20" size="140 20"
        style="Label1"
        label="DOSY on/off"
        vq="delflag"
        vc="delflag='y' ddrpm='e' setrc"
        vc2="delflag='n' ddrpm='p' setrc"
        set="$VALUE=(delflag='y')"
        />
    </group>
    <group loc="520 85" size="220 85"
      style="Heading1"
      label="Solvent presaturation"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="195 40" size="20 20"
        style="Heading1"
        label="s"
        justify="Left"
        />
      <label loc="195 60" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="105 60" size="85 20"
        style="PlainText"
        vq="satfrq"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        />
      <menu loc="105 20" size="85 20"
        style="PlainText"
        vq="satmode"
        vc="satmode='$VALUE'"
        set="$VALUE=satmode"
        editable="No"
        >
        <mlabel 
          label="never"
          chval="nn"
          />
        <mlabel 
          label="d1 only"
          chval="yn"
          />
        <mlabel 
          label="del only"
          chval="ny"
          />
        <mlabel 
          label="both"
          chval="yy"
          />
      </menu>
      <label loc="105 40" size="45 20"
        style="Label1"
        label="Delay"
        justify="Center"
        />
      <entry loc="150 40" size="40 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="2"
        />
      <label loc="10 60" size="80 20"
        style="Label1"
        label="Frequency"
        />
      <label loc="85 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Center"
        />
      <entry loc="55 40" size="30 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 20" size="90 20"
        style="Label1"
        label="Do saturation:"
        />
    </group>
    <group loc="260 0" size="255 135"
      style="Heading1"
      label="DOSY Setup"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 0" size="235 130"
        style="Heading1"
        label="DOSY Setup"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="5 85" size="140 20"
          style="Heading1"
          label="Highest gradient value"
          />
        <label loc="5 65" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="5 45" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <entry loc="170 45" size="60 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <entry loc="170 65" size="60 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="170 85" size="60 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <button loc="5 110" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 20" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
      </group>
    </group>
    <textmessage loc="10 0" size="220 20"
      style="PlainText"
      label="Dproject_cc"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
