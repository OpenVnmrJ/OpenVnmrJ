<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="750 260"
    style="Heading1"
    label="Defaults"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="270 35" size="245 205"
      style="Heading1"
      label="DOSY Setup"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="0 20" size="245 175"
        style="Heading1"
        label="DOSY Setup"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYSetup"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="10 90" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
        <label loc="215 155" size="25 20"
          style="Label1"
          label="ms"
          />
        <label loc="215 135" size="25 20"
          style="Label1"
          label="ms"
          />
        <button loc="10 0" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max)  vnmrjcmd('array','popup')"
          halignment="Center"
          valignment="Center"
          />
        <label loc="10 155" size="145 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <entry loc="160 155" size="50 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1000"
          set="$VALUE=del*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="160 115" size="50 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
        <entry loc="160 135" size="50 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="160 65" size="50 20"
          style="PlainText"
          vq="gzlvl1_max kappa"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <entry loc="160 45" size="50 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="160 25" size="50 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <label loc="10 135" size="150 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="10 115" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
        <label loc="10 25" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <label loc="10 45" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="10 65" size="140 20"
          style="Label1"
          label="Highest gradient value"
          />
      </group>
      <label loc="250 190" size="40 20"
        style="Label1"
        label="msec"
        justify="Left"
        />
      <label loc="250 170" size="40 20"
        style="Label1"
        label="msec"
        justify="Left"
        />
    </group>
    <group loc="5 35" size="260 205"
      style="Heading1"
      label="Acquisition Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 25" size="250 130"
        style="Heading1"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="NumberofScans"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <label loc="5 45" size="160 20"
          style="Label1"
          label="Relaxation Delay [s]"
          justify="Left"
          />
        <label loc="5 65" size="150 20"
          style="Label1"
          label="Number of Scans"
          justify="Left"
          />
        <menu loc="175 45" size="60 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="1.5"
            chval="1.5"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="2.5"
            chval="2.5"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="10"
            chval="10"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <menu loc="175 65" size="60 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
        </menu>
        <entry loc="175 85" size="60 20"
          style="PlainText"
          vq="ss"
          vc="ss=$VALUE"
          set="$VALUE=ss"
          disable="Grayed out"
          />
        <label loc="5 85" size="150 20"
          style="Label1"
          label="Steady State Pulses"
          justify="Left"
          />
        <label loc="7 0" size="160 20"
          style="Label1"
          label="Spectral Width [ppm]"
          justify="Left"
          />
        <group loc="7 20" size="240 20"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="swstartend"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          rows="1"
          columns="1"
          >
          <label loc="133 0" size="50 20"
            style="Label1"
            label="Upfield"
            justify="Left"
            />
          <entry loc="183 0" size="45 20"
            style="PlainText"
            vq="sw tof reffrq rfl rfp"
            vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
            set="invsetsw:$f1,$f2 $VALUE=$f2"
            digits="0"
            disable="Grayed out"
            />
          <entry loc="78 0" size="45 20"
            style="PlainText"
            vq="sw tof reffrq rfl rfp"
            vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
            set="invsetsw:$f1,$f2 $VALUE=$f1"
            digits="0"
            disable="Grayed out"
            />
          <label loc="8 0" size="70 20"
            style="Label1"
            label="Downfield"
            justify="Left"
            />
        </group>
      </group>
    </group>
    <group loc="5 5" size="735 25"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="10 0" size="450 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="485 0" size="235 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="515 35" size="235 220"
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 10" size="220 150"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <textmessage loc="10 105" size="200 25"
          style="PlainText"
          label=" "
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          label="After Dproject_cc acq:"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          label="Before Dproject_cc acq:"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label="wet1D PRESAT PROTON"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
  </group>
</template>
