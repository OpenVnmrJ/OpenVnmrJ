<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="755 235"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <button loc="255 145" size="100 30"
      style="Heading2"
      label="Display Manual"
      vc="man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
      bg=""
      decor1=""
      />
    <button loc="255 190" size="100 30"
      style="Heading2"
      label="Clear Subexp."
      vc="cleardosy"
      bg=""
      decor1=""
      icon="Clear Subexperiment"
      />
    <group loc="5 25" size="245 120"
      style="Heading1"
      label="DOSY Parameters"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSY_acq_1"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="5 5" size="240 105"
        style="Heading1"
        label="DOSY Parameters"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="DOSYbuttons"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <label loc="215 85" size="25 20"
          style="Heading1"
          label="ms"
          justify="Left"
          />
        <label loc="215 65" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 45" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <entry loc="155 85" size="55 20"
          style="PlainText"
          vq="gstab"
          vc="gstab=$VALUE/1000"
          set="$VALUE=gstab*1000"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 25" size="145 20"
          style="Label1"
          label="Diffusion gradient level"
          justify="Left"
          />
        <label loc="5 45" size="150 20"
          style="Label1"
          label="Diffusion gradient length"
          justify="Left"
          />
        <label loc="5 65" size="145 20"
          style="Label1"
          label="Diffusion delay"
          justify="Left"
          />
        <label loc="5 85" size="150 20"
          style="Label1"
          label="Grad. stabilization delay"
          justify="Left"
          />
        <entry loc="155 65" size="55 20"
          style="PlainText"
          vq="del"
          vc="del=$VALUE/1000"
          set="$VALUE=del*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="155 45" size="55 20"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE/1000"
          set="$VALUE=gt1*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="155 25" size="55 20"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          disable="Grayed out"
          />
      </group>
    </group>
    <group loc="5 145" size="245 75"
      style="Heading1"
      label="Probe"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Probeused"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="5 20" size="240 45"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="85 25" size="150 20"
          style="PlainText"
          label="---"
          vq="probe_"
          set="$VALUE=probe_"
          digits="3"
          />
        <label loc="115 0" size="100 20"
          style="Label1"
          label="Gauss/cm/DAC"
          />
        <textmessage loc="45 0" size="70 20"
          style="PlainText"
          label="0.002200"
          vq="gcal_"
          set="$VALUE=gcal_"
          digits="6"
          />
        <label loc="5 0" size="40 20"
          style="Label1"
          label="gcal_:"
          justify="Left"
          />
        <label loc="5 25" size="80 20"
          style="Label1"
          label="Probe used:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="535 100" size="200 120"
      style="Heading1"
      label="Solvent presaturation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsupp"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="175 85" size="25 20"
        style="Heading1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 65" size="25 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <label loc="175 45" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="100 85" size="70 20"
        style="PlainText"
        vq="satfrq"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        />
      <entry loc="100 65" size="70 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="3"
        />
      <entry loc="100 45" size="70 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        />
      <menu loc="100 25" size="90 20"
        style="PlainText"
        vq="satmode"
        vc="satmode='$VALUE'"
        set="$VALUE=satmode"
        editable="No"
        >
        <mlabel 
          label="never"
          chval="nn"
          />
        <mlabel 
          label="d1 only"
          chval="yn"
          />
        <mlabel 
          label="del only"
          chval="ny"
          />
        <mlabel 
          label="both"
          chval="yy"
          />
      </menu>
      <label loc="10 25" size="90 20"
        style="Label1"
        label="Do saturation:"
        />
      <label loc="10 45" size="80 20"
        style="Label1"
        label="Power"
        />
      <label loc="10 65" size="80 20"
        style="Label1"
        label="Delay"
        />
      <label loc="10 85" size="80 20"
        style="Label1"
        label="Frequency"
        />
    </group>
    <group loc="535 0" size="200 100"
      style="Heading1"
      label="DOSY Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYacq_2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 25" size="140 20"
        style="Label1"
        label="DOSY on/off"
        vq="delflag"
        vc="delflag='y' ddrpm='e' setrc"
        vc2="delflag='n' ddrpm='p' setrc"
        set="$VALUE=(delflag='y')"
        />
      <check loc="10 45" size="170 20"
        style="Label1"
        label="Alternate gradient sign"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <check loc="10 65" size="190 20"
        style="Label1"
        label="Lock gating during gradients"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
    </group>
    <group loc="255 0" size="275 135"
      style="Heading1"
      label="DOSY Setup"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="DOSYsetup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="20 0" size="235 130"
        style="Heading1"
        label="DOSY Setup"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="5 85" size="140 20"
          style="Heading1"
          label="Highest gradient value"
          />
        <label loc="5 65" size="140 20"
          style="Label1"
          label="Lowest gradient value"
          />
        <label loc="5 45" size="140 20"
          style="Label1"
          label="Number of increments"
          />
        <entry loc="170 45" size="60 20"
          style="PlainText"
          vq="gzlvl1_inc"
          vc="gzlvl1_inc=$VALUE"
          set="$VALUE=gzlvl1_inc"
          digits="0"
          />
        <entry loc="170 65" size="60 20"
          style="PlainText"
          vq="gzlvl1_min"
          vc="gzlvl1_min=$VALUE"
          set="$VALUE=gzlvl1_min"
          digits="0"
          />
        <entry loc="170 85" size="60 20"
          style="PlainText"
          vq="gzlvl1_max"
          vc="gzlvl1_max=$VALUE"
          set="$VALUE=gzlvl1_max"
          />
        <button loc="5 110" size="225 20"
          style="Heading2"
          label="Setup DOSY using conditions above"
          vc="setup_dosy('panelread') vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
        <button loc="5 20" size="225 20"
          style="Heading2"
          label="Setup coarse gradient array"
          vc="setup_dosy(7,0.04*gzlvl1_max,gzlvl1_max) vnmrjcmd('array','popup')"
          bg=""
          decor1=""
          />
      </group>
    </group>
    <group loc="360 135" size="170 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="145 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="85 60" size="55 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="85 40" size="55 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
    </group>
    <textmessage loc="10 0" size="220 20"
      style="PlainText"
      label="Dbppste"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
