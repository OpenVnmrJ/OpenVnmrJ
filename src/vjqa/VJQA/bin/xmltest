#! /bin/sh
rm -f ./*txt
echo PATH = $PATH
PATH=./:$PATH
CLASSPATH=./:../java/:$CLASSPATH
export PATH CLASPATH
echo PATH = $PATH
echo $CLASSPATH
echo Finding all directories with xml files
findxml
echo Parsing all xml files
java XmlTest
echo Searching for Fatal Errors
grep Fatal XmlTestResults.txt > failed.txt
