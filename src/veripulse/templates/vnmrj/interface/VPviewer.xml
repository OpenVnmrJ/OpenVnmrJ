<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="10 110" size="675 145"
    vq="VPmode"
    show="$SHOW=(VPmode&lt;&gt;'test')"
    border="None"
    tab="no"
    enable="no"
    useref="false"
    subtype="Untitled"
    rows="1"
    columns="1"
    >
    <label loc="10 15" size="160 25"
      style="Label1"
      label="Study name:"
      />
    <filemenu loc="180 11" size="460 25"
      style="PlainText"
      vc="VPviewer('setstudy','$VALUE') VPviewer('getfiles')"
      set="$VALUE=VPstudy"
      file="$VALUE=userdir+'/persistence/VPstudys'"
      type="file"
      bg="VJBackground"
      editable="No"
      vq2="VPchoice VPmode VPstudy"
      statshow="no"
      elastic="no"
      display="yes"
      />
    <filemenu loc="180 71" size="460 25"
      style="PlainText"
      vc="VPviewer('setfile','$VALUE')"
      set="$VALUE=VPfile"
      file="$VALUE=userdir+'/persistence/VPsfiles'"
      type="file"
      bg="VJBackground"
      editable="No"
      vq2="VPview"
      statshow="no"
      elastic="no"
      display="yes"
      />
    <label loc="10 71" size="165 25"
      style="Label1"
      label="Test"
      />
  </group>
  <group loc="10 110" size="700 125"
    vq="VPmode"
    show="$SHOW=(VPmode='test')"
    border="None"
    tab="no"
    enable="no"
    useref="no"
    subtype="Untitled"
    rows="1"
    columns="1"
    >
    <filemenu loc="180 11" size="460 25"
      style="PlainText"
      vc="VPviewer('settest','$VALUE') VPviewer('getfiles')"
      set="$VALUE=VPtest"
      file="$VALUE=userdir+'/persistence/VPtests'"
      type="file"
      bg="VJBackground"
      editable="No"
      vq2="VPchoice VPtest"
      statshow="no"
      elastic="no"
      display="yes"
      />
    <filemenu loc="180 71" size="460 25"
      style="PlainText"
      vc="VPviewer('setfile','$VALUE')"
      set="$VALUE=VPfile"
      file="$VALUE=userdir+'/persistence/VPtfiles'"
      type="file"
      bg="VJBackground"
      editable="No"
      vq2="VPtest VPchoice"
      statshow="no"
      elastic="no"
      display="yes"
      />
    <label loc="10 71" size="165 25"
      style="Label1"
      label="Study"
      />
    <label loc="10 16" size="160 25"
      style="Label1"
      label="Test name:"
      />
  </group>
  <button loc="255 295" size="175 25"
    style="PlainText"
    label="Reprocess (no autophase)"
    vc="VPprocess('nophase')"
    bg="transparent"
    decor1="yes"
    halignment="Center"
    valignment="Center"
    />
  <button loc="70 295" size="175 25"
    style="PlainText"
    label="Process"
    vc="VPprocess"
    bg="transparent"
    decor1="yes"
    halignment="Center"
    valignment="Center"
    />
  <button loc="255 260" size="175 25"
    style="PlainText"
    label="Show plots folder"
    bg="transparent"
    vc="VPviewer('showplots')"
    decor1="yes"
    halignment="Center"
    valignment="Center"
    />
  <button loc="70 260" size="175 25"
    style="PlainText"
    label="Load"
    vc="VPviewer('rt')"
    bg="transparent"
    decor1="yes"
    halignment="Center"
    valignment="Center"
    />
  <radio loc="315 75" size="80 25"
    style="PlainText"
    label="Studies"
    vq="VPmode"
    vc="VPviewer('setmode','study')"
    set="$VALUE=(VPmode&lt;&gt;'test')"
    />
  <label loc="20 70" size="160 25"
    style="Label1"
    label="Select:"
    />
  <label loc="20 40" size="160 25"
    style="Label1"
    label="VeriPulse data directory:"
    />
  <radio loc="190 75" size="80 25"
    style="PlainText"
    label="Tests"
    vq="VPmode"
    vc="VPviewer('setmode','test')"
    set="$VALUE=(VPmode='test')"
    />
  <filemenu loc="190 40" size="460 25"
    style="PlainText"
    vc="VPviewer('setchoice','$VALUE') VPviewer('gettests')"
    set="$VALUE=VPchoice"
    file="$VALUE=userdir+'/persistence/VPchoices'"
    type="file"
    bg="VJBackground"
    editable="No"
    vq2="VPchoice"
    statshow="no"
    elastic="no"
    display="yes"
    />
  <button loc="70 330" size="175 25"
    style="PlainText"
    label="Resave"
    bg="transparent"
    vc="VPviewer('resave')"
    decor1="yes"
    halignment="Center"
    valignment="Center"
    />
  <button loc="255 330" size="175 25"
    style="PlainText"
    label="Plot"
    vc="VPplot('','view')"
    bg="transparent"
    decor1="yes"
    halignment="Center"
    valignment="Center"
    />
  <group loc="430 260" size="180 30"
    vq="VPmode"
    show="$SHOW=(VPmode='test')"
    border="None"
    tab="no"
    enable="no"
    useref="no"
    subtype="Untitled"
    rows="1"
    columns="1"
    >
     <button loc="5 0" size="175 25"
       style="PlainText"
       label="History"
       vc="VPviewer('history') VPhist"
       bg="transparent"
       decor1="yes"
       halignment="Center"
       valignment="Center"
       />
  </group>
</template>
