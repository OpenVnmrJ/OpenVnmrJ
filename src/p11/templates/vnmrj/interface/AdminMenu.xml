        />
	<wchoice label = "File System..."
              key="file system"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Command Window..."
     	      key="command window"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
   </submenu>

