	<wchoice label="Automation..."
		 key="'automation' 'help:/vnmr/jhelp/dialog/automation.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="Investigator List..."
		 key="'investi' 'help:/vnmr/jhelp/dialog/investigators.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="Locator Config..."
		 key="'locator' 'help:/vnmr/jhelp/dialog/locator.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="DICOM Storage..."
		 key="'dicom' 'help:/vnmr/jhelp/dialog/dicomstorage.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Background colors..."
	      key="fonts colors"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        />
   </submenu>
   <submenu  label = "Auditing"
        key="auditing"
        font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        >
        <submenu  label = "Capture Screen"
              key="capture"
              font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        >
              <wchoice label="Save"
                  key="capture_save"
                  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
               />
              <wchoice label="Print"
                  key="capture_print"
                  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
               />
        </submenu>
        <wchoice label = "Archive Data"
              key="archive"
              font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        />

   </submenu>

   <submenu  label = "Help" hotkey = "o"
	key="help" 
        font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
   > 
	<wchoice label = "VnmrJ Help..."
		key="'vnmrj help' 'help:/vnmr/jhelp/jhelp.html'"
		font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Admin Help..."
	      key="'admin help' 'help:/vnmr/jhelp/sys_adm/vj_admin.html'"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        />
	<wchoice label = "Online Manuals (PDF)..."
	      key="'admin pdf help'"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        />
