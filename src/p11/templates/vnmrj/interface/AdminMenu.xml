	<wchoice label = "Users"
	    key="users"
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	    path1="SYSTEM/USRS/userlist"
	    path2="GRPS/grouplist"
	/>
        <wchoice label = "Auditing"
            key="part11"
            font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        />
   	<submenu label="Validation" 
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	>
            <wchoice label="System Validation..."
            	key="'validate' 'help:/vnmr/jhelp/dialog/validation.html'"
            	font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
            />
