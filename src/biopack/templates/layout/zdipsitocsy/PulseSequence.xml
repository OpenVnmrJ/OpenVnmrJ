<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="780 285"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="285 5" size="470 210"
      style="Label2"
      label="WET"
      vq="wet"
      show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="WET"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="85 180" size="50 25"
        style="PlainText"
        vq="gzlvl3"
        vc="gzlvl3=$VALUE"
        set="$VALUE=gzlvl3"
        digits="0"
        disable="Grayed out"
        tooltip="gzlvl3"
        />
      <label loc="20 180" size="65 25"
        style="PlainText"
        label="Amplitude"
        justify="Left"
        />
      <label loc="5 155" size="180 25"
        style="PlainText"
        label="Radiation Damping Gradients"
        justify="Left"
        />
      <group loc="8 16" size="184 112"
        style="Label1"
        label="RF"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        >
        <entry loc="120 80" size="56 24"
          style="PlainText"
          vq="wetpw wet"
          vc="wetpw=[$VALUE]*1e3"
          set="$VALUE=wetpw/1000"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          tooltip="wetpw=[$VALUE]*1e3"
          />
        <label loc="8 80" size="88 24"
          style="PlainText"
          label="Width (msec)"
          />
        <label loc="8 56" size="64 24"
          style="PlainText"
          label="Power"
          />
        <entry loc="128 56" size="48 24"
          style="PlainText"
          vq="wetpwr wet autosoft"
          vc="wetpwr=$VALUE"
          set="$VALUE=wetpwr"
          show="if (wet='y' and autosoft='n') then $VALUE=1 else $VALUE=0 endif"
          digits="2"
          tooltip="wetpwr=$VALUE"
          />
        <entry loc="96 32" size="80 24"
          style="PlainText"
          vq="wetshape wet autosoft"
          vc="wetshape='$VALUE'"
          set="$VALUE=wetshape"
          show="if (wet='y' and autosoft='n') then $VALUE=1 else $VALUE=0 endif"
          tooltip="wetshape"
          />
        <label loc="8 32" size="64 24"
          style="PlainText"
          label="Shape"
          />
        <check loc="56 8" size="104 24"
          style="Heading4"
          label="Automatic"
          vq="autosoft"
          vc="autosoft='y'"
          vc2="autosoft='n'"
          set="if autosoft='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="autosoft='y'"
          />
      </group>
      <group loc="208 16" size="256 128"
        style="Label1"
        label="Gradients"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        >
        <entry loc="168 96" size="72 24"
          style="PlainText"
          vq="gzcal wet"
          vc="gzcal=$VALUE"
          set="$VALUE=gzcal"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="6"
          disable="Grayed out"
          tooltip="gzcal"
          />
        <label loc="8 96" size="120 24"
          style="PlainText"
          label="G/cm per DAC value"
          justify="Left"
          />
        <entry loc="208 72" size="40 24"
          style="PlainText"
          vq="gswet wet"
          vc="gswet=$VALUE/1e6"
          set="$VALUE=gswet*1e6"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="1"
          disable="Grayed out"
          tooltip="gswet=$VALUE/1e6"
          />
        <label loc="8 72" size="192 24"
          style="PlainText"
          label="Stabilization Time in WET(usec)"
          justify="Left"
          />
        <entry loc="80 48" size="48 24"
          style="PlainText"
          vq="gtw wet"
          vc="gtw=$VALUE/1000"
          set="$VALUE=gtw*1000"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          disable="Grayed out"
          tooltip="gtw=$VALUE/1000"
          />
        <label loc="8 48" size="88 24"
          style="PlainText"
          label="Time (msec)"
          justify="Left"
          />
        <label loc="192 24" size="40 24"
          style="PlainText"
          label="G/cm"
          justify="Left"
          />
        <entry loc="144 24" size="48 24"
          style="PlainText"
          vq="gzlvlw wet gzcal"
          vc="gzlvlw=$VALUE/gzcal"
          set="$VALUE=gzlvlw*gzcal"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          disable="Grayed out"
          />
        <label loc="128 24" size="16 24"
          style="PlainText"
          label="or"
          justify="Left"
          />
        <entry loc="80 24" size="48 24"
          style="PlainText"
          vq="gzlvlw wet"
          vc="gzlvlw=$VALUE"
          set="$VALUE=gzlvlw"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="0"
          disable="Grayed out"
          tooltip="gzlvlw"
          />
        <label loc="8 24" size="64 24"
          style="PlainText"
          label="Amplitude"
          justify="Left"
          />
      </group>
      <group loc="208 136" size="232 48"
        style="DefaultTitle"
        vq="autosoft"
        show="if autosoft='n' then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <button loc="24 8" size="200 24"
          style="Heading1"
          label="Optimize WET Parameters"
          vq="wet"
          vc="if wet='y' then BPautoWet(1) endif"
          bg="VJBackground"
          decor1="no"
          />
      </group>
      <entry loc="144 128" size="40 24"
        style="PlainText"
        vq="dz wet"
        vc="dz=$VALUE/1e6"
        set="$VALUE=dz*1e6"
        show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        disable="Grayed out"
        tooltip="dz=$VALUE/1e6"
        />
      <label loc="8 128" size="128 24"
        style="PlainText"
        label="WEFT Delay (usec)"
        justify="Left"
        />
    </group>
    <selmenu loc="30 230" size="210 25"
      style="Label4"
      label="Select Suppression Mode"
      vc="exec('$VALUE')"
      bg="VJBackground"
      editable="No"
      >
      <mlabel 
        label="Water Suppression By WET"
        chval="wet=`y` satmode=`n`"
        />
      <mlabel 
        label="Water Suppression By PRESAT"
        chval="wet=`n`  satmode=`y`"
        />
      <mlabel 
        label="NO Water Suppression"
        chval="wet=`n`  satmode=`n`"
        />
    </selmenu>
    <group loc="285 215" size="470 65"
      label="Z-Filter"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <entry loc="165 40" size="40 25"
        style="PlainText"
        vq="gstab "
        vc="gstab=$VALUE/1e6"
        set="$VALUE=gstab*1e6"
        digits="1"
        disable="Grayed out"
        tooltip="gstab=$VALUE/1e6"
        />
      <label loc="10 40" size="150 25"
        style="PlainText"
        label="Stabilization Time (usec)"
        justify="Left"
        />
      <entry loc="345 15" size="50 25"
        style="PlainText"
        vq="gt2 "
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="3"
        disable="Grayed out"
        tooltip="gt2=$VALUE/1000"
        />
      <label loc="265 15" size="90 25"
        style="PlainText"
        label="Time (msec)"
        justify="Left"
        />
      <label loc="8 16" size="64 24"
        style="PlainText"
        label="Amplitude"
        justify="Left"
        />
      <entry loc="88 16" size="48 24"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        tooltip="gzlvl2"
        />
      <label loc="136 16" size="16 24"
        style="PlainText"
        label="or"
        justify="Left"
        />
      <entry loc="160 16" size="48 24"
        style="PlainText"
        vq="gzlvl2 gzcal"
        vc="gzlvl2=$VALUE/gzcal"
        set="$VALUE=gzlvl2*gzcal"
        digits="3"
        disable="Grayed out"
        />
      <label loc="216 16" size="40 24"
        style="PlainText"
        label="G/cm"
        justify="Left"
        />
    </group>
    <label loc="32 200" size="32 24"
      style="PlainText"
      label="rof1:"
      justify="Left"
      />
    <entry loc="64 200" size="40 24"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
      set="$VALUE=rof1"
      digits="1"
      disable="Grayed out"
      />
    <label loc="112 200" size="32 24"
      style="PlainText"
      label="rof2:"
      justify="Left"
      />
    <entry loc="144 200" size="32 24"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      disable="Grayed out"
      />
    <label loc="184 200" size="32 24"
      style="PlainText"
      label="alfa:"
      justify="Left"
      />
    <entry loc="224 200" size="32 24"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      disable="Grayed out"
      />
    <label loc="8 152" size="160 24"
      style="PlainText"
      label="Delays:   Relaxation (sec)"
      justify="Left"
      />
    <entry loc="176 152" size="40 24"
      style="PlainText"
      vq="d1"
      vc="d1=$VALUE"
      set="$VALUE=d1"
      show="if d1=0 then $VALUE=0 else $VALUE=1 endif"
      digits="3"
      disable="Grayed out"
      tooltip="d1"
      />
    <group loc="456 40" size="304 88"
      style="PlainText"
      label="Multi-Frequency Saturation"
      vq="mfsat"
      show="if mfsat='y' then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Multi-FrequencySaturation"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="72 24" size="40 24"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="3"
        disable="Grayed out"
        tooltip="satdly"
        />
      <button loc="120 40" size="184 24"
        style="Label3"
        label="Show List (see Text Output)"
        vc="clear cat(curexp+'/mfpresat.ll')"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="120 16" size="184 24"
        style="Label3"
        label="Instructions (see Text Output)"
        vc="man('mfpresat') "
        bg="VJBackground"
        decor1="no"
        />
      <button loc="232 64" size="72 24"
        style="Label3"
        label="Clear List"
        vc="mfll('new')"
        bg="VJBackground"
        decor1="no"
        tooltip="mfll('new')"
        />
      <entry loc="72 48" size="32 24"
        style="PlainText"
        vq="satpwr shaped presat"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="if (presat='y' or shaped='y') then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="satpwr"
        />
      <label loc="8 48" size="48 24"
        style="PlainText"
        label="Power"
        justify="Left"
        />
      <label loc="8 24" size="64 24"
        style="PlainText"
        label="Time(sec)"
        />
      <button loc="120 64" size="104 24"
        style="Label3"
        label="Add Frequency"
        vc="mfll"
        bg="VJBackground"
        decor1="no"
        tooltip="mfll"
        />
    </group>
    <group loc="504 16" size="256 24"
      label="mfsat"
      vq="satmode"
      show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="mfsat"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <radio loc="120 0" size="128 24"
        style="PlainText"
        label="Multiple Frequency"
        vq="mfsat"
        vc="mfsat='y'"
        vc2="mfsat='n'"
        set="if mfsat='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="mfsat='y'"
        />
      <radio loc="0 0" size="120 24"
        style="PlainText"
        label="Single Frequency"
        vq="mfsat"
        vc="mfsat='n'"
        vc2="mfsat='y'"
        set="if mfsat='n' then $VALUE=1 else $VALUE=0 endif"
        tooltip="mfsat='n'"
        />
    </group>
    <group loc="8 0" size="264 104"
      style="Label2"
      label="TOCSY (DIPSI)"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="16 24" size="104 24"
        style="PlainText"
        label="Mix Time (msec)"
        />
      <entry loc="120 24" size="40 24"
        style="Label2"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="1"
        tooltip="mix=$VALUE/1000"
        />
      <label loc="168 72" size="48 24"
        style="Label1"
        label="ppm)"
        />
      <textmessage loc="136 72" size="32 24"
        style="Label2"
        label="9.9"
        vq="sfrq strength"
        set="$VALUE=trunc(strength*10.0/sfrq)/10"
        digits="1"
        />
      <entry loc="32 72" size="56 24"
        style="Label2"
        vq="strength"
        vc="strength=trunc(10.0*$VALUE)/10 BPspinlock"
        set="$VALUE=strength"
        digits="0"
        tooltip="strength"
        />
      <label loc="88 72" size="48 24"
        style="Label1"
        label="Hz       ("
        />
      <label loc="8 48" size="136 24"
        style="PlainText"
        label="Spinlock Field Strength"
        />
    </group>
    <check loc="8 112" size="128 24"
      style="Label2"
      label="Steady-State Pulse"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      tooltip="sspul='y'"
      />
    <group loc="288 8" size="168 80"
      style="Label2"
      label="Presat"
      vq="satmode mfsat"
      show="if ((satmode='y' ) and (mfsat='n')) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="24 40" size="80 24"
        style="PlainText"
        label="Duration(sec)"
        justify="Left"
        />
      <entry loc="104 40" size="48 24"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        disable="Grayed out"
        tooltip="satdly"
        />
      <entry loc="72 16" size="32 24"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        disable="Grayed out"
        tooltip="satpwr"
        />
      <label loc="24 16" size="48 24"
        style="PlainText"
        label="Power"
        justify="Left"
        />
    </group>
  </group>
</template>
