<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Basic" element="pages" type="acquisition" >
  <group size="728 272"
    style="PlainText"
    label="Basic"
    show="if (BPbasic=1) then $SHOW=1 else $SHOW=0 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Basic"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="25 90" size="90 30"
      vq="ni2"
      show="exists('ni2','parameter'):$SHOW"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="0 0" size="80 24"
        style="Heading2"
        label="F1F3 2D"
        vc="BPsetupF1F3 time"
        bg="VJBackground"
        decor1="no"
        tooltip="BPsetupF1F3"
        />
    </group>
    <group loc="136 176" size="544 56"
      label="single_scan_pw90"
      vq="Console r2 customflag cppauseflg"
      show="if ((Console='vnmrs') and r2=0 and customflag='n' and cppauseflg='n') then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="single_scan_pw90"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="160 0" size="288 24"
        style="Heading2"
        label="Calibrate 1H PW90 for New Sample"
        />
      <button loc="120 32" size="80 24"
        style="PlainText"
        label="Find PW90"
        vc="BPfind1Hpw90"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="272 32" size="200 24"
        style="PlainText"
        label="Update Probefile with this pw90"
        vc="BPH1updatepw90"
        bg="VJBackground"
        decor1="no"
        />
    </group>
    <check loc="592 80" size="128 24"
      style="PlainText"
      label="Show Details"
      vq="r2"
      vc="r2=1 clear man(seqfil)"
      vc2="r2=0"
      />
    <group loc="240 48" size="272 80"
      vq="r1"
      show="if r1=1 then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <group loc="0 48" size="272 24"
        vq="ni2"
        show="exists('ni2','parameter'):$SHOW"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="232 0" size="32 24"
          style="PlainText"
          vq="ni2"
          vc="ni2=$VALUE calcdim time"
          set="$VALUE=ni2"
          digits="0"
          tooltip="ni2"
          />
        <label loc="208 0" size="24 24"
          style="Label1"
          label="to"
          />
        <textmessage loc="168 0" size="32 24"
          style="PlainText"
          label="---"
          vq="ni2"
          set="$VALUE=ni2"
          digits="0"
          />
        <label loc="0 0" size="168 24"
          style="Label1"
          label="Change # t2 increments from"
          />
      </group>
      <entry loc="232 24" size="32 24"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE calcdim time"
        set="$VALUE=ni"
        digits="0"
        tooltip="ni"
        />
      <label loc="208 24" size="24 24"
        style="Label1"
        label="to"
        />
      <textmessage loc="168 24" size="32 24"
        style="PlainText"
        label="---"
        vq="ni"
        set="$VALUE=ni"
        digits="0"
        />
      <label loc="0 24" size="168 24"
        style="Label1"
        label="Change # t1 increments from"
        />
      <entry loc="232 0" size="32 24"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE time"
        set="$VALUE=nt"
        digits="0"
        tooltip="nt"
        />
      <label loc="208 0" size="24 24"
        style="Label1"
        label="to"
        />
      <textmessage loc="168 0" size="32 24"
        style="PlainText"
        label="16"
        vq="nt"
        set="$VALUE=nt"
        digits="0"
        />
      <label loc="0 0" size="152 24"
        style="Label1"
        label="Change # transients from"
        />
    </group>
    <group loc="160 104" size="568 128"
      vq="r2"
      show="if r2=1 then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="376 0" size="80 24"
        style="Heading1"
        label="Manual"
        vc="clear man(seqfil)"
        bg="VJBackground"
        decor1="no"
        />
      <button loc="480 0" size="80 24"
        style="Heading1"
        label="Clear"
        vc="clear"
        bg="VJBackground"
        decor1="no"
        />
      <textfile loc="0 24" size="568 104"
        style="PlainText"
        vq="alphatext"
        set="alphatextname:$VALUE"
        editable="yes"
        wrap="yes"
        units="char"
        />
    </group>
    <button loc="24 56" size="80 24"
      style="Heading2"
      label="1D"
      vc="BPsetup1D time"
      bg="VJBackground"
      decor1="no"
      tooltip="BPsetup1D"
      />
    <group loc="24 176" size="88 32"
      vq="ni2"
      show="exists('ni2','parameter'):$SHOW"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="0 0" size="80 24"
        style="Heading2"
        label="3D"
        vc="BPsetup3D time"
        bg="VJBackground"
        decor1="no"
        tooltip="BPsetup3D"
        />
    </group>
    <group loc="24 136" size="88 32"
      vq="ni2"
      show="exists('ni2','parameter'):$SHOW"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="0 0" size="80 24"
        style="Heading2"
        label="F2F3 2D"
        vc="BPsetupF2F3 time"
        bg="VJBackground"
        decor1="no"
        tooltip="BPsetupF2F3"
        />
    </group>
    <group loc="24 88" size="88 32"
      vq="ni2 ni"
      show="exists('ni2','parameter'):$e2 exists('ni','parameter'):$e1 if (($e2=0) and ($e1=1)) then  $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <button loc="0 0" size="80 24"
        style="Heading2"
        label="F1F2 2D"
        vc="BPsetupF1F2 time"
        bg="VJBackground"
        decor1="no"
        tooltip="BPsetupF1F2"
        />
    </group>
    <button loc="560 8" size="152 56"
      style="Heading5"
      label="Begin Acquiring"
      vq="arraydim customflag cppauseflg"
      vc="if arraydim&gt;1 then go else ga endif time"
      show="$ENABLE=((cppauseflg='n') and (customflag='n'))*2-1"
      bg="VJBackground"
      decor1="no"
      />
    <label loc="480 24" size="80 24"
      style="Heading2"
      label="-------- &gt;"
      />
    <check loc="216 24" size="256 24"
      style="Heading2"
      label="Customize Conditions (optional)"
      vq="r1"
      vc="r1=1"
      vc2="r1=0"
      set="$VALUE=r1"
      />
    <label loc="144 24" size="80 24"
      style="Heading2"
      label="-------- &gt;"
      />
    <label loc="8 24" size="128 24"
      style="Heading2"
      label="Pick Experiment"
      />
    <textmessage loc="136 0" size="304 24"
      style="Protocol"
      label="cyclenoef"
      vq="seqfil"
      set="$VALUE=seqfil"
      />
    <label loc="8 0" size="128 24"
      style="Protocol"
      label="EXPERIMENT:"
      />
  </group>
</template>
