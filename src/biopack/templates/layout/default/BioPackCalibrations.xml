<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="BioPackCalibrations" element="pages" type="sample" >
  <group size="800 225"
    style="PlainText"
    label="Calibrations"
    vq="system panellevel cppauseflg customflag"
    show="$VALUE = ((system='spectrometer') and (panellevel &gt; 29) and (cppauseflg='n') and (customflag='n'))"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="BioPackCalibrations"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 160" size="235 50"
      style="Heading2"
      label="Single-scan 1H pw90"
      vq="Console r2 customflag cppauseflg"
      show="if ((Console='vnmrs') and r2=0 and customflag='n' and cppauseflg='n') then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="single_scan_pw90"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <button loc="85 20" size="120 25"
        style="PlainText"
        label="Update Probefile"
        vc="BPH1updatepw90"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="50 25"
        style="PlainText"
        label="Find"
        vc="BPfind1Hpw90"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <button loc="560 190" size="240 25"
      style="PlainText"
      label="Options on this Page    (Text Out Page)"
      vc="clear man('BPcalibrations')"
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <group loc="470 165" size="325 20"
      vq="BPplot"
      show="if BPplot='file' then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="BPPostScript"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="0 0" size="285 25"
        style="Label2"
        label="(PostScript only. Plots stored in AutoTripRes* fle)"
        justify="Left"
        />
    </group>
    <group loc="245 130" size="185 80"
      style="Heading2"
      label="Flipback Pulses"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FlipbackPulses"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="25 55" size="130 20"
        style="Label1"
        label="Use Any H2O Sample"
        justify="Left"
        />
      <button loc="25 25" size="130 25"
        style="Label4"
        label="Calibrate/Create"
        vc="r7=8 BP6a"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="245 0" size="185 125"
      style="Heading2"
      label="Calibrate 2H Decoupling"
      vq="numrfch Console"
      show="if ((numrfch=4) and (Console='vnmrs')) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="2HDecoupling"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="15 95" size="160 25"
        style="Label1"
        label="(Select &quot;Text Out&quot; Page)"
        justify="Left"
        />
      <button loc="40 60" size="105 25"
        style="PlainText"
        label="Instructions"
        vc="man('ddec_cal1')"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="15 25" size="60 20"
        style="Label1"
        label="Channel:"
        />
      <button loc="125 20" size="30 25"
        style="Label4"
        label="4"
        vc="BPddec4a_cal"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="85 20" size="30 25"
        style="Label4"
        label="3"
        vc="BPddec3a_cal"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="455 140" size="325 25"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="BPPlotting2:"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <radio loc="75 0" size="80 25"
        style="PlainText"
        label="No Plots"
        vq="BPplot"
        vc="BPplot='off'"
        set="if BPplot='off' then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="175 0" size="60 25"
        style="PlainText"
        label="Plots"
        vq="BPplot"
        vc="BPplot='plot'"
        set="if BPplot='plot' then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="245 0" size="90 25"
        style="Label1"
        label="Plot to File"
        vq="BPplot"
        vc="BPplot='file'"
        set="if BPplot='file' then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="0 0" size="65 25"
        style="Heading2"
        label="Plotting:"
        justify="Left"
        />
    </group>
    <group loc="455 0" size="345 135"
      style="Heading2"
      label="Automated Triple-Resonance Experiments"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AutomatedExperiments"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="215 60" size="120 20"
        style="Label1"
        label="Automatic Plot"
        vq="BP2Dplot"
        vc="BP2Dplot=1"
        vc2="BP2Dplot=0"
        set="if BP2Dplot=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <button loc="215 110" size="120 20"
        style="PlainText"
        label="Plot Auto BEST Now"
        vc="BP2Dplot=1 BPauto_2DplotBEST BP2Dplot=0"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="150 110" size="55 20"
        style="Label4"
        label="BEST"
        vc="BPauto2dBEST(.1,1)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="215 90" size="120 20"
        style="PlainText"
        label="Plot Auto 2D Now"
        vc="BPauto_2Dplot"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <check loc="215 40" size="130 20"
        style="Label1"
        label="Fully Deuterated"
        vq="BPdeuterated"
        vc="BPdeuterated=1"
        vc2="BPdeuterated=0"
        set="if BPdeuterated=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="190 20" size="60 20"
        style="Label1"
        label="Auto 2D"
        />
      <button loc="150 90" size="55 20"
        style="Label4"
        label="16 hrs"
        vc="BPauto2d1(3)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="150 65" size="55 20"
        style="Label4"
        label="8 hrs"
        vc="BPauto2d1(2)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="150 40" size="55 20"
        style="Label4"
        label="4 hrs"
        vc="BPauto2d1(1)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="45 20" size="60 20"
        style="Label1"
        label="Auto 1D"
        />
      <button loc="5 90" size="130 20"
        style="Label4"
        label="Use this tpwr/pw"
        vc="BPauto1d(2,0)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="5 65" size="130 20"
        style="Label4"
        label="First Calibrate 1H"
        vc="BPauto1d(3,0)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="5 40" size="130 20"
        style="Label4"
        label="Standard"
        vc="BPauto1d(1,0)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="245 0" size="195 120"
      style="Heading2"
      label="Calibrate 2H Decoupling"
      vq="numrfch Console"
      show="if ((numrfch&gt;4) and (Console='vnmrs')) then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="2HDecoupling"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="15 85" size="160 25"
        style="Label1"
        label="(Select &quot;Text Out&quot; Page)"
        justify="Left"
        />
      <button loc="35 60" size="105 25"
        style="PlainText"
        label="Instructions"
        vc="man('ddec_cal1')"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="15 25" size="60 20"
        style="Label1"
        label="Channel:"
        />
      <button loc="85 25" size="25 25"
        style="Label4"
        label="3"
        vc="BPddec3a_cal"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="115 25" size="30 25"
        style="Label4"
        label="4"
        vc="BPddec4a_cal"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="155 25" size="30 25"
        style="Label4"
        label="5"
        vc="BPddec5a_cal"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="0 0" size="235 155"
      style="Heading2"
      label="AutoCalibrate"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <selmenu loc="10 35" size="220 20"
        style="Label4"
        label="Make a selection"
        vc="exec('$VALUE')"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Full, Using Probefile Values"
          chval="BPAutoProteinCal"
          />
        <mlabel 
          label="Full: Use Current 1H Offset and pw"
          chval="BPAutoProteinCal(3)"
          />
        <mlabel 
          label="Full RF (Faster)"
          chval="BPAutoProteinCal(4)"
          />
        <mlabel 
          label="RF and Gradients (Fastest)"
          chval="BPAutoProteinCal(9)"
          />
        <mlabel 
          label="1H pw90 only"
          chval="BPAutoProteinCal(5)"
          />
      </selmenu>
      <button loc="20 125" size="175 20"
        style="Label4"
        label="1H and 15N PW90 Only"
        vc="BPAutoProteinCal(6)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="5 100" size="185 25"
        style="Label1"
        label="15N  Labeled Compound:"
        justify="Left"
        />
      <button loc="20 80" size="175 20"
        style="Label4"
        label="1H and 13C PW90 Only"
        vc="BPAutoProteinCal(7)"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="10 55" size="185 25"
        style="Label1"
        label="13C  Labeled Compound:"
        justify="Left"
        />
      <label loc="10 15" size="185 25"
        style="Label1"
        label="13C/15N Labeled Compound"
        justify="Left"
        />
    </group>
  </group>
</template>
