<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Shim_BioPack" element="pages" type="sample" >
  <group size="825 220"
    style="PlainText"
    label="Shim"
    vq="system BPclassic"
    vc="sethw('statrate',200):$x"
    vc2="sethw('statrate',5000):$x"
    show="$VALUE = (system='spectrometer') if BPclassic='n' then $VALUE=0 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Shim_BioPack"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 25" size="90 190"
      style="PlainText"
      vq="acqmode"
      show="if acqmode='fidscan' then $VALUE=0 else $VALUE=1 endif"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <shimbutton loc="0 160" size="80 30"
        style="Label4"
        label="Z0"
        vc="setshim('z0',$VALUE)"
        min="-32768"
        max="32768"
        statpar="z0"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="0 120" size="80 32"
        style="Label4"
        label="Spin Rate"
        vq="spin"
        vc="sethw('nowait','spin',$VALUE):$x spin=$VALUE"
        vq2="spin"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="0 80" size="80 32"
        style="Label4"
        label="Lock Phase"
        vc="sethw('nowait','lockphase',$VALUE):$x lockphase=$VALUE"
        vq2="lockphase"
        statpar="lockphase"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="true"
        />
      <shimbutton loc="0 40" size="80 32"
        style="Label4"
        label="Lock Gain"
        vc="sethw('nowait','lockgain',$VALUE):$x lockgain=$VALUE"
        vq2="lockgain"
        statpar="lockgain"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="0 0" size="80 32"
        style="Label4"
        label="Lock Power"
        vc="sethw('nowait','lockpower',$VALUE):$x lockpower=$VALUE"
        vq2="lockpower"
        statpar="lockpower"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
    </group>
    <group loc="100 190" size="145 25"
      style="PlainText"
      vq="acqmode"
      show="$VALUE = (acqmode = 'fidscan')"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <textmessage loc="88 0" size="56 24"
        style="Heading2"
        label="0.0"
        vq="fidarea"
        set="$VALUE = fidarea"
        digits="1"
        />
      <label loc="0 0" size="64 24"
        style="Label1"
        label="FID area:"
        justify="Left"
        />
    </group>
    <check loc="135 170" size="65 25"
      style="Heading1"
      label="Lock"
      vq="system"
      vc="sethw('lock','on'):$xxx"
      vc2="sethw('lock','off'):$xxx"
      show="is_spectrometer:$VALUE"
      statpar="lockon"
      />
    <group loc="5 35" size="80 180"
      style="PlainText"
      label="Fidscangroup"
      vq="acqmode"
      show="$VALUE = (acqmode = 'fidscan')"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Fidscangroup"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <toggle loc="0 160" size="80 20"
        style="PlainText"
        label="DC Correct"
        vq="dcrmv"
        vc="dcrmv='y' fidarea:fidarea"
        vc2="dcrmv='n' fidarea:fidarea"
        set="$VALUE = (dcrmv = 'y')"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="0 140" size="80 20"
        style="PlainText"
        label="Dots"
        vq="dotflag"
        vc="dotflag='y'"
        vc2="dotflag='n'"
        set="$VALUE=(dotflag = 'y')"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="0 120" size="80 20"
        style="PlainText"
        label="Envelope"
        vq="fidscanmode"
        vc="displaymode('+e', 'fidscanmode')"
        vc2="displaymode('-e', 'fidscanmode')"
        set="strstr(fidscanmode,'e'):$VALUE"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="0 90" size="80 20"
        style="PlainText"
        label="Abs Value"
        vq="fidscanmode"
        vc="displaymode('+a', 'fidscanmode')"
        vc2="displaymode('-a', 'fidscanmode')"
        set="strstr(fidscanmode,'a'):$VALUE"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="0 70" size="80 20"
        style="PlainText"
        label="Imaginary"
        vq="fidscanmode"
        vc="displaymode('+i', 'fidscanmode')"
        vc2="displaymode('-i', 'fidscanmode')"
        set="strstr(fidscanmode,'i'):$VALUE"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <toggle loc="0 50" size="80 20"
        style="PlainText"
        label="Real"
        vq="fidscanmode"
        vc="displaymode('+r', 'fidscanmode')"
        vc2="displaymode('-r', 'fidscanmode')"
        set="strstr(fidscanmode,'r'):$VALUE"
        enable="yes"
        radiobutton="no"
        subtype="no"
        />
      <radio loc="0 20" size="80 20"
        style="PlainText"
        label="FID"
        vq="fidscanmode"
        vc="displaymode('-s', 'fidscanmode')"
        set="strstr(fidscanmode,'s'):$e $VALUE=(not $e)"
        />
      <radio loc="0 0" size="80 20"
        style="PlainText"
        label="Spectrum"
        vq="fidscanmode"
        vc="displaymode('+s', 'fidscanmode')"
        set="strstr(fidscanmode,'s'):$VALUE"
        />
    </group>
    <dial loc="88 24" size="152 144"
      style="PlainText"
      digital="yes"
      min="0.0"
      max="100.0"
      minmark="no"
      maxmark="yes"
      statpar="lklvl"
      count="2"
      color1="VJInactiveBG"
      color3="black"
      color4="0x999999"
      color5="0xbb00ff"
      color6="blue"
      color7="cyan"
      color8="0x00bb00"
      color9="0x99ff99"
      color10="yellow"
      color11="orange"
      color12="red"
      decor1="yes"
      decor2="no"
      elastic="no"
      />
    <label loc="8 0" size="80 24"
      style="Heading1"
      label="Shim"
      justify="Left"
      />
    <shimset loc="248 0" size="568 208"
      style="PlainText"
      vq="shimset"
      vc="setshim('$NAME',$VALUE)"
      set="$VALUE=shimset"
      border="LoweredBevel"
      side="Top"
      justify="Left"
      tab="no"
      savekids="false"
      statpar="$NAME"
      pointy="false"
      rocker="true"
      arrow="true"
      enable="no"
      reference="shm"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      />
    <label loc="96 0" size="120 24"
      style="Heading2"
      label="Lock Amplitude"
      justify="Left"
      />
  </group>
</template>
