<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="FID" element="pages" type="display" >
  <group size="704 232" style="PlainText" label="FID"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    reference="FID"
    useref="no"
    expanded="yes"
    >
    <label loc="8 0" size="176 24" style="Heading2"
      label="FID"
      justify="Left"
      />
    <entry loc="176 200" size="40 24" style="PlainText"
      vq="vpfi"
      vc="vpfi=$VALUE"
      set="$VALUE=vpfi"
      digits="1"
      />
    <label loc="8 200" size="168 24" style="PlainText"
      label="Vertical Position (Imaginary)"
      />
    <entry loc="152 176" size="40 24" style="PlainText"
      vq="vpf"
      vc="vpf=$VALUE"
      set="$VALUE=vpf"
      digits="1"
      />
    <label loc="8 176" size="144 24" style="PlainText"
      label="Vertical Position (Real)"
      />
    <entry loc="96 136" size="48 24" style="PlainText"
      vq="vf"
      vc="vf=$VALUE"
      set="$VALUE=vf"
      digits="1"
      />
    <label loc="8 136" size="88 24" style="PlainText"
      label="Vertical Scale"
      />
    <label loc="144 88" size="24 24" style="PlainText"
      label="sec"
      />
    <entry loc="88 88" size="56 24" style="PlainText"
      vq="wf"
      vc="wf=$VALUE"
      set="$VALUE=wf"
      digits="3"
      />
    <label loc="8 88" size="64 24" style="PlainText"
      label="Length"
      />
    <label loc="144 64" size="24 24" style="PlainText"
      label="sec"
      />
    <entry loc="88 64" size="56 24" style="PlainText"
      vq="sf"
      vc="sf=$VALUE"
      set="$VALUE=sf"
      digits="3"
      />
    <label loc="8 64" size="80 24" style="PlainText"
      label="Starting Time"
      />
    <label loc="144 40" size="24 24" style="PlainText"
      label="sec"
      />
    <entry loc="88 40" size="56 24" style="PlainText"
      vq="crf"
      vc="cfr=$VALUE"
      set="$VALUE=crf"
      digits="3"
      />
    <label loc="8 40" size="80 24" style="PlainText"
      label="Set Cursor at"
      />
    <radio loc="368 72" size="80 24" style="PlainText"
      label="line"
      vq="dotflag"
      vc="dotflag='n'"
      vc2="dotflag='y'"
      set="if dotflag='n' then $VALUE=1 else $VALUE=0 endif"
      />
    <radio loc="368 48" size="80 24" style="PlainText"
      label="dots"
      vq="dotflag"
      vc="dotflag='y'"
      vc2="dotflag='n'"
      set="if dotflag='y' then $VALUE=1 else $VALUE=0 endif"
      />
    <label loc="344 24" size="64 24" style="PlainText"
      label="Mode"
      />
    <group loc="480 24" size="112 104" style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      useref="no"
      >
      <radio loc="8 24" size="96 24" style="PlainText"
        label="Seconds"
        vq="axisf"
        vc="axisf='s'"
        />
      <radio loc="8 72" size="104 24" style="PlainText"
        label="microseconds"
        vq="axisf"
        vc="axisf='u'"
        />
      <radio loc="8 48" size="96 24" style="PlainText"
        label="Milliseconds"
        vq="axisf"
        vc="axisf='m'"
        />
      <label loc="16 0" size="64 24" style="PlainText"
        label="Axis"
        />
    </group>
  </group>
</template>
