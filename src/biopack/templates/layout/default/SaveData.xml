<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="SaveData" element="pages" type="plotting" >
  <group size="763 276"
    style="PlainText"
    label="Save Data"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="SaveData"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 10" size="755 245"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FutureActions"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="585 20" size="160 25"
        style="Label2"
        label="Create This Directory Now"
        vq="svfdir"
        vc="mkdir(svfdir)"
        decor1="No"
        />
      <button loc="585 60" size="160 25"
        style="Ready"
        label="Save FID Now"
        vq="svfdir svfname"
        vc="BPsvf(svfdir+'/'+svfname)"
        decor1="No"
        />
      <button loc="585 100" size="160 25"
        style="Ready"
        label="Save Parameter Set Now"
        vq="svfdir svfname"
        vc="svp(svfdir+'/'+svfname)"
        decor1="No"
        />
      <button loc="585 135" size="160 25"
        style="Label3"
        label="Recall Parameter Set Now"
        vq="svfdir svfname"
        vc="rtp(svfdir+'/'+svfname)"
        decor1="No"
        />
      <button loc="585 175" size="160 25"
        style="Label3"
        label="Recall FID Now"
        vq="svfdir svfname"
        vc="rt(svfdir+'/'+svfname)"
        decor1="No"
        />
      <entry loc="145 65" size="400 25"
        style="PlainText"
        vq="svfname"
        vc="svfname='$VALUE'"
        set="$VALUE=svfname"
        show="if svfname='' then $VALUE=0 else $VALUE=1 endif"
        />
      <label loc="10 65" size="135 25"
        style="PlainText"
        label="Stored under the name"
        justify="Left"
        />
      <check loc="10 140" size="350 25"
        style="PlainText"
        label="Plot When Acquisition Finishes (in automation mode)"
        vc="pltmod='fixed'"
        vc2="pltmod='off'"
        set="if (pltmod='off') then $VALUE=0 else $VALUE=1 endif"
        />
      <group loc="10 180" size="530 58"
        style="PlainText"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <check loc="0 8" size="144 24"
          style="Label3"
          label="Automatic FID save"
          vc="dofidsave='y'"
          vc2="dofidsave='n'"
          set="if dofidsave='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <textmessage loc="160 32" size="360 24"
          style="PlainText"
          label="/export/home/DATA/800coldprobe_mar2004//c13hsqc01.fid"
          set="svfname:$VALUE"
          />
        <label loc="24 32" size="128 24"
          style="PlainText"
          label="Autosave FID name:"
          justify="Left"
          />
      </group>
      <entry loc="144 24" size="400 24"
        style="PlainText"
        vq="svfdir"
        vc="svfdir='$VALUE'"
        set="$VALUE=svfdir"
        show="if svfdir='' then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        />
      <label loc="8 24" size="136 24"
        style="PlainText"
        label="Storage Directory"
        justify="Left"
        />
      <label loc="0 0" size="208 24"
        style="Heading2"
        label="FID / Parameter Set Storage"
        />
    </group>
  </group>
</template>
