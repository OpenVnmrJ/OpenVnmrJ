<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="BioPack_Options" element="pages" type="sample" >
  <group size="865 220"
    style="PlainText"
    label="BioPack Options"
    vq="system panellevel cppauseflg customflag BPclassic"
    show="$VALUE = ((system='spectrometer') and (panellevel &gt; 29) and (cppauseflg='n') and (customflag='n'))"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="BioPack_Options"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="580 25" size="275 195"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <group loc="5 90" size="240 25"
        vq="maxattench1"
        show="exists('maxattench1','parameter','global'):$e if $e then $VALUE=0 else $VALUE=1 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="225 25"
          style="PlainText"
          label="Activate Absolute Power Limits"
          vc="BPmaxattench"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <check loc="0 25" size="210 25"
        style="PlainText"
        label="Show Basic Panel in Acquire List"
        vq="BPbasic"
        vc="BPbasic=1 BPlimitupdate"
        vc2="BPbasic=0 BPlimitupdate"
        set="if BPbasic=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="0 0" size="210 25"
        style="PlainText"
        label="BioPackExpress"
        vq="BPexpress_gui"
        vc="BPexpress_gui=1 "
        vc2="BPexpress_gui=0 "
        set="if BPexpress_gui=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="0 45" size="200 25"
        style="PlainText"
        label="Use gCfhsqc in AutoCalibrates"
        vq="BPquick"
        vc="BPquick=1 BPlimitupdate"
        vc2="BPquick=0 BPlimitupdate"
        set="if BPquick then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="0 70" size="265 25"
        style="PlainText"
        label="Use &quot;classic&quot; panels for Lock/Temp/Shim"
        vq="BPclassic"
        vc="BPclassic='y' "
        vc2="BPclassic='n' "
        set="if BPclassic='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 115" size="255 25"
        vq="probeConnect Console"
        show="exists('probeConnect','parameter','global'):$e  if ($e=0 and Console='vnmrs') then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="224 24"
          style="PlainText"
          label="Activate probeConnect Option"
          vq="probeConnect"
          vc="create('probeConnect','string','global') probeConnect='' addparams('probeConnect',probeConnect,'H1') BPlimitupdate"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="0 0" size="216 24"
          style="PlainText"
          label="Activate probeConnect Option"
          vq="probeConnect"
          vc="create('probeConnect','string','global') probeConnect=''"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <group loc="5 115" size="240 25"
        vq="probeConnect Console"
        show="exists('probeConnect','parameter','global'):$e   if ($e and Console='vnmrs') then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="90 0" size="145 25"
          style="PlainText"
          vq="probeConnect"
          vc="probeConnect='$VALUE' BPlimitupdate"
          set="$VALUE=probeConnect"
          digits="0"
          disable="Grayed out"
          />
        <label loc="0 0" size="88 24"
          style="Label1"
          label="probeConnect"
          />
      </group>
      <group loc="5 140" size="240 25"
        vq="gradientdisable"
        show="exists('gradientdisable','parameter','global'):$e if $e then $SHOW=0 else $SHOW=1 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="224 24"
          style="PlainText"
          label="Activate Disabling Gradient Option"
          vq="gradientdisable"
          vc="create('gradientdisable','string','global') gradientdisable='n' addparams('gradientdisable',gradientdisable,'H1') BPlimitupdate "
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <group loc="5 140" size="225 25"
        vq="gradientdisable"
        show="exists('gradientdisable','parameter','global'):$e if $e then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <check loc="0 0" size="215 20"
          style="PlainText"
          label="Disable Gradients in Acquisition"
          vq="gradientdisable"
          vc="gradientdisable='y' BPlimitupdate"
          vc2="gradientdisable='n' BPlimitupdate"
          set="if gradientdisable='y' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <group loc="5 165" size="255 25"
        vq="gradientshaping"
        show="exists('gradientshaping','parameter','global'):$e if $e then $SHOW=0 else $SHOW=1 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="0 3" size="224 24"
          style="PlainText"
          label="Activate Gradient Shaping Option"
          vq="gradientshaping"
          vc="create('gradientshaping','string','global') gradientshaping='y' addparams('gradientshaping',gradientshaping,'H1') BPlimitupdate"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <group loc="5 165" size="250 25"
        vq="gradientshaping"
        show="exists('gradientshaping','parameter','global'):$e if $e then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <check loc="0 0" size="240 25"
          style="PlainText"
          label="Automatic Gradient  Shaping"
          vq="gradientshaping"
          vc="gradientshaping='y' BPlimitupdate"
          vc2="gradientshaping='n' BPlimitupdate"
          set="if gradientshaping='y' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
    </group>
    <label loc="575 0" size="80 15"
      style="Heading2"
      label="Options"
      justify="Left"
      />
    <group loc="340 0" size="185 105"
      style="Heading2"
      label="Database"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="BioPackDatabase"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="90 80" size="80 25"
        style="PlainText"
        vq="owner"
        vc="setvalue('owner','$VALUE','global')"
        set="getvalue('owner','global'):$VALUE"
        disable="Grayed out"
        />
      <label loc="10 80" size="50 25"
        style="Label1"
        label="Owner"
        justify="Left"
        />
      <label loc="10 20" size="65 25"
        style="Label1"
        label="Operator"
        justify="Left"
        />
      <label loc="10 50" size="80 25"
        style="Label1"
        label="Investigator"
        justify="Left"
        />
      <entry loc="90 50" size="80 25"
        style="PlainText"
        vq="investigator"
        vc="setvalue('investigator','$VALUE','global')"
        set="getvalue('investigator','global'):$VALUE"
        disable="Grayed out"
        />
      <textmessage loc="90 20" size="80 25"
        style="Label1"
        vq="operator"
        set="$VALUE=operator"
        />
    </group>
    <group loc="0 0" size="330 220"
      style="Heading2"
      label="FID Noise Tests"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="FIDNoise"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="5 150" size="240 25"
        style="PlainText"
        label="Do Automatic FID Noise Check at &quot;go&quot;"
        vq="BPautonoisecheck"
        vc="BPautonoisecheck=1 BPlimitupdate"
        vc2="BPautonoisecheck=0 BPlimitupdate"
        set="if BPautonoisecheck=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="5 90" size="185 25"
        style="PlainText"
        label="FID Noise with/without Sample:"
        justify="Left"
        />
      <group loc="5 120" size="320 25"
        vq="sample_noise empty_noise"
        show="if ((sample_noise&gt;0.0) and (empty_noise&gt;0.0)) then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <textmessage loc="264 0" size="48 24"
          style="PlainText"
          label="0.00"
          vq="sample_noise empty_noise"
          set="if empty_noise&gt;0.0 then $VALUE=sample_noise/empty_noise else $VALUE=0.0 endif"
          digits="2"
          />
        <label loc="0 0" size="264 24"
          style="PlainText"
          label="Ratio of Sample Noise to Empty Probe Noise:"
          justify="Left"
          />
      </group>
      <textmessage loc="260 90" size="50 25"
        style="PlainText"
        vq="empty_noise"
        set="$VALUE=empty_noise"
        digits="2"
        />
      <textmessage loc="200 90" size="50 25"
        style="PlainText"
        vq="sample_noise"
        set="$VALUE=sample_noise"
        digits="2"
        />
      <label loc="140 55" size="180 25"
        style="PlainText"
        label="(Use Eject Sample Button first)"
        justify="Left"
        />
      <button loc="20 60" size="110 20"
        style="PlainText"
        label="Empty Probe"
        vq="empty_noise"
        vc="BPchecknoise('e'):empty_noise"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="20 40" size="110 20"
        style="PlainText"
        label="Sample in Probe"
        vq="_noise"
        vc="BPchecknoise('s'):sample_noise"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="80 195" size="150 20"
        style="PlainText"
        label="Manual (Use Text Out)"
        vc="clear man('BPLock')"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <label loc="10 20" size="145 25"
        style="PlainText"
        label="Measure FID Noise for:"
        justify="Left"
        />
    </group>
    <group loc="345 195" size="230 25"
      style="PlainText"
      vq="acqmode"
      show="$VALUE = (acqmode = '')"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <button loc="0 0" size="224 24"
        style="PlainText"
        label="Auto-find best z1 during decoupling"
        vc="BPfindz1"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="340 110" size="230 75"
      style="Heading2"
      label="Hardware Shimming"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="BioPackHardwareShimming"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="15 50" size="50 25"
        style="PlainText"
        label="Value:"
        justify="Left"
        />
      <textmessage loc="55 50" size="25 25"
        style="PlainText"
        vq="hdwshim"
        set="$VALUE=hdwshim"
        />
      <entry loc="160 50" size="70 25"
        style="PlainText"
        vq="hdwshimlist hdwshim"
        vc="hdwshimlist='$VALUE'"
        set="$VALUE=hdwshimlist"
        disable="Grayed out"
        />
      <label loc="85 50" size="100 20"
        style="PlainText"
        label="Active Shims"
        justify="Left"
        />
      <selmenu loc="10 20" size="185 25"
        style="PlainText"
        label="Set Background Shimming"
        vq="hdwshim"
        vc="hdwshim='$VALUE'"
        set="$VALUE=hdwshim"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="None"
          chval="n"
          />
        <mlabel 
          label="During Delay"
          chval="y"
          />
        <mlabel 
          label="During Presat"
          chval="p"
          />
      </selmenu>
    </group>
  </group>
</template>
