<actionpanel>
  <group size="600 20" fg="PlainText" bg="transparent" border="None" side="Top"
    vq="customflag cppauseflg" show="$VALUE=(customflag='n')"
    justify="Left" tab="no" reference="plot_action" useref="no"
   >
  <button value="Show time" vc="time" style="Heading2"
      point="12" font="Dialog" />
  <button value="Display spectrum" vc="ds" style="Heading2"
      point="12" font="Dialog" />
  <button value="Stacked display" vc="dssh" style="Heading2"
      point="12" font="Dialog" />
  <button value=" Display text " vc="dtext" style="Heading2"
      point="12" font="Dialog" />
  <button value=" Cancel " vc="M@cancel" style="Heading2"
      point="12" font="Dialog" />
  </group>
</actionpanel>
