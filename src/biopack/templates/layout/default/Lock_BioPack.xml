<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Lock_BioPack" element="pages" type="sample" >
  <group size="775 215"
    style="PlainText"
    label="Lock"
    vq="system BPclassic"
    vc="sethw('statrate',200):$x"
    vc2="sethw('statrate',5000):$x"
    show="$VALUE = (system='spectrometer') if BPclassic='n' then $VALUE=0 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Lock_BioPack"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="160 5" size="510 155"
      style="Label2"
      label="Lock Control"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <shimbutton loc="5 110" size="95 25"
        style="Heading2"
        label="Gain"
        vc="sethw('nowait','lockgain',$VALUE):$x lockgain=$VALUE"
        vq2="lockgain"
        statpar="lockgain"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="5 20" size="95 25"
        style="Heading2"
        label="Z0"
        vc="sethw('nowait','z0',$VALUE):$x z0=$VALUE"
        vq2="z0"
        statpar="z0"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <shimbutton loc="5 50" size="95 25"
        style="Heading2"
        label="Phase"
        vc="sethw('nowait','lockphase',$VALUE):$x lockphase=$VALUE"
        vq2="lockphase"
        statpar="lockphase"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="true"
        />
      <shimbutton loc="5 80" size="95 25"
        style="Heading2"
        label="Power"
        vc="sethw('nowait','lockpower',$VALUE):$x lockpower=$VALUE"
        vq2="lockpower"
        statpar="lockpower"
        pointy="false"
        rocker="true"
        arrow="true"
        arrowcolor="GraphForeground"
        wrap="false"
        />
      <slider loc="110 20" size="380 25"
        style="PlainText"
        vc="setshim('z0',$VALUE)"
        digital="true"
        vq2="z0"
        digits="8"
        statpar="z0"
        drag="0"
        />
      <slider loc="110 50" size="380 25"
        style="PlainText"
        vq="system"
        vc="sethw('nowait','lockphase',$VALUE):$xxx lockphase=$VALUE"
        set="$VALUE=lockphase"
        show="is_spectrometer:$VALUE"
        digital="false"
        vq2="lockphase"
        digits="8"
        statpar="lockphase"
        />
      <slider loc="110 110" size="380 25"
        style="PlainText"
        vc="sethw('nowait','lockgain',$VALUE):$xxx lockgain=$VALUE"
        digital="true"
        vq2="lockgain"
        digits="8"
        statpar="lockgain"
        />
      <slider loc="110 80" size="380 25"
        style="PlainText"
        vc="sethw('nowait','lockpower',$VALUE):$xxx lockpower=$VALUE"
        digital="false"
        vq2="lockpower"
        digits="8"
        statpar="lockpower"
        />
    </group>
    <dial loc="5 25" size="145 145"
      style="PlainText"
      digital="yes"
      min="0.0"
      max="100.0"
      minmark="no"
      maxmark="yes"
      statpar="lklvl"
      count="1"
      color1="Label1"
      color3="black"
      color4="0x999999"
      color5="0xbb00ff"
      color6="blue"
      color7="cyan"
      color8="0x00bb00"
      color9="0x99ff99"
      color10="yellow"
      color11="orange"
      color12="red"
      decor1="yes"
      decor2="no"
      elastic="no"
      />
    <button loc="160 170" size="100 25"
      style="Heading2"
      label="Find z0"
      vc="doifidle('findz0_au')"
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <button loc="470 170" size="200 25"
      style="Heading2"
      label="Gradient Autoshim on Z"
      vc="gmapshim"
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <button loc="265 170" size="200 25"
      style="Heading2"
      label="Setup Gradient Shimming"
      vq="panellevel"
      vc="exists('panellevel','parameter','global'):$e if $e then panellevel=50 endif gmapsys"
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <check loc="0 0" size="128 24"
      style="Heading1"
      label="Activate Lock"
      vq="system"
      vc="sethw('lock','on'):$xxx"
      vc2="sethw('lock','off'):$xxx"
      show="is_spectrometer:$VALUE"
      statpar="lockon"
      />
  </group>
</template>
