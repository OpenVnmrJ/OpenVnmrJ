<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="1DOptions" element="pages" type="display" >
  <group size="752 256"
    style="PlainText"
    label="1D Options"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="1DOptions"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <radio loc="385 70" size="90 25"
      style="PlainText"
      label="Cancel DC"
      vc="cdc"
      />
    <group loc="616 8" size="136 184"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <radio loc="0 128" size="136 24"
        style="PlainText"
        label="TMS/DSS by cursor"
        vc="rl(0)"
        />
      <radio loc="0 96" size="80 24"
        style="PlainText"
        label="Cancel"
        vc="$rfl=rfl crl sp=sp + $rfl"
        />
      <radio loc="0 64" size="88 24"
        style="PlainText"
        label="By TMS"
        vc="setref tmsref sp=sp-rfl ds"
        />
      <radio loc="0 32" size="88 24"
        style="PlainText"
        label="By Solvent"
        vc="setref sp=sp-rfl ds"
        />
      <label loc="0 0" size="112 24"
        style="Heading2"
        label="AutoReference"
        />
    </group>
    <group loc="376 8" size="96 128"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <radio loc="8 96" size="80 24"
        style="PlainText"
        label="BC"
        vc="bc"
        />
      <radio loc="8 32" size="80 24"
        style="PlainText"
        label="DC"
        vc="dc"
        />
      <label loc="8 0" size="72 24"
        style="Heading2"
        label="Baseline"
        />
    </group>
    <button loc="448 184" size="144 32"
      style="Heading5"
      label="Display Text"
      vc="dtext"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <group loc="304 152" size="96 96"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <button loc="8 32" size="80 24"
        style="Heading4"
        label="Full"
        vc="aph ds rp=rp lp=lp"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="8 64" size="80 24"
        style="Heading4"
        label="Zero Order"
        vc="aph0 ds rp=rp"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="8 0" size="80 24"
        style="Heading2"
        label="AutoPhase"
        />
    </group>
    <button loc="56 184" size="208 32"
      style="Heading5"
      label="Show Whole Spectrum"
      vc="f ds"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <group loc="496 8" size="104 104"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <radio loc="8 32" size="80 24"
        style="PlainText"
        label="Full Scale"
        vc="vsadj ds"
        />
      <radio loc="8 64" size="80 24"
        style="PlainText"
        label="x10"
        vq="vs"
        vc="vsadj vs=vs*10 ds"
        />
      <label loc="8 0" size="88 24"
        style="Heading2"
        label="AutoScale"
        />
    </group>
    <group loc="256 8" size="96 144"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <radio loc="0 32" size="80 24"
        style="PlainText"
        label="Hertz"
        vq="axis"
        vc="axis='h' ds('dscale')"
        set="if (axis='h') then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="0 64" size="80 24"
        style="PlainText"
        label="PPM"
        vq="axis"
        vc="axis='p' ds('dscale')"
        set="if (axis='p') then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="0 96" size="80 24"
        style="PlainText"
        label="kHz"
        vq="axis"
        vc="axis='k' ds('dscale')"
        set="if (axis='k') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="0 0" size="64 24"
        style="Heading2"
        label="Axis"
        />
    </group>
    <group loc="16 8" size="96 160"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <radio loc="0 32" size="88 24"
        style="PlainText"
        label="phased"
        vq="dmg"
        vc="dmg='ph'"
        set="if (dmg='ph') then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="0 64" size="88 24"
        style="PlainText"
        label="absval"
        vq="dmg"
        vc="dmg='av'"
        set="if (dmg='av') then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="0 96" size="88 24"
        style="PlainText"
        label="power"
        vq="dmg"
        vc="dmg='pwr'"
        set="if (dmg='pwr') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="0 0" size="72 24"
        style="Heading2"
        label="Mode"
        />
    </group>
    <group loc="136 8" size="96 160"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <radio loc="0 128" size="80 24"
        style="PlainText"
        label="full"
        vc="full ds"
        />
      <radio loc="0 32" size="80 24"
        style="PlainText"
        label="left"
        vc="left ds"
        />
      <radio loc="0 96" size="80 24"
        style="PlainText"
        label="right"
        vc="right ds"
        />
      <radio loc="0 64" size="80 24"
        style="PlainText"
        label="center"
        vc="center ds"
        />
      <label loc="0 0" size="64 24"
        style="Heading2"
        label="Position"
        />
    </group>
  </group>
</template>
