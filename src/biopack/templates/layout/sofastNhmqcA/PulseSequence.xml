<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="768 256"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="416 0" size="352 176"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="yes"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <entry loc="272 104" size="64 24"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE"
        set="$VALUE=gt2"
        digits="6"
        />
      <label loc="96 144" size="32 24"
        style="PlainText"
        label="gzcal"
        justify="Left"
        />
      <entry loc="136 144" size="56 24"
        style="PlainText"
        vq="gzcal"
        vc="gzcal=$VALUE"
        set="$VALUE=gzcal"
        digits="6"
        disable="Grayed out"
        />
      <entry loc="272 144" size="56 24"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE"
        set="$VALUE=gstab"
        digits="6"
        disable="Grayed out"
        />
      <label loc="232 144" size="40 24"
        style="PlainText"
        label="gstab"
        justify="Left"
        />
      <label loc="8 80" size="72 24"
        style="Label1"
        label="Crushers"
        />
      <label loc="8 104" size="72 24"
        style="Label1"
        label="Homospoil"
        />
      <entry loc="272 80" size="64 24"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE"
        set="$VALUE=gt1"
        digits="6"
        />
      <entry loc="200 104" size="56 24"
        style="PlainText"
        vq="gzlvl2 gzcal"
        vc="gzlvl2=$VALUE/gzcal"
        set="$VALUE=gzlvl2*gzcal"
        digits="3"
        />
      <entry loc="200 80" size="56 24"
        style="PlainText"
        vq="gzlvl1 gzcal"
        vc="gzlvl1=$VALUE/gzcal"
        set="$VALUE=gzlvl1*gzcal"
        digits="3"
        />
      <entry loc="136 104" size="48 24"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        />
      <label loc="96 104" size="40 24"
        style="PlainText"
        label="gzlvl2"
        />
      <entry loc="136 80" size="48 24"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        />
      <label loc="96 80" size="40 24"
        style="PlainText"
        label="gzlvl1"
        />
      <label loc="272 56" size="64 24"
        style="Label3"
        label="Time (sec)"
        />
      <label loc="144 56" size="120 24"
        style="Label3"
        label="DAC    or    G/cm"
        />
      <label loc="168 32" size="64 24"
        style="Label3"
        label="Strength"
        />
      <label loc="8 8" size="80 24"
        style="Heading2"
        label="Gradients"
        />
    </group>
    <button loc="600 208" size="128 24"
      style="Heading2"
      label="sofastNhmqcHT"
      vq="seqfil ipap_flg phase ab_flg ni proc1 dm2 layout"
      vc="seqfil='sofastNhmqcHT' layout=seqfil phase=1 ni=1 dm2='nny' ab_flg='a' ipap_flg='n' shname1='hn_excit140' proc1='ht' BPsofast"
      bg="VJBackground"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <button loc="432 208" size="128 24"
      style="Heading2"
      label="sofastNhmqc"
      vq="seqfil ipap_flg phase ab_flg dm2 layout"
      vc="seqfil='sofastNhmqc' phase=1 dm2='nny' ab_flg='a' ipap_flg='n' shname1='hn_excit140' BPsofast layout=seqfil"
      bg="VJBackground"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <label loc="416 176" size="272 24"
      style="Heading2"
      label="Switch to Other Versions"
      />
    <group loc="8 0" size="400 256"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <selmenu loc="42 30" size="160 25"
        style="PlainText"
        label="Select  Experiment"
        vq="phase  ab_flg dm2 ni"
        vc="exec('$VALUE')"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="1D ab_flg='a'"
          chval="dm2='nnn' ni=1 ab_flg='a' phase=1"
          />
        <mlabel 
          label="1D ab_flg='n'"
          chval="dm2='nnn' ab_flg='b' phase=1"
          />
        <mlabel 
          label="2D IPAP"
          chval="dm2='nnn' ab_flg=`a`,`b` phase=1,2 array='ab_flg,phase'"
          />
      </selmenu>
      <entry loc="144 136" size="56 24"
        style="PlainText"
        vq="fla"
        vc="fla=$VALUE"
        set="$VALUE=fla"
        digits="0"
        disable="Grayed out"
        tooltip="fla"
        />
      <label loc="8 136" size="136 24"
        style="Label1"
        label="Flip-Angle (first pulse)"
        />
      <radio loc="200 112" size="80 24"
        style="PlainText"
        label="PC9"
        vq="shname1"
        vc="shname1='pc9'"
        vc2="shname1='vap'"
        set="if shname1='pc9' then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="120 112" size="80 24"
        style="PlainText"
        label="VAP"
        vq="shname1"
        vc="shname1='vap'"
        vc2="shname1='pc9'"
        set="if shname1='vap' then $VALUE=1 else $VALUE=0 endif"
        tooltip="shname1"
        />
      <label loc="8 112" size="112 24"
        style="Label1"
        label="First Pulse Shape"
        />
      <textmessage loc="88 88" size="40 24"
        style="PlainText"
        label="1"
        vq="phase"
        set="$VALUE=phase"
        />
      <label loc="8 88" size="40 24"
        style="Label1"
        label="phase:"
        />
      <textmessage loc="88 64" size="40 24"
        style="PlainText"
        label="a"
        vq="ab_flg"
        set="$VALUE=ab_flg"
        />
      <label loc="8 64" size="56 24"
        style="Label1"
        label="ab Flag"
        />
      <textmessage loc="256 80" size="24 24"
        style="PlainText"
        label="nny"
        vq="dm2"
        set="$VALUE=dm2"
        />
      <label loc="216 80" size="40 24"
        style="Label1"
        label="dm2:"
        />
      <entry loc="128 176" size="64 24"
        style="PlainText"
        vq="adjust"
        vc="adjust=$VALUE"
        set="$VALUE=adjust"
        digits="0"
        disable="Grayed out"
        tooltip="adjust"
        />
      <label loc="8 176" size="120 24"
        style="Label1"
        label="t1 correction (usec)"
        justify="Left"
        />
      <entry loc="128 200" size="48 24"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        disable="Grayed out"
        tooltip="JNH"
        />
      <label loc="8 200" size="64 24"
        style="Label1"
        label="J(15N-1H)"
        justify="Left"
        />
      <entry loc="352 224" size="32 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="312 224" size="32 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="272 224" size="32 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="8 224" size="240 24"
        style="PlainText"
        label="Pulse Pre- and Post-Delays  (rof1,rof2,alfa)"
        justify="Left"
        />
      <label loc="10 5" size="85 25"
        style="Heading2"
        label="Experiment"
        justify="Left"
        />
      <label loc="95 5" size="65 25"
        style="Heading2"
        label="Options"
        justify="Left"
        />
    </group>
  </group>
</template>
