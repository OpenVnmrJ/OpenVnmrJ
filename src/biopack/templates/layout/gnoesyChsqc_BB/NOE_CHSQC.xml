<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="NOE_CHSQC" element="pages" type="acquisition" >
  <group size="895 395"
    style="Heading2"
    label="NOESY CLUB C-HSQC"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="NOE_CHSQC"
    useref="no"
    subtype="Major"
    expanded="yes"
    >
    <group loc="5 120" size="630 130"
      vq="detailsC13"
      show="if detailsC13=1 then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Minor"
      >
      <group loc="5 5" size="605 30"
        style="Label1"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="ms"
        useref="no"
        subtype="Major"
        expanded="yes"
        >
        <check loc="455 5" size="140 20"
          style="PlainText"
          label="Sens-Enhanced"
          vq="SE"
          vc="SE ='y'  f2coef = '1 0 -1 0 0 1 0 1' ddrtc=alfa"
          vc2=" SE='n'   f2coef = '1 0 0 0 0 0 -1 0' ddrtc=alfa+rof2"
          set="if SE='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <check loc="270 5" size="195 20"
          style="PlainText"
          label="decouple from CO in CHSQC"
          vq="refocCO"
          vc="refocCO='y' {setup_macro}('CO')"
          vc2="refocCO='n'"
          set="if refocCO='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="5 5" size="65 25"
          style="Heading2"
          label="Options"
          />
        <check loc="75 5" size="215 20"
          style="PlainText"
          label="decouple from N15 in CHSQC"
          vq="refocN15"
          vc="refocN15='y'"
          vc2="refocN15='n'"
          set="if refocN15='y' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <group loc="5 35" size="605 90"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Major"
        expanded="yes"
        >
        <group loc="5 35" size="595 30"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="10 5" size="60 20"
            style="PlainText"
            label="180 BIPs:"
            justify="Left"
            />
          <entry loc="75 5" size="45 20"
            style="PlainText"
            vq="pwXshp"
            vc="pwXshp=$VALUE"
            set="$VALUE=pwXshp"
            show="exists('pwXshp','parameter'):$VALUE"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="325 5" size="165 20"
            style="PlainText"
            vq="X180shape"
            vc="X180Shape='$VALUE'"
            set="$VALUE=X180shape"
            />
          <label loc="250 5" size="70 20"
            style="Label1"
            label="shape file"
            />
          <label loc="123 5" size="70 20"
            style="PlainText"
            label="us at power"
            justify="Left"
            />
          <entry loc="198 5" size="30 20"
            style="PlainText"
            vq="pwX180lvl"
            vc="pwX180lvl=$VALUE"
            set="$VALUE=pwX180lvl"
            show="exists('pwX180lvl','parameter'):$VALUE"
            digits="0"
            disable="Grayed out"
            />
        </group>
        <group loc="110 5" size="490 30"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <button loc="255 5" size="225 20"
            style="PlainText"
            label="setup all 13C ch  using pw90 as ref"
            vc="{setup_macro}('CO') {setup_macro}('WURST') {setup_macro}('BIP')"
            bg="VJBackground"
            decor1="yes"
            />
          <label loc="10 5" size="90 20"
            style="PlainText"
            label="90 deg pulse"
            justify="Left"
            />
          <entry loc="225 5" size="30 20"
            style="PlainText"
            vq="pwXlvl"
            vc="pwXlvl=$VALUE"
            set="$VALUE=pwXlvl"
            show="exists('pwXlvl','parameter'):$VALUE"
            digits="0"
            disable="Grayed out"
            />
          <label loc="150 5" size="70 20"
            style="PlainText"
            label="us at power"
            justify="Left"
            />
          <entry loc="100 5" size="45 20"
            style="PlainText"
            vq="pwX"
            vc="pwX=$VALUE"
            set="$VALUE=pwX"
            show="exists('pwX','parameter'):$VALUE"
            digits="1"
            disable="Grayed out"
            />
        </group>
        <button loc="405 65" size="185 20"
          style="PlainText"
          label="(re)setup WURST decopling"
          vc="{setup_macro}('WURST')"
          bg="VJBackground"
          decor1="yes"
          />
        <button loc="280 65" size="125 20"
          style="PlainText"
          label="(re)make CO shape"
          vq="refocCO"
          vc="{setup_macro}('CO')   "
          show="if (refocCO='y') then $SHOW=1 else $SHOW=0 endif"
          bg="VJBackground"
          decor1="yes"
          />
        <button loc="5 65" size="275 20"
          style="PlainText"
          label="(re)create 180 BIP pulses using 90deg as ref"
          vc="{setup_macro}('BIP')"
          bg="VJBackground"
          decor1="yes"
          />
        <label loc="45 5" size="60 20"
          style="Heading4"
          label="channel"
          />
        <textmessage loc="5 5" size="35 20"
          style="Heading4"
          label="C13"
          vq="dn"
          set="$VALUE=dn"
          />
      </group>
    </group>
    <button loc="10 90" size="510 30"
      style="Heading2"
      label="setup full 3D with reasonable resolution"
      vc="ni=0.012*sw1 phase=1,2 ni2=0.005*sw2 phase2=1,2 array='phase,phase2' su"
      bg="VJBackground"
      decor1="yes"
      />
    <button loc="350 60" size="170 30"
      style="Heading2"
      label="setup 2D NOE plane"
      vc="ni2=0 phase2=1 ni=0.016*sw1 phase=1,2 su"
      bg="VJBackground"
      decor1="yes"
      />
    <button loc="180 60" size="170 30"
      style="Heading2"
      label="setup 2D HSQC plane"
      vc="ni=0 phase=0 ni2=0.006*sw2 phase2=1,2 su"
      bg="VJBackground"
      decor1="yes"
      />
    <button loc="10 60" size="170 30"
      style="Heading1"
      label="Run test 1D"
      vc="ni=0 phase=1 ni2=0 phase2=1 ga"
      bg="VJBackground"
      decor1="yes"
      />
    <group loc="10 25" size="630 30"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <check loc="395 5" size="120 20"
        style="PlainText"
        label="more controls"
        vq="detailsC13"
        vc="detailsC13=1"
        vc2="detailsC13=0"
        set="$VALUE=detailsC13"
        />
      <entry loc="335 5" size="50 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        />
      <label loc="265 5" size="65 20"
        style="Label1"
        label="scans:"
        justify="Right"
        />
      <label loc="215 5" size="30 20"
        style="Label1"
        label="ms"
        />
      <entry loc="155 5" size="50 20"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1e3"
        set="$VALUE=mix*1e3"
        show="exists('mix','parameter'):$VALUE"
        digits="1"
        />
      <label loc="75 5" size="80 20"
        style="Label1"
        label="mixing time"
        />
      <label loc="5 5" size="55 20"
        style="Heading4"
        label="NOESY"
        />
    </group>
  </group>
</template>
