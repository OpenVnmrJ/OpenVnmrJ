        />
      <label loc="248 8" size="32 24"
        style="PlainText"
        label="gzcal"
        />
      <entry loc="280 8" size="64 24"
        style="PlainText"
        vq="gzcal"
        vc="gzcal=$VALUE"
        set="$VALUE=gzcal"
        digits="6"
        />
