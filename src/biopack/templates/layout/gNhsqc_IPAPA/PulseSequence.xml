<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="776 240"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 0" size="390 240"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <menu loc="240 65" size="105 25"
        style="Label2"
        label="menu"
        vq="autocal"
        vc="autocal='$VALUE'"
        set="$VALUE=autocal"
        bg="VJBackground"
        editable="No"
        tooltip="autocal"
        >
        <mlabel 
          label="Yes"
          chval="y"
          />
        <mlabel 
          label="No"
          chval="n"
          />
        <mlabel 
          label="Read From File"
          chval="r"
          />
        <mlabel 
          label="Semi-Automatic"
          chval="s"
          />
        <mlabel 
          label="Quiet"
          chval="q"
          />
      </menu>
      <group loc="70 5" size="225 25"
        label="switch to standard"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="switchtostandard"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="208 24"
          style="Label3"
          label="Switch to Standard Version"
          vq="seqfil layout"
          vc="length(seqfil):$length substr(seqfil,1,$length-1):seqfil layout=seqfil "
          bg="VJBackground"
          decor1="yes"
          tooltip="seqfil"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <check loc="240 88" size="104 24"
        style="Label2"
        label="Check Offset"
        vq="checkofs"
        vc="checkofs='y'"
        vc2="checkofs='n'"
        set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="checkofs"
        />
      <label loc="192 64" size="56 24"
        style="Label2"
        label="AutoCal"
        justify="Left"
        />
      <label loc="280 144" size="64 24"
        style="Label1"
        label="J(15N-1H)"
        />
      <entry loc="344 144" size="40 24"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        tooltip="JNH"
        />
      <entry loc="184 144" size="64 24"
        style="PlainText"
        vq="d3"
        vc="d3=$VALUE"
        set="$VALUE=d3"
        digits="6"
        tooltip="d3"
        />
      <label loc="48 144" size="136 24"
        style="Label1"
        label="interpulse-delay (usec)"
        />
      <label loc="16 120" size="120 24"
        style="Heading2"
        label="3919 Watergate"
        />
      <check loc="48 96" size="144 24"
        style="Label2"
        label="IPAP"
        vq="IPAP"
        vc="IPAP='y'"
        vc2="IPAP='n'"
        set="if (IPAP='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="IPAP"
        />
      <check loc="48 72" size="128 24"
        style="Label2"
        label="Refocus C13 in t1"
        vq="C13refoc"
        vc="C13refoc='y'"
        vc2="C13refoc='n'"
        set="if (C13refoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13refoc"
        />
      <label loc="16 48" size="64 24"
        style="Heading2"
        label="Options"
        />
      <label loc="296 184" size="40 24"
        style="PlainText"
        label="usec"
        />
      <entry loc="264 184" size="32 24"
        style="PlainText"
        vq="rof2"
