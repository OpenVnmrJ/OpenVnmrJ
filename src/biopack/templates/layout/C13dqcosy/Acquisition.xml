<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Acquisition" element="pages" type="acquisition" >
  <group size="672 248"
    style="PlainText"
    label="Acquisition"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Acquisition"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <label loc="0 216" size="264 24"
      style="Heading2"
      label="C13 DQ COSY"
      />
    <group loc="0 0" size="320 216"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <entry loc="288 184" size="32 24"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        disable="Grayed out"
        />
      <label loc="224 184" size="64 24"
        style="PlainText"
        label="at power"
        justify="Left"
        />
      <entry loc="168 184" size="48 24"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="if (pw=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="8 184" size="120 24"
        style="PlainText"
        label="Observe Pulse (us)"
        justify="Left"
        />
      <entry loc="248 128" size="32 16"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="if ss=0 then $VALUE=0 else $VALUE=1 endif"
        />
      <label loc="168 128" size="80 16"
        style="PlainText"
        label="Steady-State"
        />
      <textmessage loc="112 128" size="48 16"
        style="Label3"
        label="4"
        vq="ct"
        set="$VALUE=ct"
        />
      <label loc="48 128" size="64 16"
        style="PlainText"
        label="Completed"
        />
      <entry loc="168 112" size="64 16"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        />
      <label loc="8 112" size="120 16"
        style="PlainText"
        label="Scans:  Requested"
        />
      <entry loc="168 80" size="64 24"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        />
      <label loc="24 80" size="120 24"
        style="PlainText"
        label="complex points"
        />
      <menu loc="232 56" size="64 24"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <entry loc="168 56" size="64 24"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        />
      <label loc="8 56" size="120 24"
        style="PlainText"
        label="Acquisition time"
        />
      <menu loc="232 32" size="64 24"
        style="PlainText"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="168 32" size="64 24"
        style="PlainText"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        />
      <label loc="8 32" size="136 24"
        style="PlainText"
        label="Spectral width"
        />
      <label loc="224 152" size="32 24"
        style="Label1"
        label="Auto"
        />
      <check loc="208 152" size="16 24"
        style="PlainText"
        vq="gain"
        vc="gain='n'"
        vc2="gain='y'"
        set="off('gain'):$VALUE"
        />
      <entry loc="168 152" size="40 24"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        />
      <label loc="8 152" size="88 24"
        style="PlainText"
        label="Receiver Gain"
        />
      <label loc="8 0" size="264 24"
        style="Heading2"
        label="C13 Acquisition in F2 (Observe)"
        />
    </group>
    <group loc="328 0" size="344 240"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="40 160" size="248 24"
        style="Heading1"
        vq="d2"
        show="if d2&gt;0 then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="0 0" size="240 24"
          style="Heading1"
          label="RESET TO ZERO BEFORE START"
          justify="Left"
          />
      </group>
      <entry loc="152 136" size="48 24"
        style="Label2"
        vq="d2"
        vc="d2=$VALUE"
        set="$VALUE=d2"
        show="if d2=0 then $VALUE=0 else $VALUE=1 endif"
        />
      <label loc="8 136" size="152 24"
        style="Label3"
        label="Initial Evolution Time"
        justify="Left"
        />
      <radio loc="136 184" size="200 24"
        style="PlainText"
        label="Hypercomplex First Increment"
        vq="phase"
        vc="phase=1 ni=1"
        set="if phase=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="136 208" size="136 24"
        style="PlainText"
        label="Hypercomplex 2D"
        vq="phase"
        vc="phase=1,2"
        set="$VALUE=0 $phase=size('phase') if $phase&gt;1.5 then $v=(phase[1]=1) $w=(phase[2]=2) $VALUE=($v and $w) endif"
        />
      <label loc="8 184" size="120 24"
        style="PlainText"
        label="Acquisition Mode:"
        />
      <group loc="8 80" size="296 48"
        style="PlainText"
        vq="ni"
        show="if ni&gt;1 then $VALUE=1 else $VALUE=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="208 24" size="48 24"
          style="PlainText"
          label="ppm"
          />
        <textmessage loc="176 24" size="32 24"
          style="PlainText"
          label="0.43"
          vq="ni sw1 sfrq"
          set="if ni&gt;1 then $VALUE=(sw1/ni )/sfrq else $VALUE=0 endif"
          digits="2"
          />
        <label loc="128 24" size="48 24"
          style="PlainText"
          label="Hz, or"
          />
        <textmessage loc="96 24" size="32 24"
          style="PlainText"
          label="65"
          vq="ni sw1"
          set="if ni&gt;1 then $VALUE=sw1/ni else $VALUE=0 endif"
          digits="0"
          />
        <label loc="24 24" size="72 24"
          style="PlainText"
          label="Resolution:"
          />
        <label loc="192 0" size="32 24"
          style="PlainText"
          label="sec"
          />
        <textmessage loc="136 0" size="56 24"
          style="PlainText"
          label="0.015436"
          vq="ni sw1"
          set="if ni&gt;1 then $VALUE=ni/sw1 else $VALUE=0 endif"
          digits="6"
          />
        <label loc="0 0" size="136 24"
          style="PlainText"
          label="Acquisition time (max.)"
          />
      </group>
      <menu loc="224 32" size="80 24"
        style="PlainText"
        label="menu"
        vq="sw1"
        vc="parunits('set','sw1','$VALUE')"
        set="parunits('get','sw1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="144 56" size="48 24"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        />
      <entry loc="144 32" size="80 24"
        style="PlainText"
        vq="sw1"
        vc="vnmrunits('set','sw1',$VALUE)"
        set="vnmrunits('get','sw1'):$VALUE"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        digits="1"
        />
      <label loc="8 56" size="112 24"
        style="PlainText"
        label="increments in t1"
        />
      <label loc="8 32" size="104 24"
        style="PlainText"
        label="Spectral width"
        />
      <label loc="8 0" size="232 24"
        style="Heading2"
        label="C13 Acquisition in F1"
        />
    </group>
  </group>
</template>
