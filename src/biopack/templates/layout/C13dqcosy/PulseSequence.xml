<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="395 280"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <check loc="15 145" size="150 25"
      style="PlainText"
      label="Trim(x)-Trim(y)"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      set="if sspul='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip="sspul='y'"
      />
    <label loc="15 180" size="135 25"
      style="PlainText"
      label="Pulse Pre-Delay (rof1)"
      justify="Left"
      />
    <entry loc="145 180" size="50 25"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
      set="$VALUE=rof1"
      digits="1"
      />
    <label loc="200 180" size="40 25"
      style="PlainText"
      label="(usec)"
      />
    <label loc="200 210" size="40 25"
      style="PlainText"
      label="(usec)"
      />
    <entry loc="155 210" size="40 25"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      />
    <label loc="230 235" size="40 25"
      style="PlainText"
      label="(usec)"
      />
    <entry loc="175 235" size="50 25"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      />
    <label loc="15 210" size="135 25"
      style="PlainText"
      label="Pulse Post-Delay (rof2)"
      />
    <label loc="15 235" size="170 25"
      style="PlainText"
      label="Pre-Acquisition Delay (alfa)"
      />
    <label loc="232 88" size="72 24"
      style="PlainText"
      label="( tpwr)"
      justify="Left"
      />
    <entry loc="176 88" size="40 24"
      style="PlainText"
      vq="tpwr"
      vc="tpwr=$VALUE"
      set="$VALUE=tpwr"
      digits="0"
      disable="Grayed out"
      />
    <entry loc="176 64" size="40 24"
      style="PlainText"
      vq="pw"
      vc="pw=$VALUE"
      set="$VALUE=pw"
      digits="2"
      disable="Grayed out"
      />
    <label loc="232 64" size="72 24"
      style="PlainText"
      label="( pw, usec)"
      justify="Left"
      />
    <entry loc="176 24" size="40 24"
      style="PlainText"
      vq="d1"
      vc="d1=$VALUE"
      set="$VALUE=d1"
      digits="3"
      disable="Grayed out"
      tooltip="d1"
      />
    <label loc="16 24" size="136 24"
      style="PlainText"
      label="Relaxation Delay (sec)"
      vq="p1"
      justify="Left"
      />
    <label loc="16 64" size="104 24"
      style="PlainText"
      label="Observe pulse"
      vq="p1"
      justify="Left"
      />
    <label loc="16 88" size="128 24"
      style="PlainText"
      label="Observe pulse power"
      vq="p1"
      justify="Left"
      />
    <entry loc="176 112" size="64 24"
      style="PlainText"
      vq="d3"
      vc="d3=$VALUE"
      set="$VALUE=d3"
      show="if (d3=0) then $VALUE=0 else $VALUE=1 endif"
      digits="6"
      disable="Grayed out"
      tooltip="d3"
      />
    <label loc="16 112" size="160 24"
      style="PlainText"
      label="Spinecho interpulse delay"
      justify="Left"
      />
  </group>
</template>
