<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="645 230"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 0" size="365 230"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="220 150" size="120 25"
        style="PlainText"
        label="Sample Type"
        justify="Left"
        />
      <radio loc="225 175" size="80 25"
        style="PlainText"
        label="Protein"
        vq="bio_type"
        vc="bio_type='PROTEIN' BPC13_TROSY_CH2"
        vc2="bio_type='RNA'"
        set="if bio_type='PROTEIN' then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="225 200" size="80 25"
        style="PlainText"
        label="RNA"
        vq="bio_type"
        vc="bio_type='RNA' BPC13_TROSY_CH2"
        vc2="bio_type='PROTEIN'"
        set="if bio_type='RNA' then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="230 35" size="130 25"
        style="Label2"
        label="Refocus CO in t1"
        vq="refocCO"
        vc="refocCO='y'"
        vc2="refocCO='n'"
        set="if (refocCO='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="230 10" size="130 25"
        style="Label2"
        label="Refocus N15 in t1"
        vq="refocN15"
        vc="refocN15='y'"
        vc2="refocN15='n'"
        set="if (refocN15='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="160 10" size="50 25"
        style="PlainText"
        vq="tauch"
        vc="tauch=$VALUE/1e3"
        set="$VALUE=tauch*1e3"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="160 35" size="50 25"
        style="PlainText"
        vq="tauch1"
        vc="tauch1=$VALUE/1e3"
        set="$VALUE=tauch1*1e3"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="160 60" size="50 25"
        style="PlainText"
        vq="tauch2"
        vc="tauch2=$VALUE/1e3"
        set="$VALUE=tauch2*1e3"
        digits="3"
        disable="Grayed out"
        />
      <label loc="5 65" size="120 25"
        style="PlainText"
        label="tauch2(ms)"
        justify="Left"
        />
      <button loc="45 100" size="135 25"
        style="PlainText"
        label="Re-create Shapes"
        vc="BPC13_TROSY_CH2"
        bg="VJBackground"
        decor1="yes"
        />
      <label loc="5 200" size="170 25"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <label loc="5 175" size="145 25"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <label loc="5 150" size="135 25"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <label loc="5 5" size="120 25"
        style="PlainText"
        label="tauch(ms)"
        justify="Left"
        />
      <label loc="5 35" size="120 25"
        style="PlainText"
        label="tauch1(ms)"
        justify="Left"
        />
      <entry loc="176 152" size="32 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 176" size="32 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 200" size="32 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <entry loc="475 200" size="50 25"
