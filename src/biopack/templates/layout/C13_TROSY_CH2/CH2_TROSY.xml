<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="CH2_TROSY" element="pages" type="acquisition" >
  <group size="645 210"
    style="PlainText"
    label="CH2 TROSY"
    vq="bio_type"
    show="if (bio_type='RNA') then $SHOW=1 else $SHOW=0 endif"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="CH2_TROSY"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="0 5" size="630 195"
      vq="bio_type"
      show="if (bio_type='RNA') then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="1H180s"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 60" size="625 120"
        vq="detailsC13"
        show="if detailsC13=1 then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <button loc="355 10" size="210 25"
          style="PlainText"
          label="Regenerate Shaped Pulses"
          vc="BPC13_TROSY_CH2"
          bg="transparent"
          decor1="yes"
          />
        <label loc="90 60" size="65 20"
          style="PlainText"
          label="C13 pulse"
          justify="Left"
          />
        <entry loc="165 60" size="50 20"
          style="PlainText"
          vq="pwC"
          vc="pwC=$VALUE"
          set="$VALUE=pwC"
          show="exists('pwC','parameter'):$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="215 60" size="80 20"
          style="PlainText"
          label="us, at power"
          justify="Left"
          />
        <entry loc="300 60" size="25 20"
          style="PlainText"
          vq="pwClvl"
          vc="pwClvl=$VALUE"
          set="$VALUE=pwClvl"
          show="exists('pwClvl','parameter'):$VALUE"
          digits="0"
          disable="Grayed out"
          />
        <label loc="5 90" size="155 20"
          style="PlainText"
          label="selective 180 on C5' pulse"
          justify="Left"
          />
        <entry loc="165 90" size="50 20"
          style="PlainText"
          vq="pwC180"
          vc="pwC180=$VALUE"
          set="$VALUE=pwC180"
          show="exists('pwC180','parameter'):$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="215 90" size="85 20"
          style="PlainText"
          label="us, at power"
          justify="Left"
          />
        <entry loc="300 90" size="25 20"
          style="PlainText"
          vq="pwC180lvl"
          vc="pwC180lvl=$VALUE"
          set="$VALUE=pwC180lvl"
          show="exists('pwC180lvl','parameter'):$VALUE"
          digits="0"
          disable="Grayed out"
          />
        <label loc="335 90" size="110 20"
          style="PlainText"
          label="db,    shape name"
          justify="Left"
          />
        <entry loc="445 90" size="165 20"
          style="PlainText"
          vq="C180shape"
          vc="C180shape=$VALUE"
          set="$VALUE=C180shape"
          show="exists('C180shape','parameter'):$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <check loc="10 10" size="170 20"
          style="PlainText"
          label="decouple from C4' in t1"
          vq="refocCO"
          vc="refocCO='y'"
          vc2="refocCO='n'"
          set="if refocCO='y' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <button loc="155 30" size="175 25"
        style="PlainText"
        label="selectively labeled: C1' C5'"
        vc="refocCO='n' pwC180lvl=pwClvl pwCshp=2*pwC C180shape='hard'"
        bg="transparent"
        decor1="yes"
        tooltip="hard 180 on C5' and do not decouple from C4' "
        />
      <label loc="5 0" size="210 25"
        style="Heading2"
        label="RNA experiment options"
        />
      <check loc="345 30" size="115 25"
        style="PlainText"
        label="more controls"
        vq="detailsC13"
        vc="detailsC13=1"
        vc2="detailsC13=0"
        set="$VALUE=detailsC13"
        />
      <button loc="5 30" size="145 25"
        style="PlainText"
        label="Uniformly labled RNA"
        vc="refocCO='y' BPC13_TROSY_CH2"
        bg="transparent"
        decor1="yes"
        tooltip="decouple from C4' and use selective pulse on C5'"
        />
    </group>
  </group>
</template>
