<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="776 240"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="0 0" size="752 240"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Pre-AcquisitionDelay(alfa)"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="176 144" size="456 24"
        style="Label2"
        label="Warning:  N15-bound protons will not be filtered and will give noe's"
        />
      <label loc="168 32" size="144 24"
        style="Label1"
        label="Largest  J(CH)  to Purge"
        />
      <label loc="168 8" size="152 24"
        style="Label1"
        label="Smallest J(CH)  to Purge"
        />
      <group loc="392 32" size="304 104"
        style="PlainText"
        label="Multi-Frequency Saturation"
        vq="mfsat satmode"
        show="if ((mfsat='y') and (satmode&lt;&gt;'n')) then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Multi-FrequencySaturation"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <button loc="160 20" size="70 20"
          style="Label3"
          label="Clear List"
          vc="mfll('new')"
          bg="transparent"
          decor1="no"
          />
        <button loc="150 40" size="100 20"
          style="Label3"
          label="Add Frequency"
          vc="mfll"
          bg="transparent"
          decor1="no"
          />
        <button loc="110 80" size="180 20"
          style="Label3"
          label="Instructions (see Text Output)"
          vc="man('mfpresat') "
          bg="transparent"
          decor1="no"
          />
        <button loc="110 60" size="180 20"
          style="Label3"
          label="Show List (see Text Output)"
          vc="clear cat(curexp+'/mfpresat.ll')"
          bg="transparent"
          decor1="no"
          />
        <entry loc="70 60" size="30 20"
          style="PlainText"
          vq="satpwr "
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          tooltip="satpwr"
          />
        <label loc="10 60" size="50 20"
          style="PlainText"
          label="Power"
          justify="Left"
          />
        <entry loc="72 26" size="40 24"
          style="PlainText"
          vq="satdly"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          digits="3"
          disable="Grayed out"
          tooltip="satdly"
          />
        <label loc="8 26" size="64 24"
          style="PlainText"
          label="Time(sec)"
          />
      </group>
      <check loc="392 8" size="208 24"
        style="Label2"
        label="Multi-Frequency Presaturation"
        vq="mfsat"
        vc="mfsat='y'"
        vc2="mfsat='n'"
        set="if (mfsat='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="mfsat='y'"
        />
      <entry loc="320 32" size="48 24"
        style="PlainText"
        vq="jhc2"
        vc="jhc2=$VALUE"
        set="$VALUE=jhc2"
        digits="1"
        tooltip="jhc2"
        />
      <entry loc="320 8" size="48 24"
        style="PlainText"
        vq="jhc1"
        vc="jhc1=$VALUE"
        set="$VALUE=jhc1"
        digits="1"
        tooltip="jhc1"
        />
      <check loc="16 96" size="128 24"
        style="Label2"
        label="In Both Delays"
        vq="satmode"
        vc="satmode='yy'"
        vc2="satmode='nn'"
        set="if satmode='yy' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='yy'"
        />
      <group loc="16 136" size="120 72"
        vq="satmode"
        show="if satmode='nn' then $VALUE=0 else $VALUE=10 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="80 0" size="40 24"
          style="PlainText"
          vq="satpwr"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          disable="Grayed out"
          tooltip="satpwr"
          />
        <entry loc="80 24" size="40 24"
          style="PlainText"
          vq="satfrq"
          vc="satfrq=$VALUE"
          set="$VALUE=satfrq"
          digits="0"
          disable="Grayed out"
          tooltip="satfrq"
          />
        <entry loc="80 48" size="40 24"
          style="PlainText"
          vq="satdly"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          digits="2"
          disable="Grayed out"
          tooltip="satdly"
          />
        <label loc="8 48" size="64 24"
          style="PlainText"
          label="time(sec)"
          justify="Left"
          />
        <label loc="8 24" size="64 24"
          style="PlainText"
          label="at freq."
          justify="Left"
          />
        <label loc="8 0" size="64 24"
          style="PlainText"
          label="at power"
          justify="Left"
          />
      </group>
      <entry loc="640 200" size="40 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="496 200" size="144 24"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <entry loc="640 176" size="72 24"
        style="PlainText"
        vq="d0"
        vc="d0=$VALUE"
        set="$VALUE=d0"
        digits="6"
        disable="Grayed out"
        />
      <label loc="496 176" size="160 24"
        style="PlainText"
        label="Pre Sequence Delay (d0)"
        justify="Left"
        />
      <check loc="16 72" size="128 24"
        style="Label2"
        label="Mix Delay Only"
        vq="satmode"
        vc="satmode='ny'"
        vc2="satmode='nn'"
        set="if satmode='ny' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='ny'"
        />
      <check loc="16 48" size="128 24"
        style="Label2"
        label="Relax Delay Only"
        vq="satmode"
        vc="satmode='yn'"
        vc2="satmode='nn'"
        set="if satmode='yn' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='yn'"
        />
      <label loc="8 24" size="160 24"
        style="Heading2"
        label="Presaturation in"
        justify="Left"
        />
      <entry loc="320 100" size="50 20"
        style="PlainText"
        vq="taua"
        vc="taua=$VALUE/1000"
        set="$VALUE=taua*1000"
        digits="3"
        disable="Grayed out"
        tooltip="taua=$VALUE/1000"
        />
      <entry loc="448 200" size="40 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="448 176" size="40 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <label loc="272 200" size="168 24"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <label loc="272 176" size="144 24"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <label loc="8 0" size="160 24"
        style="Heading2"
        label="Options"
        justify="Left"
        />
      <entry loc="320 64" size="48 24"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="1"
        disable="Grayed out"
        tooltip="mix=$VALUE/1000"
        />
      <label loc="200 64" size="112 24"
        style="PlainText"
        label="NOESY mix (msec)"
        justify="Left"
        />
      <label loc="232 96" size="80 24"
        style="PlainText"
        label="taua (msec)"
        justify="Left"
        />
    </group>
  </group>
</template>
