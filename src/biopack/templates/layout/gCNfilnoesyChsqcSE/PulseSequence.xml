<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="752 160"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="8 0" size="712 160"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="CNfil"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="552 80" size="88 24"
        style="PlainText"
        label="Aromatics"
        vq="aliph arom dseq dpwr,dres,dmf"
        vc="arom='y' aliph='n' BPpboxget('wurst30.DEC'):dseq,$dmmy,dpwr,$dmmy,dres,dmf"
        vc2="arom='n' aliph='y' BPpboxget('wurst80.DEC'):dseq,$dmmy,dpwr,$dmmy,dres,dmf"
        set="if arom='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="arom='y' "
        />
      <label loc="280 96" size="56 24"
        style="Label1"
        label="Mix(sec)"
        />
      <entry loc="344 96" size="48 24"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE"
        set="$VALUE=mix"
        digits="4"
        tooltip="mix"
        />
      <entry loc="136 120" size="40 24"
        style="PlainText"
        vq="JCH2"
        vc="JCH2=$VALUE"
        set="$VALUE=JCH2"
        digits="1"
        disable="Grayed out"
        />
      <label loc="104 120" size="40 24"
        style="Label1"
        label="JCH2"
        justify="Left"
        />
      <entry loc="136 96" size="40 24"
        style="PlainText"
        vq="JCH1"
        vc="JCH1=$VALUE"
        set="$VALUE=JCH1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="104 96" size="40 24"
        style="Label1"
        label="JCH1"
        justify="Left"
        />
      <entry loc="48 120" size="40 24"
        style="PlainText"
        vq="JCH"
        vc="JCH=$VALUE"
        set="$VALUE=JCH"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="48 96" size="40 24"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        disable="Grayed out"
        />
      <label loc="8 120" size="40 24"
        style="Label1"
        label="JCH"
        justify="Left"
        />
      <label loc="8 96" size="40 24"
        style="Label1"
        label="JNH"
        justify="Left"
        />
      <check loc="552 56" size="88 24"
        style="PlainText"
        label="Aliphatics"
        vq="aliph arom dseq dpwr,dres,dmf"
        vc="aliph='y' arom='n' BPpboxget('wurst80.DEC'):dseq,$dmmy,dpwr,$dmmy,dres,dmf"
        vc2="aliph='n' arom='y' BPpboxget('wurst30.DEC'):dseq,$dmmy,dpwr,$dmmy,dres,dmf"
        set="if aliph='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="aliph='y' "
        />
      <check loc="536 32" size="136 24"
        style="PlainText"
        label="Auto Set C13 Xmtr"
        vq="auto_dof"
        vc="auto_dof='y' "
        vc2="auto_dof='n' "
        set="if auto_dof='y'  then $VALUE=1 else $VALUE=0 endif"
        tooltip="auto_dof='y' "
        />
      <check loc="528 0" size="136 24"
        style="PlainText"
        label="Refocus N15 in t1"
        vq="N15refoc"
        vc="N15refoc='y'"
        vc2="N15refoc='n'"
        set="if N15refoc='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="N15refoc='y'"
        />
      <check loc="336 0" size="168 24"
        style="Label2"
        label="STUD Decoupling in Acq."
        vc="STUD='y'"
        vc2="STUD='n'"
        set="if STUD='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="STUD='y'"
        />
      <check loc="256 0" size="96 24"
        style="Label2"
        label="TROSY"
        vq="TROSY dm2"
        vc="TROSY='y' dm2='nnn'"
        vc2="TROSY='n'"
        set="if TROSY='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="TROSY='y'"
        />
      <label loc="0 0" size="72 24"
        style="Heading2"
        label="Options:"
        />
      <label loc="80 0" size="56 24"
        style="Label2"
        label="AutoCal"
        justify="Left"
        />
      <menu loc="136 0" size="104 24"
        style="Label2"
        label="menu"
        vq="autocal"
        vc="autocal='$VALUE'"
        set="$VALUE=autocal"
        bg="VJBackground"
        editable="No"
        tooltip="autocal"
        >
        <mlabel 
          label="Yes"
          chval="y"
          />
        <mlabel 
          label="No"
          chval="n"
          />
        <mlabel 
          label="Read From File"
          chval="r"
          />
        <mlabel 
          label="Semi-Automatic"
          chval="s"
          />
        <mlabel 
          label="Quiet"
          chval="q"
          />
      </menu>
      <radio loc="448 104" size="64 24"
        style="PlainText"
        label="Three"
        vq="Cfil"
        vc="Cfil=3"
        set="if Cfil=3 then $VALUE=1 else $VALUE=0 endif"
        tooltip="Cfil=3"
        />
      <radio loc="448 80" size="64 24"
        style="PlainText"
        label="Two"
        vq="Cfil"
        vc="Cfil=2"
        set="if Cfil=2 then $VALUE=1 else $VALUE=0 endif"
        tooltip="Cfil=2"
        />
      <radio loc="448 56" size="64 24"
        style="PlainText"
        label="One"
        vq="Cfil"
        vc="Cfil=1"
        set="if Cfil=1 then $VALUE=1 else $VALUE=0 endif"
        tooltip="Cfil=1"
        />
      <label loc="432 32" size="96 16"
        style="graphics51"
        label="Cfilter Option"
        />
      <label loc="17 56" size="184 24"
        style="Label1"
        label="(Set to zero for Default Values)"
        />
      <label loc="9 32" size="328 24"
        style="Label1"
        label="Simultaneous 13C/15N Pulse Widths (3dB lower power):"
        />
      <entry loc="235 55" size="50 25"
        style="PlainText"
        vq="pwClw"
        vc="pwClw=$VALUE"
        set="$VALUE=pwClw"
        digits="1"
        tooltip="pwClw"
        />
      <label loc="200 55" size="40 25"
        style="PlainText"
        label="C13"
        />
      <label loc="300 55" size="40 25"
        style="PlainText"
        label="N15"
        />
      <entry loc="345 55" size="50 25"
        style="PlainText"
        vq="pwNlw"
        vc="pwNlw=$VALUE"
        set="$VALUE=pwNlw"
        digits="1"
        tooltip="pwNlw"
        />
    </group>
  </group>
</template>
