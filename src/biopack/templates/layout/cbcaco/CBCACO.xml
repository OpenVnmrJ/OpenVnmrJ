<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="CBCACO" element="pages" type="acquisition" >
  <group size="500 220"
    style="PlainText"
    label="CBCACO"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="CBCACO"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 10" size="455 200"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <check loc="330 105" size="110 25"
        style="PlainText"
        label="start from H1"
        vq="Hstart dm dmm"
        vc="Hstart='y' dm='nyy' dmm='cpp'"
        vc2="Hstart='n' dm='yyy' dmm='ppp'"
        set="if Hstart='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="Hstart='y' dm='nyy' dmm='cpp'"
        />
      <radio loc="330 75" size="90 25"
        style="PlainText"
        label="anti-phase"
        vq="IPAP"
        vc="IPAP='a'"
        set="if IPAP='a' then $VALUE=1 else $VALUE=0 endif"
        tooltip="IPAP='a'"
        />
      <button loc="15 155" size="295 30"
        style="Heading2"
        label="Setup 3D exp with full-resolution"
        vc="ni=sw1*TC phase=1,2 ni2=sw2*(TC-2.0*gt2-2.0*gstab)  phase2=1,2 array='phase,phase2'  su time"
        bg="VJBackground"
        decor1="yes"
        tooltip="ni=sw1*TC phase=1,2 ni2=sw2*(TC-2.0*gt2-2.0*gstab)  phase2=1,2 array='phase,phase2'  su time"
        />
      <button loc="15 125" size="295 30"
        style="Heading2"
        label="Setup 2D: CA-CO (t2-t3) plane"
        vc="ni=1 phase=1 ni2=sw2*(TC-2.0*gt2-2.0*gstab)  phase2=1,2  su time"
        bg="VJBackground"
        decor1="yes"
        tooltip="ni=1 phase=1 ni2=sw2*(TC-2.0*gt2-2.0*gstab)  phase2=1,2  su time"
        />
      <button loc="15 65" size="295 30"
        style="Heading2"
        label="Run 1D"
        vc="ni=1 phase=1 ni2=1 phase2=1 ga"
        bg="VJBackground"
        decor1="yes"
        tooltip="ni=1 phase=1 ni2=1 phase2=1 ga"
        />
      <button loc="15 95" size="295 30"
        style="Heading2"
        label="Setup 2D: CAB-CO (t1-t3) plane"
        vc="ni2=1 phase2=1 ni=sw1*TC phase=1,2  su time"
        bg="VJBackground"
        decor1="yes"
        tooltip="ni2=1 phase2=1 ni=sw1*TC phase=1,2  su time"
        />
      <label loc="255 10" size="85 25"
        style="Label1"
        label="scans per FID"
        />
      <entry loc="345 10" size="50 25"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        />
      <radio loc="330 50" size="90 25"
        style="PlainText"
        label="in-phase"
        vq="IPAP"
        vc="IPAP='i'"
        set="if IPAP='i' then $VALUE=1 else $VALUE=0 endif"
        tooltip="IPAP='i'"
        />
      <label loc="5 5" size="70 25"
        style="Heading2"
        label="CBCACO"
        />
    </group>
  </group>
</template>
