<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="715 240"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 0" size="330 240"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <label loc="170 90" size="65 25"
        style="Label1"
        label="Sequence"
        />
      <textmessage loc="240 90" size="85 25"
        style="PlainText"
        label="gNhsqc_IPAP"
        vq="seqfil"
        set="$VALUE=seqfil"
        />
      <button loc="175 60" size="150 25"
        style="Label3"
        label="Suppress Gel NH2s"
        vq="seqfil layout"
        vc="seqfil='gNhsqc_IPAP_gel' layout=seqfil "
        bg="VJBackground"
        decor1="yes"
        tooltip="seqfil='gNhsqc_IPAP_gel'"
        halignment="Center"
        valignment="Center"
        />
      <entry loc="170 155" size="65 25"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        tooltip="JNH"
        />
      <label loc="100 150" size="65 25"
        style="Label1"
        label="J(15N-1H)"
        />
      <entry loc="170 125" size="65 25"
        style="PlainText"
        vq="d3"
        vc="d3=$VALUE"
        set="$VALUE=d3"
        digits="6"
        tooltip="d3"
        />
      <label loc="30 125" size="135 25"
        style="Label1"
        label="interpulse-delay (usec)"
        />
      <label loc="15 105" size="120 25"
        style="Heading2"
        label="3919 Watergate"
        />
      <check loc="30 85" size="145 25"
        style="Label2"
        label="IPAP"
        vq="IPAP"
        vc="IPAP='y'"
        vc2="IPAP='n'"
        set="if (IPAP='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="IPAP"
        />
      <check loc="30 60" size="130 25"
        style="Label2"
        label="Refocus C13 in t1"
        vq="C13refoc"
        vc="C13refoc='y'"
        vc2="C13refoc='n'"
        set="if (C13refoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13refoc"
        />
      <label loc="15 30" size="65 25"
        style="Heading2"
        label="Options"
        />
      <button loc="60 5" size="210 25"
        style="Label3"
        label="Switch to Automatic Pbox Version"
        vq="seqfil layout"
        vc="seqfil=seqfil+'A' layout=seqfil "
        bg="VJBackground"
        decor1="yes"
        tooltip="seqfil=seqfil+'A'"
        halignment="Center"
        valignment="Center"
        />
      <label loc="296 184" size="40 24"
        style="PlainText"
        label="usec"
        />
      <entry loc="264 184" size="32 24"
        style="PlainText"
        vq="rof2"
