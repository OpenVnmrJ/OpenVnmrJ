<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="728 272"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 0" size="335 270"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <button loc="70 20" size="215 25"
        style="Label3"
        label="Switch to Standard Version"
        vq="seqfil layout"
        vc="seqfil='rna_HNNcosy' layout=seqfil"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <group loc="24 96" size="288 24"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="AutoCal"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <menu loc="56 0" size="120 24"
          style="Label2"
          label="menu"
          vq="autocal"
          vc="autocal='$VALUE'"
          set="$VALUE=autocal"
          editable="No"
          >
          <mlabel 
            label="Yes"
            chval="y"
            />
          <mlabel 
            label="No"
            chval="n"
            />
          <mlabel 
            label="Read From File"
            chval="r"
            />
          <mlabel 
            label="Semi-Automatic"
            chval="s"
            />
          <mlabel 
            label="Quiet"
            chval="q"
            />
        </menu>
        <check loc="184 0" size="104 24"
          style="Label2"
          label="Check Offset"
          vq="checkofs"
          vc="checkofs='y'"
          vc2="checkofs='n'"
          set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="0 0" size="56 24"
          style="Label2"
          label="AutoCal"
          justify="Left"
          />
      </group>
      <label loc="232 160" size="72 24"
        style="Label1"
        label="TROSY :"
        justify="Left"
        />
      <label loc="224 184" size="104 24"
        style="Label1"
        label="Peak Selection"
        justify="Left"
        />
