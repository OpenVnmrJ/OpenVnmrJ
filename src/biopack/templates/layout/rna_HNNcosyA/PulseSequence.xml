        style="PlainText"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <check loc="16 24" size="72 24"
          style="PlainText"
          label="right"
          vq="right TROSY"
          vc="right='y' "
          vc2="right='n'"
          set="if right='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <check loc="16 0" size="72 24"
          style="PlainText"
          label="bottom"
          vq="bottom TROSY"
          vc="bottom='y'"
          vc2="bottom='n'"
          set="if bottom='y' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <label loc="8 136" size="120 24"
        style="Label3"
        label="Constant Time (msec)"
        justify="Left"
        />
      <label loc="8 160" size="168 24"
        style="Label3"
        label="Water Flipback Pulse (msec)"
        justify="Left"
        />
      <entry loc="176 136" size="40 24"
        style="PlainText"
        vq="timeT"
        vc="timeT=$VALUE/1000"
        set="$VALUE=timeT*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 160" size="40 24"
        style="PlainText"
        vq="pwHs"
        vc="pwHs=[$VALUE]*1e3"
        set="$VALUE=pwHs/1e3"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="176 240" size="32 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 216" size="32 24"
        style="PlainText"
