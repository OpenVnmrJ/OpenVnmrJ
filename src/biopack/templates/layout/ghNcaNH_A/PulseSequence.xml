<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="770 245"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="15 0" size="375 225"
      style="Heading2"
      label="Transfer times"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="Maxtimeinniindirectdim,ms"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <radio loc="10 30" size="180 25"
        style="PlainText"
        label="double-TROSY hNcaNH"
        vq="exp_mode"
        vc="exp_mode='t'"
        set="if (exp_mode='t') then $VALUE=1 else $VALUE= 0 endif"
        tooltip="exp_mode='t'"
        />
      <radio loc="10 55" size="195 20"
        style="PlainText"
        label="1D test for transfer efficiency"
        vq="exp_mode"
        vc="exp_mode='R' ni=0 ni2=0 phase=1 phase2=1"
        set="if exp_mode='R' then $VALUE=1 else $VALUE=0 endif"
        tooltip="exp_mode='R' ni=0 ni2=0 phase=1 phase2=1"
        />
      <entry loc="245 85" size="60 20"
        style="PlainText"
        vq="timeTN1"
        vc="timeTN1=$VALUE/1000/2"
        set="$VALUE=timeTN1*1000*2"
        digits="2"
        tooltip="timeTN1=$VALUE/1000/2"
        />
      <label loc="5 85" size="200 20"
        style="Label1"
        label="N-&gt;CaN transfer time, ms"
        />
      <entry loc="245 105" size="60 20"
        style="PlainText"
        vq="timeTN"
        vc="timeTN=$VALUE/1000/2"
        set="$VALUE=timeTN*1000*2"
        digits="2"
        tooltip="timeTN=$VALUE/1000/2"
        />
      <label loc="5 105" size="200 20"
        style="Label1"
        label="CaN-&gt;N transfer time, ms"
        />
      <entry loc="245 125" size="60 20"
        style="PlainText"
        vq="timeCN"
        vc="timeCN=$VALUE/1000/2"
        set="$VALUE=timeCN*1000*2"
        digits="2"
        tooltip="timeCN=$VALUE/1000/2"
        />
      <label loc="5 125" size="235 20"
        style="Label1"
        label="Ca[i]N[i]&lt;-&gt;Ca[j]N[i]  transfer time, ms"
        />
      <label loc="5 165" size="195 20"
        style="Label1"
        label="Max time in ni indirect dim, ms"
        />
      <textmessage loc="200 165" size="45 20"
        style="PlainText"
        label="6.41"
        vq="ni sw1"
        set="$VALUE=0 if (ni&gt;1) then $VALUE=(ni-1)/sw1*1000 endif"
        digits="2"
        />
      <label loc="250 165" size="90 15"
        style="Label1"
        label="(based on ni)"
        />
      <label loc="250 185" size="85 20"
        style="Label1"
        label="(based on ni2)"
        />
      <textmessage loc="200 185" size="45 20"
        style="PlainText"
        label="6.41"
        vq="ni2 sw2"
        set="$VALUE=0 if (ni2&gt;1) then $VALUE=(ni2-1)/sw1*1000 endif"
        digits="2"
        />
      <label loc="5 185" size="195 20"
        style="Label1"
        label="Max time in ni2 indirect dim, ms"
        />
      <check loc="410 5" size="205 25"
        style="PlainText"
        label="2H decoupling during CA time"
        vq="dm3"
        vc="dm3='nyn'"
        vc2="dm3='nnn'"
        set="if (dm3='nyn') then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
    <entry loc="710 145" size="50 20"
      style="PlainText"
      vq="tauNH1"
      vc="tauNH1=[$VALUE]/1000"
      set="$VALUE=tauNH1*1000"
      digits="3"
      tooltip="tauNH1=[$VALUE]/1000"
      />
    <label loc="635 140" size="70 25"
      style="PlainText"
      label="tauNH1, ms"
      />
    <label loc="635 115" size="70 25"
      style="PlainText"
      label="tauNH, ms"
      />
    <entry loc="710 115" size="50 20"
      style="PlainText"
      vq="tauNH"
      vc="tauNH=$VALUE/1000"
      set="$VALUE=tauNH*1000"
      digits="3"
      tooltip="tauNH=$VALUE/1000"
      />
    <group loc="405 115" size="220 75"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="flipbacks"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <entry loc="95 0" size="50 20"
        style="PlainText"
        vq="pwHs"
        vc="pwHs=[$VALUE]*1e3"
        set="$VALUE=pwHs/1.0e3"
        digits="3"
        tooltip="pwHs=[$VALUE]*1e3"
        />
      <entry loc="95 25" size="50 20"
        style="PlainText"
        vq="tpwrsf_d"
        vc="tpwrsf_d=$VALUE"
        set="$VALUE=tpwrsf_d"
        show="if tpwrsf_d&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf_d"
        />
      <label loc="0 25" size="85 20"
        style="PlainText"
        label="Fine Power(d)"
        />
      <button loc="150 50" size="70 25"
        style="PlainText"
        label="Optimize"
        vq="tpwrsf_u ni phase"
        vc="array('tpwrsf_u',20,2800,-50)  ni=1 phase=1 nt=2 wnt='wft dssh' wexp='' au"
        bg="transparent"
        decor1="no"
        />
      <entry loc="95 50" size="50 20"
        style="PlainText"
        vq="tpwrsf_u"
        vc="tpwrsf_u=$VALUE"
        set="$VALUE=tpwrsf_u"
        show="if tpwrsf_u&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf_u"
        />
      <label loc="0 50" size="85 20"
        style="PlainText"
        label="Fine Power(u)"
        />
      <button loc="150 25" size="70 25"
        style="PlainText"
        label="Optimize"
        vq="tpwrsf_d ni phase"
        vc="array('tpwrsf_d',20,3900,-50) ni=1 phase=1 nt=2 wnt='wft dssh' wexp='' au"
        bg="transparent"
        decor1="no"
        />
      <label loc="0 0" size="90 25"
        style="PlainText"
        label="Flipback Width"
        vq="wet"
        />
    </group>
    <group loc="400 0" size="355 110"
      style="Heading2"
      label="Pbox input parameters for C13 shaped pulses"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Excitation"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="120 70" size="165 20"
        style="PlainText"
        vq="CO180offCA_in_str"
        vc="CO180offCA_in_str='$VALUE'"
        set="$VALUE=CO180offCA_in_str"
        disable="Grayed out"
        tooltip="Pbox shape parameters: shape bandwith offset spin state phase flip_angle"
        />
      <entry loc="120 50" size="165 20"
        style="PlainText"
        vq="CA180n_in_str"
        vc="CA180n_in_str='$VALUE'"
        set="$VALUE=CA180n_in_str"
        tooltip="Pbox shape parameters: shape bandwith offset spin state phase flip_angle"
        />
      <entry loc="120 30" size="165 20"
        style="PlainText"
        vq="CA180_in_str"
        vc="CA180_in_str='$VALUE'"
        set="$VALUE=CA180_in_str"
        disable="Grayed out"
        tooltip="Pbox shape parameters: shape bandwith offset spin state phase flip_angle"
        />
      <label loc="10 70" size="100 20"
        style="PlainText"
        label="CO 180"
        justify="Left"
        />
      <label loc="10 50" size="100 20"
        style="Label1"
        label="CA 180 narrow"
        />
      <label loc="10 30" size="100 20"
        style="PlainText"
        label="CA 180"
        justify="Left"
        />
    </group>
  </group>
</template>
