<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="776 235"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 0" size="745 230"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <label loc="10 25" size="100 25"
        style="Heading2"
        label="Carbon Type"
        />
      <group loc="352 128" size="288 24"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="AutoCal"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <check loc="184 0" size="104 24"
          style="Label2"
          label="Check Offset"
          vq="checkofs"
          vc="checkofs='y'"
          vc2="checkofs='n'"
          set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <menu loc="56 0" size="104 24"
          style="Label2"
          label="menu"
          vq="autocal"
          vc="autocal='$VALUE'"
          set="$VALUE=autocal"
          editable="No"
          >
          <mlabel 
            label="Yes"
            chval="y"
            />
          <mlabel 
            label="No"
            chval="n"
            />
          <mlabel 
            label="Read From File"
            chval="r"
            />
          <mlabel 
            label="Semi-Automatic"
            chval="s"
            />
          <mlabel 
            label="Quiet"
            chval="q"
            />
        </menu>
        <label loc="0 0" size="56 24"
          style="Label2"
          label="AutoCal"
          justify="Left"
          />
      </group>
      <entry loc="680 160" size="40 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="544 160" size="136 24"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <label loc="536 184" size="144 24"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <entry loc="680 184" size="40 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="680 208" size="40 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <label loc="512 208" size="168 24"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <group loc="328 96" size="224 24"
        label="switch to standard"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="switchtostandard"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="208 24"
          style="Label3"
          label="Switch to Standard Version"
          vq="seqfil layout"
          vc="length(seqfil):$length substr(seqfil,1,$length-1):seqfil layout=seqfil"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <label loc="632 88" size="64 24"
        style="PlainText"
        label="(msec)"
        justify="Left"
        />
      <entry loc="592 88" size="40 24"
        style="PlainText"
        vq="CTdelay CT"
        vc="CTdelay=[$VALUE]*1e-3"
        set="$VALUE=CTdelay/1e-3"
        show="if CT='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        disable="Grayed out"
        />
      <check loc="568 64" size="152 24"
        style="PlainText"
        label="Constant Time"
        vq="CT"
        vc="CT='y'"
        vc2="CT='n'"
        set="if CT='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="8 136" size="328 88"
        style="PlainText"
        label="WET"
        vq="wet"
        show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="WET"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <entry loc="272 48" size="40 24"
          style="PlainText"
          vq="dz wet"
          vc="dz1=$VALUE/1.0e6"
          set="$VALUE=dz*1.0e6"
          show="if (wet='y') then $VALUE=1 else $VALUE=0 endif"
          digits="1"
          disable="Grayed out"
          />
        <label loc="144 48" size="136 24"
          style="PlainText"
          label="WEFT Delays (usec)"
          justify="Left"
          />
        <entry loc="72 48" size="48 24"
          style="PlainText"
          vq="wetpwr wet"
          vc="wetpwr=$VALUE"
          set="$VALUE=wetpwr"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="1"
          disable="Grayed out"
          />
        <label loc="8 48" size="48 24"
          style="PlainText"
          label="Power"
          vq="wet"
          justify="Left"
          />
        <label loc="176 24" size="48 24"
          style="PlainText"
          label="Width"
          vq="wet"
          justify="Left"
          />
        <entry loc="224 24" size="48 24"
          style="PlainText"
          vq="wetpw wet"
          vc="wetpw=[$VALUE]*1e3"
          set="$VALUE=wetpw/1.0e3"
          show="if (wet='y')  then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          disable="Grayed out"
          />
        <label loc="280 24" size="40 24"
          style="PlainText"
          label="(msec)"
          vq="wet"
          justify="Left"
          />
        <entry loc="72 24" size="80 24"
          style="PlainText"
          vq="wetshape wet"
          vc="wetshape='$VALUE'"
          set="$VALUE=wetshape"
          show="if wet='y'  then $VALUE=1 else $VALUE=0 endif"
          disable="Grayed out"
          />
        <label loc="8 24" size="48 24"
          style="PlainText"
          label="Shape"
          vq="wet"
          justify="Left"
          />
      </group>
      <check loc="160 96" size="168 24"
        style="PlainText"
        label="WET Water Suppression"
        vq="wet"
        vc="wet='y'"
        vc2="wet='n'"
        set="if (wet='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="336 72" size="144 24"
        style="PlainText"
        label="right"
        vq="right"
        vc="right='y'"
        vc2="right='n'"
        set="if (right='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="336 48" size="144 24"
        style="PlainText"
        label="bottom"
        vq="bottom"
        vc="bottom='y'"
        vc2="bottom='n'"
        set="if (bottom='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="328 24" size="64 24"
        style="Heading2"
        label="TROSY"
        justify="Left"
        />
      <entry loc="664 32" size="48 24"
        style="PlainText"
        vq="JCH"
        vc="JCH=$VALUE"
        set="$VALUE=JCH"
        digits="1"
        disable="Grayed out"
        />
      <check loc="160 72" size="144 24"
        style="PlainText"
        label="SE3"
        vq="SE3"
        vc="SE3='y'"
        vc2="SE3='n'"
        set="if (SE3='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="16 48" size="112 72"
        style="PlainText"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <radio loc="8 48" size="80 24"
          style="PlainText"
          label="Aromatic"
          vq="allC ribose aromatic"
          vc="allC='n' ribose='n' aromatic='y' rna_setwurstparams"
          set="if aromatic='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <radio loc="8 24" size="80 24"
          style="PlainText"
          label="Ribose"
          vq="allC ribose aromatic"
          vc="allC='n' ribose='y' aromatic='n' rna_setwurstparams"
          set="if ribose='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <radio loc="8 0" size="104 24"
          style="PlainText"
          label="All Carbons"
          vq="allC aromatic ribose"
          vc="allC='y' aromatic='n' ribose='n'  rna_setwurstparams"
          set="if allC='y' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <check loc="160 48" size="128 24"
        style="PlainText"
        label="N15 Refocus in t1"
        vq="N15refoc"
        vc="N15refoc='y'"
        vc2="N15refoc='n'"
        set="if N15refoc='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="144 24" size="64 24"
        style="Heading2"
        label="Options"
        justify="Left"
        />
      <label loc="600 32" size="64 24"
        style="Label1"
        label="J(13C-1H)"
        />
    </group>
  </group>
</template>
