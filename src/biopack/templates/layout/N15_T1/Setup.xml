<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Setup" element="pages" type="acquisition" >
  <group size="840 275"
    style="PlainText"
    label="Setup"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Setup"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="595 10" size="235 155"
      vq="water_sat"
      show="if (water_sat='n') then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="flipbacks"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="0 5" size="220 25"
        style="Heading2"
        label="Water Flipback Pulses"
        />
      <entry loc="95 40" size="50 20"
        style="PlainText"
        vq="pwHs"
        vc="pwHs=[$VALUE]*1e3"
        set="$VALUE=pwHs/1.0e3"
        digits="3"
        tooltip="pwHs=[$VALUE]*1e3"
        />
      <label loc="0 35" size="90 25"
        style="PlainText"
        label="Flipback Width"
        vq="wet"
        />
      <label loc="0 65" size="85 20"
        style="PlainText"
        label="Fine Power(d)"
        />
      <entry loc="95 65" size="50 20"
        style="PlainText"
        vq="tpwrsf_d"
        vc="tpwrsf_d=$VALUE"
        set="$VALUE=tpwrsf_d"
        show="if tpwrsf_d&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf_d"
        />
      <button loc="150 65" size="70 25"
        style="PlainText"
        label="Optimize"
        vq="tpwrsf_d ni phase"
        vc="array('tpwrsf_d',20,3900,-50) ni=1 phase=1 nt=2 wnt='wft dssh' wexp='' au"
        bg="transparent"
        decor1="no"
        />
      <button loc="150 95" size="70 25"
        style="PlainText"
        label="Optimize"
        vq="tpwrsf_u ni phase"
        vc="array('tpwrsf_u',20,2800,-50)  ni=1 phase=1 nt=2 wnt='wft dssh' wexp='' au"
        bg="transparent"
        decor1="no"
        />
      <entry loc="95 95" size="50 20"
        style="PlainText"
        vq="tpwrsf_u"
        vc="tpwrsf_u=$VALUE"
        set="$VALUE=tpwrsf_u"
        show="if tpwrsf_u&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf_u"
        />
      <label loc="0 95" size="85 20"
        style="PlainText"
        label="Fine Power(u)"
        />
    </group>
    <group loc="5 5" size="585 230"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="pwC180"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <button loc="390 10" size="185 25"
        style="Heading4"
        label="setup test for test 1D"
        vc="ncyc=0 phase=1 ni=0 "
        bg="VJBackground"
        decor1="yes"
        tooltip="ncyc=0 phase=1 ni=0 "
        />
      <group loc="0 105" size="575 115"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="465 30" size="65 20"
          style="PlainText"
          label="0"
          vq="ncyc tau_cp"
          set="$VALUE=4.0*ncyc*tau_cp"
          />
        <button loc="150 80" size="275 25"
          style="Heading4"
          label="Set phase/array for pseudo-3D"
          vc="phase=1,2 array='ncyc,phase'"
          bg="VJBackground"
          decor1="yes"
          tooltip="phase=1,2 array='ncyc,phase'"
          />
        <button loc="90 55" size="400 25"
          style="Heading4"
          label="Generate interleaved aray of T1 relaxation delays"
          vc="BPN15_T1"
          bg="VJBackground"
          decor1="yes"
          tooltip="BPN15_T1"
          />
        <label loc="256 30" size="195 20"
          style="PlainText"
          label="current T1 relaxation delay, sec:"
          justify="Left"
          />
        <label loc="6 30" size="185 20"
          style="PlainText"
          label="current number of relax cycles"
          justify="Left"
          />
        <entry loc="196 30" size="55 20"
          style="PlainText"
          vq="ncyc"
          vc="ncyc=$VALUE"
          set="$VALUE=ncyc"
          disable="Grayed out"
          tooltip="ncyc"
          />
        <check loc="361 5" size="205 20"
          style="PlainText"
          label="saturate water (recommended)"
          vq="water_sat"
          vc="water_sat='y' "
          vc2="water_sat='n' "
          set="if water_sat='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="water_sat='y' "
          />
        <entry loc="306 5" size="30 20"
          style="PlainText"
          vq="ni"
          vc="ni=$VALUE"
          set="$VALUE=ni"
          disable="Grayed out"
          tooltip="ni"
          />
        <label loc="146 5" size="160 20"
          style="PlainText"
          label="number of 2D increments"
          justify="Left"
          />
        <entry loc="116 5" size="25 20"
          style="PlainText"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          disable="Grayed out"
          tooltip="nt"
          />
        <label loc="6 5" size="105 20"
          style="PlainText"
          label="number of scans"
          justify="Left"
          />
      </group>
      <button loc="10 70" size="565 30"
        style="Heading4"
        label="Setup pseudo-3D ( Check parameters after pressing this buton)"
        vc="ni=0.025*sw1 phase=1,2 BPN15_T1 array='ncyc,phase'"
        bg="VJBackground"
        decor1="yes"
        tooltip="ni=0.025*sw1 phase=1,2 BPN15_T1 array='ncyc,phase'"
        />
      <entry loc="520 40" size="55 25"
        style="PlainText"
        vq="relaxTmax"
        vc="relaxTmax=$VALUE"
        set="$VALUE=relaxTmax"
        digits="1"
        disable="Grayed out"
        tooltip="relaxTmax"
        />
      <label loc="15 10" size="245 25"
        style="Heading2"
        label="15N T1 relaxation setup"
        />
      <label loc="20 40" size="485 25"
        style="PlainText"
        label="longest relaxation delay (used for generation of array of T1 relaxation delays), sec"
        justify="Left"
        />
    </group>
  </group>
</template>
