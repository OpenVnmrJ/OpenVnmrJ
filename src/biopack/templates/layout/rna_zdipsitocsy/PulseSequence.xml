<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="760 256"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="280 0" size="470 160"
      style="Label2"
      label="WET"
      vq="wet"
      show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <group loc="10 10" size="185 110"
        style="Label1"
        label="RF"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="120 80" size="56 24"
          style="PlainText"
          vq="wetpw wet"
          vc="wetpw=[$VALUE]*1e3"
          set="$VALUE=wetpw/1000"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          />
        <label loc="8 80" size="88 24"
          style="PlainText"
          label="Width (msec)"
          />
        <label loc="8 56" size="64 24"
          style="PlainText"
          label="Power"
          />
        <entry loc="128 56" size="48 24"
          style="PlainText"
          vq="wetpwr wet autosoft"
          vc="wetpwr=$VALUE"
          set="$VALUE=wetpwr"
          show="if (wet='y' and autosoft='n') then $VALUE=1 else $VALUE=0 endif"
          digits="2"
          tooltip="wetpwr"
          />
        <entry loc="96 32" size="80 24"
          style="PlainText"
          vq="wetshape wet autosoft"
          vc="wetshape='$VALUE'"
          set="$VALUE=wetshape"
          show="if (wet='y' and autosoft='n') then $VALUE=1 else $VALUE=0 endif"
          tooltip="wetshape"
          />
        <label loc="8 32" size="64 24"
          style="PlainText"
          label="Shape"
          />
        <check loc="56 8" size="104 24"
          style="Heading4"
          label="Automatic"
          vq="autosoft"
          vc="autosoft='y'"
          vc2="autosoft='n'"
          set="if autosoft='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="autosoft='y'"
          />
      </group>
      <group loc="208 16" size="256 128"
        style="Label1"
        label="Gradients"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="168 96" size="72 24"
          style="PlainText"
          vq="gzcal wet"
          vc="gzcal=$VALUE"
          set="$VALUE=gzcal"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="6"
          disable="Grayed out"
          tooltip="gzcal"
          />
        <label loc="8 96" size="120 24"
          style="PlainText"
          label="G/cm per DAC value"
          justify="Left"
          />
        <entry loc="208 72" size="40 24"
          style="PlainText"
          vq="gswet wet"
          vc="gswet=$VALUE/1e6"
          set="$VALUE=gswet*1e6"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="1"
          disable="Grayed out"
          tooltip="gswet=$VALUE/1e6"
          />
        <label loc="8 72" size="192 24"
          style="PlainText"
          label="Stabilization Time in WET(usec)"
          justify="Left"
          />
        <entry loc="80 48" size="48 24"
          style="PlainText"
          vq="gtw wet"
          vc="gtw=$VALUE/1000"
          set="$VALUE=gtw*1000"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          disable="Grayed out"
          tooltip="gtw=$VALUE/1000"
          />
        <label loc="8 48" size="88 24"
          style="PlainText"
          label="Time (msec)"
          justify="Left"
          />
        <label loc="192 24" size="40 24"
          style="PlainText"
          label="G/cm"
          justify="Left"
          />
        <entry loc="144 24" size="48 24"
          style="PlainText"
          vq="gzlvlw wet gzcal"
          vc="gzlvlw=$VALUE/gzcal"
          set="$VALUE=gzlvlw*gzcal"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          disable="Grayed out"
          />
        <label loc="128 24" size="16 24"
          style="PlainText"
          label="or"
          justify="Left"
          />
        <entry loc="80 24" size="48 24"
          style="PlainText"
          vq="gzlvlw wet"
          vc="gzlvlw=$VALUE"
          set="$VALUE=gzlvlw"
          show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
          digits="0"
          disable="Grayed out"
          tooltip="gzlvlw="
          />
        <label loc="8 24" size="64 24"
          style="PlainText"
          label="Amplitude"
          justify="Left"
          />
      </group>
      <entry loc="144 128" size="40 24"
        style="PlainText"
        vq="dz wet"
        vc="dz=$VALUE/1e6"
        set="$VALUE=dz*1e6"
        show="if wet='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        disable="Grayed out"
        tooltip="dz=$VALUE/1e6"
        />
      <label loc="8 128" size="128 24"
        style="PlainText"
        label="WEFT Delay (usec)"
        justify="Left"
        />
      <group loc="208 232" size="232 48"
        style="DefaultTitle"
        vq="autosoft"
        show="if autosoft='n' then $VALUE=1 else $VALUE=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <button loc="24 8" size="200 24"
          style="Heading1"
          label="Optimize WET Parameters"
          vq="wet"
          vc="if wet='y' then BPautoWet(1) endif"
          bg="VJBackground"
          decor1="yes"
          />
      </group>
    </group>
    <group loc="464 168" size="264 88"
      label="Z-Filter"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="8 64" size="152 24"
        style="PlainText"
        label="Stabilization Time (usec)"
        justify="Left"
        />
      <entry loc="168 64" size="40 24"
        style="PlainText"
        vq="gstab "
        vc="gstab=$VALUE/1e6"
        set="$VALUE=gstab*1e6"
        digits="1"
        disable="Grayed out"
        tooltip="gstab=$VALUE/1e6"
        />
      <label loc="8 40" size="88 24"
        style="PlainText"
        label="Time (msec)"
        justify="Left"
        />
      <entry loc="88 40" size="48 24"
        style="PlainText"
        vq="gt2 "
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="3"
        disable="Grayed out"
        tooltip="gt2=$VALUE/1000"
        />
      <label loc="8 16" size="64 24"
        style="PlainText"
        label="Amplitude"
        justify="Left"
        />
      <entry loc="88 16" size="48 24"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        tooltip="gzlvl2"
        />
      <label loc="136 16" size="16 24"
        style="PlainText"
        label="or"
        justify="Left"
        />
      <entry loc="160 16" size="48 24"
        style="PlainText"
        vq="gzlvl2 gzcal"
        vc="gzlvl2=$VALUE/gzcal"
        set="$VALUE=gzlvl2*gzcal"
        digits="3"
        disable="Grayed out"
        />
      <label loc="216 16" size="40 24"
        style="PlainText"
        label="G/cm"
        justify="Left"
        />
    </group>
    <group loc="288 8" size="168 80"
      style="Label2"
      label="Presat"
      vq="satmode"
      show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="24 40" size="80 24"
        style="PlainText"
        label="Duration(sec)"
        justify="Left"
        />
      <entry loc="104 40" size="48 24"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        disable="Grayed out"
        tooltip="satdly"
        />
      <entry loc="72 16" size="32 24"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        disable="Grayed out"
        tooltip="satpwr"
        />
      <label loc="24 16" size="48 24"
        style="PlainText"
        label="Power"
        justify="Left"
        />
    </group>
    <label loc="216 160" size="144 24"
      style="PlainText"
      label="Delays:   Relaxation (sec)"
      justify="Left"
      />
    <label loc="328 184" size="32 24"
      style="PlainText"
      label="rof1:"
      justify="Left"
      />
    <entry loc="360 160" size="40 24"
      style="PlainText"
      vq="d1"
      vc="d1=$VALUE"
      set="$VALUE=d1"
      show="if d1=0 then $VALUE=0 else $VALUE=1 endif"
      digits="3"
      disable="Grayed out"
      tooltip="d1"
      />
    <entry loc="360 232" size="32 24"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      disable="Grayed out"
      />
    <label loc="328 232" size="32 24"
      style="PlainText"
      label="alfa:"
      justify="Left"
      />
    <entry loc="360 208" size="32 24"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      disable="Grayed out"
      />
    <label loc="328 208" size="32 24"
      style="PlainText"
      label="rof2:"
      justify="Left"
      />
    <entry loc="360 184" size="40 24"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
      set="$VALUE=rof1"
      digits="1"
      disable="Grayed out"
      />
    <selmenu loc="32 232" size="208 24"
      style="Label4"
      label="Select Suppression Mode"
      vc="exec('$VALUE')"
      bg="VJBackground"
      editable="No"
      >
      <mlabel 
        label="Water Suppression By WET"
        chval="wet=`y` satmode=`n`"
        />
      <mlabel 
        label="Water Suppression By PRESAT"
        chval="wet=`n`  satmode=`y`"
        />
      <mlabel 
        label="NO Water Suppression"
        chval="wet=`n`  satmode=`n`"
        />
    </selmenu>
    <check loc="8 152" size="128 24"
      style="Label2"
      label="Steady-State Pulse"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      tooltip="sspul='y'"
      />
    <group loc="8 104" size="264 48"
      style="Label2"
      label="Excitation Pulse"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="168 16" size="24 24"
        style="PlainText"
        vq="p1lvl"
        vc="p1lvl=$VALUE"
        set="$VALUE=p1lvl"
        digits="0"
        tooltip="p1lvl"
        />
      <label loc="104 16" size="64 24"
        style="PlainText"
        label="at Power"
        />
      <entry loc="64 16" size="40 24"
        style="PlainText"
        vq="p1"
        vc="p1=$VALUE"
        set="$VALUE=p1"
        digits="2"
        tooltip="p1"
        />
      <label loc="16 16" size="48 24"
        style="PlainText"
        label="pw90"
        />
    </group>
    <group loc="8 0" size="264 96"
      style="Label2"
      label="TOCSY (DIPSI)"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="96 64" size="48 24"
        style="PlainText"
        vq="window"
        vc="window=$VALUE"
        set="$VALUE=window"
        digits="2"
        tooltip="window"
        />
      <label loc="8 64" size="88 24"
        style="PlainText"
        label="window (usec)"
        />
      <label loc="16 40" size="64 24"
        style="PlainText"
        label="at Power"
        />
      <entry loc="80 40" size="32 24"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        digits="0"
        tooltip="tpwr"
        />
      <label loc="136 40" size="72 24"
        style="Label2"
        label="trim (msec)"
        />
      <entry loc="216 40" size="40 24"
        style="Label2"
        vq="trim"
        vc="trim=$VALUE/1000"
        set="$VALUE=trim*1000"
        digits="1"
        tooltip="trim=$VALUE/1000"
        />
      <entry loc="216 16" size="40 24"
        style="Label2"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="1"
        tooltip="mix=$VALUE/1000"
        />
      <label loc="112 16" size="104 24"
        style="Label2"
        label="Mix Time (msec)"
        />
      <entry loc="56 16" size="48 24"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE"
        set="$VALUE=pw"
        digits="2"
        tooltip="pw"
        />
      <label loc="8 16" size="48 24"
        style="PlainText"
        label="pw90"
        />
    </group>
  </group>
</template>
