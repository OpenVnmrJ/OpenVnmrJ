<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="688 264"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="376 0" size="312 256"
      style="Label2"
      label="Presat"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <selmenu loc="24 15" size="255 25"
        style="Label3"
        label="Set Saturation Mode"
        vc="exec('$VALUE')"
        bg="VJBackground"
        editable="No"
        tooltip="satmode"
        >
        <mlabel 
          label="During Relaxation Delay"
          chval="satmode=`ynn`"
          />
        <mlabel 
          label="None"
          chval="satmode=`nnn`"
          />
      </selmenu>
      <group loc="0 160" size="304 88"
        style="PlainText"
        label="Multi-Frequency Saturation"
        vq="mfsat"
        show="if mfsat='y' then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Multi-FrequencySaturation"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <button loc="224 64" size="72 24"
          style="Label3"
          label="Clear List"
          vc="mfll('new')"
          bg="VJBackground"
          decor1="no"
          tooltip="mfll('new')"
          />
        <button loc="120 16" size="176 24"
          style="Label3"
          label="Instructions (see Text Output)"
          vc="man('mfpresat') "
          bg="VJBackground"
          decor1="no"
          />
        <button loc="8 16" size="104 24"
          style="Label3"
          label="Add Frequency"
          vc="mfll"
          bg="VJBackground"
          decor1="no"
          tooltip="mfll"
          />
        <entry loc="160 64" size="32 24"
          style="PlainText"
          vq="satpwr "
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          tooltip="satpwr"
          />
        <entry loc="160 40" size="48 24"
          style="PlainText"
          vq="satdly"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          digits="3"
          disable="Grayed out"
          tooltip="satdly"
          />
        <label loc="8 40" size="64 24"
          style="PlainText"
          label="Time(sec)"
          />
        <label loc="8 64" size="48 24"
          style="PlainText"
          label="Power"
          justify="Left"
          />
      </group>
      <group loc="16 136" size="256 24"
        label="mfsat"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="mfsat"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <radio loc="120 0" size="128 24"
          style="PlainText"
          label="Multiple Frequency"
          vq="mfsat"
          vc="mfsat='y'"
          vc2="mfsat='n'"
          set="if mfsat='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="mfsat='y'"
          />
        <radio loc="0 0" size="120 24"
          style="PlainText"
          label="Single Frequency"
          vq="mfsat"
          vc="mfsat='n'"
          vc2="mfsat='y'"
          set="if mfsat='n' then $VALUE=1 else $VALUE=0 endif"
          tooltip="mfsat='n'"
          />
      </group>
      <entry loc="80 112" size="32 24"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        tooltip="satpwr"
        />
      <label loc="8 112" size="64 24"
        style="PlainText"
        label="Power"
        />
      <label loc="8 88" size="168 24"
        style="PlainText"
        label="Total Relaxation Delay(sec)"
        />
      <entry loc="184 88" size="56 24"
        style="PlainText"
        vq="satdly d1"
        vc="d1=$VALUE-satdly"
        set="$VALUE=d1+satdly"
        digits="3"
        tooltip="d1=$VALUE-satdly"
        />
      <entry loc="184 64" size="56 24"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="3"
        tooltip="satdly"
        />
      <label loc="8 64" size="176 24"
        style="PlainText"
        label="Duration in Relax Delay (sec)"
        />
      <label loc="232 40" size="16 24"
        style="PlainText"
        label=")"
        />
      <textmessage loc="200 40" size="32 24"
        style="PlainText"
        vq="satmode"
        set="$VALUE=satmode"
        />
      <label loc="128 40" size="72 24"
        style="PlainText"
        label="(satmode ="
        />
    </group>
    <group loc="0 0" size="368 232"
      style="Label2"
      label="TOCSY"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <group loc="16 152" size="272 72"
        style="Label2"
        label="Refocus in t1"
        bg="transparent"
        border="Etched"
        tab="no"
        enable="no"
        reference="Refocusint1"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <radio loc="8 16" size="80 24"
          style="PlainText"
          label="None"
          vq="C13refoc N15refoc CNrefoc"
          vc="C13refoc='n' N15refoc='n' CNrefoc='n'"
          set="if ((C13refoc='n') and (N15refoc='n') and (CNrefoc='n')) then $VALUE=1 else $VALUE=0 endif"
          tooltip="C13refoc='n' N15refoc='n' CNrefoc='n'"
          />
        <radio loc="8 40" size="80 24"
          style="PlainText"
          label="N15"
          vq="N15refoc C13refoc CNrefoc"
          vc="N15refoc='y' C13refoc='n' CNrefoc='n'"
          set="if (N15refoc='y') then $VALUE=1 else $VALUE=0 endif"
          tooltip="N15refoc='y' C13refoc='n' CNrefoc='n'"
          />
        <radio loc="144 40" size="88 24"
          style="PlainText"
          label="C13 &amp; N15"
          vq="CNrefoc N15refoc C13refoc"
          vc="CNrefoc='y' N15refoc='n' C13refoc='n'"
          set="if (CNrefoc='y') then $VALUE=1 else $VALUE=0 endif"
          tooltip="CNrefoc='y' N15refoc='n' C13refoc='n'"
          />
        <radio loc="144 16" size="80 24"
          style="PlainText"
          label="C13"
          vq="C13refoc N15refoc CNrefoc"
          vc="C13refoc='y' N15refoc='n' CNrefoc='n'"
          set="if (C13refoc='y') then $VALUE=1 else $VALUE=0 endif"
          tooltip="C13refoc='y' N15refoc='n' CNrefoc='n'"
          />
      </group>
      <check loc="16 120" size="160 24"
        style="Label1"
        label="Steady-State Pulse"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="if sspul='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="sspul='y'"
        />
      <button loc="216 96" size="120 24"
        style="Label2"
        label="Auto Clean Setup"
        vq="window strength"
        vc="window=1.5/(4.0*strength*1e-6)"
        bg="VJBackground"
        decor1="no"
        tooltip="window=1.5/(4.0*strength*1e-6)"
        />
      <entry loc="160 96" size="40 24"
        style="PlainText"
        vq="window"
        vc="window=trunc(10.0*$VALUE)/10"
        set="$VALUE=window"
        digits="1"
        tooltip="window"
        />
      <label loc="32 96" size="136 24"
        style="PlainText"
        label="&quot;clean&quot; window (usec)"
        />
      <label loc="302 38" size="64 24"
        style="Label1"
        label="ppm )"
        />
      <textmessage loc="262 38" size="40 24"
        style="PlainText"
        vq="strength sfrq"
        set="$VALUE=strength/sfrq"
        digits="1"
        />
      <label loc="206 38" size="56 24"
        style="PlainText"
        label="Hz       ("
        />
      <entry loc="158 62" size="48 24"
        style="PlainText"
        vq="trim"
        vc="trim=$VALUE/1e3"
        set="$VALUE=trim*1e3"
        digits="1"
        tooltip="trim=$VALUE/1e3"
        />
      <entry loc="158 14" size="48 24"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="1"
        tooltip="mix=$VALUE/1000"
        />
      <entry loc="158 38" size="48 24"
        style="PlainText"
        vq="strength"
        vc="strength=trunc(10.0*$VALUE)/10 BPspinlock"
        set="$VALUE=strength"
        digits="0"
        tooltip="strength"
        />
      <label loc="30 62" size="112 24"
        style="PlainText"
        label="Trim Pulse (msec)"
        />
      <label loc="30 38" size="176 24"
        style="PlainText"
        label="at RF Field Strength of"
        />
      <label loc="16 16" size="128 24"
        style="PlainText"
        label="Spinlock Time (msec)"
        />
    </group>
    <label loc="320 232" size="32 24"
      style="PlainText"
      label="usec"
      />
    <entry loc="280 232" size="40 24"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      />
    <label loc="248 232" size="32 24"
      style="PlainText"
      label="alfa:"
      />
    <entry loc="184 232" size="40 24"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      />
    <label loc="152 232" size="32 24"
      style="PlainText"
      label="rof2:"
      />
    <entry loc="96 232" size="40 24"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
      set="$VALUE=rof1"
      digits="1"
      />
    <label loc="8 232" size="88 24"
      style="PlainText"
      label="Delays:   rof1:"
      />
  </group>
</template>
