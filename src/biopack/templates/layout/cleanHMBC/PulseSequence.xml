<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="760 240"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <menu loc="200 210" size="215 25"
      style="PlainText"
      label="Final Low-Pass Filter Order"
      vq="mboblpjf"
      vc="mboblpjf=$VALUE"
      set="$VALUE= mboblpjf"
      bg="VJBackground"
      editable="No"
      tooltip="mboblpjf=$VALUE"
      >
      <mlabel 
        label="Final  Low-Pass Filter Order =1"
        chval="1"
        />
      <mlabel 
        label="Final  Low-Pass Filter Order =2"
        chval="2"
        />
      <mlabel 
        label="Final  Low-Pass Filter Order =3"
        chval="3"
        />
      <mlabel 
        label="Final  Low-Pass Filter Order =4"
        chval="4"
        />
    </menu>
    <menu loc="200 175" size="215 25"
      style="PlainText"
      label="Initial Low-Pass Filter Order"
      vq="lpjf"
      vc="lpjf=$VALUE"
      set="$VALUE=lpjf"
      bg="VJBackground"
      editable="No"
      tooltip="lpjf=$"
      >
      <mlabel 
        label="Initial Low-Pass Filter Order =1"
        chval="1"
        />
      <mlabel 
        label="Initial Low-Pass Filter Order =2"
        chval="2"
        />
      <mlabel 
        label="Initial Low-Pass Filter Order =3"
        chval="3"
        />
      <mlabel 
        label="Inital Low-Pass Filter Order =4"
        chval="4"
        />
    </menu>
    <label loc="192 144" size="152 24"
      style="PlainText"
      label="Long-Range CH Coupling"
      justify="Left"
      />
    <button loc="104 8" size="176 24"
      style="Label3"
      label="Switch to gChsqc"
      vq="seqfil pmode f1coef SE ZZ phase dm gzlvl1 gt1 gzlvl2 layout"
      vc="seqfil='gChsqc' getparam('dofCO','C13'):dof dof=dof-(174-70)*dfrq pmode='full' f1coef='' SE='n' ZZ='y' phase=1 ni=1 ph ph1 dm='nny' getparam('CHgrad1_time','C13'):gt1 getparam('CHgrad2_lvl','C13'):gzlvl2  getparam('CHgrad1_lvl','C13'):gzlvl1 layout=seqfil "
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <check loc="24 176" size="136 24"
      style="PlainText"
      label="MRC"
      vq="MRC"
      vc="MRC='y' "
      vc2="MRC='n'"
      set="if MRC='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip="MRC='y' "
      />
    <check loc="24 152" size="136 24"
      style="PlainText"
      label="Alternate Gradients"
      vq=" altgrad"
      vc=" altgrad='y' "
      vc2=" altgrad='n'"
      set="if altgrad ='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip=" altgrad='y' "
      />
    <label loc="552 200" size="168 24"
      style="PlainText"
      label="Pre-Acquisition Delay (alfa)"
      justify="Left"
      />
    <label loc="552 176" size="144 24"
      style="PlainText"
      label="Pulse Post-Delay (rof2)"
      justify="Left"
      />
