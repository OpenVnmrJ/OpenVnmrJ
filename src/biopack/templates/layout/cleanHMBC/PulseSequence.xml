      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      disable="Grayed out"
      />
    <entry loc="720 176" size="40 24"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
