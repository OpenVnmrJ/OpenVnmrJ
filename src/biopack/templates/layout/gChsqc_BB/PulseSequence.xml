<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="645 230"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 0" size="390 230"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <button loc="45 100" size="135 25"
        style="PlainText"
        label="Re-create Shapes"
        vc="BPgChsqc_BB"
        bg="VJBackground"
        decor1="yes"
        tooltip="BPgChsqc_BB"
        />
      <check loc="250 60" size="130 25"
        style="Label2"
        label="Constant Time"
        vq="CT"
        vc="CT='y'"
        vc2="CT='n'"
        set="if (CT='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="CT='y'"
        />
      <check loc="250 35" size="130 25"
        style="Label2"
        label="Refocus CO in t1"
        vq="refocCO"
        vc="refocCO='y'"
        vc2="refocCO='n'"
        set="if (refocCO='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="refocCO='y'"
        />
      <check loc="250 10" size="130 25"
        style="Label2"
        label="Refocus N15 in t1"
        vq="refocN15"
        vc="refocN15='y'"
        vc2="refocN15='n'"
        set="if (refocN15='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="refocN15='y'"
        />
      <label loc="5 200" size="170 25"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <label loc="5 175" size="145 25"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <label loc="5 150" size="135 25"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <label loc="5 5" size="120 25"
        style="PlainText"
        label="tauch(ms)"
        justify="Left"
        />
      <label loc="5 35" size="120 25"
        style="PlainText"
        label="tauch1(ms)"
        justify="Left"
        />
      <entry loc="162 35" size="50 25"
        style="PlainText"
        vq="tauch1"
        vc="tauch1=$VALUE/1e3"
        set="$VALUE=tauch1*1e3"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="162 10" size="50 25"
        style="PlainText"
        vq="tauch"
        vc="tauch=$VALUE/1e3"
        set="$VALUE=tauch*1e3"
        digits="3"
        disable="Grayed out"
        />
      <group loc="247 85" size="135 25"
        vq="CT"
        show="if CT='y' then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="70 0" size="50 25"
          style="PlainText"
          vq="timeCC"
          vc="timeCC=$VALUE/1e3"
          set="$VALUE=timeCC*1e3"
          digits="3"
          disable="Grayed out"
          tooltip="timeCC=$VALUE/1e3"
          />
        <label loc="0 0" size="70 25"
          style="Label1"
          label="timeCC(ms)"
          />
      </group>
      <entry loc="176 152" size="32 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 176" size="32 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 200" size="32 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
    </group>
