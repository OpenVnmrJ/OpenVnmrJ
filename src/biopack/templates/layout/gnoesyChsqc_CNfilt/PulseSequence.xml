<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="780 270"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="0 0" size="750 270"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Pre-AcquisitionDelay(alfa)"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <entry loc="200 240" size="40 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="200 208" size="64 24"
        style="PlainText"
        vq="d0"
        vc="d0=$VALUE"
        set="$VALUE=d0"
        digits="6"
        disable="Grayed out"
        />
      <entry loc="200 176" size="64 24"
        style="PlainText"
        vq="offco"
        vc="offco=$VALUE*dfrq"
        set="$VALUE=offco/dfrq"
        digits="0"
        disable="Grayed out"
        tooltip="offco=$VALUE*dfrq"
        />
      <entry loc="200 152" size="64 24"
        style="PlainText"
        vq="dof_t1"
        vc="dof_t1=$VALUE"
        set="$VALUE=dof_t1"
        digits="1"
        disable="Grayed out"
        tooltip="dof_t1"
        />
      <label loc="8 152" size="192 24"
        style="PlainText"
        label="C13 offset during t1"
        justify="Left"
        />
      <label loc="8 176" size="192 24"
        style="PlainText"
        label="CO offset (ppm from 13C xmtr)"
        justify="Left"
        />
      <entry loc="320 128" size="48 24"
        style="PlainText"
        vq="taunh"
        vc="taunh=$VALUE/1000"
        set="$VALUE=taunh*1000"
        digits="3"
        disable="Grayed out"
        tooltip="taunh=$VALUE/1000"
        />
      <label loc="232 128" size="80 24"
        style="PlainText"
        label="taunh (msec)"
        justify="Left"
        />
      <entry loc="320 96" size="48 24"
        style="PlainText"
        vq="tauch"
        vc="tauch=$VALUE/1000"
        set="$VALUE=tauch*1000"
        digits="3"
        disable="Grayed out"
        tooltip="tauch=$VALUE/1000"
        />
      <label loc="232 96" size="80 24"
        style="PlainText"
        label="tauch (msec)"
        justify="Left"
        />
      <label loc="232 64" size="80 24"
        style="PlainText"
        label="tauxh (msec)"
        justify="Left"
        />
      <entry loc="320 64" size="48 24"
        style="PlainText"
        vq="tauxh"
        vc="tauxh=$VALUE/1000"
        set="$VALUE=tauxh*1000"
        digits="3"
        disable="Grayed out"
        tooltip="tauxh=$VALUE/1000"
        />
      <entry loc="320 32" size="48 24"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="1"
        disable="Grayed out"
        tooltip="mix=$VALUE/1000"
        />
      <label loc="200 32" size="112 24"
        style="PlainText"
        label="NOESY mix (msec)"
        justify="Left"
        />
      <label loc="272 216" size="144 24"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <entry loc="448 216" size="40 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="450 240" size="40 20"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <label loc="270 240" size="170 20"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <label loc="10 210" size="160 20"
        style="PlainText"
        label="Pre Sequence Delay (d0)"
        justify="Left"
        />
      <label loc="10 240" size="140 20"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <menu loc="624 56" size="104 24"
        style="Label2"
        label="menu"
        vq="autocal"
        vc="autocal='$VALUE'"
        set="$VALUE=autocal"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Yes"
          chval="y"
          />
        <mlabel 
          label="No"
          chval="n"
          />
        <mlabel 
          label="Read From File"
          chval="r"
          />
        <mlabel 
          label="Semi-Automatic"
          chval="s"
          />
        <mlabel 
          label="Quiet"
          chval="q"
          />
      </menu>
      <label loc="560 56" size="64 24"
        style="Label2"
        label="AutoCal"
        justify="Left"
        />
      <group loc="432 80" size="232 96"
        vq="dec_co autocal"
        show="if ((dec_co='y') and (autocal='n')) then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="176 72" size="48 24"
          style="PlainText"
          vq="dpwrf_cod"
          vc="dpwrf_cod=$VALUE"
          set="$VALUE=dpwrf_cod"
          digits="0"
          disable="Grayed out"
          tooltip="dpwrf_cod"
          />
        <label loc="0 72" size="176 24"
          style="PlainText"
          label="CO Refocusing Fine Power"
          justify="Left"
          />
        <label loc="0 48" size="176 24"
          style="PlainText"
          label="CO Refocusing Pulse Power"
          justify="Left"
          />
        <entry loc="176 48" size="48 24"
          style="PlainText"
          vq="dpwr_cod"
          vc="dpwr_cod=$VALUE"
          set="$VALUE=dpwr_cod"
          digits="0"
          disable="Grayed out"
          tooltip="dpwr_cod"
          />
        <label loc="0 24" size="136 24"
          style="PlainText"
          label="CO Refocusing Pulse"
          justify="Left"
          />
        <label loc="0 0" size="168 24"
          style="PlainText"
          label="CO Refocusing Pulse Width"
          justify="Left"
          />
        <entry loc="144 24" size="80 24"
          style="PlainText"
          vq="co_dec"
          vc="co_dec='$VALUE'"
          set="$VALUE=co_dec"
          digits="0"
          disable="Grayed out"
          tooltip="co_dec"
          />
        <entry loc="176 0" size="48 24"
          style="PlainText"
          vq="pwco180"
          vc="pwco180=$VALUE"
          set="$VALUE=pwco180"
          digits="1"
          disable="Grayed out"
          tooltip="pwco180"
          />
      </group>
      <group loc="16 56" size="120 72"
        vq="satmode"
        show="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="80 0" size="40 24"
          style="PlainText"
          vq="satpwr"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          disable="Grayed out"
          tooltip="satpwr"
          />
        <entry loc="80 24" size="40 24"
          style="PlainText"
          vq="satfrq"
          vc="satfrq=$VALUE"
          set="$VALUE=satfrq"
          digits="0"
          disable="Grayed out"
          tooltip="satfrq"
          />
        <entry loc="80 48" size="40 24"
          style="PlainText"
          vq="satdly"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          digits="3"
          disable="Grayed out"
          tooltip="satdly"
          />
        <label loc="8 48" size="64 24"
          style="PlainText"
          label="time(sec)"
          justify="Left"
          />
        <label loc="8 24" size="64 24"
          style="PlainText"
          label="at freq."
          justify="Left"
          />
        <label loc="8 0" size="64 24"
          style="PlainText"
          label="at power"
          justify="Left"
          />
      </group>
      <check loc="0 32" size="120 24"
        style="Label2"
        label="Presaturation"
        vq="satmode"
        vc="satmode='y'"
        vc2="satmode='n'"
        set="if satmode='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='y'"
        />
      <check loc="416 24" size="136 24"
        style="Label2"
        label="N15 Refocus in t2"
        vq="dec_15n"
        vc="dec_15n='y'"
        vc2="dec_15n='n'"
        set="if (dec_15n='y' ) then $VALUE=1 else $VALUE=0 endif"
        tooltip="dec_15n='y'"
        />
      <check loc="416 56" size="136 24"
        style="Label2"
        label="C13 Refocus in t2"
        vq="dec_co "
        vc="dec_co='y'"
        vc2="dec_co='n'"
        set="if (dec_co='y' ) then $VALUE=1 else $VALUE=0 endif"
        tooltip="dec_co='y'"
        />
      <label loc="8 0" size="160 24"
        style="Heading2"
        label="Options"
        justify="Left"
        />
    </group>
  </group>
</template>
