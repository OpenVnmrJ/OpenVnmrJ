<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="375 300"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="0 0" size="360 280"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="210 65" size="45 25"
        style="PlainText"
        label="pwPlvl:"
        justify="Left"
        />
      <entry loc="265 65" size="40 25"
        style="PlainText"
        vq="pwPlvl"
        vc="pwPlvl=$VALUE"
        set="$VALUE=pwPlvl"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="265 40" size="40 25"
        style="PlainText"
        vq="pwP"
        vc="pwP=$VALUE"
        set="$VALUE=pwP"
        digits="1"
        disable="Grayed out"
        />
      <label loc="225 40" size="30 25"
        style="PlainText"
        label="pwP:"
        justify="Left"
        />
      <check loc="30 35" size="160 25"
        style="PlainText"
        label="Steady-State Pulse"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="if sspul='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="30 75" size="140 25"
        style="PlainText"
        label="Proton Saturation"
        vq="hdflg"
        vc="hdflg='y'"
        vc2="hdflg='n'"
        set="if hdflg='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="25 100" size="330 90"
        style="PlainText"
        vq="hdflg"
        show="if hdflg='y' then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        >
        <label loc="165 25" size="50 25"
          style="PlainText"
          label="Width"
          vq="wet"
          justify="Left"
          />
        <label loc="165 50" size="105 25"
          style="PlainText"
          label="Resolution (deg)"
          justify="Left"
          />
        <entry loc="272 48" size="40 24"
          style="PlainText"
          vq="hdres"
          vc="hdres=$VALUE"
          set="$VALUE=hdres"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="72 48" size="48 24"
          style="PlainText"
          vq="hdpwr"
          vc="hdpwr=$VALUE"
          set="$VALUE=hdpwr"
          digits="0"
          disable="Grayed out"
          />
        <label loc="8 48" size="48 24"
          style="PlainText"
          label="Power"
          vq="wet"
          justify="Left"
          />
        <entry loc="224 24" size="48 24"
          style="PlainText"
          vq="hd90"
          vc="hd90=$VALUE"
          set="$VALUE=hd90"
          digits="1"
          disable="Grayed out"
          />
        <label loc="280 24" size="40 24"
          style="PlainText"
          label="(usec)"
          vq="wet"
          justify="Left"
          />
        <entry loc="72 24" size="80 24"
          style="PlainText"
          vq="hdshape"
          vc="hdshape='$VALUE'"
          set="$VALUE=hdshape"
          disable="Grayed out"
          />
        <label loc="8 24" size="48 24"
          style="PlainText"
          label="Shape"
          vq="wet"
          justify="Left"
          />
      </group>
      <label loc="312 216" size="32 24"
        style="PlainText"
        label="usec"
        justify="Left"
        />
      <entry loc="272 216" size="40 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <label loc="240 216" size="32 24"
        style="PlainText"
        label="alfa:"
        justify="Left"
        />
      <entry loc="176 216" size="40 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <label loc="144 216" size="32 24"
        style="PlainText"
        label="rof2:"
        justify="Left"
        />
      <entry loc="96 216" size="40 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <label loc="8 216" size="88 24"
        style="PlainText"
        label="Delays:   rof1:"
        justify="Left"
        />
    </group>
  </group>
</template>
