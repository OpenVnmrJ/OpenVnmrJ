<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Acquisition" element="pages" type="acquisition" >
  <group size="680 256"
    style="PlainText"
    label="Acquisition"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Acquisition"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <label loc="576 232" size="104 24"
      style="Heading2"
      label="sea_gNtrosy"
      />
    <label loc="8 232" size="568 24"
      style="PlainText"
      label="SEA-TROSY  Contributed by Guang Zhu and D. Lin, Hong Kong Univ. of Science and Technology"
      />
    <group loc="328 0" size="344 232"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <radio loc="122 160" size="200 20"
        style="PlainText"
        label="Hypercomplex First Increment"
        vq="phase"
        vc="phase=1 ni=1"
        set="if phase=1 then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="132 200" size="160 20"
        style="PlainText"
        label="Half-Dwell Delay in t1"
        vq="f1180"
        vc="f1180='y'"
        vc2="f1180='n'"
        set="if f1180='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="12 160" size="110 20"
        style="PlainText"
        label="Acquisition Mode:"
        justify="Left"
        />
      <radio loc="122 180" size="140 20"
        style="PlainText"
        label="Hypercomplex 2D"
        vq="phase"
        vc="phase=1,2"
        set="$VALUE=0 $phase=size('phase') if $phase&gt;1.5 then $v=(phase[1]=1) $w=(phase[2]=2) $VALUE=($v and $w) endif"
        />
      <entry loc="152 128" size="80 24"
        style="Label2"
        vq="d2"
        vc="d2=$VALUE"
        set="$VALUE=d2"
        show="if d2=0 then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        />
      <label loc="8 128" size="152 24"
        style="Label3"
        label="Initial Evolution Time"
        justify="Left"
        />
      <group loc="8 80" size="288 48"
        style="PlainText"
        vq="ni"
        show="if ni&gt;1 then $VALUE=1 else $VALUE=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="208 24" size="48 24"
          style="PlainText"
          label="ppm"
          />
        <textmessage loc="176 24" size="32 24"
          style="PlainText"
          vq="ni sw1 dfrq2"
          set="if ni&gt;1 then $VALUE=(sw1/ni )/dfrq2 else $VALUE=0 endif"
          digits="2"
          />
        <label loc="128 24" size="48 24"
          style="PlainText"
          label="Hz, or"
          />
        <textmessage loc="96 24" size="32 24"
          style="PlainText"
          vq="ni sw1"
          set="if ni&gt;1 then $VALUE=sw1/ni else $VALUE=0 endif"
          digits="0"
          />
        <label loc="24 24" size="72 24"
          style="PlainText"
          label="Resolution:"
          />
        <label loc="192 0" size="32 24"
          style="PlainText"
          label="sec"
          />
        <textmessage loc="136 0" size="56 24"
          style="PlainText"
          vq="ni sw1"
          set="if ni&gt;1 then $VALUE=ni/sw1 else $VALUE=0 endif"
          digits="6"
          />
        <label loc="0 0" size="136 24"
          style="PlainText"
          label="Acquisition time (max.)"
          />
      </group>
      <entry loc="144 56" size="80 24"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        />
      <label loc="8 56" size="112 24"
        style="PlainText"
        label="increments in t1"
        justify="Left"
        />
      <menu loc="224 32" size="80 24"
        style="PlainText"
        label="menu"
        vq="sw1"
        vc="parunits('set','sw1','$VALUE')"
        set="parunits('get','sw1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="N15 ppm"
          chval="ppm3"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="144 32" size="80 24"
        style="PlainText"
        vq="sw1"
        vc="vnmrunits('set','sw1',$VALUE)"
        set="vnmrunits('get','sw1'):$VALUE"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        digits="1"
        />
      <label loc="8 32" size="104 24"
        style="PlainText"
        label="Spectral width"
        justify="Left"
        />
      <label loc="8 0" size="232 24"
        style="Heading2"
        label="Acquisition in F1 (N15)"
        />
    </group>
    <group loc="0 0" size="320 232"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="232 48" size="64 24"
        style="PlainText"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="250 130" size="40 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="if ss=0 then $VALUE=0 else $VALUE=1 endif"
        />
      <entry loc="280 200" size="32 24"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        />
      <label loc="216 200" size="64 24"
        style="PlainText"
        label="at power"
        />
      <entry loc="168 200" size="48 24"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="if (pw=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        />
      <label loc="8 200" size="120 24"
        style="PlainText"
        label="Observe Pulse"
        />
      <menu loc="232 176" size="56 24"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <entry loc="168 176" size="48 24"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="if (d1=0) then $VALUE=0 else $VALUE=1 endif"
        digits="3"
        />
      <label loc="8 176" size="112 24"
        style="PlainText"
        label="Relaxation delay"
        />
      <label loc="216 152" size="32 24"
        style="Label1"
        label="Auto"
        />
      <check loc="200 152" size="16 24"
        style="PlainText"
        vq="gain"
        vc="gain='n'"
        vc2="gain='y'"
        set="off('gain'):$VALUE"
        />
      <entry loc="168 152" size="32 24"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        />
      <label loc="8 152" size="88 24"
        style="PlainText"
        label="Receiver Gain"
        />
      <label loc="8 0" size="264 24"
        style="Heading2"
        label="Acquisition in F2 (Observe H1)"
        />
      <label loc="168 136" size="80 16"
        style="PlainText"
        label="Steady-State"
        />
      <textmessage loc="112 136" size="48 16"
        style="Label3"
        label="0"
        vq="ct"
        set="$VALUE=ct"
        />
      <label loc="48 136" size="64 16"
        style="PlainText"
        label="Completed"
        />
      <label loc="8 120" size="120 16"
        style="PlainText"
        label="Scans:  Requested"
        />
      <menu loc="232 72" size="64 24"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <entry loc="168 72" size="64 24"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        />
      <label loc="8 72" size="120 24"
        style="PlainText"
        label="Acquisition time"
        />
      <entry loc="168 96" size="64 24"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        />
      <label loc="24 96" size="120 24"
        style="PlainText"
        label="complex points"
        />
      <entry loc="168 120" size="64 16"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        />
      <entry loc="168 48" size="64 24"
        style="PlainText"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        />
      <entry loc="168 24" size="64 24"
        style="PlainText"
        vq="tn"
        vc="tn='$VALUE'"
        set="$VALUE=tn"
        />
      <label loc="8 24" size="64 24"
        style="PlainText"
        label="Nucleus"
        />
      <label loc="8 48" size="136 24"
        style="PlainText"
        label="Spectral width"
        />
    </group>
  </group>
</template>
