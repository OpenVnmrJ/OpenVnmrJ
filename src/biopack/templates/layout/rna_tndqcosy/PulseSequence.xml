<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="640 180"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="340 5" size="280 175"
      style="Label2"
      label="Presat"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <selmenu loc="10 10" size="255 25"
        style="PlainText"
        label="Set Saturation Mode"
        vc="exec('$VALUE')"
        bg="VJBackground"
        editable="No"
        tooltip="satmode"
        >
        <mlabel 
          label="During Relaxation Delay Only"
          chval="satmode=`ynn`"
          />
        <mlabel 
          label="During Evolution Delay Only"
          chval="satmode=`nyn`"
          />
        <mlabel 
          label="During Both Delays"
          chval="satmode=`yyn`"
          />
        <mlabel 
          label="None"
          chval="satmode=`nnn`"
          />
      </selmenu>
      <entry loc="81 127" size="30 25"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        tooltip="satpwr"
        />
      <label loc="6 127" size="65 25"
        style="PlainText"
        label="Power"
        />
      <label loc="6 102" size="170 25"
        style="PlainText"
        label="Total Relaxation Delay(sec)"
        />
      <entry loc="186 102" size="55 25"
        style="PlainText"
        vq="satdly d1"
        vc="d1=$VALUE-satdly"
        set="$VALUE=d1+satdly"
        digits="3"
        tooltip="d1=$VALUE-satdly"
        />
      <entry loc="186 77" size="55 25"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="3"
        tooltip="satdly"
        />
      <label loc="6 77" size="175 25"
        style="PlainText"
        label="Duration in Relax Delay (sec)"
        />
      <label loc="231 42" size="15 25"
        style="PlainText"
        label=")"
        />
      <textmessage loc="201 42" size="30 25"
        style="PlainText"
        label="ynn"
        vq="satmode"
        set="$VALUE=satmode"
        />
      <label loc="126 42" size="70 25"
        style="PlainText"
        label="(satmode ="
        />
    </group>
    <group loc="10 5" size="320 175"
      style="Label2"
      label="DQFCOSY"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <label loc="275 145" size="30 25"
        style="PlainText"
        label="usec"
        />
      <entry loc="230 145" size="40 25"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        />
      <label loc="205 145" size="30 25"
        style="PlainText"
        label="alfa:"
        />
      <entry loc="160 145" size="40 25"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        />
      <label loc="130 145" size="30 25"
        style="PlainText"
        label="rof2:"
        />
      <entry loc="85 145" size="40 25"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        />
      <label loc="0 145" size="90 25"
        style="PlainText"
        label="Delays:   rof1:"
        />
      <entry loc="170 90" size="55 25"
        style="PlainText"
        vq="hs hst"
        vc="hst=$VALUE"
        set="$VALUE=hst"
        show="if hs='nnyn' then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        disable="Grayed out"
        tooltip="hst"
        />
      <label loc="35 90" size="135 25"
        style="PlainText"
        label="Homospoil Time (sec)"
        justify="Left"
        />
      <label loc="125 60" size="15 25"
        style="PlainText"
        label=")"
        justify="Left"
        />
      <textmessage loc="95 60" size="25 25"
        style="PlainText"
        label="n"
        vq="hs"
        set="$VALUE=hs"
        />
      <label loc="65 60" size="30 25"
        style="PlainText"
        label="(hs ="
        justify="Left"
        />
      <check loc="5 35" size="290 25"
        style="Label1"
        label="Shim Homospoil at Start of Relaxation Delay"
        vq="hs"
        vc="hs='y'"
        vc2="hs='n'"
        set="if hs='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="hs='y'"
        />
      <check loc="7 7" size="160 25"
        style="Label1"
        label="Steady-State Pulse"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="if sspul='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="sspul='y'"
        />
    </group>
  </group>
</template>
