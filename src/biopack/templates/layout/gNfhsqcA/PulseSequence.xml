<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="775 245"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="415 0" size="345 245"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="yes"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <check loc="20 215" size="85 25"
        style="Label2"
        label="TROSY"
        vq="TROSY f1coef dm2"
        vc="TROSY='y' f1coef='1 0 -1 0 0 1 0 1' dm2='nnn'"
        vc2="TROSY='n' f1coef='' dm2='nny'"
        set="if (TROSY='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="175 215" size="35 25"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        />
      <label loc="10 190" size="285 20"
        style="PlainText"
        label="Pulse Pre-, Post- and Alfa Delays (rof1, rof2, alfa)"
        />
      <label loc="95 95" size="40 25"
        style="PlainText"
        label="gzlvl0"
        />
      <entry loc="260 215" size="30 25"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        />
      <entry loc="220 215" size="30 25"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        />
      <label loc="8 8" size="72 24"
        style="Heading2"
        label="Gradients"
        />
      <label loc="16 96" size="72 24"
        style="Label1"
        label="Crushers"
        />
      <entry loc="272 120" size="64 24"
        style="PlainText"
        vq="gt3"
        vc="gt3=$VALUE"
        set="$VALUE=gt3"
        digits="6"
        />
      <entry loc="136 96" size="48 24"
        style="PlainText"
        vq="gzlvl0"
        vc="gzlvl0=$VALUE"
        set="$VALUE=gzlvl0"
        />
      <label loc="96 120" size="40 24"
        style="PlainText"
        label="gzlvl3"
        />
      <label loc="248 8" size="32 24"
        style="PlainText"
        label="gzcal"
        />
      <entry loc="280 8" size="64 24"
        style="PlainText"
        vq="gzcal"
        vc="gzcal=$VALUE"
        set="$VALUE=gzcal"
        digits="6"
        />
      <label loc="144 72" size="120 24"
        style="Label3"
        label="DAC    or    G/cm"
        />
      <label loc="168 48" size="64 24"
        style="Label3"
        label="Strength"
        />
      <label loc="272 72" size="64 24"
        style="Label3"
        label="Time (sec)"
        />
      <entry loc="200 96" size="56 24"
        style="PlainText"
        vq="gzlvl0 gzcal"
        vc="gzlvl0=$VALUE/gzcal"
        set="$VALUE=gzlvl0*gzcal"
        digits="3"
        />
      <entry loc="272 96" size="64 24"
        style="PlainText"
        vq="gt0"
        vc="gt0=$VALUE"
        set="$VALUE=gt0"
        digits="6"
        />
      <entry loc="200 120" size="56 24"
        style="PlainText"
        vq="gzlvl3 gzcal"
        vc="gzlvl3=$VALUE/gzcal"
        set="$VALUE=gzlvl3*gzcal"
        digits="3"
        />
      <entry loc="136 120" size="48 24"
        style="PlainText"
        vq="gzlvl3"
        vc="gzlvl3=$VALUE"
        set="$VALUE=gzlvl3"
        />
    </group>
    <group loc="5 0" size="400 245"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="285 210" size="90 25"
        vq="C13refoc"
        show="if C13refoc='y' then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <check loc="0 0" size="80 25"
          style="Label2"
          label="Adiabatic"
          vq="C13refoc C13shape"
          vc="C13shape='y'"
          vc2="C13shape='n'"
          set="if ((C13refoc='y') and (C13shape='y')) then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <group loc="10 170" size="255 60"
        vq="wtg3919"
        show="if wtg3919='y' then $VALUE=0 else $VALUE=1 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <button loc="175 40" size="70 20"
          style="PlainText"
          label="Optimize"
          vc="BPwatergate2"
          decor1="No"
          />
        <button loc="175 20" size="70 20"
          style="PlainText"
          label="Optimize"
          vc="BPwatergate1"
          decor1="No"
          />
        <label loc="20 40" size="90 20"
          style="Label1"
          label="Second Pulse"
          />
        <entry loc="120 40" size="50 20"
          style="PlainText"
          vq="tpwrsf2"
          vc="tpwrsf2=$VALUE"
          set="$VALUE=tpwrsf2"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="120 20" size="50 20"
          style="PlainText"
          vq="tpwrsf1"
          vc="tpwrsf1=$VALUE"
          set="$VALUE=tpwrsf1"
          digits="0"
          disable="Grayed out"
          />
        <label loc="20 20" size="65 20"
          style="Label1"
          label="First Pulse"
          />
        <label loc="0 0" size="190 25"
          style="PlainText"
          label="Fine Power Adjust on Soft Pulses"
          justify="Left"
          />
      </group>
      <label loc="270 160" size="65 25"
        style="Label1"
        label="J(15N-1H)"
        justify="Left"
        />
      <entry loc="340 160" size="40 25"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        disable="Grayed out"
        />
      <check loc="265 190" size="125 25"
        style="Label2"
        label="Refocus C13 in t1"
        vq="C13refoc"
        vc="C13refoc='y'"
        vc2="C13refoc='n'"
        set="if (C13refoc='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="210 60" size="185 95"
        vq="wtg3919"
        show="if wtg3919='u' then $VALUE=1 else $VALUE=0 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="130 75" size="50 20"
          style="PlainText"
          vq="tpwrs"
          vc="tpwrs=$VALUE"
          set="$VALUE=tpwrs"
          digits="0"
          disable="Grayed out"
          />
        <label loc="15 75" size="65 20"
          style="Label1"
          label="Power"
          />
        <label loc="5 0" size="165 25"
          style="PlainText"
          label="For User-Created Pulses:"
          justify="Left"
          />
        <entry loc="130 50" size="50 25"
          style="PlainText"
          vq="pwHs2"
          vc="pwHs2=$VALUE"
          set="$VALUE=pwHs2"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="130 25" size="50 25"
          style="PlainText"
          vq="pwHs1"
          vc="pwHs1=$VALUE"
          set="$VALUE=pwHs1"
          digits="0"
          disable="Grayed out"
          />
        <label loc="15 50" size="115 25"
          style="PlainText"
          label="Second Pulse Width "
          justify="Left"
          />
        <label loc="15 25" size="115 25"
          style="PlainText"
          label="First Pulse Width "
          justify="Left"
          />
      </group>
      <menu loc="290 30" size="105 25"
        style="Label2"
        label="menu"
        vq="autocal"
        vc="autocal='$VALUE'"
        set="$VALUE=autocal"
        >
        <mlabel 
          label="Yes"
          chval="y"
          />
        <mlabel 
          label="No"
          chval="n"
          />
        <mlabel 
          label="Read From File"
          chval="r"
          />
        <mlabel 
          label="Semi-Automatic"
          chval="s"
          />
        <mlabel 
          label="Quiet"
          chval="q"
          />
      </menu>
      <group loc="75 5" size="195 25"
        label="switch to standard"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="switchtostandard"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <button loc="0 0" size="180 25"
          style="Label3"
          label="Switch to Standard Version"
          vq="seqfil"
          vc="length(seqfil):$length substr(seqfil,1,$length-1):seqfil"
          decor1="No"
          />
      </group>
      <check loc="290 5" size="105 25"
        style="Label2"
        label="Check Offset"
        vq="checkofs"
        vc="checkofs='y'"
        vc2="checkofs='n'"
        set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="230 30" size="55 25"
        style="Label2"
        label="AutoCal"
        justify="Left"
        />
      <group loc="10 30" size="200 130"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <group loc="10 100" size="185 25"
          vq="wtg3919"
          show="if wtg3919='y' then $VALUE=1 else $VALUE=0 endif"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <entry loc="128 0" size="48 24"
            style="PlainText"
            vq="d3"
            vc="d3=$VALUE/1e6"
            set="$VALUE=d3*1e6"
            digits="1"
            disable="Grayed out"
            />
          <label loc="16 0" size="112 24"
            style="PlainText"
            label="3919 Delay (usec)"
            justify="Left"
            />
        </group>
        <radio loc="5 50" size="160 25"
          style="PlainText"
          label="Automatic Sinc Soft Pulse"
          vq="wtg3919"
          vc="wtg3919='n'"
          vc2="wtg3919='y'"
          set="if wtg3919='n' then $VALUE=1 else $VALUE=0 endif"
          />
        <radio loc="5 25" size="165 25"
          style="PlainText"
          label="User-Created Soft Pulse"
          vq="wtg3919"
          vc="wtg3919='u'"
          vc2="wtg3919='y'"
          set="if wtg3919='u' then $VALUE=1 else $VALUE=0 endif"
          />
        <radio loc="5 75" size="75 25"
          style="PlainText"
          label="3919"
          vq="wtg3919"
          vc="wtg3919='y'"
          vc2="wtg3919='n'"
          set="if wtg3919='y' then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="8 0" size="112 24"
          style="Label1"
          label="Watergate Option"
          justify="Left"
          />
      </group>
    </group>
  </group>
</template>
