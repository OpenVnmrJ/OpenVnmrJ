<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="480 265"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <selmenu loc="30 230" size="210 25"
      style="Label4"
      label="Select Suppression Mode"
      vc="exec('$VALUE')"
      bg="VJBackground"
      editable="No"
      tooltip="satmode"
      >
      <mlabel 
        label="Water Suppression By PRESAT"
        chval="satmode=`ynn`"
        />
      <mlabel 
        label="NO Water Suppression"
        chval="satmode=`n`"
        />
    </selmenu>
    <group loc="200 135" size="265 90"
      label="Z-Filter"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="8 64" size="152 24"
        style="PlainText"
        label="Stabilization Time (usec)"
        justify="Left"
        />
      <entry loc="168 64" size="40 24"
        style="PlainText"
        vq="gstab "
        vc="gstab=$VALUE/1e6"
        set="$VALUE=gstab*1e6"
        digits="1"
        disable="Grayed out"
        tooltip="gstab=$VALUE/1e6"
        />
      <label loc="8 40" size="88 24"
        style="PlainText"
        label="Time (msec)"
        justify="Left"
        />
      <entry loc="88 40" size="48 24"
        style="PlainText"
        vq="gt2 "
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="3"
        disable="Grayed out"
        tooltip="gt2=$VALUE/1000"
        />
      <label loc="8 16" size="64 24"
        style="PlainText"
        label="Amplitude"
        justify="Left"
        />
      <entry loc="88 16" size="48 24"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        tooltip="gzlvl2"
        />
      <label loc="136 16" size="16 24"
        style="PlainText"
        label="or"
        justify="Left"
        />
      <entry loc="160 16" size="48 24"
        style="PlainText"
        vq="gzlvl2 gzcal"
        vc="gzlvl2=$VALUE/gzcal"
        set="$VALUE=gzlvl2*gzcal"
        digits="3"
        disable="Grayed out"
        tooltip="gzlvl2=$VALUE/gzcal"
        />
      <label loc="216 16" size="40 24"
        style="PlainText"
        label="G/cm"
        justify="Left"
        />
    </group>
    <group loc="290 10" size="170 80"
      style="Label2"
      label="Presat"
      vq="satmode"
      show="if satmode='ynn' then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="satdly"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        digits="3"
        tooltip="satdly"
        />
      <entry loc="90 20" size="40 20"
        style="PlainText"
        vq="satpwr"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        digits="0"
        tooltip="satpwr"
        />
      <label loc="24 40" size="80 24"
        style="PlainText"
        label="Duration(sec)"
        justify="Left"
        />
      <label loc="24 16" size="48 24"
        style="PlainText"
        label="Power"
        justify="Left"
        />
    </group>
    <group loc="8 0" size="264 104"
      style="Label2"
      label="TOCSY (DIPSI)"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="16 24" size="104 24"
        style="PlainText"
        label="Mix Time (msec)"
        />
      <entry loc="120 24" size="40 24"
        style="Label2"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="1"
        tooltip="mix"
        />
      <label loc="168 72" size="48 24"
        style="Label1"
        label="ppm)"
        />
      <textmessage loc="136 72" size="32 24"
        style="Label2"
        label="9.9"
        vq="sfrq strength"
        set="$VALUE=trunc(strength*10.0/sfrq)/10"
        digits="1"
        />
      <entry loc="32 72" size="56 24"
        style="Label2"
        vq="strength"
        vc="strength=trunc(10.0*$VALUE)/10 BPspinlock"
        set="$VALUE=strength"
        digits="0"
        tooltip="strength"
        />
      <label loc="88 72" size="48 24"
        style="Label1"
        label="Hz       ("
        />
      <label loc="8 48" size="136 24"
        style="PlainText"
        label="Spinlock Field Strength"
        />
    </group>
    <entry loc="128 200" size="32 24"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      disable="Grayed out"
      />
    <label loc="96 200" size="32 24"
      style="PlainText"
      label="alfa:"
      justify="Left"
      />
    <entry loc="128 176" size="32 24"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      disable="Grayed out"
      />
    <label loc="96 176" size="32 24"
      style="PlainText"
      label="rof2:"
      justify="Left"
      />
    <entry loc="48 176" size="40 24"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
      set="$VALUE=rof1"
      digits="1"
      disable="Grayed out"
      />
    <label loc="16 176" size="32 24"
      style="PlainText"
      label="rof1:"
      justify="Left"
      />
    <entry loc="152 152" size="40 24"
      style="PlainText"
      vq="d1"
      vc="d1=$VALUE"
      set="$VALUE=d1"
      show="if d1=0 then $VALUE=0 else $VALUE=1 endif"
      digits="3"
      disable="Grayed out"
      tooltip="d1"
      />
    <label loc="8 152" size="144 24"
      style="PlainText"
      label="Delays:   Relaxation (sec)"
      justify="Left"
      />
  </group>
</template>
