    <group loc="8 0" size="256 72"
      style="Label2"
      label="NOESY"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="200 40" size="48 24"
        style="PlainText"
        vq="d4 flippw gstab0 mix"
        vc="d4=mix - gstab0 - flippw*1e-6 -$VALUE"
        set="$VALUE=mix-gstab0-flippw*1e-6-d4"
