        style="PlainText"
        vq="d3 flag3919"
        vc="d3=$VALUE/1e6"
        set="$VALUE=d3*1e6"
        show="if flag3919='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        tooltip="d3=$VALUE/1e6"
        />
      <entry loc="64 64" size="48 24"
        style="PlainText"
        vq="p180 flag3919"
        vc="p180=$VALUE"
        set="$VALUE=p180"
        show="if (flag3919='n') then $VALUE=1 else $VALUE=0 endif"
        digits="2"
        tooltip="p180"
