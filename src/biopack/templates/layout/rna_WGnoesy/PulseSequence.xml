<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="752 232"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <check loc="280 24" size="152 24"
      style="Label3"
      label="Steady-State Pulse"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      tooltip="sspul='y'"
      />
    <group loc="456 16" size="280 168"
      style="Label2"
      label="Watergate"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <selmenu loc="134 14" size="130 25"
        style="Label3"
        label="Set Mode"
        vc="exec('$VALUE')"
        bg="VJBackground"
        editable="No"
        tooltip="flag3919"
        >
        <mlabel 
          label="By Soft Pulse"
          chval="flag3919=`n`"
          />
        <mlabel 
          label="By 3919"
          chval="flag3919=`y`"
          />
      </selmenu>
      <entry loc="96 88" size="32 24"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        digits="0"
        tooltip="tpwr"
        />
      <label loc="32 88" size="64 24"
        style="PlainText"
        label="at Power"
        />
      <label loc="16 112" size="64 24"
        style="PlainText"
        label="tau (usec)"
        />
      <entry loc="152 112" size="48 24"
        style="PlainText"
        vq="tau"
        vc="tau=$VALUE*1e-6"
        set="$VALUE=tau*1e6"
        digits="0"
        />
      <label loc="88 136" size="128 24"
        style="PlainText"
        label="3919 Delay (usec)"
        />
