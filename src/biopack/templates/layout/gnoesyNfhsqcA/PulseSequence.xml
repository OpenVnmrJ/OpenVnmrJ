        style="PlainText"
        label="Make it an Adiabatic Pulse"
        vq="C13shape"
        vc="C13shape='y'"
        vc2="C13shape='n'"
        set="if C13shape='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13shape='y'"
        />
      <check loc="5 95" size="135 25"
        style="PlainText"
        label="Refocus 13C in t1"
        vq="C13refoc"
        vc="C13refoc='y'"
        vc2="C13refoc='n'"
        set="if C13refoc='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13refoc='y'"
        />
      <label loc="300 55" size="40 25"
        style="PlainText"
