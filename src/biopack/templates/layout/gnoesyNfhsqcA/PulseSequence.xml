        style="PlainText"
        label="Pre-,&amp; Post- Acquisition Delays (rof1, rof2 &amp;  alfa)"
        />
      <entry loc="260 230" size="40 25"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
