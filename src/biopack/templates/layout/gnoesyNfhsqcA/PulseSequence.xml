        <entry loc="216 24" size="64 24"
          style="PlainText"
          vq="gt3"
          vc="gt3=$VALUE"
          set="$VALUE=gt3"
          digits="6"
          />
        <entry loc="128 24" size="64 24"
          style="PlainText"
          vq="gzlvl3 gzcal"
          vc="gzlvl3=$VALUE/gzcal"
          set="$VALUE=gzlvl3*gzcal"
          digits="3"
          />
        <entry loc="56 24" size="48 24"
          style="PlainText"
          vq="gzlvl3"
          vc="gzlvl3=$VALUE"
          set="$VALUE=gzlvl3"
          digits="0"
          />
        <label loc="8 24" size="40 24"
          style="PlainText"
          label="gzlvl3"
          />
        <entry loc="216 0" size="64 24"
          style="PlainText"
          vq="gt0"
          vc="gt0=$VALUE"
          set="$VALUE=gt0"
          digits="6"
          />
        <entry loc="128 0" size="64 24"
          style="PlainText"
          vq="gzlvl0 gzcal"
          vc="gzlvl0=$VALUE/gzcal"
          set="$VALUE=gzlvl0*gzcal"
          digits="3"
          />
        <entry loc="56 0" size="48 24"
          style="PlainText"
          vq="gzlvl0"
          vc="gzlvl0=$VALUE"
          set="$VALUE=gzlvl0"
          digits="0"
          />
      </group>
      <label loc="8 0" size="88 24"
        style="Heading2"
        label="Gradients"
        />
    </group>
  </group>
</template>
