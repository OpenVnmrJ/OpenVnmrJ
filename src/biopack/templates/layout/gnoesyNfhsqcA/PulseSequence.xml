<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="755 270"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="15 0" size="400 270"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <check loc="5 120" size="140 30"
        style="PlainText"
        label="Watergate 3919"
        vq="wtg3919"
        vc="wtg3919='y'"
        vc2="wtg3919='n'"
        set="if wtg3919='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="wtg3919='y'"
        />
      <group loc="10 150" size="150 45"
        vq="wtg3919"
        show="if wtg3919='y' then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="5 5" size="70 25"
          style="Label1"
          label="3919 delay"
          />
        <entry loc="85 5" size="60 25"
          style="PlainText"
          vq="tauWG"
          vc="tauWG=$VALUE"
          set="$VALUE=tauWG"
          digits="6"
          tooltip="tauWG"
          />
      </group>
      <group loc="20 155" size="270 110"
        vq="wtg3919"
        show="if wtg3919='y' then $SHOW=0 else $SHOW=1 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="205 85" size="45 20"
          style="PlainText"
          vq="phincr_d"
          vc="phincr_d=$VALUE"
          set="$VALUE=phincr_d"
          digits="0"
          tooltip="phincr_d"
          />
        <entry loc="205 65" size="45 20"
          style="PlainText"
          vq="phincr_u"
          vc="phincr_u=$VALUE"
          set="$VALUE=phincr_u"
          digits="0"
          tooltip="phincr_u"
          />
        <label loc="10 85" size="135 25"
          style="PlainText"
          label="Phase Adjust (down)"
          />
        <label loc="10 65" size="135 25"
          style="PlainText"
          label="Phase Adjust (up)"
          />
        <label loc="10 45" size="155 25"
          style="PlainText"
          label="Fine Power Adjust (down)"
          />
        <entry loc="205 45" size="45 20"
          style="PlainText"
          vq="tpwrsf_d"
          vc="tpwrsf_d=$VALUE"
          set="$VALUE=tpwrsf_d"
          show="if tpwrsf_d&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
          digits="0"
          tooltip="tpwrsf_d"
          />
        <entry loc="205 5" size="50 20"
          style="PlainText"
          vq="pwHs"
          vc="pwHs=[$VALUE]*1e3"
          set="$VALUE=pwHs*1e-3"
          digits="3"
          tooltip="pwHs=[$VALUE]*1e3"
          />
        <label loc="10 25" size="135 25"
          style="PlainText"
          label="Fine Power Adjust (up)"
          />
        <entry loc="205 25" size="45 20"
          style="PlainText"
          vq="tpwrsf_u"
          vc="tpwrsf_u=$VALUE"
          set="$VALUE=tpwrsf_u"
          show="if tpwrsf_u&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
          digits="0"
          tooltip="tpwrsf_u"
          />
        <label loc="5 5" size="175 20"
          style="PlainText"
          label="Water Flipback Pulse (msec)"
          />
      </group>
      <entry loc="235 55" size="50 25"
        style="PlainText"
        vq="pwClw"
        vc="pwClw=$VALUE"
        set="$VALUE=pwClw"
        digits="1"
        tooltip="pwClw"
        />
