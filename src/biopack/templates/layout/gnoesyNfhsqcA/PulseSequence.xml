        style="PlainText"
        vq="mix"
        vc="mix=$VALUE"
        set="$VALUE=mix"
        digits="4"
        tooltip="mix"
        />
      <entry loc="335 185" size="40 25"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        disable="Grayed out"
        tooltip="JNH"
        />
      <label loc="295 185" size="40 25"
        style="Label1"
        label="J(NH)"
        justify="Left"
        />
      <check loc="295 160" size="95 25"
        style="Label2"
        label="TROSY"
        vq="TROSY dm2"
        vc="TROSY='y' dm2='nnn'"
        vc2="TROSY='n'"
        set="if TROSY='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="10 30" size="310 25"
        style="Label1"
        label="Simultaneous 13C/15N Pulse Widths (3dB lower power):"
        />
      <check loc="180 5" size="105 25"
        style="Label2"
        label="Check Offset"
        vq="checkofs"
        vc="checkofs='y'"
        vc2="checkofs='n'"
        set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="checkofs='y'"
        />
      <label loc="5 5" size="55 20"
        style="Label2"
        label="AutoCal"
        justify="Left"
        />
      <menu loc="60 5" size="105 20"
        style="Label2"
        label="menu"
        vq="autocal"
        vc="autocal='$VALUE'"
        set="$VALUE=autocal"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Yes"
          chval="y"
          />
        <mlabel 
          label="No"
          chval="n"
          />
        <mlabel 
          label="Read From File"
          chval="r"
          />
        <mlabel 
          label="Semi-Automatic"
          chval="s"
          />
        <mlabel 
          label="Quiet"
          chval="q"
          />
      </menu>
    </group>
    <group loc="425 0" size="330 270"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
