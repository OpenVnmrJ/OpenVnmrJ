<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="CACO" element="pages" type="acquisition" >
  <group size="500 190"
    style="PlainText"
    label="CACO"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="CACO"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 10" size="465 170"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="scansperFID"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <button loc="185 90" size="270 35"
        style="Heading2"
        label="Setup pseudo 3D IPAP"
        vc="$xx=0.0 if decCB='y' then $xx=2.0 endif   ni=sw1*(TC-$xx*pwCB_180*1e-6) phase=1,2 IPAP='i','a' array='IPAP,phase' su"
        bg="VJBackground"
        decor1="yes"
        />
      <check loc="10 90" size="145 25"
        style="PlainText"
        label="start from 1H"
        vq="Hstart"
        vc="Hstart='y' dm='nyyy' dmm='cppp'"
        vc2="Hstart='n' dm='yyyy' dmm='pppp'  "
        set="if Hstart='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="10 140" size="90 25"
        style="PlainText"
        label="anti-phase"
        vq="IPAP"
        vc="IPAP='a'"
        set="if IPAP='a' then $VALUE=1 else $VALUE=0 endif"
        />
      <radio loc="10 115" size="90 25"
        style="PlainText"
        label="in-phase"
        vq="IPAP"
        vc="IPAP='i'"
        set="if IPAP='i' then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 65" size="160 25"
        style="PlainText"
        label="decouple from CB in t1"
        vq="decCB"
        vc="decCB='y' TC=0.008"
        vc2="decCB='n' TC=0.006"
        set="if decCB='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="dec on CB gives more sensitivity and resolution on CA, but may decrease THR and SER signals and does not show sidechain CO"
        />
      <button loc="185 30" size="270 30"
        style="Heading2"
        label="Run 1D"
        vc="ni=1 phase=1 ga"
        bg="VJBackground"
        decor1="yes"
        />
      <button loc="185 60" size="270 30"
        style="Heading2"
        label="Setup 2D with full resolution in CA"
        vc="$xx=0.0 if decCB='y' then $xx=2.0 endif   ni=sw1*(TC-$xx*pwCB_180*1e-6) phase=1,2 su"
        bg="VJBackground"
        decor1="yes"
        />
      <entry loc="95 35" size="40 25"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        />
      <label loc="5 35" size="85 25"
        style="Label1"
        label="scans per FID"
        />
      <label loc="5 5" size="50 25"
        style="Heading2"
        label="CACO"
        />
    </group>
  </group>
</template>
