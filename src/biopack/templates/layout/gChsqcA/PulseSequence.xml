<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="776 268"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 0" size="745 265"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <menu loc="625 110" size="105 25"
        style="Label2"
        label="menu"
        vq="autocal"
        vc="autocal='$VALUE'"
        set="$VALUE=autocal"
        bg="VJBackground"
        editable="No"
        tooltip="autocal='$VALUE'"
        >
        <mlabel 
          label="Yes"
          chval="y"
          />
        <mlabel 
          label="No"
          chval="n"
          />
        <mlabel 
          label="Read From File"
          chval="r"
          />
        <mlabel 
          label="Semi-Automatic"
          chval="s"
          />
        <mlabel 
          label="Quiet"
          chval="q"
          />
      </menu>
      <button loc="370 0" size="200 25"
        style="Label3"
        label="Switch to Standard Version"
        vq="seqfil dof dfrq layout"
        vc="seqfil='gChsqc' BPmake90CO_Ca BPmake180CO_Ca BPmake180COa layout=seqfil "
        bg="VJBackground"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="145 0" size="200 25"
        style="Label3"
        label="Switch to BioNMR Version"
        vq="seqfil dof dfrq layout"
        vc="seqfil='gChsqcP' BPmake90CO_Ca BPmake180CO_Ca BPmake180COa dof=dof+(174-35)*dfrq layout=seqfil "
        bg="VJBackground"
        decor1="yes"
        tooltip="seqfil='gChsqcP'"
        halignment="Center"
        valignment="Center"
        />
      <check loc="624 136" size="104 24"
        style="Label2"
        label="Check Offset"
        vq="checkofs"
        vc="checkofs='y'"
        vc2="checkofs='n'"
        set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="checkofs='y'"
        />
      <label loc="568 112" size="56 24"
        style="Label2"
        label="AutoCal"
        justify="Left"
        />
      <entry loc="688 240" size="40 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        />
      <entry loc="688 216" size="40 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        />
      <entry loc="688 192" size="40 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        />
      <label loc="520 240" size="168 24"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        />
      <label loc="544 216" size="144 24"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        />
      <label loc="552 192" size="136 24"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        />
      <entry loc="464 240" size="40 24"
        style="PlainText"
        vq="calC"
        vc="calC=$VALUE"
        set="$VALUE=calC"
        show="if calC &lt;&gt; 1.000 then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        />
      <label loc="424 240" size="40 24"
        style="PlainText"
        label="calC"
        />
      <entry loc="464 216" size="40 24"
        style="PlainText"
        vq="calH"
        vc="calH=$VALUE"
        set="$VALUE=calH"
        show="if calH &lt;&gt; 1.000 then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        />
      <label loc="424 216" size="40 24"
        style="PlainText"
        label="calH"
        />
      <group loc="8 168" size="120 96"
        style="PlainText"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <radio loc="8 72" size="112 24"
          style="PlainText"
          label="Normal HSQC"
          vq="T1 T2 T1rho"
          vc="T1='n' T2='n' T1rho='n'"
          set="if (T1='n' and T2='n' and T1rho='n') then $VALUE=1 else $VALUE=0 endif"
          tooltip="T1='n' T2='n' T1rho='n'"
          />
        <radio loc="8 0" size="80 24"
          style="PlainText"
          label="C13 T1"
          vq="T1 T2 T1rho"
          vc="T1='y' T2='n' T1rho='n'"
          set="if (T1='y') then $VALUE=1 else $VALUE=0 endif"
          tooltip="T1='y' "
          />
        <radio loc="8 48" size="88 24"
          style="PlainText"
          label="C13 T1rho"
          vq="T1 T2 T1rho"
          vc="T1='n' T2='n' T1rho='y'"
          set="if (T1rho='y') then $VALUE=1 else $VALUE=0 endif"
          tooltip="T1rho='y'"
          />
        <radio loc="8 24" size="80 24"
          style="PlainText"
          label="C13 T2"
          vq="T1 T2 T1rho"
          vc="T1='n' T2='y' T1rho='n'"
          set="if (T2='y') then $VALUE=1 else $VALUE=0 endif"
          tooltip="T2='y' "
          />
      </group>
      <label loc="8 144" size="104 24"
        style="Heading2"
        label="Relaxation"
        />
      <group loc="176 216" size="192 48"
        style="PlainText"
        vq="T1 T2 T1rho"
        show="if (T1='y' or T2='y' or T1rho='y') then $VALUE=1 else $VALUE=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="8 24" size="120 24"
          style="PlainText"
          label="Maximum Mix Time"
          />
        <entry loc="128 24" size="48 24"
          style="PlainText"
          vq="maxrelaxT"
          vc="maxrelaxT=$VALUE"
          set="$VALUE=maxrelaxT"
          show="if (T1='y' or T2='y' or T1rho='y') then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          tooltip="maxrelaxT"
          />
        <entry loc="128 0" size="48 24"
          style="PlainText"
          vq="relaxT"
          vc="relaxT=$VALUE"
          set="$VALUE=relaxT"
          show="if (T1='y' or T2='y' or T1rho='y') then $VALUE=1 else $VALUE=0 endif"
          digits="3"
          tooltip="relaxT"
          />
        <label loc="48 0" size="80 24"
          style="PlainText"
          label="Mixing Time"
          />
      </group>
      <check loc="176 192" size="224 24"
        style="PlainText"
        label="Refocus Beta's below 67ppm in t1"
        vq="Crefoc_l alphaC"
        vc="if alphaC='y' then Crefoc_l='y' endif"
        vc2="Crefoc_l='n'"
        tooltip="if alphaC='y' then Crefoc_l='y' endif"
        />
      <check loc="176 168" size="224 24"
        style="PlainText"
        label="Refocus Beta's above 46ppm in t1"
        vq="Crefoc_r alphaC"
        vc="if alphaC='y' then Crefoc_r='y' endif"
        vc2="Crefoc_r='n'"
        tooltip="if alphaC='y' then Crefoc_r='y' endif"
        />
      <label loc="160 144" size="184 24"
        style="Label1"
        label="For the case of Alpha Carbons"
        />
      <check loc="160 120" size="208 24"
        style="PlainText"
        label="STUD Decoupling in Acquisition"
        vq="STUD"
        vc="STUD='y'"
        vc2="STUD='n'"
        set="if (STUD='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="STUD='y'"
        />
      <check loc="160 96" size="144 24"
        style="PlainText"
        label="Observe only CH2's"
        vq="CH2only"
        vc="CH2only='y'"
        vc2="CH2only='n'"
        set="if (CH2only='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="CH2only='y'"
        />
      <check loc="160 72" size="136 24"
        style="PlainText"
        label="Refocus C=O in t1"
        vq="COrefoc"
        vc="COrefoc='y'"
        vc2="COrefoc='n'"
        set="if (COrefoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="COrefoc='y'"
        />
      <check loc="160 48" size="128 24"
        style="PlainText"
        label="N15 Refocus in t1"
        vq="N15refoc"
        vc="N15refoc='y'"
        vc2="N15refoc='n'"
        set="if N15refoc='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="N15refoc='y'"
        />
      <group loc="16 48" size="112 96"
        style="PlainText"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <radio loc="8 0" size="104 24"
          style="PlainText"
          label="All Carbons"
          vq="allC aliph arom alphaC"
          vc="allC='y' aliph='n' arom='n' alphaC='n' BPsetwurstparams"
          set="if allC='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="allC='y' "
          />
        <radio loc="8 72" size="80 24"
          style="PlainText"
          label="Aromatic"
          vq="allC aliph arom alphaC"
          vc="allC='n' aliph='n' arom='y' alphaC='n' BPsetwurstparams"
          set="if arom='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="arom='y' "
          />
        <radio loc="8 48" size="80 24"
          style="PlainText"
          label="Alpha"
          vq="allC aliph arom alphaC"
          vc="allC='n' aliph='n' arom='n' alphaC='y' BPsetwurstparams"
          set="if alphaC='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="alphaC='y' "
          />
        <radio loc="8 24" size="80 24"
          style="PlainText"
          label="Aliphatic"
          vq="allC aliph arom alphaC"
          vc="allC='n' aliph='y' arom='n' alphaC='n' BPsetwurstparams"
          set="if aliph='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="aliph='y' "
          />
      </group>
      <label loc="144 24" size="64 24"
        style="Heading2"
        label="Options"
        />
      <label loc="8 24" size="88 24"
        style="Heading2"
        label="Carbon Type"
        />
      <label loc="392 32" size="160 24"
        style="Heading2"
        label="F1 Detection Mode"
        />
      <radio loc="416 80" size="128 24"
        style="PlainText"
        label="Phase-Cycling"
        vq="SE ZZ f1coef"
        vc="ZZ='y' SE='n' f1coef='' BPsetddrtc"
        set="if (ZZ='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="ZZ='y' "
        />
      <radio loc="416 56" size="176 24"
        style="PlainText"
        label="Sensitivity-Enhancement"
        vq="SE ZZ  CT f1coef"
        vc="SE='y' ZZ='n' if CT='y' then f1coef='1 0 -1 0 0 1 0 1' else f1coef='1 0 -1 0 0 -1 0 -1' endif  BPsetddrtc"
        set="if (SE='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="SE='y' "
        />
      <check loc="408 120" size="152 24"
        style="PlainText"
        label="Constant Time"
        vq="CT"
        vc="CT='y'"
        vc2="CT='n'"
        tooltip="CT='y'"
        />
      <entry loc="440 144" size="40 24"
        style="PlainText"
        vq="CTdelay CT"
        vc="CTdelay=[$VALUE]*1e-3"
        set="$VALUE=CTdelay/1e-3"
        show="if CT='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        tooltip="CTdelay=[$VALUE]*1e-3"
        />
      <label loc="480 144" size="64 24"
        style="PlainText"
        label="(msec)"
        />
      <entry loc="672 80" size="48 24"
        style="PlainText"
        vq="tCH"
        vc="tCH=$VALUE"
        set="$VALUE=tCH"
        digits="4"
        tooltip="tCH"
        />
      <label loc="608 80" size="64 24"
        style="PlainText"
        label="tCH"
        />
      <label loc="600 32" size="64 24"
        style="Label1"
        label="J(13C-1H)"
        />
      <entry loc="672 32" size="40 24"
        style="PlainText"
        vq="tCH lambda arom"
        vc="tCH=1/(4.0*$VALUE) if arom='y' then lambda=0.0013 else lambda=0.0016 endif"
        set="$VALUE=1/(4.0*tCH)"
        digits="1"
        tooltip="tCH=1/(4.0*$VALUE) "
        />
      <entry loc="672 56" size="48 24"
        style="PlainText"
        vq="lambda"
        vc="lambda=$VALUE"
        set="$VALUE=lambda"
        digits="4"
        tooltip="lambda"
        />
      <label loc="608 56" size="64 24"
        style="PlainText"
        label="lambda"
        />
    </group>
  </group>
</template>
