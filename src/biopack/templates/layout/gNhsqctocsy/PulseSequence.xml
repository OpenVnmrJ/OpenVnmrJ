<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="760 280"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="340 0" size="420 280"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <check loc="260 10" size="100 20"
        style="Label2"
        label="Magic-Angle"
        vq="mag_flg pfgon"
        vc="if pfgon='yyy' then mag_flg='y' endif"
        vc2="mag_flg='n'"
        set="if ((mag_flg='y') and (pfgon='yyy')) then $VALUE=1 else $VALUE=0 endif"
        tooltip="mag_flg='y' "
        />
      <label loc="120 240" size="30 20"
        style="PlainText"
        label="gzcal"
        justify="Left"
        />
      <entry loc="160 240" size="60 20"
        style="PlainText"
        vq="gzcal"
        vc="gzcal=$VALUE"
        set="$VALUE=gzcal"
        digits="6"
        disable="Grayed out"
        />
      <label loc="250 240" size="30 20"
        style="PlainText"
        label="gstab"
        justify="Left"
        />
      <entry loc="290 240" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE"
        set="$VALUE=gstab"
        digits="6"
        disable="Grayed out"
        />
      <group loc="110 180" size="300 40"
        style="PlainText"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        rows="1"
        columns="1"
        >
        <entry loc="216 8" size="64 24"
          style="PlainText"
          vq="gt3"
          vc="gt3=$VALUE"
          set="$VALUE=gt3"
          digits="6"
          />
        <entry loc="128 8" size="64 24"
          style="PlainText"
          vq="gzlvl3 gzcal"
          vc="gzlvl3=$VALUE/gzcal"
          set="$VALUE=gzlvl3*gzcal"
          digits="3"
          />
        <entry loc="56 8" size="48 24"
          style="PlainText"
          vq="gzlvl3"
          vc="gzlvl3=$VALUE"
          set="$VALUE=gzlvl3"
          digits="0"
          />
        <label loc="8 8" size="40 24"
          style="PlainText"
          label="gzlvl3"
          />
      </group>
      <label loc="8 0" size="88 24"
        style="Heading2"
        label="Gradients"
        />
      <label loc="16 184" size="96 24"
        style="Label1"
        label="Homospoils"
        />
      <group loc="112 120" size="296 48"
        style="PlainText"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        rows="1"
        columns="1"
        >
        <entry loc="128 0" size="64 24"
          style="PlainText"
          vq="gzlvl0 gzcal"
          vc="gzlvl0=$VALUE/gzcal"
          set="$VALUE=gzlvl0*gzcal"
          digits="3"
          />
        <entry loc="216 24" size="64 24"
          style="PlainText"
          vq="gt5"
          vc="gt5=$VALUE"
          set="$VALUE=gt5"
          digits="6"
          />
        <entry loc="216 0" size="64 24"
          style="PlainText"
          vq="gt0"
          vc="gt0=$VALUE"
          set="$VALUE=gt0"
          digits="6"
          />
        <entry loc="128 24" size="64 24"
          style="PlainText"
          vq="gzlvl5 gzcal"
          vc="gzlvl5=$VALUE/gzcal"
          set="$VALUE=gzlvl5*gzcal"
          digits="3"
          />
        <entry loc="56 24" size="48 24"
          style="PlainText"
          vq="gzlvl5"
          vc="gzlvl5=$VALUE"
          set="$VALUE=gzlvl5"
          digits="0"
          />
        <entry loc="56 0" size="48 24"
          style="PlainText"
          vq="gzlvl0"
          vc="gzlvl0=$VALUE"
          set="$VALUE=gzlvl0"
          digits="0"
          />
        <label loc="8 24" size="40 24"
          style="PlainText"
          label="gzlvl5"
          />
        <label loc="8 0" size="40 24"
          style="PlainText"
          label="gzlvl0"
          />
      </group>
      <label loc="16 120" size="96 24"
        style="Label1"
        label="Crushers"
        />
      <group loc="112 64" size="296 48"
        style="PlainText"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        rows="1"
        columns="1"
        >
        <entry loc="216 0" size="64 24"
          style="PlainText"
          vq="gt1"
          vc="gt1=$VALUE"
          set="$VALUE=gt1"
          digits="6"
          />
        <entry loc="128 0" size="64 24"
          style="PlainText"
          vq="gzlvl1 gzcal"
          vc="gzlvl1=$VALUE/gzcal"
          set="$VALUE=gzlvl1*gzcal"
          digits="3"
          />
        <entry loc="128 24" size="64 24"
          style="PlainText"
          vq="gzlvl2 gzcal"
          vc="gzlvl2=$VALUE/gzcal"
          set="$VALUE=gzcal*gzlvl2"
          digits="3"
          />
        <entry loc="56 0" size="48 24"
          style="PlainText"
          vq="gzlvl1"
          vc="gzlvl1=$VALUE"
          set="$VALUE=gzlvl1"
          digits="0"
          />
        <entry loc="56 24" size="48 24"
          style="PlainText"
          vq="gzlvl2"
          vc="gzlvl2=$VALUE"
          set="$VALUE=gzlvl2"
          digits="0"
          />
        <label loc="8 0" size="40 24"
          style="PlainText"
          label="gzlvl1"
          />
        <label loc="8 24" size="40 24"
          style="PlainText"
          label="gzlvl2"
          />
      </group>
      <label loc="16 64" size="88 24"
        style="Label1"
        label="Coherence"
        />
      <label loc="328 48" size="64 16"
        style="Label3"
        label="Time (sec)"
        />
      <label loc="240 48" size="64 16"
        style="Label3"
        label="(G/cm)"
        />
      <label loc="168 48" size="72 16"
        style="Label3"
        label="DAC     or"
        />
      <label loc="200 32" size="64 16"
        style="Label3"
        label="Strength"
        />
    </group>
    <group loc="0 0" size="333 280"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <label loc="0 8" size="136 24"
        style="PlainText"
        label="Tocsy Mix Time (sec)"
        />
      <group loc="0 32" size="330 48"
        vq="mix"
        show="if mix&gt;0.05 then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="60 20" size="260 20"
          style="Label2"
          label="You might consider gtocsyNhsqc as alternative"
          />
        <label loc="10 0" size="320 20"
          style="Label2"
          label="Warning: Longer mix times can lose signal from diffusion."
          />
      </group>
      <entry loc="270 10" size="50 20"
        style="PlainText"
        vq="mix strength"
        vc="strength=$VALUE BPspinlock"
        set="$VALUE=strength"
        show="if mix&gt;0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="strength"
        />
      <label loc="180 10" size="100 20"
        style="PlainText"
        label="at Strength (Hz)"
        />
      <entry loc="130 10" size="50 20"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE"
        set="$VALUE=mix"
        digits="4"
        tooltip="mix"
        />
      <group loc="40 248" size="224 24"
        label="switch to Automatic"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="switchtoautomatic"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="208 24"
          style="Label3"
          label="Switch to Automatic Pbox Version"
          vq="seqfil layout"
          vc="seqfil=seqfil+'A' layout=seqfil"
          bg="VJBackground"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <entry loc="184 104" size="48 24"
        style="PlainText"
        vq="tpwrsf"
        vc="tpwrsf=$VALUE"
        set="$VALUE=tpwrsf"
        show="if tpwrsf&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf"
        />
      <label loc="56 104" size="120 24"
        style="PlainText"
        label="Fine Power Adjust"
        />
      <entry loc="232 128" size="40 24"
        style="PlainText"
        vq="JNH"
        vc="JNH=$VALUE"
        set="$VALUE=JNH"
        digits="1"
        tooltip="JNH"
        />
      <label loc="192 128" size="40 24"
        style="Label1"
        label="J(NH)"
        />
      <check loc="16 128" size="136 24"
        style="Label2"
        label="Refocus 13C in t1/t2"
        vq="C13refoc"
        vc="C13refoc='y'"
        vc2="C13refoc='n'"
        set="if C13refoc='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13refoc='y'"
        />
      <check loc="16 152" size="80 24"
        style="Label2"
        label="NH2only"
        vq="NH2only"
        vc="NH2only='y'"
        vc2="NH2only='n'"
        set="if NH2only='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="NH2only='y'"
        />
      <label loc="256 176" size="32 24"
        style="PlainText"
        label="usec"
        />
      <entry loc="208 176" size="48 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        />
      <label loc="56 176" size="160 24"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        />
      <label loc="48 200" size="160 24"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        />
      <entry loc="208 200" size="48 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        />
      <label loc="256 200" size="32 24"
        style="PlainText"
        label="usec"
        />
      <label loc="256 224" size="32 24"
        style="PlainText"
        label="usec"
        />
      <entry loc="208 224" size="48 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        />
      <label loc="32 224" size="168 24"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        />
      <entry loc="184 80" size="48 24"
        style="PlainText"
        vq="pwHs"
        vc="pwHs=[$VALUE]*1e3"
        set="$VALUE=pwHs*1e-3"
        digits="3"
        tooltip="pwHs=[$VALUE]*1e3"
        />
      <label loc="8 80" size="176 24"
        style="PlainText"
        label="Water Flipback Pulse (msec)"
        />
    </group>
  </group>
</template>
