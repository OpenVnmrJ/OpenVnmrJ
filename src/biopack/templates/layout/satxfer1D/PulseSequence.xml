          vq="satpwr satmode"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          show="$ENABLE=((satmode='yy')  or (satmode='yn'))"
          digits="0"
          disable="Grayed out"
          tooltip="satpwr"
