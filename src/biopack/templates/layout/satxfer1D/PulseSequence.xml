        <entry loc="50 30" size="45 20"
          style="PlainText"
          vq="satdly satmode"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          show="$ENABLE=((satmode='yy') or (satmode='yn'))"
          digits="3"
          disable="Grayed out"
          tooltip="satdly"
          />
