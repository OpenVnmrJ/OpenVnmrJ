          style="PlainText"
          vq="satfrq satmode"
          vc="satfrq=$VALUE"
          set="$VALUE=satfrq"
          show="$ENABLE=((satmode='yy') or (satmode='yn'))"
          digits="1"
          disable="Grayed out"
          tooltip="satfrq"
