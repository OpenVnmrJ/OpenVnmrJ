        tooltip="mix"
        />
      <label loc="100 75" size="135 25"
        style="Label1"
        label="Noesy Mix Time (sec)"
        justify="Left"
        />
      <label loc="105 10" size="45 25"
        style="Label1"
        label="tauCH1"
        justify="Left"
        />
      <label loc="105 35" size="45 25"
        style="Label1"
        label="tauCH2"
        justify="Left"
        />
      <entry loc="150 35" size="50 25"
