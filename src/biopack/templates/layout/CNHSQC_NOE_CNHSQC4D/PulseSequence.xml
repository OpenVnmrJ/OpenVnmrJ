      <entry loc="48 104" size="48 24"
        style="PlainText"
        vq="swN"
        vc="swN=$VALUE"
        set="$VALUE=swN"
        disable="Grayed out"
        />
      <label loc="8 104" size="40 24"
        style="PlainText"
        label="swN"
        justify="Left"
        />
      <entry loc="48 72" size="48 24"
        style="PlainText"
        vq="swC"
        vc="swC=$VALUE"
        set="$VALUE=swC"
        disable="Grayed out"
        />
      <label loc="8 72" size="40 24"
        style="PlainText"
        label="swC"
        justify="Left"
        />
      <label loc="8 8" size="40 24"
        style="Label1"
