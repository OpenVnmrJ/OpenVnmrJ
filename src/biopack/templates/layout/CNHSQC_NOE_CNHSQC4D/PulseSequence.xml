        />
      <entry loc="290 100" size="55 25"
        style="PlainText"
        vq="ch90dmf"
        vc="ch90dmf=$VALUE"
        set="$VALUE=ch90dmf"
        digits="0"
        disable="Grayed out"
        tooltip="ch90dmf"
        />
      <entry loc="205 75" size="60 25"
        style="PlainText"
        vq="caco180shape"
        vc="caco180shape='$VALUE'"
        set="$VALUE=caco180shape"
        digits="0"
        disable="Grayed out"
        tooltip="caco180shape"
        />
      <entry loc="265 75" size="25 25"
        style="PlainText"
        vq="caco80pwr"
        vc="caco180pwr=$VALUE"
        set="$VALUE=caco180pwr"
        digits="0"
        disable="Grayed out"
        tooltip="caco180pwr"
        />
      <entry loc="290 75" size="50 25"
        style="PlainText"
        vq="caco180pw"
        vc="caco180pw=$VALUE"
        set="$VALUE=caco180pw"
        digits="1"
        disable="Grayed out"
        tooltip="caco180pw"
        />
      <label loc="5 50" size="200 25"
        style="PlainText"
        label="1H 180   Pulse Shape/pwr/pw"
        justify="Left"
        />
      <entry loc="205 50" size="60 25"
        style="PlainText"
        vq="ch180shape"
        vc="ch180shape='$VALUE'"
        set="$VALUE=ch180shape"
        digits="0"
        disable="Grayed out"
        tooltip="ch180shape"
        />
      <entry loc="265 50" size="25 25"
        style="PlainText"
