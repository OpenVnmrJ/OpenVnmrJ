        style="Heading2"
        label="Shaped  Pulses"
        justify="Left"
        />
    </group>
    <entry loc="820 155" size="35 25"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      disable="Grayed out"
      />
    <group loc="5 150" size="710 145"
      style="Heading2"
      label="Setup Experiment"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="4D exp type"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <radio loc="5 70" size="700 20"
        style="PlainText"
        label="4D (TS 15N/13C-HSQC)-NOE-(TS 15N-TROSY/13C-seHSQC); HN&lt;-&gt;H3C cross-peaks of opposite sign as diagonal"
        vq="exp_mode"
        vc="exp_mode='4Da'"
        set="if (exp_mode='4Da') then $VALUE=1 else $VALUE=0 endif"
        tooltip="exp_mode='4Da'"
        />
      <radio loc="5 90" size="705 20"
        style="PlainText"
        label="4D (TS 15N/13C-HSQC)-NOE-(TS 15N-TROSY/13C-seHSQC); HN&lt;-&gt;H3C cross-peaks of the same sign as diagonal"
        vq="exp_mode"
        vc="exp_mode='4D'"
        set="if (exp_mode='4D') then $VALUE=1 else $VALUE=0 endif"
        tooltip="exp_mode='4D'"
        />
      <button loc="5 115" size="700 20"
        style="PlainText"
        label="Setup a full 4Dx2  template (an interleaved series of 4D spectra with alternating signs for HN &lt;-&gt;H3C cross-peaks )"
        vc="ni=10 ni2=12 ni3=8 phase=1,2 phase2=1,2 phase3=1,2 exp_mode='4D','4Da'  array='phase,phase2,phase3,exp_mode'"
        bg="VJBackground"
        decor1="yes"
        tooltip="ni=10 ni2=12 ni3=8 phase=1,2 phase2=1,2 phase3=1,2 exp_mode='4D','4Da'  array='phase,phase2,phase3,exp_mode'"
        />
      <radio loc="5 45" size="270 25"
        style="PlainText"
        label="3D NOE - (TS 15N-TROSY/13C-seHSQC)"
        vq="exp_mode"
        vc="exp_mode='3D' ni3=0 phase3=1"
        set="if (exp_mode='3D') then $VALUE=1 else $VALUE=0 endif"
        tooltip="exp_mode='3D' ni3=0 phase3=1"
        />
      <radio loc="5 20" size="230 25"
        style="PlainText"
        label="2D TS 15N-TROSY/13C-seHSQC"
        vq="exp_mode"
        vc="exp_mode='2D' ni3=0 phase3=1 ni2=0 phase2=1"
        set="if (exp_mode='2D') then $VALUE=1 else $VALUE=0 endif"
        tooltip="exp_mode='2D' ni3=0 phase3=1 ni2=0 phase2=1"
        />
    </group>
    <group loc="640 5" size="220 105"
      style="Heading2"
      label="Flipback Pulses"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="flipbacks"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="0 30" size="90 25"
        style="PlainText"
        label="Flipback Width"
        vq="wet"
        />
      <entry loc="95 30" size="50 20"
        style="PlainText"
        vq="pwHs"
        vc="pwHs=[$VALUE]*1e3"
        set="$VALUE=pwHs/1.0e3"
        digits="3"
        tooltip="pwHs=[$VALUE]*1e3"
        />
      <button loc="150 55" size="70 25"
        style="PlainText"
        label="Optimize"
        vq="tpwrsf_d ni phase"
        vc="array('tpwrsf_d',20,3900,-50) ni=1 phase=1 nt=2 wnt='wft dssh' wexp='' au"
        bg="transparent"
        decor1="no"
        />
      <entry loc="95 55" size="50 20"
        style="PlainText"
        vq="tpwrsf_d"
        vc="tpwrsf_d=$VALUE"
        set="$VALUE=tpwrsf_d"
        show="if tpwrsf_d&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf_d"
        />
      <label loc="0 55" size="85 20"
        style="PlainText"
        label="Fine Power(d)"
        />
      <label loc="0 80" size="85 20"
        style="PlainText"
        label="Fine Power(u)"
        />
      <entry loc="95 80" size="50 20"
        style="PlainText"
        vq="tpwrsf_u"
        vc="tpwrsf_u=$VALUE"
        set="$VALUE=tpwrsf_u"
        show="if tpwrsf_u&lt;4095.0 then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tpwrsf_u"
        />
      <button loc="150 80" size="70 25"
        style="PlainText"
        label="Optimize"
        vq="tpwrsf_u ni phase"
        vc="array('tpwrsf_u',20,2800,-50)  ni=1 phase=1 nt=2 wnt='wft dssh' wexp='' au"
        bg="transparent"
        decor1="no"
        />
    </group>
    <label loc="785 180" size="30 25"
      style="PlainText"
      label="alfa"
      justify="Left"
      />
    <entry loc="820 180" size="35 25"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      disable="Grayed out"
      />
    <label loc="785 155" size="35 25"
      style="PlainText"
      label="rof2"
      justify="Left"
      />
    <label loc="785 130" size="35 25"
      style="PlainText"
      label="rof1"
      justify="Left"
      />
