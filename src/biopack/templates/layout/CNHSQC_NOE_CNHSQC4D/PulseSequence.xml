        bg="VJBackground"
        decor1="yes"
        />
      <check loc="5 25" size="165 25"
        style="Label2"
        label="Decouple Ca and CO"
        vq="decCACO"
        vc="decCACO='y'"
        vc2="decCACO='n'"
        set="if decCACO='y' then $VALUE=1 else $VALUE=0 endif "
        tooltip="decCACO='y'"
        />
      <entry loc="205 100" size="60 25"
        style="PlainText"
        vq="ch90corr"
        vc="ch90corr=$VALUE"
