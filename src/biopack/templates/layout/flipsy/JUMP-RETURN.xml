<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="JUMP-RETURN" element="pages" type="acquisition" >
  <group size="568 232"
    style="PlainText"
    label="JUMP-RETURN"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="JUMP-RETURN"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <button loc="272 136" size="272 32"
      style="Heading2"
      label="Optimize Jump-Return Parameters"
      vc="BPautojumpret(1)"
      bg="transparent"
      decor1="no"
      tooltip="BPautojumpret(1)"
      halignment="Center"
      valignment="Center"
      />
    <check loc="416 200" size="112 24"
      style="Label3"
      label="Steady-State"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      set="if sspul='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip="sspul='y'"
      />
    <check loc="24 24" size="152 24"
      style="Heading2"
      label="Jump-Return"
      vq="presat shaped wet watergate jumpret dpfgse purge pslabel seqfl fliipsy layout"
      vc="presat='n' shaped='n' wet='n' watergate='n' jumpret='y' dpfgse='n' flipsy='n' purge='n' seqfil='water' layout=seqfil pslabel='water_jumpreturn' acqdisp"
      vc2="jumpret='n' pslabel='water' acqdisp seqfil='water' layout=seqfil"
      set="if jumpret='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip="jumpret='y' "
      />
    <entry loc="200 80" size="48 24"
      style="PlainText"
      vq="d1 jumpret"
      vc="d1=$VALUE"
      set="$VALUE=d1"
      show="if jumpret='y' then $VALUE=1 else $VALUE=0 endif"
      digits="3"
      disable="Grayed out"
      tooltip="d1"
      />
    <label loc="48 80" size="144 24"
      style="PlainText"
      label="Relaxation Delay (sec)"
      />
    <label loc="48 112" size="64 24"
      style="PlainText"
      label="First Pulse"
      />
    <entry loc="200 112" size="48 24"
      style="PlainText"
      vq="jrp1 jumpret"
      vc="jrp1=$VALUE"
      set="$VALUE=jrp1"
      show="if jumpret='y' then $VALUE=1 else $VALUE=0 endif"
      digits="3"
      disable="Grayed out"
      tooltip="jrp1"
      />
    <entry loc="200 144" size="48 24"
      style="PlainText"
      vq="jrdelay jumpret"
      vc="jrdelay=$VALUE/1e6"
      set="$VALUE=jrdelay*1e6"
      show="if jumpret='y' then $VALUE=1 else $VALUE=0 endif"
      digits="3"
      disable="Grayed out"
      tooltip="jrdelay=$VALUE/1e6"
      />
    <label loc="48 144" size="152 24"
      style="PlainText"
      label="Inter-pulse Delay (usec)"
      />
    <label loc="48 176" size="96 24"
      style="PlainText"
      label="Second Pulse"
      />
    <entry loc="200 176" size="48 24"
      style="PlainText"
      vq="jrpw jumpret"
      vc="jrpw=$VALUE"
      set="$VALUE=jrpw"
      show="if jumpret='y' then $VALUE=1 else $VALUE=0 endif"
      digits="3"
      disable="Grayed out"
      tooltip="jrpw"
      />
    <entry loc="200 208" size="48 24"
      style="PlainText"
      vq="tpwr jumpret"
      vc="tpwr=$VALUE"
      set="$VALUE=tpwr"
      show="if jumpret='y' then $VALUE=1 else $VALUE=0 endif"
      digits="0"
      disable="Grayed out"
      tooltip="tpwr"
      />
    <label loc="48 208" size="88 24"
      style="PlainText"
      label="Pulse Power"
      />
  </group>
</template>
