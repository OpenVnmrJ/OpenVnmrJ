<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="605 230"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="260 0" size="345 230"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="yes"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <label loc="16 120" size="72 24"
        style="Label1"
        label="Watergate"
        justify="Left"
        />
      <entry loc="200 96" size="56 24"
        style="PlainText"
        vq="gzlvl0 gzcal"
        vc="gzlvl0=$VALUE/gzcal"
        set="$VALUE=gzlvl0*gzcal"
        digits="3"
        disable="Grayed out"
        />
      <label loc="8 8" size="72 24"
        style="Heading2"
        label="Gradients"
        />
      <label loc="16 96" size="72 24"
        style="Label1"
        label="Crushers"
        />
      <entry loc="272 120" size="64 24"
        style="PlainText"
        vq="gt3"
        vc="gt3=$VALUE"
        set="$VALUE=gt3"
        digits="6"
        />
      <entry loc="136 96" size="48 24"
        style="PlainText"
        vq="gzlvl0"
        vc="gzlvl0=$VALUE"
        set="$VALUE=gzlvl0"
        disable="Grayed out"
        />
      <label loc="96 96" size="40 24"
        style="PlainText"
        label="gzlvl0"
        justify="Left"
        />
      <label loc="96 120" size="40 24"
        style="PlainText"
        label="gzlvl3"
        />
      <label loc="248 8" size="32 24"
        style="PlainText"
        label="gzcal"
        />
      <entry loc="280 8" size="64 24"
        style="PlainText"
        vq="gzcal"
        vc="gzcal=$VALUE"
        set="$VALUE=gzcal"
        digits="6"
        />
      <label loc="144 72" size="120 24"
        style="Label3"
        label="DAC    or    G/cm"
        />
      <label loc="168 48" size="64 24"
        style="Label3"
        label="Strength"
        />
      <label loc="272 72" size="64 24"
        style="Label3"
        label="Time (sec)"
        />
      <entry loc="272 96" size="64 24"
        style="PlainText"
        vq="gt0"
        vc="gt0=$VALUE"
        set="$VALUE=gt0"
        digits="6"
        disable="Grayed out"
        />
      <entry loc="200 120" size="56 24"
        style="PlainText"
        vq="gzlvl1 gzcal"
        vc="gzlvl1=$VALUE/gzcal"
        set="$VALUE=gzlvl1*gzcal"
        digits="3"
        />
      <entry loc="136 120" size="48 24"
        style="PlainText"
        vq="gzlvl3"
        vc="gzlvl3=$VALUE"
        set="$VALUE=gzlvl3"
        disable="Grayed out"
        />
    </group>
    <group loc="10 0" size="240 230"
      style="PlainText"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <check loc="5 85" size="130 25"
        style="Label2"
        label="TROSY"
        vq="TROSY dm"
        vc="TROSY='y' dm='nnn'"
        vc2="TROSY='n'"
        set="if (TROSY='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="TROSY='y' dm='nnn'"
        />
      <check loc="5 105" size="130 25"
        style="Label2"
        label="Refocus N15 in t1"
        vq="N15refoc"
        vc="N15refoc='y'"
        vc2="N15refoc='n'"
        set="if (N15refoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="N15refoc='y'"
        />
      <group loc="8 0" size="224 24"
        label="switch to Automatic"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="switchtoautomatic"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="0 0" size="208 24"
          style="Label3"
          label="Switch to Automatic Pbox Version"
          vq="seqfil autocal layout"
          vc="seqfil=seqfil+'A' autocal='y' layout=seqfil"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <label loc="8 128" size="80 24"
        style="Label1"
        label="J(13CH-1H)"
        justify="Left"
        />
      <label loc="8 152" size="136 24"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <label loc="8 176" size="144 24"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <entry loc="168 128" size="40 24"
        style="PlainText"
        vq="JCH"
        vc="JCH=$VALUE"
        set="$VALUE=JCH"
        digits="1"
        disable="Grayed out"
        tooltip="JCH"
        />
      <entry loc="176 152" size="32 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 176" size="32 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 200" size="32 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <label loc="8 200" size="168 24"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <entry loc="152 64" size="48 24"
        style="PlainText"
        vq="cor"
        vc="cor=$VALUE"
        set="$VALUE=cor"
        digits="1"
        disable="Grayed out"
        tooltip="cor"
        />
      <label loc="16 64" size="144 24"
        style="PlainText"
        label="Delay Correction (usec)"
        justify="Left"
        />
      <entry loc="152 40" size="48 24"
        style="PlainText"
        vq="d3"
        vc="d3=$VALUE/1e6"
        set="$VALUE=d3*1e6"
        digits="1"
        disable="Grayed out"
        tooltip="d3=$VALUE/1e6"
        />
      <label loc="16 40" size="144 24"
        style="PlainText"
        label="Watergate Delay (usec)"
        justify="Left"
        />
    </group>
  </group>
</template>
