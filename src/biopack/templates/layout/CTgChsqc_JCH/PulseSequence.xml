<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="645 230"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <button loc="355 160" size="135 25"
      style="PlainText"
      label="Re-create Shapes"
      vc="BPCTgChsqc_JCH"
      bg="VJBackground"
      decor1="yes"
      />
    <entry loc="325 120" size="50 25"
      style="PlainText"
      vq="CACB180pwrf"
      vc="CACB180pwrf=$VALUE"
      set="$VALUE=CACB180pwrf"
      digits="0"
      disable="Grayed out"
      />
    <label loc="235 125" size="95 25"
      style="PlainText"
      label="CACB180pwrf"
      justify="Left"
      />
    <entry loc="325 95" size="50 25"
      style="PlainText"
      vq="CACB180pwr"
      vc="CACB180pwr=$VALUE"
      set="$VALUE=CACB180pwr"
      digits="0"
      disable="Grayed out"
      />
    <label loc="235 95" size="85 25"
      style="PlainText"
      label="CACB180pwr"
      justify="Left"
      />
    <entry loc="325 70" size="50 25"
      style="PlainText"
      vq="CACB180pw"
      vc="CACB180pw=$VALUE"
      set="$VALUE=CACB180pw"
      digits="1"
      disable="Grayed out"
      />
    <label loc="235 70" size="85 25"
      style="PlainText"
      label="CACB180pw"
      justify="Left"
      />
    <label loc="425 45" size="90 25"
      style="PlainText"
      label="CO180shape"
      justify="Left"
      />
    <entry loc="325 45" size="95 25"
      style="PlainText"
      vq="CACB180shape"
      vc="CACB180shape='$VALUE'"
      set="$VALUE=CACB180shape"
      disable="Grayed out"
      />
    <entry loc="525 45" size="100 25"
      style="PlainText"
      vq="CO180shape"
      vc="CO180shape='$VALUE'"
      set="$VALUE=CO180shape"
      disable="Grayed out"
      />
    <label loc="235 45" size="90 25"
      style="PlainText"
      label="CACB180shape"
      justify="Left"
      />
    <label loc="490 5" size="120 25"
      style="PlainText"
      label="set by setup macro)"
      justify="Left"
      />
    <label loc="245 5" size="250 25"
      style="PlainText"
      label="C13 Shapes (shapes created and settings"
      justify="Left"
      />
    <group loc="5 0" size="225 230"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="timeCT(ms)"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="5 100" size="130 25"
        style="Label2"
        label="Measure JCH"
        vq="modJCH"
        vc="modJCH='y'"
        vc2="modJCH='n'"
        set="if (modJCH='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="modJCH='y'"
        />
      <label loc="5 35" size="80 25"
        style="Label1"
        label="timeCT(ms)"
        />
      <label loc="5 65" size="85 25"
        style="Label1"
        label="timeJCH(ms)"
        />
      <entry loc="160 10" size="50 25"
        style="PlainText"
        vq="tauxh"
        vc="tauxh=$VALUE/1e3"
        set="$VALUE=tauxh*1e3"
        digits="3"
        disable="Grayed out"
        tooltip="tauxh=$VALUE/1e3"
        />
      <entry loc="160 70" size="50 25"
        style="PlainText"
        vq="timeJCH"
        vc="timeJCH=$VALUE/1e3"
        set="$VALUE=timeJCH*1e3"
        digits="3"
        disable="Grayed out"
        tooltip="timeJCH=$VALUE/1e3"
        />
      <entry loc="160 40" size="50 25"
        style="PlainText"
        vq="timeCT"
        vc="timeCT=$VALUE/1e3"
        set="$VALUE=timeCT*1e3"
        digits="3"
        disable="Grayed out"
        tooltip="timeCT=$VALUE/1e3"
        />
      <label loc="5 5" size="120 25"
        style="PlainText"
        label="tauxh(ms)"
        justify="Left"
        />
      <label loc="5 200" size="170 25"
        style="PlainText"
        label="Pre-Acquisition Delay (alfa)"
        justify="Left"
        />
      <label loc="5 175" size="145 25"
        style="PlainText"
        label="Pulse Post-Delay (rof2)"
        justify="Left"
        />
      <label loc="5 150" size="135 25"
        style="PlainText"
        label="Pulse Pre-Delay (rof1)"
        justify="Left"
        />
      <entry loc="176 152" size="32 24"
        style="PlainText"
        vq="rof1"
        vc="rof1=$VALUE"
        set="$VALUE=rof1"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 176" size="32 24"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="176 200" size="32 24"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <entry loc="525 70" size="50 25"
      style="PlainText"
      vq="CO180pw"
      vc="CO180pw=$VALUE"
      set="$VALUE=CO180pw"
      digits="1"
      disable="Grayed out"
      />
    <label loc="425 70" size="65 25"
      style="PlainText"
      label="CO180pw"
      justify="Left"
      />
    <label loc="425 95" size="65 25"
      style="PlainText"
      label="CO180pwr"
      justify="Left"
      />
    <entry loc="525 95" size="50 25"
      style="PlainText"
      vq="CO180pwr"
      vc="CO180pwr=$VALUE"
      set="$VALUE=CO180pwr"
      digits="1"
