<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="685 240"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="395 110" size="285 100"
      style="Label1"
      label="Watergate"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <selmenu loc="185 15" size="95 25"
        style="Label1"
        label="Set Mode"
        vc="exec('$VALUE')"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="Soft Pulse"
          chval="watergate=`y` flag3919=`n`"
          />
        <mlabel 
          label="3919"
          chval="watergate=`y` flag3919=`y`"
          />
        <mlabel 
          label="None"
          chval="flag3919=`n` watergate=`n`"
          />
      </selmenu>
      <entry loc="125 65" size="50 25"
        style="PlainText"
        vq="d3 flag3919"
        vc="d3=$VALUE/1e6"
        set="$VALUE=d3*1e6"
        show="if flag3919='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        tooltip="d3=$VALUE/1e6"
        />
      <entry loc="125 15" size="50 25"
        style="PlainText"
        vq="p180 watergate flag3919"
        vc="p180=$VALUE"
        set="$VALUE=p180"
        show="if (watergate='y' and flag3919='n') then $VALUE=1 else $VALUE=0 endif"
        digits="2"
        tooltip="p180"
        />
      <entry loc="125 40" size="50 25"
        style="PlainText"
        vq="tau"
        vc="tau=$VALUE*1e-6"
        set="$VALUE=tau*1e6"
        show="if watergate='y' then $VALUE=1 else $VALUE=0 endif"
        digits="0"
        tooltip="tau=$VALUE*1e-6"
        />
      <label loc="10 65" size="130 25"
        style="PlainText"
        label="3919 Delay (usec)"
        />
      <label loc="10 40" size="65 25"
        style="PlainText"
        label="tau (usec)"
        />
      <label loc="10 20" size="65 25"
        style="PlainText"
        label="pw180"
        />
    </group>
    <group loc="395 10" size="280 85"
      style="Label1"
      label="Flipback Pulse (&quot;H2Osinc&quot;)"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <check loc="192 16" size="72 24"
        style="PlainText"
        label="Active"
        vq="flipback"
        vc="flipback='y'"
        vc2="flipback='n'"
        set="if flipback='y' then $VALUE=1 else $VALUE=0 endif"
        tooltip="flipback='y'"
        />
      <entry loc="112 56" size="56 24"
        style="PlainText"
        vq="phincr1 flipback"
        vc="phincr1=$VALUE"
        set="$VALUE=phincr1"
        show="if flipback='y' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        tooltip="phincr1"
        />
      <label loc="8 56" size="88 24"
        style="PlainText"
        label="Phase Corr."
        />
      <entry loc="112 24" size="56 24"
        style="PlainText"
        vq="pwHs flipback"
        vc="pwHs=[$VALUE]*1e3"
        set="$VALUE=pwHs/1e3"
        show="if flipback='y' then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        tooltip="pwHs=[$VALUE]*1e3"
        />
      <label loc="8 24" size="104 24"
        style="PlainText"
        label="Duration (msec)"
        />
    </group>
    <group loc="260 100" size="120 110"
      style="Label2"
      label="Refocus in t1"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="Refocusint1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <radio loc="10 20" size="80 20"
        style="PlainText"
        label="None"
        vq="C13refoc N15refoc CNrefoc"
        vc="C13refoc='n' N15refoc='n' CNrefoc='n'"
        set="if ((C13refoc='n') and (N15refoc='n') and (CNrefoc='n')) then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13refoc='n' N15refoc='n' CNrefoc='n'"
        />
      <radio loc="10 40" size="80 20"
        style="PlainText"
        label="C13"
        vq="C13refoc N15refoc CNrefoc"
        vc="C13refoc='y' N15refoc='n' CNrefoc='n'"
        set="if (C13refoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="C13refoc='y' N15refoc='n' CNrefoc='n'"
        />
      <radio loc="10 60" size="80 20"
        style="PlainText"
        label="N15"
        vq="N15refoc C13refoc CNrefoc"
        vc="N15refoc='y' C13refoc='n' CNrefoc='n'"
        set="if (N15refoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="N15refoc='y' C13refoc='n' CNrefoc='n'"
        />
      <radio loc="10 80" size="90 20"
        style="PlainText"
        label="C13 &amp; N15"
        vq="CNrefoc N15refoc C13refoc"
        vc="CNrefoc='y' N15refoc='n' C13refoc='n'"
        set="if (CNrefoc='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="CNrefoc='y' N15refoc='n' C13refoc='n'"
        />
    </group>
    <check loc="240 70" size="150 20"
      style="Label3"
      label="Steady-State Pulse"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      set="if sspul='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip="sspul='y'"
      />
    <group loc="8 8" size="336 56"
      style="Label1"
      label="TOCSY (DIPSI)"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <entry loc="272 24" size="48 24"
        style="Label2"
        vq="mix"
        vc="mix=$VALUE"
        set="$VALUE=mix"
        digits="4"
        tooltip="mix"
        />
      <label loc="184 24" size="88 24"
        style="Label2"
        label="Mix Time (sec)"
        justify="Left"
        />
      <entry loc="96 24" size="48 24"
        style="Label2"
        vq="strength"
        vc="strength=$VALUE BPspinlock"
        set="$VALUE=strength"
        digits="0"
        tooltip="strength"
        />
      <label loc="8 24" size="88 24"
        style="Label2"
        label="Strength (Hz)"
        justify="Left"
        />
    </group>
    <label loc="320 216" size="32 24"
      style="PlainText"
      label="usec"
      />
    <entry loc="280 216" size="40 24"
      style="PlainText"
      vq="alfa"
      vc="alfa=$VALUE"
      set="$VALUE=alfa"
      digits="1"
      />
    <label loc="248 216" size="32 24"
      style="PlainText"
      label="alfa:"
      />
    <entry loc="184 216" size="40 24"
      style="PlainText"
      vq="rof2"
      vc="rof2=$VALUE"
      set="$VALUE=rof2"
      digits="1"
      />
    <label loc="152 216" size="32 24"
      style="PlainText"
      label="rof2:"
      />
    <label loc="8 216" size="88 24"
      style="PlainText"
      label="Delays:   rof1:"
      />
    <entry loc="96 216" size="40 24"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
      set="$VALUE=rof1"
      digits="1"
      />
    <group loc="8 88" size="208 120"
      style="Label1"
      label="Soft Pulse Used in Watergate"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <entry loc="96 88" size="40 24"
        style="PlainText"
        vq="phincr2 flag3919"
        vc="phincr2=$VALUE"
        set="$VALUE=phincr2"
        show="if flag3919='n' then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        tooltip="phincr2"
        />
      <label loc="16 88" size="80 24"
        style="PlainText"
        label="Phase Corr."
        />
      <label loc="104 64" size="40 24"
        style="PlainText"
        label="(msec)"
        />
      <entry loc="56 64" size="48 24"
        style="PlainText"
        vq="flippw flag3919"
        vc="flippw=[$VALUE]*1e3"
        set="$VALUE=flippw/1.0e3"
        show="if (flag3919='n') then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        tooltip="flippw=[$VALUE]*1e3"
        />
      <label loc="8 64" size="48 24"
        style="PlainText"
        label="Width"
        vq="wet"
        />
      <entry loc="56 40" size="48 24"
        style="PlainText"
        vq="flippwr flag3919"
        vc="flippwr=$VALUE"
        set="$VALUE=flippwr"
        show="if (flag3919='n') then $VALUE=1 else $VALUE=0 endif"
        digits="1"
        tooltip="flippwr"
        />
      <label loc="8 40" size="48 24"
        style="PlainText"
        label="Power"
        />
      <entry loc="56 16" size="80 24"
        style="PlainText"
        vq="flipshap flag3919"
        vc="flipshap='$VALUE'"
        set="$VALUE=flipshap"
        show="if (flag3919='n') then $VALUE=1 else $VALUE=0 endif"
        tooltip="flipshap"
        />
      <label loc="8 16" size="48 24"
        style="PlainText"
        label="Shape"
        vq="wet"
        />
    </group>
  </group>
</template>
