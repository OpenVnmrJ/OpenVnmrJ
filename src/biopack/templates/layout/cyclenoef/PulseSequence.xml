<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="710 205"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <check loc="475 170" size="230 25"
      style="Heading2"
      label="Steady-State Purge Pulse"
      vq="sspul"
      vc="sspul='y'"
      vc2="sspul='n'"
      set="if (sspul='y') then $VALUE=1 else $VALUE=0 endif"
      tooltip="sspul='y'"
      />
    <group loc="472 0" size="240 160"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="112 128" size="80 24"
        style="PlainText"
        label="Hz   Power"
        justify="Left"
        />
      <radio loc="8 24" size="192 24"
        style="PlainText"
        label="None"
        vq="satmode"
        vc="satmode='nnn'"
        set="if satmode='nn' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='nnn'"
        />
      <radio loc="8 48" size="208 24"
        style="PlainText"
        label="Presat  in Relaxation Delay"
        vq="satmode"
        vc="satmode='ynn'"
        set="if satmode='yn' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='ynn'"
        />
      <radio loc="8 72" size="208 24"
        style="PlainText"
        label="Presat  in Mix Delay"
        vq="satmode"
        vc="satmode='nny'"
        set="if satmode='nny' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='nny'"
        />
      <radio loc="8 96" size="224 24"
        style="PlainText"
        label="Presat  in Relaxation and Mix Delay"
        vq="satmode"
        vc="satmode='yny'"
        set="if satmode='yny' then $VALUE=1 else $VALUE=0 endif"
        tooltip="satmode='yny'"
        />
      <entry loc="192 128" size="32 24"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="$ENABLE=((satmode='yy')  or (satmode='yn'))"
        digits="0"
        disable="Grayed out"
        tooltip="satpwr"
        />
      <entry loc="56 128" size="48 24"
        style="PlainText"
        vq="satfrq satmode"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        show="$ENABLE=((satmode='yy') or (satmode='yn'))"
        digits="1"
        disable="Grayed out"
        tooltip="satfrq"
        />
      <label loc="8 128" size="40 24"
        style="PlainText"
        label="Offset"
        justify="Left"
        />
      <label loc="0 0" size="168 24"
        style="Heading2"
        label="Solvent Saturation"
        />
    </group>
    <group loc="0 0" size="464 200"
      vq="scan"
      show="if scan='y' then $SHOW=0 else $SHOW=1 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      >
      <entry loc="360 160" size="64 24"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE"
        set="$VALUE=mix"
        digits="3"
        disable="Grayed out"
        tooltip="mix"
        />
      <label loc="232 160" size="128 24"
        style="Label1"
        label="NOE Mix time (sec)"
        justify="Left"
        />
      <group loc="224 48" size="232 104"
        vq="cycle"
        show="if (cycle='y') then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="MLEV17c"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <entry loc="140 10" size="40 20"
          style="PlainText"
          vq="pattern"
          vc="pattern=$VALUE"
          set="$VALUE=pattern"
          digits="0"
          disable="Grayed out"
          tooltip="pattern"
          />
        <entry loc="140 40" size="40 20"
          style="PlainText"
          vq="spacing"
          vc="spacing=$VALUE"
          set="$VALUE=spacing"
          digits="1"
          disable="Grayed out"
          tooltip="spacing"
          />
        <label loc="180 40" size="20 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <label loc="180 70" size="40 20"
          style="PlainText"
          label="sec"
          justify="Left"
          />
        <entry loc="140 70" size="40 20"
          style="PlainText"
          vq="tau"
          vc="tau=$VALUE"
          set="$VALUE=tau"
          digits="1"
          disable="Grayed out"
          tooltip="tau"
          />
        <label loc="10 70" size="130 20"
          style="Label1"
          label="Time per component"
          justify="Left"
          />
        <label loc="10 40" size="100 20"
          style="Label1"
          label="pattern spacing:"
          justify="Left"
          />
        <label loc="10 10" size="100 20"
          style="Label1"
          label="pattern:"
          justify="Left"
          />
      </group>
      <check loc="216 24" size="168 24"
        style="Heading2"
        label="Frequency-Cycling"
        vq="cycle"
        vc="cycle='y'"
        vc2="cycle='n'"
        set="if (cycle='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="cycle='y'"
        />
      <check loc="216 0" size="192 24"
        style="Heading2"
        label="Difference Spectrum"
        vq="intsub"
        vc="intsub='y'"
        vc2="intsub='n'"
        set="if (intsub='y') then $VALUE=1 else $VALUE=0 endif"
        tooltip="intsub='y'"
        />
      <group loc="0 0" size="208 200"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="dpfgsegroup"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <label loc="8 0" size="192 24"
          style="Heading2"
          label="Selective Saturation"
          justify="Left"
          />
        <label loc="8 32" size="104 24"
          style="Label1"
          label="Saturation time"
          justify="Left"
          />
        <entry loc="112 32" size="56 24"
          style="PlainText"
          vq="noetime"
          vc="noetime=$VALUE"
          set="$VALUE=noetime"
          digits="3"
          disable="Grayed out"
          tooltip="noetime"
          />
        <label loc="168 32" size="32 24"
          style="PlainText"
          label="sec"
          justify="Left"
          />
        <entry loc="168 56" size="40 24"
          style="PlainText"
          vq="finepwr"
          vc="finepwr=$VALUE"
          set="$VALUE=finepwr"
          disable="Grayed out"
          tooltip="finepwr"
          />
        <entry loc="56 56" size="32 24"
          style="PlainText"
          vq="noepwr"
          vc="noepwr=$VALUE"
          set="$VALUE=noepwr"
          disable="Grayed out"
          tooltip="noepwr"
          />
        <label loc="96 56" size="72 24"
          style="Label1"
          label="fine power:"
          justify="Left"
          />
        <label loc="8 56" size="64 24"
          style="Label1"
          label="power:"
          justify="Left"
          />
        <check loc="10 90" size="200 20"
          style="Heading2"
          label="Shaped Pulse Saturation"
          vq="shaped cycle"
          vc="shaped='y' cycle='n'"
          vc2="shaped='n'"
          set="if ((shaped='y') and (cycle='n')) then $VALUE=1 else $VALUE=0 endif"
          tooltip="shaped='y' "
          />
        <group loc="70 110" size="140 20"
          vq="shaped"
          show="if shaped='y' then $SHOW=1 else $SHOW=0 endif"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <entry loc="0 0" size="120 20"
            style="PlainText"
            vq="shape"
            vc="shape='$VALUE'"
            set="$VALUE=shape"
            digits="0"
            disable="Grayed out"
            tooltip="shape"
            />
        </group>
        <entry loc="130 140" size="60 20"
          style="PlainText"
          vq="noefrq"
          vc="noefrq=$VALUE"
          set="$VALUE=noefrq"
          digits="1"
          disable="Grayed out"
          tooltip="noefrq"
          />
        <label loc="10 140" size="110 20"
          style="PlainText"
          label="&quot;On&quot; Frequency"
          justify="Left"
          />
        <label loc="10 170" size="110 20"
          style="PlainText"
          label="&quot;Off&quot; Frequency"
          justify="Left"
          />
        <entry loc="130 170" size="60 20"
          style="PlainText"
          vq="control"
          vc="control=$VALUE"
          set="$VALUE=control"
          digits="1"
          disable="Grayed out"
          tooltip="control"
          />
      </group>
    </group>
  </group>
</template>
