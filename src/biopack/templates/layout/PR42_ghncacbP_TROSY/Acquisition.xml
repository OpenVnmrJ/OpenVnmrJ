      vq="phase2 phase angle_Ca angle_Cb axis"
      vc="angle_Ca=90.0 angle_Cb=90.0 phase=1 phase2=1,2 axis='p2' full BP_PRupdate('angle_Cb','angle_Ca',angle_Cb,angle_Ca)"
      set="$VALUE=0 $phase=size('phase') $phase2=size('phase2') if ($phase2&gt;1.5 and $phase&lt;1.5) then $v=(phase2[1]=1) $w=(phase2[2]=2) $VALUE=($v and $w) endif"
      tooltip="angle_Ca=90.0 angle_Cb=90.0 phase=1 phase2=1,2"
