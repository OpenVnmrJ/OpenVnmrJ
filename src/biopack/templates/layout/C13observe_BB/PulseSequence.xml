        style="PlainText"
        vq="shp_pw"
        vc="shp_pw=$VALUE/2.0"
        set="$VALUE=shp_pw"
        digits="2"
        disable="Grayed out"
        />
      <label loc="215 0" size="55 25"
        style="PlainText"
        label="(usec)"
        justify="Left"
        />
      <entry loc="155 50" size="85 25"
        style="PlainText"
        vq="shape"
        vc="shape='$VALUE'"
        set="$VALUE=shape"
        digits="0"
        disable="Grayed out"
        />
