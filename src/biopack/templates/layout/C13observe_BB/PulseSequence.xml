<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="745 230"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <radio loc="290 180" size="175 25"
      style="PlainText"
      label="Crusher Gradient Pulses"
      vq="gpurge"
      vc="gpurge='ny' dps"
      set="if gpurge='ny' then $VALUE=1 else $VALUE=0 endif"
      />
    <radio loc="290 155" size="185 25"
      style="PlainText"
      label="Homospoil Gradient Pulse"
      vq="gpurge"
      vc="gpurge='yn' dps"
      set="if gpurge='yn' then $VALUE=1 else $VALUE=0 endif"
      />
