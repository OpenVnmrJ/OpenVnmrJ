        unitssize="60"
        unitscmd="gt=$VALUE"
        unitsvalue="$VALUE=gt"
        enable="no"
        reference="parameter"
        useref="no"
        disable="Grayed out"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="0 0" size="59 20"
          style="PlainText"
          label="gt"
          vq="null gt"
          subtype="parameter"
