        <label loc="0 0" size="59 20"
          style="PlainText"
          label="gzlvl"
          vq="null gzlvl"
          subtype="parameter"
          />
        <entry loc="63 0" size="90 20"
          style="PlainText"
          vq="null gzlvl"
          vc="gzlvl=$VALUE"
          set="$VALUE=gzlvl"
          disable="Grayed out"
