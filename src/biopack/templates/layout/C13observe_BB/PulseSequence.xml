      style="PlainText"
      label="Baseline Compensation"
      vq="phase_roll_comp"
      vc="phase_roll_comp='y' dps"
      vc2="phase_roll_comp='n' dps"
      set="if phase_roll_comp='y' then $VALUE=1 else $VALUE=0 endif"
      />
