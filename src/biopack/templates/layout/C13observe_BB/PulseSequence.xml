      style="PlainText"
      label="Homospoil/Crushers"
      vq="gpurge"
      vc="gpurge='yy' dps"
      set="if gpurge='yy' then $VALUE=1 else $VALUE=0 endif"
      />
    <radio loc="290 130" size="145 25"
      style="PlainText"
      label="No Gradient Pulses"
      vq="gpurge"
      vc="gpurge='nn' dps"
      set="if gpurge='nn' then $VALUE=1 else $VALUE=0 endif"
      />
