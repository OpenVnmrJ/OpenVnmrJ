          />
        <label loc="155 0" size="60 20"
          style="PlainText"
          vq="null gzlvl"
          key="units"
          subtype="parameter"
          />
      </parameter>
      <label loc="0 0" size="215 25"
        style="PlainText"
        label=" Gradients"
        justify="Left"
        />
    </group>
    <label loc="225 175" size="40 25"
      style="PlainText"
      label="(usec)"
      />
    <entry loc="175 175" size="50 25"
      style="PlainText"
      vq="rof1"
      vc="rof1=$VALUE"
