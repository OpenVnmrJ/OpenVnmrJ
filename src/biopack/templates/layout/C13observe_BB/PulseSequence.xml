        <check loc="45 1" size="17 17"
          style="PlainText"
          vq="null gt"
          subtype="parameter"
          />
        <label loc="155 0" size="60 20"
          style="PlainText"
          vq="null gt"
          key="units"
          subtype="parameter"
          />
      </parameter>
      <parameter loc="15 25" size="215 25"
        style="PlainText"
        label="gzlvl"
        bg="transparent"
        tab="no"
        vq2="gzlvl"
        entryvalue="$VALUE=gzlvl"
        entrycmd="gzlvl=$VALUE"
        entrysize="90"
        unitsenabled="Label"
        unitssize="60"
        unitscmd="gzlvl=VALUE"
        unitsvalue="$VALUE=gzlvl"
        enable="no"
        reference="parameter"
        useref="no"
        disable="Grayed out"
