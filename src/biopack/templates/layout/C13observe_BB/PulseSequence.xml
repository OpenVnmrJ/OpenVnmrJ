        entrycmd="gstab=$VALUE"
        entrysize="90"
        unitsenabled="Label"
        unitssize="60"
        unitscmd="gstab=$VALUE"
        unitsvalue="$VALUE=gstab"
        enable="no"
        reference="parameter"
        useref="no"
        disable="Grayed out"
        subtype="Untitled"
        expanded="yes"
        >
