<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="750 260"
    style="PlainText"
    label="Pulse Sequence"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <check loc="95 50" size="70 25"
      style="PlainText"
      label="N15 T2"
      vq="T1 T2 T1rho"
      vc="T2='y' T1='n' T1rho='n'"
      vc2="T2='n'"
      set="if (T2='y') then $VALUE=1 else $VALUE=0 endif"
      tooltip="T2='y' "
      />
    <check loc="5 50" size="90 25"
      style="PlainText"
      label="N15 T1rho"
      vq="T1 T2 T1rho"
      vc="T1rho='y' T1='n' T2='n'"
      vc2="T1rho='n'"
      set="if (T1rho='y') then $VALUE=1 else $VALUE=0 endif"
      tooltip="T1rho='y' "
      />
    <group loc="5 25" size="100 25"
      vq="T1 T2 T1rho"
      show="if ((T1='y') or (T2='y') or (T1rho='y')) then $SHOW=0 else $SHOW=1 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <check loc="0 0" size="75 25"
        style="Heading1"
        label="TROSY"
        vq="TROSY dm2 NHonly"
        vc="TROSY='y' dm2='nnn' NHonly='n'"
        vc2="TROSY='n'"
        set="if ((TROSY='y')  and (NHonly='n')) then $VALUE=1 else $VALUE=0 endif"
        tooltip="TROSY='y' "
        />
    </group>
    <group loc="5 160" size="200 50"
      style="PlainText"
      vq="T1 T2 T1rho"
      show="if ((T1='y') or (T2='y' ) or (T1rho='y') ) then $SHOW=1 else $SHOW=1 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <entry loc="152 24" size="48 24"
        style="PlainText"
        vq="maxrelaxT"
        vc="maxrelaxT=$VALUE"
        set="$VALUE=maxrelaxT"
        show="if (T1='y' or T2='y' or T1rho='y') then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        tooltip="maxrelaxT"
        />
      <entry loc="152 0" size="48 24"
        style="PlainText"
        vq="relaxT"
        vc="relaxT=$VALUE"
        set="$VALUE=relaxT"
        show="if (T1='y' or T2='y' or T1rho='y') then $VALUE=1 else $VALUE=0 endif"
        digits="3"
        tooltip="relaxT"
        />
      <label loc="8 24" size="128 24"
        style="PlainText"
        label="Maximum Mix Time"
        />
      <label loc="8 0" size="88 24"
        style="PlainText"
        label="Mixing Time"
        />
    </group>
    <radio loc="150 130" size="105 25"
      style="PlainText"
      label="Check Offsets"
      vq="checkofs"
      vc="checkofs='y'"
      vc2="checkofs='n'"
      set="if checkofs='y' then $VALUE=1 else $VALUE=0 endif"
      tooltip="checkofs='y'"
      />
    <label loc="260 130" size="90 25"
