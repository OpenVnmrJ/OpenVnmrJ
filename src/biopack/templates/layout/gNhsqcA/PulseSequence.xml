      />
    <textmessage loc="355 130" size="25 25"
      style="PlainText"
      label="y"
      vq="Lopt"
      set="$VALUE=Lopt"
      />
    <menu loc="285 80" size="105 25"
      style="Label2"
      label="menu"
      vq="autocal"
      vc="autocal='$VALUE'"
      set="$VALUE=autocal"
      bg="VJBackground"
      editable="No"
      >
      <mlabel 
        label="Yes"
        chval="y"
        />
      <mlabel 
        label="No"
        chval="n"
        />
      <mlabel 
        label="Read From File"
        chval="r"
        />
      <mlabel 
        label="Semi-Automatic"
        chval="s"
        />
      <mlabel 
        label="Quiet"
        chval="q"
        />
    </menu>
    <selmenu loc="150 105" size="240 25"
      style="PlainText"
      label="Longitudinal Relaxation Optimization"
      vq="Lopt"
      vc="Lopt='$VALUE'"
      set="$VALUE=Lopt"
      bg="VJBackground"
      editable="No"
      >
      <mlabel 
        label="(x) Put CH magnetizatin on -Z"
        chval="x"
        />
      <mlabel 
        label="(y)Put CH magnetization on +Z"
        chval="y"
        />
      <mlabel 
        label="(s)Use Eburp on Aliphatics for Flipback"
        chval="s"
        />
      <mlabel 
        label="(n)Normal Flipback Pulse"
        chval="n"
        />
    </selmenu>
    <button loc="184 0" size="184 24"
      style="Label3"
      label="Switch to Standard Version"
      vq="seqfil layout"
      vc="seqfil='gNhsqc' layout=seqfil "
