<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="bpexpress_quick_setup" element="groups" type="sample" >
  <group size="230 210"
    style="Heading1"
    label="Express setup"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="bpexpress_quick_setup"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 60" size="220 110"
      label="3D"
      vq="ni"
      show="$SHOW=1 exists('ni','parameter'):$e  if ( ($e&gt;0)  ) then $SHOW=1 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="3D"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 30" size="210 75"
        vq="ni2"
        show="$SHOW=0 exists('ni2','parameter'):$e2  if ( ($e2&gt;0)  ) then $SHOW=1 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="5 45" size="200 25"
          style="Heading2"
          label="CA x N x HN 3D"
          vq="nuc_obs nuc_ni nuc_ni2 sampling"
          vc=" ni=at_ni*sw1 ni2=at_ni2*sw2 phase=1,2 phase2=1,2 array='phase,phase2'   if (sampling='sparse') then {setup_nus_cmd} endif time"
          set="$VALUE=nuc_ni +' x ' +nuc_ni2 + ' x ' +nuc_obs + ' 3D'"
          bg="VJBackground"
          vq2="nuc_obs nuc_ni nuc_ni2"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 10" size="200 25"
          style="Heading2"
          label="N x HN 2D"
          vq="nuc_obs nuc_ni2 sampling"
          vc=" ni2=at_ni2*sw2  phase2=1,2 ni=0 phase=1  if (sampling='sparse') then {setup_nus_cmd} endif time"
          set="$VALUE=  nuc_ni2 +' x ' +nuc_obs + ' 2D'"
          bg="VJBackground"
          vq2="nuc_obs nuc_ni2"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <button loc="10 5" size="200 25"
        style="Heading2"
        label="CA x HN 2D"
        vq="nuc_ni sampling "
        vc=" ni=at_ni*sw1  phase=1,2  exists('ni2','parameter'):$e  if ($e&gt;0) then ni2=0 phase2=1 endif  if (sampling='sparse') then {setup_nus_cmd} endif time"
        set="$VALUE=  nuc_ni +' x ' +nuc_obs + ' 2D'"
        bg="VJBackground"
        vq2="nuc_obs nuc_ni"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <button loc="15 30" size="200 25"
      style="Heading2"
      label="HN First Increment"
      vq="nuc_obs "
      vc="exists('ni','parameter'):$e  if ( ($e&gt;0)  ) then ni=0 phase=1 endif     exists('ni2','parameter'):$e2  if ( ($e2&gt;0)  ) then ni2=0 phase2=1 endif time"
      set="$VALUE= nuc_obs + ' First Increment '"
      bg="VJBackground"
      vq2="nuc_obs"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="15 170" size="200 25"
      style="Heading4"
      label="Acquire"
      vq="arraydim customflag cppauseflg sampling  "
      vc="  if (arraydim=1) then {proc_obs} ga else   if (sampling='sparse') then {setup_nus_cmd} endif go endif exists('ni','parameter'):$e if ($e&gt;0.5) then vnmrjcmd('pnew','ni')     endif exists('ni2','parameter'):$e2 if ($e2&gt;0.5) then vnmrjcmd('pnew','ni2')     endif"
      show="$ENABLE=((cppauseflg='n') and (customflag='n'))*2-1"
      bg="VJBackground"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
  </group>
</template>
