<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="bpexpress_quick_setup_n15t1" element="groups" type="sample" >
  <group size="230 210"
    style="Heading1"
    label="Express setup"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="bpexpress_quick_setup_n15t1"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <button loc="15 100" size="200 25"
      style="Heading2"
      label="HN x  T1 test  1D series"
      vq="nuc_obs nuc_ni sampling"
      vc=" ni=1 phase=1    {setup_macro}('larray')   time"
      set="$VALUE=nuc_obs  + ' x  T1 test  1D series '"
      bg="VJBackground"
      vq2="nuc_obs nuc_ni "
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="15 135" size="200 25"
      style="Heading2"
      label="N x HN x  T1  &quot;3D&quot;"
      vq="nuc_obs nuc_ni nuc_ni2 sampling"
      vc=" ni=at_ni*sw1  phase=1,2  {setup_macro}('ncycs') array='ncyc,phase'  if (sampling='sparse') then CSschedule endif time"
      set="$VALUE=nuc_ni +' x ' +nuc_obs + ' x  T1  &quot;3D&quot; '"
      bg="VJBackground"
      vq2="nuc_obs nuc_ni nuc_ni2"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="15 65" size="200 25"
      style="Heading2"
      label="N x HN reference 2D"
      vq="nuc_ni sampling "
      vc=" ncyc=0 ni=at_ni*sw1  phase=1,2  exists('ni2','parameter'):$e  if ($e&gt;0) then ni2=0 phase2=0 endif  if (sampling='sparse') then CSschedule endif time"
      set="$VALUE=  nuc_ni +' x ' +nuc_obs + ' reference 2D'"
      bg="VJBackground"
      vq2="nuc_obs nuc_ni"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="15 170" size="200 25"
      style="Heading4"
      label="Acquire"
      vq="arraydim customflag cppauseflg sampling  "
      vc=" if (sampling='sparse') then CSschedule endif  if (arraydim=1) then ga else go endif exists('ni','parameter'):$e if ($e&gt;0.5) then vnmrjcmd('pnew','ni')     endif"
      show="$ENABLE=((cppauseflg='n') and (customflag='n'))*2-1"
      bg="VJBackground"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="15 30" size="200 25"
      style="Heading2"
      label="HN First Increment"
      vq="nuc_obs "
      vc="exists('ni','parameter'):$e  if ( ($e&gt;0)  ) then ni=0 phase=1 endif     exists('ni2','parameter'):$e2  if ( ($e2&gt;0)  ) then ni2=0 phase2=1 endif  ncyc=1 time"
      set="$VALUE= nuc_obs + ' First Increment '"
      bg="VJBackground"
      vq2="nuc_obs"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
  </group>
</template>
