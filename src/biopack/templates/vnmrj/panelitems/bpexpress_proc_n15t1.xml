<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="bpexpress_proc_n15t1" element="groups" type="sample" >
  <group size="230 210"
    style="Heading1"
    label="Express process"
    vq="ni sampling"
    show="$SHOW=0 exists('sampling','parameter','processed'):$es if ($es&gt;0.5) then getvalue('sampling','processed'):$s if ($s&lt;&gt;'sparse') then $SHOW=1 endif endif"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="bpexpress_proc_n15t1"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 95" size="220 35"
      label="Process 2D"
      vq="ni"
      show="$SHOW=0 exists('ni','parameter'):$e   exists('ni','parameter','processed'):$e2  if ( ($e&gt;0.5)  and ($e2&gt;0.5) )  then $SHOW=1 endif"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <button loc="70 5" size="140 25"
        style="Heading2"
        label="N x HN 2D"
        vq="ni   nuc_ni nuc_obs"
        vc="{proc_obs} {proc_ni} wft2da('ni',1)"
        set="$VALUE=  nuc_ni +' x ' +nuc_obs + ' 2D'"
        show="$SHOW=0  $ni=0 exists('ni','parameter','processed'):$e2  if($e2&gt;0.5) then getvalue('ni','processed'):$ni endif if ( ($ni&gt;1)  ) then $SHOW=1 endif"
        bg="VJBackground"
        vq2="nuc_obs nuc_ni"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <check loc="10 5" size="65 25"
        style="Label1"
        label="LP x2"
        vq="proc1 ni"
        vc="BP_set_2x_lp('ni'):proc1"
        vc2="proc1='ft'"
        set="$VALUE=0 if (proc1='lp')   then $VALUE=1 endif"
        show="$VALUE=-1 $ni=0 exists('ni','parameter','processed'):$e2  if($e2&gt;0.5) then getvalue('ni','processed'):$ni endif  if ($ni&gt;1)   then $VALUE=1 endif"
        />
    </group>
    <button loc="15 65" size="200 25"
      style="Heading2"
      label="HN test 1D series"
      vq="nuc_obs ni ncyc "
      vc="{proc_obs}  wft dssh dssl('value')"
      set="$VALUE= nuc_obs +  ' test 1D series '"
      show="$SHOW=0 if ( (arraydim=size('ncyc')) and (arraydim&gt;1)  ) then $SHOW=1 endif"
      bg="VJBackground"
      vq2="nuc_obs"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="15 30" size="200 25"
      style="Heading2"
      label="HN First Increment"
      vq="nuc_obs"
      vc="{proc_obs}  wft(1)"
      set="$VALUE= nuc_obs +  ' First Increment '"
      bg="VJBackground"
      vq2="nuc_obs"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
  </group>
</template>
