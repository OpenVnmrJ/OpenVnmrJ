  <group loc="0 0" size="700 205"
    style="PlainText"
    label="Acquisition"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Acquisition"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <check loc="500 150" size="60 20"
      style="PlainText"
      label="Auto"
      vq="gain"
      vc="gain='n'"
      vc2="gain='y'"
      set="off('gain'):$VALUE"
      />
    <entry loc="440 150" size="50 20"
      style="PlainText"
      vq="gain"
      vc="gain=$VALUE"
      set="$VALUE=gain"
      show="on('gain'):$VALUE"
      disable="Grayed out"
      />
    <label loc="320 150" size="90 20"
      style="PlainText"
      label="Receiver Gain:"
      justify="Left"
      />
    <group loc="320 170" size="320 20"
      style="Label2"
      label="Calibration"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <entry loc="240 0" size="30 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        digits="0"
        disable="Grayed out"
        />
      <label loc="180 0" size="60 20"
        style="PlainText"
        label="at Power"
        justify="Left"
        />
      <entry loc="120 0" size="50 20"
        style="PlainText"
        vq="pw90"
        vc="pw90=$VALUE"
        set="$VALUE=pw90"
        digits="2"
        disable="Grayed out"
        />
      <label loc="80 0" size="50 20"
        style="PlainText"
        label="pw90"
        justify="Left"
        />
      <label loc="0 0" size="80 20"
        style="PlainText"
        label="Calibration:"
        justify="Left"
        />
    </group>
    <group loc="320 30" size="360 110"
      style="Label2"
      label="Excitation"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <entry loc="120 40" size="60 20"
        style="PlainText"
        vq="p1"
        vc="vnmrunits('set','p1',$VALUE)"
        set="vnmrunits('get','p1'):$VALUE"
        show="$ENABLE=(p1[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <label loc="300 80" size="60 20"
        style="Label1"
        label="degrees"
        justify="Left"
        />
      <label loc="300 40" size="60 20"
        style="Label1"
        label="degrees"
        justify="Left"
        />
      <label loc="240 80" size="20 20"
        style="Label1"
        label="or"
        justify="Left"
        />
      <label loc="240 40" size="20 20"
        style="Label1"
        label="or"
        justify="Left"
        />
      <label loc="10 80" size="110 20"
        style="PlainText"
        label="Observe Pulse"
        justify="Left"
        />
      <label loc="10 60" size="100 20"
        style="PlainText"
        label="Inter-pulse delay"
        justify="Left"
        />
      <label loc="10 40" size="90 20"
        style="PlainText"
        label="First pulse"
        justify="Left"
        />
      <label loc="10 20" size="110 20"
        style="PlainText"
        label="Relaxation delay"
        justify="Left"
        />
      <label loc="0 0" size="120 20"
        style="Heading2"
        label="Excitation"
        justify="Left"
        />
      <entry loc="260 80" size="40 20"
        style="PlainText"
        vq="pw pw90"
        vc="pw($VALUE)"
        set="$VALUE=90*pw[1]/pw90"
        show="$VALUE=(pw90 &gt; 0)*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="260 40" size="40 20"
        style="PlainText"
        vq="p1 pw90"
        vc="p1($VALUE)"
        set="$VALUE=90*p1[1]/pw90"
        show="$VALUE=(pw90&gt;0)*2-1 if ($VALUE&gt;-0.5) then $VALUE=(p1[1]&gt;0) endif"
        digits="0"
        disable="Grayed out"
        />
      <menu loc="180 20" size="60 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 40" size="60 20"
        style="PlainText"
        label="menu"
        vq="p1"
        vc="parunits('set','p1','$VALUE')"
        set="parunits('get','p1'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="us"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 60" size="60 20"
        style="PlainText"
        vq="d2"
        vc="parunits('set','d2','$VALUE')"
        set="parunits('get','d2'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="120 60" size="60 20"
        style="PlainText"
        vq="d2"
        vc="vnmrunits('set','d2',$VALUE)"
        set="vnmrunits('get','d2'):$VALUE"
        show="$ENABLE=(d2[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <menu loc="180 80" size="60 20"
        style="PlainText"
        label="menu"
        vq="pw"
        vc="parunits('set','pw','$VALUE')"
        set="parunits('get','pw'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="us"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="120 80" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="$ENABLE=(pw[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 20" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="$ENABLE=(d1[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
    </group>
    <group loc="10 30" size="300 80"
      style="Label2"
      label="Data"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <label loc="10 60" size="120 20"
        style="PlainText"
        label="Complex points"
        justify="Left"
        />
      <label loc="10 40" size="120 20"
        style="PlainText"
        label="Acquisition time"
        justify="Left"
        />
      <label loc="0 0" size="120 20"
        style="Heading2"
        label="Data"
        justify="Left"
        />
      <label loc="10 20" size="120 20"
        style="PlainText"
        label="Spectral width"
        justify="Left"
        />
      <menu loc="190 40" size="60 20"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="130 60" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        disable="Grayed out"
        />
      <entry loc="130 40" size="60 20"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        disable="Grayed out"
        />
      <menu loc="190 20" size="60 20"
        style="PlainText"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="130 20" size="60 20"
        style="PlainText"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="10 120" size="300 80"
      style="Label2"
      label="Scans"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <check loc="190 60" size="60 20"
        style="PlainText"
        label="off"
        vq="ss"
        vc="ss='n'"
        vc2="ss='y'"
        set="off('ss'):$VALUE if (ss=0) then $VALUE=1 endif"
        />
      <textmessage loc="130 40" size="60 20"
        style="PlainText"
        vq="ct"
        set="$VALUE=ct"
        />
      <label loc="0 0" size="120 20"
        style="Heading2"
        label="Scans"
        justify="Left"
        />
      <label loc="10 60" size="90 20"
        style="PlainText"
        label="Steady-State"
        justify="Left"
        />
      <label loc="10 40" size="90 20"
        style="PlainText"
        label="Completed"
        justify="Left"
        />
      <label loc="10 20" size="90 20"
        style="PlainText"
        label="Requested"
        justify="Left"
        />
      <entry loc="130 60" size="30 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="on('ss'):$VALUE if (ss=0) then $VALUE=0 endif"
        disable="Grayed out"
        />
      <entry loc="130 20" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        disable="Grayed out"
        />
    </group>
    <button loc="440 0" size="90 20"
      style="PlainText"
      label="Arrays"
      vc="vnmrjcmd('array','popup')"
      bg="VJBackground"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <textmessage loc="10 0" size="240 20"
      style="Heading5"
      vq="tn pslabel"
      set="$VALUE=tn+' -- 1D'"
      />
    <button loc="260 0" size="130 20"
      style="PlainText"
      label="Display Sequence"
      vc="dps"
      bg="VJBackground"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
