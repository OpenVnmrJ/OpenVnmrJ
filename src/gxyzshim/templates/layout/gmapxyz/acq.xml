  </group>
  <group loc="0 0" size="680 250"
    style="PlainText"
    label="Future Actions"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="FutureActions"
    useref="yes"
    subtype="Untitled"
    expanded="no"
    rows="1"
    columns="1"
    >
  </group>
</template>
