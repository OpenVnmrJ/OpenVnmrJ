<template element="panels" type="acquisition" >
  <port size="506 298" style="Bold" point="8"
    fg="black"
    >
    <wgroup  label="User Login"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="accname"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="User Login:"
        justify="Left"
      />
      <wentry  size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        editable="no"
	keyword="accname"
      />
    </wgroup>
    <wgroup  label="User Name"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="name"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="User Name:"
        justify="Left"
      />
     <wentry  size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
	editable="yes"
	keyword="name"
      />
    </wgroup>
    <wgroup  label="Home Directory"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="home"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="Home Directory:"
        justify="Left"
       />
      <wentry  size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
	editable="yes"
        disable="Grayed out"
      />
    </wgroup>
    <wgroup label="VNMR User Directory"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="userdir"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="VNMR User Directory:"
        justify="Left"
      />
      <wentry size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
    </wgroup>
    <wgroup label="VNMR System Directory"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="sysdir"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="VNMR System Directory:"
        justify="Left"
      />
      <wentry  size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
    </wgroup>
    <wgroup  label="VNMR Data Directory"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="datadir"
      >
      <wlabel  size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="VNMR Data Directory:"
        justify="Left"
      />
      <wentry  size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
    </wgroup>
    <wgroup  label="Application"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="appdir"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
      	fg="Heading2"
        label="Application:"
        justify="Left"
      />
      <wentry size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
    </wgroup>
    <wgroup label="User Owned Directory"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="owned"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="User Owned Directory:"
	justify="Left"
      />
      <wentry size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
    </wgroup>
    <wgroup label="VnmrJ Operators"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="operators"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="VnmrJ Operators:"
	justify="Left"
      />
      <wentry size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
     </wgroup>
     <wgroup label="Access"
       border="None" side="Top" justify="Left" tab="no"
       useref="no"
       keyword="access"
      >
      <wlabel size="100 30" font="Heading2"
        style="Heading2"
        point="Heading2"
        fg="Heading2"
        label="Access:"
	justify="Left"
      />
      <wentry size="350 30" font="Menu4"
        style="Menu4"
        point="Menu4"
        fg="Menu4"
        disable="Grayed out"
	editable="yes"
      />
    </wgroup>
    <wgroup label="Interface Type"
       border="None"
       side="Top"
       justify="Left"
       tab="no"
       useref="no"
       keyword="itype"
      >
      <wlabel size="80 20" font="Heading1"
     	style="Heading1"
      	point="Heading1"
      	fg="Heading1"
      	label="Interface Type"
      	justify="Left"
      />
      <wgroup size="350 30" label="Labels"
         border="None"
         side="Top"
         justify="Left"
         tab="no"
         useref="no"
         keyword="itype"
	>
      	<wradio font="PlainText"
      	  style="PlainText"
      	  point="PlainText"
      	  fg="PlainText"
      	  label="Spectroscopy"
      	  keyval="Spectroscopy"
    	 />
        <wradio font="PlainText"
      	  style="PlainText"
      	  point="PlainText"
      	  fg="PlainText"
      	  label="Imaging"
      	  keyval="Imaging"
        />
	<wradio font="PlainText"
      	  style="PlainText"
      	  point="PlainText"
      	  fg="PlainText"
      	  label="LC-NMR/MS"
      	  keyval="LC-NMR/MS"
        />
      </wgroup>
    </wgroup>
    <wgroup label="Update Home Dir"
       border="None"
       side="Top"
       justify="Left"
       tab="no"
       useref="no"
       keyword="update"
      >
      <wlabel size="80 20" font="Heading1"
     	style="Heading1"
      	point="Heading1"
      	fg="Heading1"
      	label="Update Home Dir:"
      	justify="Left"
      />
      <wgroup size="350 30" label="Labels"
         border="None"
         side="Top"
         justify="Left"
         tab="no"
         useref="no"
         keyword="update"
	>
      	<wradio font="PlainText"
      	  style="PlainText"
      	  point="PlainText"
      	  fg="PlainText"
      	  label="Yes"
      	  vq="ir"
    	 />
        <wradio font="PlainText"
      	  style="PlainText"
      	  point="PlainText"
      	  fg="PlainText"
      	  label="No"
      	  vq="ir"
        />
      </wgroup>
    </wgroup>
  </port>
</template>
