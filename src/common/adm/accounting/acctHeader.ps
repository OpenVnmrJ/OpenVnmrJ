%!PS-Adobe-3.0
%%Title: OpenVnmrJ Plotting
%%Creator: Ace Spectroscopist
gsave
%%BeginProlog
/mv { moveto } def
/C { setrgbcolor } def
/vpos 3000 def
/newline
{/vpos vpos 52 sub def
 36 vpos moveto } def
%%EndProlog
%%Orientation: Landscape
%%Page: 1 1
%start
0.0 0.0 0.0 C
44 44 translate
0.2400 0.2400 scale
/Courier-Bold findfont 48 scalefont setfont
newline
(Summary for spectrometer:  HOSTNAME) show
newline
/Courier findfont 44 scalefont setfont
(Date of report: DATE) show
newline
newline
/Courier findfont 44 scalefont setfont
(           acquisition time      charge      login time         charge   total charge) show
newline
(                  [d+h:m:s]                   [d+h:m:s]) show
newline

