<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="5 5" size="450 375"
    bg="transparent"
    border="Etched"
    tab="no"
    enable="no"
    useref="no"
    subtype="Basic"
    rows="1"
    columns="1"
    >
        <textfile loc="5 10" size="440 280"
          style="StdPar"
          vq="craftllTable curexp craftpath"
          set="$VALUE=curexp+'/craftll.txt'"
          editable="no"
          wrap="no"
          units="char"
          />
        <button loc="55 295" size="150 20"
          style="Heading2"
          label="Print"
          vq="file craftpath seqfil layout"
          vc="craftllout('print')"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="55 320" size="90 20"
          style="Heading2"
          label="Email"
          vq="file craftpath seqfil layout craftemailaddr"
          vc="craftllout('email',craftemailaddr)"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <entry loc="160 320" size="215 20"
          style="PlainText"
          vq="craftemailaddr"
          vc="craftemailaddr='$VALUE'"
          set="$VALUE=craftemailaddr"
          />
        <button loc="225 295" size="150 20"
          style="Heading2"
          label="Copy to Desktop"
          vq="file craftpath seqfil layout emailaddr"
          vc="craftllout('copy2dt')"
          show="exists('craftpath','parameter'):$P2 $f=curexp+'/craftll.txt' exists($f,'file'):$ex $ENABLE=(($P2) and ($ex))*2-1"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="120 345" size="200 20"
          style="Heading2"
          label="Do PCA analysis"
          vq="file craftpath seqfil layout"
          vc="craftllout('dopca')"
          show="craftllout('dopca','isok'):$ok $ENABLE=($ok)*2-1"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
    </group>
</template>
