<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="git2" element="groups" type="sample" >
  <group size="570 525"
    style="Heading1"
    label="OpenVnmrJ submission"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="git2"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <textfile loc="10 345" size="545 160"
      style="PlainText"
      vq="curexp"
      set="$VALUE=curexp+'/submitlog'"
      editable="no"
      wrap="yes"
      units="char"
      />
    <button loc="10 305" size="140 25"
      style="PlainText"
      label="Submit Application"
      vc="ovjsubmit('save',1)"
      bg="transparent"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <entry loc="150 55" size="400 20"
      style="PlainText"
      vq="gitTitle"
      vc="gitTitle='$VALUE'"
      set="$VALUE=gitTitle"
      disable="Label"
      />
    <entry loc="150 25" size="400 20"
      style="PlainText"
      vq="gitDir"
      vc="gitDir='$VALUE'"
      set="$VALUE=gitDir"
      disable="Label"
      />
    <entry loc="150 115" size="400 20"
      style="PlainText"
      vq="gitUser"
      vc="gitUser='$VALUE'"
      set="$VALUE=gitUser"
      disable="Label"
      />
    <entry loc="150 85" size="400 20"
      style="PlainText"
      vq="gitTag"
      vc="gitTag='$VALUE'"
      set="$VALUE=gitTag"
      disable="Label"
      />
    <textfile loc="10 165" size="545 125"
      style="PlainText"
      vq="gitDir"
      set="$VALUE=gitDir+'/description'"
      editable="yes"
      wrap="yes"
      units="char"
      />
    <label loc="10 55" size="140 20"
      style="Label1"
      label="Title"
      />
    <label loc="10 145" size="90 20"
      style="Label1"
      label="Description:"
      />
    <label loc="10 25" size="140 20"
      style="Label1"
      label="Path to appdir"
      />
    <label loc="10 115" size="140 20"
      style="Label1"
      label="GitHub Username"
      />
    <label loc="10 85" size="140 20"
      style="Label1"
      label="Tag (e.g. v1.0)"
      />
  </group>
</template>
