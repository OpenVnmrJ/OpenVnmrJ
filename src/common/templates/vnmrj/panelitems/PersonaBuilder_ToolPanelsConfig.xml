<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Do NOT edit this file manually.  Use BuildPersonaXml macro -->
<template name="PersonaBuilder_ToolPanelsConfig" element="groups" type="acquisition" >
   <group size="200 135"
     bg="transparent"
     border="None"
     useref="no"
     tab="no"
     subtype="Basic"
     expanded="yes"
     >
     <label loc="10 5" size="155 20" style="Info"
    label="Tools" />
    <check loc="25 30" size="170 20" style="Label1"
      label="Command Line" set="$VALUE=(personaTpPar[2]='yes')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[2]='yes'"
      vc2="personaTpPar[2]='no'"
      show="$ENABLE=(personaTpPar[2]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 50" size="170 20" style="Label1" 
      label="Hardware Toolbar" set="$VALUE=(personaTpPar[5]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[5]='open'"
      vc2="personaTpPar[5]='close'"
      show="$ENABLE=(personaTpPar[5]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 70" size="170 20" style="Label1" 
      label="System Toolbar" set="$VALUE=(personaTpPar[8]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[8]='open'"
      vc2="personaTpPar[8]='close'"
      show="$ENABLE=(personaTpPar[8]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 90" size="170 20" style="Label1"
      label="User Toolbar" set="$VALUE=(personaTpPar[11]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[11]='open'"
      vc2="personaTpPar[11]='close'"
      show="$ENABLE=(personaTpPar[11]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 110" size="170 20" style="Label1"
      label="Graphics Toolbar" set="$VALUE=(personaTpPar[14]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[14]='open'"
      vc2="personaTpPar[14]='close'"
      show="$ENABLE=(personaTpPar[14]&lt;&gt;'notused')*2-1"
      />
  </group>
</template>
