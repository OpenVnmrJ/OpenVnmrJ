<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="DPFGSEgrp" element="groups" type="acquisition" >

    <group size="270 165"
      style="Heading1"
      border="Etched"
      tab="no"
      enable="no"
      reference="dpfgse"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="10 20" size="65 20"
        style="Label1"
        label="Pulse"
        justify="Left"
        />
      <label loc="90 20" size="80 20"
        style="Label1"
        label="Echo1"
        justify="Center"
        />
      <label loc="180 20" size="80 20"
        style="Label1"
        label="Echo2"
        justify="Center"
        />
      <label loc="20 40" size="65 20"
        style="Label1"
        label="widths (ms)"
        justify="Left"
        />
     <entry loc="90 40" size="80 20"
        style="PlainText"
        vq="selpwA"
        vc="selpwA=$VALUE*1000"
        set="$VALUE=selpwA/1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="180 40" size="80 20"
        style="PlainText"
        vq="selpwB"
        vc="selpwB=$VALUE*1000"
        set="$VALUE=selpwB/1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="20 60" size="65 20"
        style="Label1"
        label="powers (dB)"
        justify="Left"
        />
      <entry loc="90 60" size="80 20"
        style="PlainText"
        vq="selpwrA"
        vc="selpwrA=$VALUE"
        set="$VALUE=selpwrA"
        disable="Grayed out"
        />
      <entry loc="180 60" size="80 20"
        style="PlainText"
        vq="selpwrB"
        vc="selpwrB=$VALUE"
        set="$VALUE=selpwrB"
        disable="Grayed out"
        />
      <label loc="20 80" size="65 20"
        style="Label1"
        label="shapes"
        justify="Left"
        />
      <entry loc="90 80" size="80 20"
        style="PlainText"
        vq="selshapeA"
        vc="selshapeA='$VALUE'"
        set="$VALUE=selshapeA"
        disable="Grayed out"
        />
      <entry loc="180 80" size="80 20"
        style="PlainText"
        vq="selshapeB"
        vc="selshapeB='$VALUE'"
        set="$VALUE=selshapeB"
        disable="Grayed out"
        />
      <label loc="10 100" size="75 20"
        style="Label1"
        label="G-strength"
        justify="Left"
        />
      <entry loc="90 100" size="80 20"
        style="PlainText"
        vq="gzlvlA"
        vc="gzlvlA=$VALUE"
        set="$VALUE=gzlvlA"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="180 100" size="80 20"
        style="PlainText"
        vq="gzlvlB"
        vc="gzlvlB=$VALUE"
        set="$VALUE=gzlvlB"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 120" size="65 20"
        style="Label1"
        label="time (ms)"
        justify="Left"
        />
      <entry loc="90 120" size="80 20"
        style="PlainText"
        vq="gtA"
        vc="gtA=$VALUE/1000"
        set="$VALUE=gtA*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="180 120" size="80 20"
        style="PlainText"
        vq="gtB"
        vc="gtB=$VALUE/1000"
        set="$VALUE=gtB*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="20 140" size="130 20"
        style="Label1"
        label="Recovery delay (ms)"
        justify="Left"
        />
      <entry loc="180 140" size="80 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="2"
        disable="Grayed out"
        />

    </group>

</template>
