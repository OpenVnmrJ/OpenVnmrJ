<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="spinlock" element="groups" type="acquisition" >
  <group size="220 190"
    style="Heading1"
    label="SpinLock"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="spinlock"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <menu loc="70 40" size="135 20"
      style="PlainText"
      label="menu"
      vq="slpatT"
      vc="slpatT='$VALUE'"
      set="$VALUE=slpatT"
      editable="No"
      >
      <mlabel 
        label="MLEV17"
        chval="mlev17"
        />
      <mlabel 
        label="clean MLEV17"
        chval="mlev17c"
        />
      <mlabel 
        label="DIPSI2"
        chval="dipsi2"
        />
      <mlabel 
        label="DIPSI3"
        chval="dipsi3"
        />
    </menu>
    <label loc="10 120" size="90 20"
      style="Heading1"
      label="Trim pulse"
      justify="Left"
      />
    <entry loc="110 120" size="55 20"
      style="PlainText"
      vq="trim"
      vc="trim=$VALUE/1000"
      set="$VALUE=trim*1000"
      disable="Grayed out"
      />
    <label loc="170 120" size="30 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <label loc="170 100" size="25 20"
      style="Label1"
      label="Âµs"
      justify="Left"
      />
    <entry loc="110 100" size="55 20"
      style="PlainText"
      vq="slpwT"
      vc="slpwT=$VALUE"
      set="$VALUE=slpwT"
      digits="3"
      disable="Grayed out"
      />
    <label loc="10 100" size="100 20"
      style="Label1"
      label="Pi/2 pulse width"
      justify="Left"
      />
    <label loc="10 80" size="95 20"
      style="Label1"
      label="Power"
      justify="Left"
      />
    <entry loc="110 80" size="55 20"
      style="PlainText"
      vq="slpwrT"
      vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
      set="$VALUE=slpwrT"
      disable="Grayed out"
      />
    <label loc="170 80" size="30 20"
      style="Label1"
      label="dB"
      justify="Left"
      />
    <label loc="170 60" size="30 20"
      style="Label1"
      label="Hz"
      justify="Left"
      />
    <entry loc="110 60" size="55 20"
      style="PlainText"
      vq="spinlockT"
      vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
      set="$VALUE=spinlockT"
      disable="Grayed out"
      />
    <button loc="70 60" size="40 20"
      style="Heading2"
      label="Set"
      vc="setslgh2"
      halignment="Center"
      valignment="Center"
      />
    <label loc="10 60" size="55 20"
      style="Label1"
      label="Strength"
      justify="Left"
      />
    <label loc="10 40" size="60 20"
      style="Label1"
      label="Pattern"
      justify="Left"
      />
    <label loc="10 20" size="80 20"
      style="Label1"
      label="Mixing time"
      justify="Left"
      />
    <entry loc="110 20" size="55 20"
      style="PlainText"
      vq="mixT"
      vc="mixT=$VALUE/1000"
      set="$VALUE=mixT*1000"
      disable="Grayed out"
      />
    <label loc="170 20" size="30 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
  </group>
</template>
