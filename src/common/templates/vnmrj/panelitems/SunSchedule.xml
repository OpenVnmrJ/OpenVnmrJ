<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<template name="SunSchedule" element="groups" type="acquisition" >
      <group size="625 50"
        bg="transparent"
        border="Etched"
        tab="no"
        enable="no"
        useref="no"
	reference="SunSchedule"
        subtype="Basic"
	expanded="yes"
        >
	<group loc="10 5" size="115 20"
	  vq="AutoSchdl"
	  show="$SHOW=(AutoSchdl[2]='current')"
	  bg="transparent"
	  subtype="Convenience"
	  >
          <check loc="0 0" size="115 20"
            style="PlainText"
            label="DayQ"
            vq="SunSchdl"
            vc="autoschedule('setval','Sun',1,'8:00')"
            vc2="autoschedule('setval','Sun',1,'25:00')"
            set="if (SunSchdl[1]='25:00') then $VALUE=0 else $VALUE=1 endif"
          />
	</group>
	<group loc="10 5" size="115 20"
	  vq="AutoSchdl"
	  show="$SHOW=(AutoSchdl[2]&lt;&gt;'current')"
	  bg="transparent"
	  subtype="Convenience"
	  >
          <check loc="0 0" size="115 20"
            style="PlainText"
            label="New Autodir"
            vq="SunSchdl"
            vc="autoschedule('setval','Sun',1,'8:00')"
            vc2="autoschedule('setval','Sun',1,'25:00')"
            set="if (SunSchdl[1]='25:00') then $VALUE=0 else $VALUE=1 endif"
          />
	</group>
        <check loc="10 25" size="115 20"
          style="PlainText"
          label="NightQ"
          vq="SunSchdl"
          vc="autoschedule('setval','Sun',3,'18:00')"
          vc2="autoschedule('setval','Sun',3,'25:00')"
          set="if (SunSchdl[3]='25:00') then $VALUE=0 else $VALUE=1 endif"
          />
        <group loc="127 5" size="1 40"
          bg="transparent"
          border="Etched"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <filemenu loc="140 15" size="90 20"
          style="PlainText"
          vq="SunSchdl"
          vc="autoschedule('setval','Sun',1,'$VALUE')"
          set="$VALUE=SunSchdl[1]"
          show="$ENABLE=(SunSchdl[1]&lt;&gt;'25:00')*2-1"
          file="$VALUE=userdir+'/persistence/cpADMIN/ampmtable'"
          type="file"
          editable="No"
          vq2="SunSchdl"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <filemenu loc="240 15" size="90 20"
          style="PlainText"
          vq="SunSchdl"
          vc="autoschedule('setval','Sun',2,'$VALUE')"
          set="$VALUE=SunSchdl[2]"
          show="$ENABLE=(SunSchdl[1]&lt;&gt;'25:00')*2-1"
          file="$VALUE=userdir+'/persistence/cpADMIN/lengthtable'"
          type="file"
          editable="No"
          vq2="SunSchdl"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <check loc="340 5" size="65 20"
          style="PlainText"
          label="variable"
          vq="SunSchdl"
          show="$ENABLE=(SunSchdl[1]&lt;&gt;'25:00')*2-1"
          vc="SunSchdl[8]='flex'"
          vc2="SunSchdl[8]=''"
          set="$VALUE=(SunSchdl[8]='flex')"
          />
        <group loc="335 25" size="75 20"
          vq="SunSchdl"
          show="$SHOW=(SunSchdl[8]='flex')"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <button loc="5 0" size="65 20"
            style="PlainText"
            label="set/view"
            vq="SunSchdl"
            show="$ENABLE=(SunSchdl[1]&lt;&gt;'25:00')*2-1"
            vc="autoschedule('flexpopup','Sunday') vnmrjcmd('pnew','AutoSchdl')"
            bg="transparent"
            decor1="yes"
            />
        </group>
        <group loc="412 5" size="1 40"
          bg="transparent"
          border="Etched"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
        </group>
        <filemenu loc="425 15" size="90 20"
          style="PlainText"
          vq="SunSchdl"
          vc="autoschedule('setval','Sun',3,'$VALUE')"
          set="$VALUE=SunSchdl[3]"
          show="$ENABLE=((SunSchdl[1]&lt;&gt;'25:00') and (SunSchdl[3]&lt;&gt;'25:00'))*2-1"
          file="$VALUE=userdir+'/persistence/cpADMIN/ampmtable'"
          type="file"
          editable="No"
          vq2="SunSchdl"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <filemenu loc="525 15" size="90 20"
          style="PlainText"
          vq="SunSchdl"
          vc="SunSchdl[4]='$VALUE'"
          set="$VALUE=SunSchdl[4]"
          show="$ENABLE=(SunSchdl[3]&lt;&gt;'25:00')*2-1"
          file="$VALUE=userdir+'/persistence/cpADMIN/lengthtable'"
          type="file"
          editable="No"
          vq2="SunSchdl"
          statshow="yes"
          elastic="no"
          display="yes"
          />
      </group>
</template>
