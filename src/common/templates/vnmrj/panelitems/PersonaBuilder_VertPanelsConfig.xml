<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!-- Do NOT edit this file manually.  Use BuildPersonaXml macro -->
<template name="PersonaBuilder_VertPanelsConfig" element="groups" type="acquisition" >
   <group size="270 225"
     bg="transparent"
     border="None"
     useref="no"
     tab="no"
     subtype="Basic"
     expanded="yes"
     >
     <label loc="10 5" size="155 20" style="Info"
    label="Vertical Panels" />
    <check loc="25 30" size="170 20" style="Label1"
      label="Frame" set="$VALUE=(personaTpPar[17]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[17]='open'"
      vc2="personaTpPar[17]='close'"
      show="$ENABLE=(personaTpPar[17]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 50" size="170 20" style="Label1"
      label="QuickSubmit" set="$VALUE=(personaTpPar[20]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[20]='open'"
      vc2="personaTpPar[20]='close'"
      show="$ENABLE=(personaTpPar[20]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 70" size="170 20" style="Label1"
      label="Viewport" set="$VALUE=(personaTpPar[23]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[23]='open'"
      vc2="personaTpPar[23]='close'"
      show="$ENABLE=(personaTpPar[23]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 90" size="170 20" style="Label1"
      label="ProcessPlot" set="$VALUE=(personaTpPar[26]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[26]='open'"
      vc2="personaTpPar[26]='close'"
      show="$ENABLE=(personaTpPar[26]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 110" size="170 20" style="Label1"
      label="Cryo" set="$VALUE=(personaTpPar[29]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[29]='open'"
      vc2="personaTpPar[29]='close'"
      show="$ENABLE=(personaTpPar[29]&lt;&gt;'notused')*2-1"
      />
    <check loc="25 130" size="170 20" style="Label1"
      label="ArrayedSpectra" set="$VALUE=(personaTpPar[32]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[32]='open'"
      vc2="personaTpPar[32]='close'"
      show="$ENABLE=(personaTpPar[32]&lt;&gt;'notused')*2-1"
      />
    <group loc="25 150" size="170 20" >
    <radio loc="0 0" size="170 20" style="Label1"
      label="Experiment Selector" set="$VALUE=(personaTpPar[35]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[35]='open' if (personaTpPar[38]='open') then personaTpPar[38]='close' endif"
      vc2="personaTpPar[35]='close'"
      show="$ENABLE=(personaTpPar[35]&lt;&gt;'notused')*2-1"
      />
    </group>
    <group loc="25 170" size="190 20" >
    <radio loc="0 0" size="190 20" style="Label1"
      label="Experiment Selector Tree" set="$VALUE=(personaTpPar[38]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[38]='open' if (personaTpPar[35]='open') then personaTpPar[35]='close' endif"
      vc2="personaTpPar[38]='close'"
      show="$ENABLE=(personaTpPar[38]&lt;&gt;'notused')*2-1"
      />
    </group>
    <check loc="25 190" size="170 20" style="Label1"
      label="Study Queue" set="$VALUE=(personaTpPar[41]='open')"
      vq="personaTpPar personaPopupPar"
      vc="personaTpPar[41]='open'"
      vc2="personaTpPar[41]='close'"
      show="$ENABLE=(personaTpPar[41]&lt;&gt;'notused')*2-1"
      />
  </group>
</template>
