<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="cp" element="groups" type="acquisition" >
  <group size="370 100"
    vq="customflag procdim"
    show="$SHOW=((procdim&lt;1) and (customflag='y'))"
    border="None"
    tab="no"
    enable="no"
    reference="cp"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="0 25" size="370 75"
      vq="wds"
      show="strstr(wds,'wds'):$is if ($is&lt;1) then $SHOW=1 else $SHOW=0 endif"
      border="None"
      tab="no"
      enable="no"
      reference="customizepar-sppm"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="245 5" size="40 20"
        style="PlainText"
        vq="Array"
        vc="$wds=wds $wstart=wstart mkpar('integer','Array',0) Array=$VALUE if (Array &gt; 5) then Array=5 endif  selppm2frq('init',Array) wds=$wds wstart=$wstart"
        set="$VALUE=Array"
        />
      <label loc="10 5" size="225 20"
        style="Label1"
        label="No. of Selective Freqencies (max: 5)"
        />
      <label loc="335 50" size="30 20"
        style="Label1"
        label="Hz"
        />
      <label loc="335 30" size="30 20"
        style="Label1"
        label="ppm"
        />
      <label loc="10 50" size="100 20"
        style="Label1"
        label="Selective Width"
        />
      <entry loc="110 50" size="40 20"
        style="PlainText"
        vq="selfrqw"
        vc="if (Array &gt; 0) then selfrqw[1]=$VALUE  endif"
        set="$sps=size('selfrqw') if ($sps &gt; 0) then $VALUE=selfrqw[1] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 0)"
        digits="1"
        />
      <entry loc="155 50" size="40 20"
        style="PlainText"
        vq="selfrqw"
        vc="if (Array &gt; 1) then selfrqw[2]=$VALUE  endif"
        set="$sps=size('selfrqw') if ($sps &gt; 1) then $VALUE=selfrqw[2] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 1)"
        digits="1"
        />
      <entry loc="200 50" size="40 20"
        style="PlainText"
        vq="selfrqw"
        vc="if (Array &gt; 2) then selfrqw[3]=$VALUE  endif"
        set="$sps=size('selfrqw') if ($sps &gt; 2) then $VALUE=selfrqw[3] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 2)"
        digits="1"
        />
      <entry loc="245 50" size="40 20"
        style="PlainText"
        vq="selfrqw"
        vc="if (Array &gt; 3) then selfrqw[4]=$VALUE  endif"
        set="$sps=size('selfrqw') if ($sps &gt; 3) then $VALUE=selfrqw[4] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 3)"
        digits="1"
        />
      <entry loc="290 50" size="40 20"
        style="PlainText"
        vq="selfrqw"
        vc="if (Array &gt; 4) then selfrqw[5]=$VALUE  endif"
        set="$sps=size('selfrqw') if ($sps &gt; 4) then $VALUE=selfrqw[5] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 4)"
        digits="1"
        />
      <entry loc="290 30" size="40 20"
        style="PlainText"
        vq="selfrqppm "
        vc="if (Array &gt; 4) then selfrqppm[5]=$VALUE  endif"
        set="$sps=size('selfrqppm') if ($sps &gt; 4) then $VALUE=selfrqppm[5] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 4)"
        digits="2"
        />
      <entry loc="245 30" size="40 20"
        style="PlainText"
        vq="selfrqppm "
        vc="if (Array &gt; 3) then selfrqppm[4]=$VALUE  endif"
        set="$sps=size('selfrqppm') if ($sps &gt; 3) then $VALUE=selfrqppm[4] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 3)"
        digits="2"
        />
      <entry loc="200 30" size="40 20"
        style="PlainText"
        vq="selfrqppm "
        vc="if (Array &gt; 2) then selfrqppm[3]=$VALUE  endif"
        set="$sps=size('selfrqppm') if ($sps &gt; 2) then $VALUE=selfrqppm[3] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 2)"
        digits="2"
        />
      <entry loc="155 30" size="40 20"
        style="PlainText"
        vq="selfrqppm "
        vc="if (Array &gt; 1) then selfrqppm[2]=$VALUE  endif"
        set="$sps=size('selfrqppm') if ($sps &gt; 1) then $VALUE=selfrqppm[2] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 1)"
        digits="2"
        />
      <entry loc="110 30" size="40 20"
        style="PlainText"
        vq="selfrqppm "
        vc="if (Array &gt; 0) then selfrqppm[1]=$VALUE  endif"
        set="$sps=size('selfrqppm') if ($sps &gt; 0) then $VALUE=selfrqppm[1] else $VALUE=0 endif"
        show="$ENABLE=(Array &gt; 0)"
        digits="2"
        />
      <label loc="10 30" size="100 20"
        style="Label1"
        label="Selective Freq."
        />
    </group>
    <group loc="0 5" size="335 30"
      vq="customflag"
      show="$SHOW=(customflag='y')"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <check loc="110 5" size="55 20"
        style="Label3"
        label="Later"
        vq="wds"
        vc="rmpar('Array') selppm2frq('init',0) wds='wds=\'suselfrq setF1selshp\''"
        vc2="wds=''"
        set="strstr(wds,'wds'):$is if ($is&gt;0) then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="10 5" size="95 20"
        style="Label1"
        label="Define Bands"
        />
      <check loc="200 5" size="60 20"
        style="Label3"
        label="Now"
        vq="wds"
        vc="mkpar('integer','Array',0) selppm2frq('init',Array) wstart='setF1selshp' wds='wstart=\'suselfrq setF1selshp\''"
        vc2="rmpar('Array') selppm2frq('init',0) wds=''"
        set="strstr(wds,'wstart'):$is if ($is&gt;0) then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
  </group>
</template>
