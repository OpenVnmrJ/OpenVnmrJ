<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="GradientsZQ" element="groups" type="acquisition" >
  <group size="245 180"
    style="Heading1"
    label="Gradients"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="GradientsZQ"
    useref="no"
    subtype="Major"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <label loc="155 20" size="50 20"
      style="Label1"
      label="Delay"
      />
    <label loc="55 20" size="50 20"
      style="Label1"
      label="Level"
      />
    <label loc="210 40" size="30 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <label loc="210 60" size="30 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <label loc="210 80" size="30 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <label loc="210 100" size="30 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <entry loc="155 100" size="50 20"
      style="PlainText"
      vq="gstab"
      vc="gstab=$VALUE/1000"
      set="$VALUE=gstab*1000"
      digits="3"
      />
    <entry loc="155 80" size="50 20"
      style="PlainText"
      vq="gt3"
      vc="gt3=$VALUE/1000"
      set="$VALUE=gt3*1000"
      digits="3"
      />
    <entry loc="155 60" size="50 20"
      style="PlainText"
      vq="gt2"
      vc="gt2=$VALUE/1000"
      set="$VALUE=gt2*1000"
      digits="3"
      />
    <entry loc="155 40" size="50 20"
      style="PlainText"
      vq="gt0"
      vc="gt0=$VALUE/1000"
      set="$VALUE=gt0*1000"
      digits="3"
      />
    <label loc="130 40" size="25 20"
      style="Label1"
      label="gt0"
      />
    <label loc="130 60" size="25 20"
      style="Label1"
      label="gt2"
      />
    <label loc="130 80" size="25 20"
      style="Label1"
      label="gt3"
      />
    <label loc="115 100" size="40 20"
      style="Label1"
      label="gstab"
      />
    <entry loc="55 40" size="60 20"
      style="PlainText"
      vq="gzlvl0"
      vc="gzlvl0=$VALUE"
      set="$VALUE=gzlvl0"
      />
    <entry loc="55 60" size="60 20"
      style="PlainText"
      vq="gzlvl2"
      vc="gzlvl2=$VALUE"
      set="$VALUE=gzlvl2"
      />
    <entry loc="55 80" size="60 20"
      style="PlainText"
      vq="gzlvl3"
      vc="gzlvl3=$VALUE"
      set="$VALUE=gzlvl3"
      />
    <entry loc="55 100" size="60 20"
      style="PlainText"
      vq="gzlvlzq"
      vc="gzlvlzq=$VALUE"
      set="$VALUE=gzlvlzq"
      />
    <check loc="10 125" size="60 20"
      style="Label1"
      label="sspul"
      vq="sspul"
      vc="sspul='y' dps"
      vc2="sspul='n' dps"
      set="if (sspul='y') then $VALUE=1 else $VALUE=0 endif"
      tooltip="Suppress steady-state magnetization"
      />
    <label loc="10 100" size="45 20"
      style="Label1"
      label="gzlvlzq"
      />
    <label loc="10 80" size="45 20"
      style="Label1"
      label="gzlvl3"
      />
    <label loc="10 60" size="40 20"
      style="Label1"
      label="gzlvl2"
      />
    <label loc="10 40" size="40 20"
      style="Label1"
      label="gzlvl0"
      />
  </group>
</template>
