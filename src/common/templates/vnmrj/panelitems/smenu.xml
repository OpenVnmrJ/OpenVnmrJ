<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="smenu" element="groups" type="sample" >
  <group size="440 30"
    vq="wshim"
    show="$SHOW=(wshim&lt;&gt;'n')"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="smenu"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <filemenu loc="130 5" size="295 20"
      style="PlainText"
      vq="method Gmapname"
      vc="exec('$VALUE')"
      set="$VALUE=`method='`+method+`' Gmapname='`+Gmapname+`'`"
      file="exists('shimmenuInfo','templates/vnmrj/choicefiles'):$r,$VALUE"
      type="file"
      bg="VJBackground"
      editable="No"
      statshow="yes"
      elastic="no"
      display="yes"
      />
    <label loc="5 5" size="115 20"
      style="Label1"
      label="Select shim menu"
      />
  </group>
</template>
