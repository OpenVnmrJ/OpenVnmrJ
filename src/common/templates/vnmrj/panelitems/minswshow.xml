<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="minswshow" element="groups" type="acquisition" >
  <group size="215 40"
    vq="customflag"
    show="$SHOW=(customflag='y')"
    border="None"
    tab="no"
    enable="no"
    reference="minswshow"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <menu loc="120 0" size="75 20"
      style="PlainText"
      vq="MinSW"
      vc="MinSW='$VALUE'"
      set="$VALUE=MinSW"
      editable="No"
      >
      <mlabel 
        label="Auto"
        chval="auto"
        />
      <mlabel 
        label="Manual"
        chval="manual"
        />
      <mlabel 
        label="Off"
        chval="off"
        />
      <mlabel 
        label="Skip"
        chval="skip"
        />
    </menu>
    <label loc="10 0" size="100 20"
      style="Label1"
      label="Minimize SW"
      justify="Left"
      />
    <group loc="0 20" size="215 20"
      vq="procdim"
      show="$SHOW=(procdim&lt;1)"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <label loc="10 0" size="100 20"
        style="Label1"
        label="Calibrate pw90"
        justify="Left"
        />
      <check loc="120 0" size="25 20"
        style="Label1"
        vq="wstart"
        vc="wstart='pw90proc'"
        vc2="wstart=''"
        set="$VALUE=(wstart&lt;&gt;'')"
        />
    </group>
  </group>
</template>
