<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="bipgroup" element="groups" type="acquisition" >
  <group size="200 210"
    border="None"
    tab="no"
    enable="no"
    reference="bipgroup"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 0" size="185 105"
      style="Heading1"
      label="HiBand-Inversion"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AD-Xinversion"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 40" size="175 60"
        vq="bipflg"
        show="$bip='' substr(bipflg,1,1):$bip $SHOW=($bip='y')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="130 40" size="25 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <label loc="130 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <entry loc="45 0" size="120 20"
          style="PlainText"
          vq="tnbipshp"
          vc="tnbipshp='$VALUE'"
          set="$VALUE=tnbipshp"
          disable="Grayed out"
          />
        <entry loc="45 20" size="80 20"
          style="PlainText"
          vq="tnbippw"
          vc="tnbippw=$VALUE"
          set="$VALUE=tnbippw"
          disable="Grayed out"
          />
        <entry loc="45 40" size="80 20"
          style="PlainText"
          vq="tnbippwr"
          vc="tnbippwr=$VALUE"
          set="$VALUE=tnbippwr"
          disable="Grayed out"
          />
        <label loc="0 40" size="45 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="0 20" size="45 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
        <label loc="0 0" size="45 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
      <check loc="55 20" size="45 20"
        style="Label1"
        label="BIP"
        vq="bipflg"
        vc="$bip2='' substr(bipflg,2,1):$bip2 bipflg='y'+$bip2 dps"
        vc2="$bip2='' substr(bipflg,2,1):$bip2 bipflg='n'+$bip2 dps"
        set="$bip='' substr(bipflg,1,1):$bip $VALUE=($bip='y')"
        />
      <textmessage loc="10 20" size="45 20"
        style="PlainText"
        label="H1"
        vq="tn"
        set="$VALUE=tn"
        />
      <group loc="5 40" size="175 60"
        vq="bipflg"
        show="$bip='' substr(bipflg,1,1):$bip $SHOW=($bip='n')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="135 40" size="25 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <label loc="135 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <entry loc="50 40" size="80 20"
          style="PlainText"
          vq="tpwr180"
          vc="tpwr180=$VALUE"
          set="$VALUE=tpwr180"
          disable="Grayed out"
          />
        <entry loc="50 20" size="80 20"
          style="PlainText"
          vq="pw180"
          vc="pw180=$VALUE"
          set="$VALUE=pw180"
          disable="Grayed out"
          />
        <entry loc="50 0" size="120 20"
          style="PlainText"
          vq="pw180ad"
          vc="pw180ad='$VALUE'"
          set="$VALUE=pw180ad"
          disable="Grayed out"
          />
        <label loc="5 40" size="45 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="5 20" size="45 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
        <label loc="5 0" size="45 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 105" size="185 105"
      style="Heading1"
      label="LoBand-Inversion"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AD-Xinversion"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 40" size="175 60"
        vq="bipflg"
        show="$bip='' substr(bipflg,2,1):$bip $SHOW=($bip='y')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="130 40" size="25 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <label loc="130 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <entry loc="45 0" size="120 20"
          style="PlainText"
          vq="dnbipshp"
          vc="dnbipshp='$VALUE'"
          set="$VALUE=dnbipshp"
          disable="Grayed out"
          />
        <entry loc="45 20" size="80 20"
          style="PlainText"
          vq="dnbippw"
          vc="dnbippw=$VALUE"
          set="$VALUE=dnbippw"
          disable="Grayed out"
          />
        <entry loc="45 40" size="80 20"
          style="PlainText"
          vq="dnbippwr"
          vc="dnbippwr=$VALUE"
          set="$VALUE=dnbippwr"
          disable="Grayed out"
          />
        <label loc="0 40" size="45 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="0 20" size="45 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
        <label loc="0 0" size="45 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
      <check loc="55 20" size="45 20"
        style="Label1"
        label="BIP"
        vq="bipflg"
        vc="$bip1='' substr(bipflg,1,1):$bip1 bipflg=$bip1+'y' dps"
        vc2="$bip1='' substr(bipflg,1,1):$bip1 bipflg=$bip1+'n' dps"
        set="$bip='' substr(bipflg,2,1):$bip $VALUE=($bip='y')"
        />
      <group loc="5 40" size="175 60"
        vq="bipflg"
        show="$bip='' substr(bipflg,2,1):$bip $SHOW=($bip='n')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="135 40" size="25 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <label loc="135 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <entry loc="50 40" size="80 20"
          style="PlainText"
          vq="pwxlvl180"
          vc="pwxlvl180=$VALUE"
          set="$VALUE=pwxlvl180"
          disable="Grayed out"
          />
        <entry loc="50 20" size="80 20"
          style="PlainText"
          vq="pwx180"
          vc="pwx180=$VALUE"
          set="$VALUE=pwx180"
          disable="Grayed out"
          />
        <entry loc="50 0" size="120 20"
          style="PlainText"
          vq="pwx180ad"
          vc="pwx180ad='$VALUE'"
          set="$VALUE=pwx180ad"
          disable="Grayed out"
          />
        <label loc="5 40" size="45 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="5 20" size="45 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
        <label loc="5 0" size="45 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
      <textmessage loc="10 20" size="45 20"
        style="PlainText"
        label="C13"
        vq="dn"
        set="$VALUE=dn"
        />
    </group>
  </group>
</template>
