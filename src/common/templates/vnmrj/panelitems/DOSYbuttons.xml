<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="DOSYbuttons" element="groups" type="acquisition" >
  <group size="240 105"
    style="Heading1"
    label="DOSY Parameters"
    border="None"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="DOSYbuttons"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <label loc="5 75" size="150 20"
      style="Label1"
      label="Grad. stabilization delay"
      justify="Left"
      />
    <label loc="5 55" size="145 20"
      style="Label1"
      label="Diffusion delay"
      justify="Left"
      />
    <label loc="5 35" size="150 20"
      style="Label1"
      label="Diffusion gradient length"
      justify="Left"
      />
    <label loc="215 75" size="25 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <label loc="215 55" size="25 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <label loc="215 35" size="25 20"
      style="Label1"
      label="ms"
      justify="Left"
      />
    <entry loc="155 75" size="55 20"
      style="PlainText"
      vq="gstab"
      vc="gstab=$VALUE/1000"
      set="$VALUE=gstab*1000"
      digits="1"
      disable="Grayed out"
      />
    <entry loc="155 55" size="55 20"
      style="PlainText"
      vq="del"
      vc="del=$VALUE/1000"
      set="$VALUE=del*1000"
      digits="1"
      disable="Grayed out"
      />
    <entry loc="155 35" size="55 20"
      style="PlainText"
      vq="gt1"
      vc="gt1=$VALUE/1000"
      set="$VALUE=gt1*1000"
      digits="1"
      disable="Grayed out"
      />
    <entry loc="155 15" size="55 20"
      style="PlainText"
      vq="gzlvl1"
      vc="gzlvl1=$VALUE"
      set="$VALUE=gzlvl1"
      disable="Grayed out"
      />
    <label loc="5 15" size="145 20"
      style="Label1"
      label="Diffusion gradient level"
      justify="Left"
      />
  </group>
</template>
