<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="cpmggroup" element="groups" type="acquisition" >

   <group size="270 155"
      style="Heading2"
      bg="transparent"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FlipBack"
      useref="no"
      expanded="yes"
      rows="1"
      columns="1"
      >
    <group loc="5 0" size="260 150"
      style="Heading2"
      bg="transparent"
      show="$SHOW=(flipback='y')"
      vq="flipback"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="10 0" size="140 20"
        style="Label1"
        label="Keep water along Z"
        justify="Left"
        />
      <label loc="210 20" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="150 20" size="60 20"
        style="PlainText"
        vq="flippw flipback"
        vc="flippw=$VALUE*1000"
        set="$VALUE=flippw/1000"
        show="$ENABLE=(flipback='y' )"
        digits="1"
        disable="Grayed out"
        tooltip="Solvent Flipback pulse length (flippw)"
        />
      <label loc="40 20" size="60 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <label loc="40 40" size="60 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <label loc="210 40" size="30 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <entry loc="150 40" size="60 20"
        style="PlainText"
        vq="flippwr flipback"
        vc="flippwr=$VALUE"
        set="$VALUE=flippwr"
        show="$ENABLE=(flipback='y' )"
        digits="1"
        disable="Grayed out"
        tooltip="Solvent Flipback pulse power (flippwr)"
        />
      <entry loc="150 60" size="60 20"
        style="PlainText"
        vq="flippwrf flipback"
        vc="flippwrf=$VALUE"
        set="$VALUE=flippwrf"
        show="$ENABLE=(flipback='y' )"
        digits="1"
        disable="Grayed out"
        />
      <label loc="40 60" size="70 20"
        style="Label1"
        label="fine power"
        justify="Left"
        />
      <label loc="40 80" size="60 20"
        style="Label1"
        label="shape"
        justify="Left"
        />
      <entry loc="110 80" size="130 20"
        style="PlainText"
        vq="flipshape flipback"
        vc="flipshape='$VALUE'"
        set="$VALUE=flipshape"
        show="$ENABLE=(flipback='y' )"
        disable="Grayed out"
        tooltip="Shape definition of the Flipback Pulse (flipshape)"
        />
      <label loc="210 100" size="30 20"
        style="PlainText"
        label="deg"
        justify="Left"
        />
      <entry loc="150 100" size="60 20"
        style="PlainText"
        vq="phincr1 flipback"
        vc="phincr1=$VALUE"
        set="$VALUE=phincr1"
        show="$ENABLE=(flipback='y' )"
        disable="Grayed out"
        tooltip="Small angle phase shift for the Flipback Pulse (phincr1)"
        />
      <label loc="10 100" size="140 20"
        style="Label1"
        label="small angle phase shift"
        justify="Left"
        />

      <button loc="40 125" size="170 20"
        style="Label1"
        label="Recreate flipback shape"
        vc="make_es_flipshape"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    </group>
</template>
