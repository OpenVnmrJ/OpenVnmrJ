<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<template name="FlexScheduler" element="groups" type="acquisition" >
      <group size="450 260"
        vq="AutoSchdl"
        bg="transparent"
        border="LoweredBevel"
        tab="no"
        enable="no"
        reference="FlexScheduler"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <button loc="105 205" size="80 25"
          style="Heading3"
          label="Clear"
          vc="autoschedule('flexclear')"
          decor1="yes"
          />
        <button loc="190 205" size="80 25"
          style="Heading3"
          label="Cancel"
          vc="autoschedule('flexcancel')"
          decor1="yes"
          />
        <button loc="20 205" size="80 25"
          style="Heading3"
          label="Save"
          vc="autoschedule('flexsave')"
          decor1="yes"
          />
        <textfile loc="15 95" size="255 105"
          style="PlainText"
          vq="userdir AutoSchdl"
          set="$VALUE=userdir+'/persistence/cpADMIN/autoconftmp2'"
          show="$ENABLE=0"
          editable="no"
          wrap="no"
          units="char"
          />
        <label loc="295 70" size="120 25"
          style="Heading2"
          label="New schedule"
          justify="Center"
          />
        <label loc="275 100" size="65 20"
          style="Label1"
          label="Start:"
          justify="Right"
          />
        <label loc="275 125" size="65 20"
          style="Label1"
          label="End:"
          justify="Right"
          />
        <label loc="275 150" size="65 20"
          style="Label1"
          label="Maxtime:"
          justify="Right"
          />
        <filemenu loc="345 150" size="90 20"
          style="PlainText"
          vq="AutoSchdl"
          vc="AutoSchdl[10]='$VALUE'"
          set="$VALUE=AutoSchdl[10]"
          file="$VALUE=userdir+'/persistence/cpADMIN/lengthtable'"
          type="file"
          editable="No"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <filemenu loc="345 125" size="90 20"
          style="PlainText"
          vq="AutoSchdl"
          vc="AutoSchdl[9]='$VALUE'"
          set="$VALUE=AutoSchdl[9]"
          file="$VALUE=userdir+'/persistence/cpADMIN/ampmtable'"
          type="file"
          editable="No"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <filemenu loc="345 100" size="90 20"
          style="PlainText"
          vq="AutoSchdl"
          vc="AutoSchdl[8]='$VALUE'"
          set="$VALUE=AutoSchdl[8]"
          file="$VALUE=userdir+'/persistence/cpADMIN/ampmtable'"
          type="file"
          editable="No"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <button loc="310 180" size="80 25"
          style="Heading3"
          label="Add"
          vc="autoschedule('flexwrite')"
          decor1="yes"
          />
        <textmessage loc="175 70" size="65 20"
          style="Label1"
          vq="AutoSchdl"
          set="$VALUE=AutoSchdl[7]"
          />
        <label loc="55 70" size="115 20"
          style="Label1"
          label="Maxtime/sample"
          justify="Right"
          />
        <textmessage loc="240 50" size="65 20"
          style="Label1"
          vq="AutoSchdl"
          set="$VALUE=AutoSchdl[6]"
          />
        <label loc="185 50" size="50 20"
          style="Label1"
          label="ends:"
          justify="Right"
          />
        <textmessage loc="115 50" size="65 20"
          style="Label1"
          vq="AutoSchdl"
          set="$VALUE=AutoSchdl[5]"
          />
        <label loc="15 50" size="95 20"
          style="Label1"
          label="DayQ begins:"
          justify="Right"
          />
        <textmessage loc="15 20" size="350 25"
          style="Heading2"
          vq="AutoSchdl"
          set="$VALUE='DayQ time limits for '+AutoSchdl[4]+' :'"
          />
      </group>

</template>
