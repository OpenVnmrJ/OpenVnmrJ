<template type="basic" element="elements" >
  <parameter size="300 20"
    style="PlainText"
    label="parameter"
    bg="transparent"
    tab="no"
    vq2="notebook"
    entryvalue="$VALUE=notebook"
    entrycmd="notebook='$VALUE'"
    entrysize="90"
    unitsenabled="Label"
    unitssize="60"
    unitscmd="notebook='$VALUE'"
    unitsvalue="$VALUE=notebook"
    enable="no"
    reference="parameter"
    useref="no"
    disable="Grayed out"
    expanded="yes"
    >
    <label loc="0 0" size="130 20"
      style="PlainText"
      label="parameter"
      vq="notebook"
      subtype="parameter"
      />
    <entry loc="150 0" size="90 20"
      style="PlainText"
      vq="notebook"
      vc="notebook='$VALUE'"
      set="$VALUE=notebook"
      disable="Grayed out"
      subtype="parameter"
      />
    <check loc="130 0" size="20 20"
      style="PlainText"
      vq="notebook"
      subtype="parameter"
      />
    <label loc="240 0" size="60 20"
      style="PlainText"
      vq="notebook"
      key="units"
      subtype="parameter"
      />
  </parameter>
</template>
