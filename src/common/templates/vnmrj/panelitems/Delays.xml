<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Delays" element="groups" type="acquisition" >
  <group size="380 200"
    style="Heading2"
    label="Shapes"
    bg="transparent"
    border="Etched"
    side="Top"
    justify="Left"
    tab="no"
    enable="no"
    reference="Delays"
    useref="no"
    subtype="Titled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <label loc="10 100" size="120 20"
      style="PlainText"
      label="Calibration: pw90"
      justify="Left"
      />
    <button loc="10 125" size="215 20"
      style="Label3"
      label="Recreate with current pw90"
      vc="setgmapshapes('current')"
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <button loc="10 150" size="215 20"
      style="Label3"
      label="Recreate with pw90 from probefile"
      vc="setgmapshapes"
      bg="transparent"
      decor1="no"
      halignment="Center"
      valignment="Center"
      />
    <label loc="140 20" size="50 20"
      style="PlainText"
      label="90"
      justify="Center"
      />
    <label loc="280 20" size="60 20"
      style="PlainText"
      label="180"
      justify="Center"
      />
    <check loc="10 20" size="140 20"
      style="PlainText"
      label="Shaped pulses Y/N"
      vq="shaped"
      vc="exists('shapename90','parameter'):$e if not $e then setgmapshapes endif shaped='y' "
      vc2="shaped='n'"
      set="$VALUE=(shaped='y')"
      />
    <label loc="345 100" size="25 20"
      style="PlainText"
      label="dB"
      justify="Left"
      />
    <label loc="345 80" size="25 20"
      style="PlainText"
      label="dB"
      justify="Left"
      />
    <entry loc="280 100" size="60 20"
      style="PlainText"
      vq="tpwr shaped"
      vc="tpwr=$VALUE"
      set="$VALUE=tpwr"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <entry loc="135 100" size="60 20"
      style="PlainText"
      vq="pw90 shaped"
      vc="pw90=$VALUE"
      set="$VALUE=pw90"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <label loc="200 100" size="80 20"
      style="PlainText"
      label="at tpwr"
      justify="Left"
      />
    <entry loc="280 80" size="60 20"
      style="PlainText"
      vq="shapedpwr180 shaped"
      vc="shapedpwr180=$VALUE"
      set="$VALUE=shapedpwr180"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <label loc="345 60" size="25 20"
      style="PlainText"
      label="ms"
      justify="Left"
      />
    <entry loc="280 60" size="60 20"
      style="PlainText"
      vq="shapedpw180 shaped"
      vc="shapedpw180=$VALUE*1000"
      set="$VALUE=shapedpw180/1000"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <entry loc="200 40" size="140 20"
      style="PlainText"
      vq="shapename180 shaped"
      vc="shapename180='$VALUE'"
      set="$VALUE=shapename180"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <entry loc="135 60" size="60 20"
      style="PlainText"
      vq="shapedpw90 shaped"
      vc="shapedpw90=$VALUE*1000"
      set="$VALUE=shapedpw90/1000"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <entry loc="55 40" size="140 20"
      style="PlainText"
      vq="shapename90 shaped"
      vc="shapename90='$VALUE'"
      set="$VALUE=shapename90"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <entry loc="135 80" size="60 20"
      style="PlainText"
      vq="shapedpwr90 shaped"
      vc="shapedpwr90=$VALUE"
      set="$VALUE=shapedpwr90"
      show="$ENABLE=(shaped='y')"
      disable="Grayed out"
      />
    <label loc="10 40" size="45 20"
      style="PlainText"
      label="Name"
      justify="Left"
      />
    <label loc="10 60" size="90 20"
      style="PlainText"
      label="Pulsewidth"
      justify="Left"
      />
    <label loc="10 80" size="90 20"
      style="PlainText"
      label="Power"
      justify="Left"
      />
  </group>
</template>
