<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="cpmggroup" element="groups" type="acquisition" >

   <group size="270 230"
      style="Heading2"
      bg="transparent"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="WGecho"
      useref="no"
      expanded="yes"
      rows="1"
      columns="1"
      >
<!--
      <check loc="15 5" size="250 20"
        vq="ESmode"
        vc="ESmode='y'"
        vc2="ESmode='n'"
        set="$VALUE=(ESmode='y')"
        label="Excitation Sculpting"
        style="Label1"
        />
-->
    <group loc="5 0" size="260 225"
      style="Heading2"
      bg="transparent"
      label="DPFGSE"
      show="$SHOW=(ESmode='y')"
      vq="ESmode"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      subtype="Titled"
      useref="no"
      expanded="yes"
      rows="1"
      columns="1"
      >

      <label loc="30 175" size="100 20"
        style="PlainText"
        label="Recovery time"
        justify="Left"
        />
      <label loc="200 175" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="200 155" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="140 175" size="60 20"
        style="PlainText"
        vq="esgstab"
        vc="esgstab=$VALUE/1000"
        set="$VALUE=esgstab*1000"
        digits="1"
        disable="Grayed out"
        tooltip="Gradient recovery delay ijn the DPFGSE block (esgstab)"
        />
      <entry loc="140 155" size="60 20"
        style="PlainText"
        vq="esgt"
        vc="esgt=$VALUE/1000"
        set="$VALUE=esgt*1000"
        digits="1"
        disable="Grayed out"
        tooltip="Gradient duration in the DPFGSE block (esgt)"
        />
      <label loc="30 155" size="60 20"
        style="PlainText"
        label="time (ms)"
        justify="Left"
        />
      <entry loc="140 135" size="60 20"
        style="PlainText"
        vq="esgzlvl"
        vc="esgzlvl=$VALUE"
        set="$VALUE=esgzlvl"
        digits="0"
        disable="Grayed out"
        tooltip="Gradient level in the DPFGSE block (esgzlvl)"
        />
      <label loc="30 135" size="60 20"
        style="PlainText"
        label="strength"
        justify="Left"
        />
      <label loc="10 120" size="70 20"
        style="PlainText"
        label="Gradient -"
        justify="Left"
        />
      <entry loc="100 100" size="145 20"
        style="PlainText"
        vq="esshape"
        vc="esshape='$VALUE'"
        set="$VALUE=esshape"
        disable="Grayed out"
        tooltip="Pattern of the Solvent Selective Pulse (esshape)"
        />
      <label loc="40 100" size="60 20"
        style="Label1"
        label="shape"
        justify="Left"
        />
      <entry loc="140 80" size="60 20"
        style="PlainText"
        vq="essoftpwrf"
        vc="essoftpwrf=$VALUE"
        set="$VALUE=essoftpwrf"
        digits="1"
        disable="Grayed out"
        tooltip="Fine Power Level of the Solvent Selective Pulse (essoftpwrf)"
        />
      <label loc="40 80" size="70 20"
        style="Label1"
        label="fine power"
        justify="Left"
        />
      <label loc="40 60" size="60 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <entry loc="140 60" size="60 20"
        style="PlainText"
        vq="essoftpwr"
        vc="essoftpwr=$VALUE"
        set="$VALUE=essoftpwr"
        digits="1"
        disable="Grayed out"
        tooltip="Coarse Power Level of the Solvent Selective Pulse (essoftpwr)"
        />
      <label loc="200 60" size="30 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <label loc="200 40" size="30 20"
        style="PlainText"
        label="us"
        justify="Left"
        />
      <entry loc="140 40" size="60 20"
        style="PlainText"
        vq="essoftpw"
        vc="essoftpw=$VALUE"
        set="$VALUE=essoftpw"
        digits="1"
        disable="Grayed out"
        tooltip="Solvent Selective Pulse Width (essoftpw) "
        />
      <label loc="40 40" size="60 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <label loc="10 20" size="140 20"
        style="Label1"
        label="Solvent Selective Pulse"
        justify="Left"
        />
      <button loc="30 200" size="200 20"
        style="Label1"
        label="Recreate DPFGSE shape"
        vc="make_es_shape"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      </group>
    </group>
</template>
