<template name="CosyCorr" element="pages" type="acquisition" >
  <group size="350 110"
    style="Heading1"
    label="Cosy Correlations"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="CosyCorr"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <button loc="10 10" size="160 20"
      style="Heading2"
      label="Square 2D"
      vc="fullsq"
      decor1=""
      />
    <button loc="10 30" size="160 20"
      style="Heading2"
      label="Create 2D Line List"
      vc="ll2d('reset'):$x ll2d('peak','volume')"
      decor1=""
      />
    <button loc="10 50" size="160 20"
      style="Heading2"
      label="Find Correlations"
      vc="acosy"
      decor1=""
      />
    <button loc="10 70" size="160 20"
      style="Heading2"
      label="Redisplay"
      vc="dconi"
      decor1=""
      />
    <button loc="10 90" size="160 20"
      style="Heading2"
      label="Hide All Peak Markers"
      vc="ll2dmode='nnnn' dconi('again')"
      decor1=""
      />
    <button loc="180 10" size="160 20"
      style="Heading2"
      label="Plot Correlations"
      vc="pacosy"
      decor1=""
      />
    <button loc="180 30" size="160 20"
      style="Heading2"
      label="Plot Contours"
      vc="pcon(20,1.3)"
      decor1=""
      />
    <button loc="180 50" size="80 20"
      style="Heading2"
      label="Plot Page"
      vc="page resetplotter"
      decor1=""
      />
    <button loc="260 50" size="80 20"
      style="Heading2"
      label="Plot Preview"
      vc="pageview resetplotter"
      decor1=""
      />
  </group>
</template>
