<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="0 0" size="335 115"
    style="Heading1"
    border="None"
    tab="no"
    enable="no"
    useref="no"
    subtype="Untitled"
    rows="1"
    columns="1"
    >
    <entry loc="185 75" size="40 20"
      style="PlainText"
      vq="plotlogoy"
      vc="plotlogoy=$VALUE"
      set="$VALUE=plotlogoy"
      />
    <label loc="140 75" size="45 20"
      style="Label1"
      label="Height"
      justify="Left"
      />
    <entry loc="75 75" size="40 20"
      style="PlainText"
      vq="plotlogox"
      vc="plotlogox=$VALUE"
      set="$VALUE=plotlogox"
      />
    <label loc="30 75" size="40 20"
      style="Label1"
      label="Width"
      justify="Left"
      />
    <filebrowser loc="0 35" size="320 20"
      label="file"
      vq="plotlogo"
      border="None"
      tab="no"
      enable="no"
      reference="filebrowser"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <button loc="240 0" size="70 20"
        style="Heading2"
        label="Browse..."
        vq="plotlogo"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        actionCmd="show"
        />
      <entry loc="10 0" size="225 20"
        style="PlainText"
        vq="plotlogo"
        vc="plotlogo='$VALUE'"
        set="if plotlogo='' then $VALUE='/vnmr/iconlib/plotLogo.gif' else $VALUE=plotlogo endif"
        />
    </filebrowser>
    <label loc="10 10" size="200 25"
      style="Label1"
      label="Select logo to plot:"
      />
  </group>
</template>
