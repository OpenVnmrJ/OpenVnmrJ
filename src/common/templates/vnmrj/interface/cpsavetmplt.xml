<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="cpsavetmplt" element="groups" type="acquisition" >
  <group size="745 145"
    bg=""
    border="None"
    tab="no"
    enable="no"
    reference="cpsavetmplt"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <label loc="310 7" size="165 25"
      style="Heading1"
      label="Use Template Formats"
      justify="Center"
      />
    <entry loc="260 112" size="475 25"
      style="PlainText"
      vq="svfname"
      vc="svfname='$VALUE' setsvftmplt"
      set="$VALUE=svfname"
      />
    <label loc="10 112" size="250 25"
      style="Label1"
      label="Fid template:"
      justify="Right"
      />
    <label loc="85 87" size="340 20"
      style="Label1"
      label="Note: (loc stamp is applicable only under automation)"
      />
    <menu loc="260 62" size="475 25"
      style="PlainText"
      vq="sampdirtmplt"
      vc="sampdirtmplt='$VALUE' sdirtmplt=sampdirtmplt"
      set="$VALUE=sampdirtmplt"
      editable="No"
      >
      <mlabel 
        label="$samplename$_%DATE%_$loc$_%R0%"
        chval="samplename_date_loc_%R0%"
        />
      <mlabel 
        label="$samplename$_%DATE%_$loc$_%R2%"
        chval="samplename_date_loc"
        />
      <mlabel 
        label="$samplename$_%DATE%_%R0%"
        chval="samplename_date_%R0%"
        />
      <mlabel 
        label="$samplename$_%DATE%_%R2%"
        chval="samplename_date"
        />
      <mlabel 
        label="$samplename$_%R0%"
        chval="samplename_%R0%"
        />
      <mlabel 
        label="$samplename$_%R2%"
        chval="samplename"
        />
    </menu>
    <label loc="10 62" size="250 25"
      style="Label1"
      label="Sample Directory (aka &quot;sample&quot;): "
      justify="Right"
      />
    <entry loc="260 37" size="475 25"
      style="PlainText"
      vq="svfdir"
      vc="svfdir='$VALUE' setarchivedir"
      set="$VALUE=svfdir"
      />
    <label loc="10 37" size="250 25"
      style="Label1"
      label="Study Directory (aka &quot;archivedir&quot;): "
      justify="Right"
      />
  </group>
</template>
