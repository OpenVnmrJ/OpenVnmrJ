<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Studyprobe" element="groups" type="acquisition" >
  <group size="360 200"
    bg=""
    border="None"
    tab="no"
    enable="no"
    reference="Studyprobe"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <label loc="95 10" size="85 25"
      style="Heading1"
      label="Parameter"
      />
    <label loc="10 10" size="65 25"
      style="Label1"
      label="Group"
      />
    <label loc="185 10" size="65 25"
      style="Label1"
      label="Value"
      />
    <button loc="260 166" size="80 25"
      style="Heading2"
      label="Clear"
      vq="Studyprobepar"
      vc="Studyprobe('clear')"
      bg=""
      decor1=""
      />
    <textfile loc="15 66" size="325 100"
      style="PlainText"
      vq="Studyprobepar"
      vq2="Studyprobepar userdir"
      set="$VALUE=userdir+'/persistence/Studyprobe'"
      editable="no"
      wrap="yes"
      units="char"
      />
    <button loc="270 36" size="80 25"
      style="Heading2"
      label="Set"
      vq="Studyprobepar"
      vc="Studyprobe('set')"
      bg=""
      decor1=""
      />
    <entry loc="185 36" size="80 25"
      style="PlainText"
      vq="Studyprobepar"
      vc="Studyprobepar[3]='$VALUE'"
      set="$VALUE=Studyprobepar[3]"
      />
    <entry loc="100 36" size="80 25"
      style="PlainText"
      vq="Studyprobepar"
      vc="Studyprobepar[2]='$VALUE'"
      set="$VALUE=Studyprobepar[2]"
      />
    <entry loc="15 36" size="80 25"
      style="PlainText"
      vq="Studyprobepar"
      vc="Studyprobepar[1]='$VALUE'"
      set="$VALUE=Studyprobepar[1]"
      />
  </group>
</template>
