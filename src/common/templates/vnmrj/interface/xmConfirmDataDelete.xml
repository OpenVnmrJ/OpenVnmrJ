<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<template name="ConfirmDelete" element="pages" type="acquisition" >
  <group loc="0 0" size="360 120"
    bg=""
    border="None"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="20 0" size="340 120"
      bg=""
      border="None"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="0 10" size="1000 40"
        style="Heading1"
        label="OK to permanently delete the selected data?"
        />
      <check loc="0 60" size="300 20"
        style="Label1"
        label="Don't show this window again"
        vq="xmConfirmDataDelete"
        set="xmConfirmDataDelete:$v $VALUE=not $v"
        vc="xmConfirmDataDelete(0)"
        vc2="xmConfirmDataDelete(1)"
        />
        <label loc="20 80" size="280 20"
          style="Label1"
          label="(Also set in the StudyQueue background menu)"
          />
    </group>
  </group>
</template>
