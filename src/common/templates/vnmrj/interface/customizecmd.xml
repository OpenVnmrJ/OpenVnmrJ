<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="customizecmd" element="groups" type="acquisition" >
  <group size="420 300"
    bg=""
    border="None"
    tab="no"
    enable="no"
    reference="customizecmd"
    useref="no"
    subtype="Convenience"
    expanded="yes"
    >
    <textmessage loc="10 10" size="400 20"
      style="PlainText"
      vq="layoutargs"
      set="$VALUE='Default macro for '+layoutargs[1]+': '+layoutargs[3]"
      />
    <textfile loc="10 35" size="400 100"
      style="PlainText"
      fg=""
      vq="cursqexp layoutargs"
      set="$VALUE=cursqexp+'/tmpCMDacq0'"
      editable="no"
      wrap="no"
      />
    <textmessage loc="10 140" size="340 20"
      style="PlainText"
      vq="layoutargs"
      set="$VALUE='User customization for '+layoutargs[1]"
      />
    <textfile loc="10 165" size="400 100"
      style="PlainText"
      fg=""
      vq="cursqexp layoutargs"
      set="$VALUE=cursqexp+'/tmpCMDacq'"
      editable="yes"
      wrap="no"
      />
    <label loc="30 270" size="300 20"
      style="Label1"
      vq="layoutargs"
      label="[MAGICAL macro format!!]"
      />
  </group>
</template>
