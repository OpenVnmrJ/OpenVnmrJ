<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Prefs3D" element="groups" type="sample" >
  <group size="400 470"
    bg=""
    border="None"
    tab="no"
    enable="no"
    reference="Prefs3D"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 405" size="380 55"
      style="Heading1"
      label="3D Data Memory Mapping"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Transparency"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="13 20" size="73 24"
        style="Label1"
        label="Enable"
        vq="volmapdir g3ds"
        vc="java('g3ds',4,0,4)"
        vc2="java('g3ds',4,4,4)"
        set="if((trunc(g3ds[4]/4) mod 2)=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="87 20" size="64 24"
        style="Label1"
        label="Directory"
        vq="volmapdir"
        show="exists(volmapdir,'directory'):$e  if($e&gt;0) then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="153 21" size="214 24"
        style="PlainText"
        vq="volmapdir g3ds"
        vc="volmapdir='$VALUE'"
        set="$VALUE=volmapdir"
        show="exists('volmapdir','parameter','global'):$e if($e&gt;0)  then $VALUE=1 else $VALUE=0 endif"
        tooltip="Memory map directory path (default=~/vnmrsys)"
        />
    </group>
    <group loc="5 350" size="380 55"
      style="Heading1"
      label="3D Pseudoslices"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Transparency"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <slider loc="123 20" size="250 24"
        style="Heading1"
        vq="g3dgi"
        vc="vnmrjcmd('g3dgi',1,$VALUE)"
        set="$VALUE=g3dgi[1]"
        digital="true"
        min="10"
        max="2000"
        incr1="10"
        digits="5"
        drag="50"
        />
      <button loc="20 22" size="56 24"
        style="Heading2"
        label="Default"
        vc="vnmrjcmd('g3dgi',1,500)"
        bg=""
        decor1=""
        />
    </group>
    <group loc="5 255" size="380 95"
      style="Heading1"
      label="3D Opacity"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Transparency"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="6 20" size="110 20"
        style="Heading1"
        label="Selected Plane"
        />
      <label loc="6 40" size="107 24"
        style="Label1"
        label="Unselected Planes"
        />
      <slider loc="123 18" size="250 24"
        style="PlainText"
        vq="g3dgf"
        vc="vnmrjcmd('g3dgf',1,$VALUE/100.0)"
        set="$VALUE=g3dgf[1]*100"
        digital="true"
        min="0"
        max="100"
        digits="5"
        drag="50"
        />
      <slider loc="123 42" size="250 24"
        style="PlainText"
        vq="g3dgf"
        vc="vnmrjcmd('g3dgf',2,$VALUE/100.0)"
        set="$VALUE=g3dgf[2]*100"
        digital="true"
        min="0"
        max="100"
        digits="5"
        drag="50"
        />
      <slider loc="123 66" size="250 24"
        style="PlainText"
        vq="g3df"
        vc="vnmrjcmd('g3df',5,$VALUE/100.0)"
        set="$VALUE=g3df[5]*100"
        digital="true"
        min="0"
        max="100"
        digits="5"
        drag="50"
        />
      <label loc="6 62" size="68 24"
        style="Label1"
        label="3D Object"
        />
    </group>
    <group loc="5 160" size="380 95"
      style="Heading1"
      label="3D Slice Plane"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Locks"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="9 20" size="322 24"
        style="Label1"
        label="Faces viewer on selection"
        vq="g3ds"
        vc="java('g3ds',3,8,8)"
        vc2="java('g3ds',3,0,8)"
        set="if((trunc(g3ds[3]/8) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        tooltip="Set the front of the sliceplane in the 3D window to be visible to the viewer when a new extraction axis is selected"
        />
      <check loc="9 39" size="175 26"
        style="Label1"
        label="Tracks 2D extraction plane"
        vq="g3ds"
        vc="java('g3ds',2,1,1)"
        vc2="java('g3ds',2,0,1)"
        set="if((trunc(g3ds[2]) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        tooltip="Set the 3D sliceplane to track the currently selected extraction axis"
        />
      <check loc="9 58" size="325 26"
        style="Label1"
        label="Selected from active control (panel, slider,3D tool)"
        vq="g3ds"
        vc="java('g3ds',3,16,16)"
        vc2="java('g3ds',3,0,16)"
        set="if((trunc(g3ds[3]/16) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        tooltip="Automatically set the extraction axis from the last control used"
        />
    </group>
    <group loc="5 100" size="380 60"
      style="Heading1"
      label="3D Window Components"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Locks"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <toggle loc="10 25" size="60 25"
        style="Heading2"
        label="Object"
        vq="g3dp"
        vc="java('g3dp',3,2,2)"
        vc2="java('g3dp',3,0,2)"
        set="if((trunc(g3dp[3]/2) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        enable="yes"
        radiobutton="no"
        subtype="yes"
        tooltip="Show Volume Data"
        />
      <toggle loc="70 25" size="60 25"
        style="Heading2"
        label="Tools"
        vq="g3dp"
        vc="java('g3dp',3,1,1)"
        vc2="java('g3dp',3,0,1)"
        set="if((trunc(g3dp[3]) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        enable="yes"
        radiobutton="no"
        subtype="yes"
        tooltip="Show 3D Tools"
        />
      <toggle loc="130 25" size="60 25"
        style="Heading2"
        label="Info"
        vq="g3dp"
        vc="java('g3dp',2,2,2)"
        vc2="java('g3dp',2,0,2)"
        set="if((trunc(g3dp[2]/2) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        enable="yes"
        radiobutton="no"
        subtype="yes"
        tooltip="Show Information text in 3D window"
        />
      <toggle loc="190 25" size="60 25"
        style="Heading2"
        label="Box"
        vq="g3dp"
        vc="java('g3dp',3,16,16)"
        vc2="java('g3dp',3,0,16)"
        set="if((trunc(g3dp[3]/16) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        enable="yes"
        radiobutton="no"
        subtype="yes"
        tooltip="Show Data Bounds Box"
        />
      <toggle loc="250 25" size="60 25"
        style="Heading2"
        label="Labels"
        vq="g3dp"
        vc="java('g3dp',2,1,1)"
        vc2="java('g3dp',2,0,1)"
        set="if((trunc(g3dp[2]) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        enable="yes"
        radiobutton="no"
        subtype="yes"
        tooltip="Show Box Labels"
        />
      <toggle loc="310 25" size="60 25"
        style="Heading2"
        label="RotAxis"
        vq="g3dp"
        vc="java('g3dp',3,64,64)"
        vc2="java('g3dp',3,0,64)"
        set="if((trunc(g3dp[3]/64) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        enable="yes"
        radiobutton="no"
        subtype="yes"
        tooltip="Show View Rotation Axis in 3D window"
        />
    </group>
    <group loc="5 5" size="380 95"
      style="Heading1"
      label="Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Locks"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 60" size="305 25"
        style="Label1"
        label="Auto-Reload on changed data type"
        vq="g3ds"
        vc="java('g3ds',3,2,2)"
        vc2="java('g3ds',3,0,2)"
        set="if((trunc(g3ds[3]/2) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        tooltip="Reload when data type changes"
        />
      <check loc="5 40" size="305 25"
        style="Label1"
        label="Pace display refresh"
        vq="g3ds"
        vc="java('g3ds',4,2,2)"
        vc2="java('g3ds',4,0,2)"
        set="if((trunc(g3ds[4]/2) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        tooltip="sychronize position sliders with ortho view display cursors"
        />
      <check loc="5 20" size="305 25"
        style="Label1"
        label="Reset view after loading data"
        vq="g3ds"
        vc="java('g3ds',3,4,4)"
        vc2="java('g3ds',3,0,4)"
        set="if((trunc(g3ds[3]/4) mod 2)=1) then $VALUE=1 else $VALUE=0 endif"
        tooltip="Set to default view after a 3D data file is loaded "
        />
    </group>
  </group>
</template>
