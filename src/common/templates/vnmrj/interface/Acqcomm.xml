<template name="Acqcomm" element="pages" type="acquisition" >
  <group loc="0 0" size="325 70"
     vq="aCqCoMm"
     show="$SHOW=(aCqCoMm='on')"
    border="None"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
        <button loc="50 20" size="205 30"
          style="Heading1"
	  bg="red"
          label="Stop Communication"
          vc="acqcomm('stop')"
          />
  </group>
  <group loc="0 0" size="325 70"
     vq="aCqCoMm"
     show="$SHOW=(aCqCoMm='off')"
    border="None"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
        <button loc="50 5" size="205 30"
          style="Heading1"
      	  bg="green"
          label="Start Communication"
          vc="acqcomm('start')"
          />
        <button loc="50 40" size="205 30"
          style="Heading1"
      	  bg="green"
          label="Start Communication and su"
          vc="acqcomm('startsu')"
          />
  </group>
</template>
