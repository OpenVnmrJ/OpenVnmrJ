<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="0 0" size="480 130"
    border="None"
    tab="no"
    enable="no"
    reference="pageview"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <label loc="10 35" size="65 20"
      style="Label1"
      label="Send to:"
      />
    <entry loc="190 75" size="245 20"
      style="PlainText"
      vq="pdfplotpar email"
      vc="pdfplotpar[2]='$VALUE'"
      set="$VALUE=pdfplotpar[2]"
      show="$ENABLE=((emailflag='y') and (pdfplotpar[6]&lt;&gt;''))*2-1"
      />
    <textmessage loc="190 55" size="245 20"
      style="Info"
      vq="pdfplotpar"
      set="if (pdfplotpar[6]='') then $VALUE='' else $VALUE='in:  '+pdfplotpar[7] endif"
      />
    <textmessage loc="190 35" size="245 20"
      style="Info"
      vq="plotter maxpen pdfplotpar"
      set="if maxpen&gt;1 then $type='color' else $type='B-W' endif $VALUE=plotter+' ('+$type+')'"
      />
    <entry loc="95 10" size="340 20"
      style="PlainText"
      vq="pdfplotpar"
      vc="ckstring('$VALUE',2):pdfplotpar[1]"
      set="$VALUE=pdfplotpar[1]"
      show="$ENABLE=(pdfplotpar[6]&lt;&gt;'')*2-1"
      />
    <label loc="440 10" size="30 20"
      style="Label1"
      label=".pdf"
      vq="pdfplotpar"
      />
    <group loc="90 100" size="320 20"
      vq="pdfplotpar"
      show="$SHOW=(pdfplotpar[9]&lt;&gt;'pdf')"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="25 0" size="290 20"
        style="PlainText"
        label="Save/email a second copy in &quot;pdf&quot; format"
        vq="pdfplotpar"
        set="$VALUE='Save/email a second copy in &quot;'+pdfplotpar[9]+'&quot; format'"
        />
      <check loc="5 0" size="20 20"
        style="Label1"
        vq="pdfplotpar"
        vc="pdfplotpar[10]='y'"
        vc2="pdfplotpar[10]='n'"
	show="$ENABLE=(pdfplotpar[4]='save')*2-1"
        set="$VALUE=((pdfplotpar[10]='y') and (pdfplotpar[4]='save'))"
        />
    </group>
    <check loc="95 75" size="95 20"
      style="Label1"
      label="email"
      vq="email pdfplotpar"
      vc="pdfplotpar[5]='email' pdfplotpar[4]='save'"
      vc2="pdfplotpar[5]=''"
      set="$VALUE=(pdfplotpar[5]='email')"
      show="$ENABLE=((emailflag='y') and (pdfplotpar[6]&lt;&gt;''))*2-1"
      />
    <check loc="95 55" size="95 20"
      style="Label1"
      label="File"
      vq="pdfplotpar"
      vc="pdfplotpar[4]='save'"
      vc2="pdfplotpar[4]='' pdfplotpar[5]=''"
      set="$VALUE=(pdfplotpar[4]='save')"
      show="$ENABLE=(pdfplotpar[6]&lt;&gt;'')*2-1"
      />
    <check loc="95 35" size="95 20"
      style="Label1"
      label="Plotter"
      vq="pdfplotpar"
      vc="pdfplotpar[3]='plot'"
      vc2="pdfplotpar[3]=''"
      set="$VALUE=(pdfplotpar[3]='plot')"
      />
    <label loc="10 10" size="75 20"
      style="Label1"
      label="Plot name"
      vq="pdfplotpar"
      />
  </group>
</template>
