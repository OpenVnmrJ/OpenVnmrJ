<mainmenu>
     <submenu  label="Management" hotkey="s"
	    key="Management"
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	    >
	<wchoice label = "Users"
	    key="users"
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	    path1="SYSTEM/USRS/userlist"
	    path2="GRPS/grouplist"
	/>
	<wchoice label="Printers..."
	    key="printers"
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Cost/Time Accounting..."
	    key="accounting"
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="Console Display Sharing..."
	    key="shared console"
	    font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
        <wchoice label="Export Log to CSV..."
            key="export log to csv"
            font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Exit"
		key="exit"
		font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
   </submenu>

   <submenu  label = "UNIX" hotkey = "a"
	  key="unix"
	  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	  > 
	<wchoice label = "File System..."
              key="file system"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Command Window..."
     	      key="command window"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
   </submenu>

   <submenu  label = "Configure" hotkey = "p"
	key="configure" 
	font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	> 
	<submenu  label = "Users" hotkey = "p"
	      key="users" 
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	>
	      <wchoice label="Convert users..."
		  key="'convert users' 'help:/vnmr/jhelp/dialog/convertusers.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	       />
	      <wchoice label="Defaults..."
		  key="'user defaults' 'help:/vnmr/jhelp/dialog/defaultusers.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	       />
	      <wchoice label="Update users..."
		  key="'update users' 'help:/vnmr/jhelp/dialog/updateusers.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	      />
	</submenu>
	<submenu label="Operators"
		  key="operators"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	>
	      <wchoice label="Edit operators..."
		  key="'add operators' 'help:/vnmr/jhelp/dialog/operators.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	      />
	      <wchoice label="Delete operators..."
		  key="'delete operators' 'help:/vnmr/jhelp/dialog/operators.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	      />
	      <wchoice label="Reset password..."
		  key="'reset password operators' 'help:/vnmr/jhelp/dialog/resetpassword.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	      />	
	      <wchoice label="Preferences..."
		  key="'preferences operators' 'help:/vnmr/jhelp/dialog/operatorspref.html'"
		  font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	      />	
	</submenu>
	<wchoice label="Edit Profiles..."
		 key="'profile' 'help:/vnmr/jhelp/dialog/profile.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="Investigator List..."
		 key="'investi' 'help:/vnmr/jhelp/dialog/investigators.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="Locator Config..."
		 key="'locator' 'help:/vnmr/jhelp/dialog/locator.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label="DICOM Setup..."
		 key="'dicom' 'help:/vnmr/jhelp/dialog/dicomstorage.html'"
		 font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Background colors..."
	      key="fonts colors"
	      font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
        />
   </submenu>

   <submenu  label = "Help" hotkey = "o"
	key="help" 
        font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
   > 
	<wchoice label = "Manuals..."
		key="'html help' 'vnmr_open /vnmr/help/WebHelp/index.htm'"
		font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "Spinsights Community Help Site..."
		key="'agilent home help' 'vnmr_open https://spinsights.chem.agilent.com'"
		font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
	<wchoice label = "About VnmrJ..."
		key="'about help' 'help:About.xml'"
		font="Menu1" style="Menu1" point="Menu1" fg="Menu1"
	/>
   </submenu>

</mainmenu>

