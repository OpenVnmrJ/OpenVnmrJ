<template element="panels" type="acquisition" >
  <port size="506 298" style="Bold" point="8"
    fg="black"
    >
    <folder size="560 200" side="Top"
    >
      <wgroup label="Panel Colors" column="1">
    	<wgroup  label="Labels"
       	  border="None" side="Top" justify="Left" tab="no"
          useref="no"
        >
          <wlabel size="100 30" font="Heading2"
            style="Heading2"
            point="Heading2"
            fg="Heading2"
            label="Background"
            justify="Left"
          />
          <wcolorchooser set="WandaBgColor"
       	    savekids="false"
      	    key="WandaBgColor"
    	    keyval="gray"
    	    display="System"
    	  />
       </wgroup>
       <wgroup  label="Labels"
          border="None" side="Top" justify="Left" tab="no"
          useref="no"
        >
          <wlabel size="100 30" font="Heading2"
            style="Heading2"
            point="Heading2"
            fg="Heading2"
            label="User List Panel"
            justify="Left"
          />
          <wcolorchooser set="Item_Area1BgColor"
            savekids="false"
            key="Item_Area1BgColor"
            keyval="gray"
            display="System"
         />
       </wgroup>
       <wgroup  label="Labels"
         border="None" side="Top" justify="Left" tab="no"
         useref="no"
       >
         <wlabel size="100 30" font="Heading2"
           style="Heading2"
           point="Heading2"
           fg="Heading2"
           label="Data Directories Panel"
           justify="Left"
         />
         <wcolorchooser set="Item_Area2BgColor" 
           savekids="false"
    	   key="Item_Area2BgColor"
    	   keyval="gray"
    	   display="System"
    	/>
      </wgroup>
      <wgroup label="Labels"
        border="None" side="Top" justify="Left" tab="no"
        useref="no"
       >
        <wlabel size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
          fg="Heading2"
          label="User Detail Panel"
          justify="Left"
        />
        <wcolorchooser set="Detail_Area1BgColor" 
    	  savekids="false"
    	  key="Detail_Area1BgColor"
    	  keyval="gray"
    	  display="System"
    	/>
      </wgroup>
      <wgroup label="Labels"
       	border="None" side="Top" justify="Left" tab="no"
       	useref="no"
      >
        <wlabel size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
          fg="Heading2"
          label="Disk Space Panel"
          justify="Left"
        />
        <wcolorchooser set="Detail_Area2BgColor"
     	  savekids="false"
    	  key="Detail_Area2BgColor"
    	  keyval="gray"
    	  display="System"
    	/>
      </wgroup>
    </wgroup>
   
    <wgroup label="Messages" column="1">
      <wgroup  label="Labels"
        border="None" side="Top" justify="Left" tab="no"
        useref="no"
      >
        <wlabel  size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
          fg="Heading2"
          label="On (Active)"
          justify="Left"
        />
        <wcolorchooser set="OnColor"
    	  savekids="false"
    	  key="OnColor"
    	  keyval="cyan"
    	  display="System"
    	/>
      </wgroup>
      <wgroup  label="Labels"
        border="None" side="Top" justify="Left" tab="no"
        useref="no"
      >
        <wlabel size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
      	  fg="Heading2"
          label="Off (Inactive)"
          justify="Left"
        />
        <wcolorchooser set="OffColor" 
    	  savekids="false"
    	  key="OffColor"
    	  keyval="yellow"
    	  display="System"
    	/>
      </wgroup>
      <wgroup label="Labels"
        border="None" side="Top" justify="Left" tab="no"
        useref="no"
      >
        <wlabel size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
          fg="Heading2"
          label="Interactive"
	  justify="Left"
        />
        <wcolorchooser set="InteractiveColor" 
    	  savekids="false"
    	  key="InteractiveColor"
    	  keyval="choclate"
    	  display="System"
    	/>
      </wgroup>
      <wgroup label="Labels"
        border="None" side="Top" justify="Left" tab="no"
        useref="no"
      >
        <wlabel size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
          fg="Heading2"
          label="Ready"
	  justify="Left"
        />
        <wcolorchooser set="ReadyColor"
    	  savekids="false"
    	  key="ReadyColor"
    	  keyval="seaGreen"
    	  display="System"
    	/>
       </wgroup>
       <wgroup label="Labels"
         border="None" side="Top" justify="Left" tab="no"
         useref="no"
       >
        <wlabel size="100 30" font="Heading2"
          style="Heading2"
          point="Heading2"
          fg="Heading2"
          label="Error"
	  justify="Left"
        />
        <wcolorchooser set="ErrorColor" 
    	  savekids="false"
    	  key="ErrorColor"
    	  keyval="snow"
    	  display="System"
    	/>
      </wgroup>
      <wgroup label="Labels"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        useref="no"
      >
        <wlabel size="100 30" font="Heading1"
     	  style="Heading1"
      	  point="Heading1"
      	  fg="Heading1"
      	  label="Warning"
      	  justify="Left"
        />
        <wcolorchooser set="WarningColor" 
    	  savekids="false"
    	  key="WarningColor"
    	  keyval="yellow"
    	  display="System"
    	/>
      </wgroup>
      <wgroup  label="Labels"
        border="None" side="Top" justify="Left" tab="no"
        useref="no"
        keyword="dataacq"
      >
        <wlabel size="100 30" font="Heading1"
          style="Heading1"
          point="Heading1"
          fg="Heading1"
          label="Info"
          justify="Left"
        />
        <wcolorchooser set="InfoColor" 
    	  savekids="false"
    	  key="InfoColor"
    	  keyval="gray"
    	  display="System"
    	/>
      </wgroup>
    </wgroup>
   </folder>
  </port>
</template>
