<template name="helpoverlay" >
  <button
   name="close"
   size="32 32" 
   icon="HO_close.png"
   vc="vnmrjcmd('toolpanel', 'HelpOverlay', 'close')"
  />
  <button
   name="navigator"
   size="32 32"
   icon="HO_forward.png"
   vq="traymax"
   vc="if (traymax=12 or traymax=9) then vnmrjcmd('helpoverlay', 'file:OverlayPanel7510.xml') elseif (traymax>49) then vnmrjcmd('helpoverlay', 'file:OverlayPanel7620.xml') else vnmrjcmd('helpoverlay', 'file:OverlayPanelNoSC.xml') endif"
   />
<group 
    layout="grid"
    columns="42"
    rows="72"
  >
  <grid
    grid-column="1"
    grid-row="1"
    grid-column-span="42"
    grid-row-span="72"
    >
    <label
        style="Heading1"
        icon="HO_Page1.png"
        border="None"
        stretch="none"
    />
  </grid>

</group>
</template>

