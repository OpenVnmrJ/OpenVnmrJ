"macro res"
"*******************************************************************"
"plres - Plot the line widths at half-height, 0.55, and 0.11 levels "
"*******************************************************************"

peak(sp,wp+sp):$ht,$cr
cr=$cr
ds
dres(cr,.50):$lw
write('plotter',98,wc2max-20,'50%% linewidth %6.2f',$lw)
dres(cr,.0055):$lw
write('plotter',95,wc2max-30,'.55%% linewidth %6.2f',$lw)
dres(cr,.0011):$lw
write('plotter',95,wc2max-40,'.11%% linewidth %6.2f',$lw)
