" macro initComm"
fread('','usertree')
create('ovjIP','string','usertree',''):$e
create('ovjPort','string','usertree',''):$e
serverport:$ovjPort
setvalue('ovjIP',instrument,'usertree')
setvalue('ovjPort',$ovjPort,'usertree')
fsave(userdir+'/ovjnet','usertree')
rm('-f',userdir+'/persistence/ovjStarting')

