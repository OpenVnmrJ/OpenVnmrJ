         append($cpnodes2,'awk','$1',$tmpfile):$dum
         append($xml2,$studyxml):$dum
         append($cpnodes2,$esq):$dum
    endif
     $ts1='' $ts2='' $nodes=0
     readfile($tmpfile,'$ts1','$ts2','','local'):$nodes
     if ($nodes) then
        $ti=1
