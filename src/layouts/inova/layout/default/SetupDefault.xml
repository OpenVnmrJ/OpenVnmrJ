          vq="temp"
          vc="setvalue('temp',$VALUE) temp='y'"
          set="$VALUE=temp"
          digital="true"
          incr1="1"
          incr2="5"
          vq2="temp"
          digits="5"
          drag="0"
          />
        <button loc="210 70" size="80 20"
          style="PlainText"
          label="Temp Off"
          vc="sethw('vt','off') temp='n' vnmrinfo('set','tempOnOff',0)"
          decor1="no"
          />
        <textmessage loc="60 70" size="60 20"
