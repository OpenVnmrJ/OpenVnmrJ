          style="Heading1"
          label="Control spinner from this panel only"
          vc="vnmrinfo('set','spinExpControl',0) in=in"
          vc2="vnmrinfo('set','spinExpControl',1) in=in"
          set="vnmrinfo('get','spinExpControl'):$e $VALUE=1-$e"
          />
        <radio loc="40 71" size="210 20"
          style="PlainText"
          label="Ignore spinner error"
          vq="in"
          vc="substr(in,2,1,'n'):in vnmrinfo('set','spinErrorControl',0)"
          set="vnmrinfo('get','spinExpControl'):$cl if $cl=0 then vnmrinfo('get','spinErrorControl'):$vn $VALUE=($vn=0) else substr(in,2,1):$s $VALUE=($s='n') endif"
          />
        <radio loc="40 30" size="210 20"
          style="PlainText"
          label="Abort after spinner error"
          vq="in"
          vc="substr(in,2,1,'y'):in vnmrinfo('set','spinErrorControl',1)"
          set="vnmrinfo('get','spinExpControl'):$cl if $cl=0 then vnmrinfo('get','spinErrorControl'):$vn $VALUE=($vn=1) else substr(in,2,1):$s $VALUE=($s='y') endif"
          show="$SHOW=(spintype='mas')"
          />
        <radio loc="40 49" size="210 20"
          style="PlainText"
          label="Warn after spinner error"
          vq="in"
          vc="substr(in,2,1,'w'):in vnmrinfo('set','spinErrorControl',2)"
          set="vnmrinfo('get','spinExpControl'):$cl if $cl=0 then vnmrinfo('get','spinErrorControl'):$vn $VALUE=($vn=2) else substr(in,2,1):$s $VALUE=($s='w') endif"
          />
      </group>
    </group>
  </group>
