<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="685 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 25" size="220 120"
      style="Heading1"
      label="Hadamard Encoding"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HadamardEncoding"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="10 85" size="190 20"
        style="Label1"
        label="Bloch-Seigert Correction"
        vq="bscor"
        vc="bscor='y' "
        vc2="bscor='n'"
        set="if( bscor = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="175 40" size="30 20"
        style="Label1"
        label="Hz"
        />
      <entry loc="135 20" size="70 20"
        style="PlainText"
        vq="pshape ptype"
        vc="pshape='$VALUE'"
        set="$VALUE=pshape"
        />
      <entry loc="135 40" size="40 20"
        style="PlainText"
        vq="htbw1"
        vc="htbw1=$VALUE"
        set="$VALUE=htbw1"
        digits="0"
        />
      <entry loc="135 60" size="40 20"
        style="PlainText"
        vq="htofs1"
        vc="htofs1=$VALUE"
        set="$VALUE=htofs1"
        />
      <label loc="10 60" size="120 20"
        style="Label1"
        label="Skip First increments"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Bandwidth"
        />
      <label loc="10 20" size="110 20"
        style="Label1"
        label="HT Waveform"
        />
    </group>
    <group loc="445 125" size="220 90"
      style="Heading1"
      label="C13 Pulses"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HTDecoupling"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="95 20" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE "
        set="$VALUE=pw"
        digits="1"
        />
      <label loc="155 40" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="145 60" size="60 20"
        style="PlainText"
        vq="compC"
        vc="compC=$VALUE "
        set="$VALUE=compC"
        digits="4"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE "
        set="$VALUE=tpwr"
        digits="0"
        />
      <label loc="5 60" size="140 20"
        style="Label1"
        label="Amplifier compression"
        />
      <label loc="5 20" size="90 20"
        style="Label1"
        label="Pulse width"
        />
      <label loc="5 40" size="70 20"
        style="Label1"
        label="Power"
        />
      <label loc="155 20" size="30 20"
        style="Label1"
        label="us"
        justify="Left"
        />
    </group>
    <group loc="225 125" size="220 90"
      style="Heading1"
      label="H1 Pulses"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HTDecoupling"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="155 40" size="30 20"
        style="Heading1"
        label="dB"
        justify="Left"
        />
      <entry loc="145 60" size="60 20"
        style="PlainText"
        vq="compH"
        vc="compH=$VALUE "
        set="$VALUE=compH"
        digits="4"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="pplvl"
        vc="pplvl=$VALUE "
        set="$VALUE=pplvl"
        digits="0"
        />
      <label loc="5 60" size="140 20"
        style="Label1"
        label="Amplifier compression"
        />
      <label loc="5 20" size="90 20"
        style="Label1"
        label="Pulse width"
        />
      <entry loc="95 20" size="60 20"
        style="PlainText"
        vq="pp"
        vc="pp=$VALUE "
        set="$VALUE=pp"
        digits="1"
        />
      <label loc="5 40" size="70 20"
        style="Label1"
        label="Power"
        />
      <label loc="155 20" size="30 20"
        style="Label1"
        label="us"
        justify="Left"
        />
    </group>
    <group loc="445 25" size="220 100"
      style="Heading1"
      label="Gradients"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="25 70" size="60 20"
        style="Label1"
        label="sspul"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$s='' substr(sspul,1,1):$s $VALUE=($s='y')"
        />
      <label loc="140 20" size="60 20"
        style="Label1"
        label="Duration"
        />
      <label loc="50 20" size="40 20"
        style="Label1"
        label="Level"
        />
      <entry loc="50 40" size="60 20"
        style="PlainText"
        vq="gzlvl0"
        vc="gzlvl0=$VALUE"
        set="$VALUE=gzlvl0"
        />
      <entry loc="140 40" size="50 20"
        style="PlainText"
        vq="gt0"
        vc="gt0=$VALUE/1000"
        set="$VALUE=gt0*1000"
        digits="3"
        />
      <label loc="190 40" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="115 40" size="25 20"
        style="Label1"
        label="gt0"
        />
      <label loc="5 40" size="40 20"
        style="Label1"
        label="gzlvl0"
        />
    </group>
    <group loc="225 25" size="220 100"
      style="Heading1"
      label="J-couplings"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HTDecoupling"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="85 60" size="20 20"
        style="Label1"
        vq="refoc"
        vc="refoc='y'"
        vc2="refoc='n' dm='nnn'"
        set="$VALUE=(refoc='y')"
        />
      <label loc="15 60" size="60 20"
        style="Label1"
        label="Refocus"
        />
      <label loc="15 40" size="70 20"
        style="Label1"
        label="Multiplicity"
        />
      <label loc="15 20" size="40 20"
        style="Label1"
        label="1J(CH)"
        />
      <selmenu loc="85 40" size="60 20"
        style="PlainText"
        vq="mult"
        vc="mult=$VALUE"
        set="$VALUE=mult"
        editable="No"
        >
        <mlabel 
          label="all"
          chval="0"
          />
        <mlabel 
          label="CH"
          chval="1"
          />
        <mlabel 
          label="CH2"
          chval="2"
          />
        <mlabel 
          label="CH3"
          chval="3"
          />
      </selmenu>
      <entry loc="85 20" size="55 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE "
        set="$VALUE=j1xh"
        digits="1"
        />
      <label loc="140 20" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
    </group>
    <group loc="5 145" size="220 70"
      style="Heading1"
      label="HT Decoupling"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HTDecoupling"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <selmenu loc="50 20" size="60 20"
        style="Heading1"
        vq="Cdseq"
        vc="Cdseq='$VALUE' setHTdec1"
        set="$VALUE=Cdseq"
        editable="No"
        >
        <mlabel 
          label="garp"
          chval="garp1"
          />
        <mlabel 
          label="wurst"
          chval="wu200"
          />
      </selmenu>
      <selmenu loc="50 40" size="60 20"
        style="PlainText"
        vq="dmm"
        vc="dmm='$VALUE' setHTdec"
        set="$VALUE=dmm"
        editable="No"
        >
        <mlabel 
          label="off"
          chval="ccc"
          />
        <mlabel 
          label="waltz"
          chval="ccw"
          />
        <mlabel 
          label="garp"
          chval="ccg"
          />
        <mlabel 
          label="wurst"
          chval="ccp"
          />
      </selmenu>
      <label loc="185 40" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="185 20" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="155 40" size="30 20"
        style="PlainText"
        vq="dpwr dmm"
        vc="dpwr=$VALUE setHTdec"
        set="$VALUE=dpwr"
        show="$ENABLE=(dmm&lt;&gt;'ccp')"
        />
      <entry loc="155 20" size="30 20"
        style="PlainText"
        vq="Cdpwr Cdseq"
        vc="Cdpwr=$VALUE setHTdec1"
        set="$VALUE=Cdpwr"
        show="$ENABLE=(Cdseq='garp1')"
        digits="0"
        />
      <label loc="110 40" size="45 20"
        style="Label1"
        label="power:"
        />
      <label loc="110 20" size="45 20"
        style="Label1"
        label="power:"
        />
      <label loc="10 40" size="40 20"
        style="Label1"
        label="H-1:"
        />
      <label loc="10 20" size="40 20"
        style="Label1"
        label="C-13:"
        />
    </group>
    <entry loc="305 0" size="40 20"
      style="PlainText"
      vq="r7"
      vc="r7=$VALUE importHT(r7)"
      set="$VALUE=r7"
      />
    <button loc="105 0" size="200 20"
      style="Heading2"
      label="Import HT Frequencies from exp"
      vc="importHT(r7)"
      bg=""
      decor1=""
      />
    <button loc="355 0" size="140 20"
      style="Heading2"
      label="Edit HT Frequencies"
      vc="editht('show')"
      bg=""
      decor1=""
      />
    <button loc="505 0" size="150 20"
      style="Heading2"
      label="Display Pulse Sequence"
      vc="dps"
      bg=""
      decor1=""
      />
    <group loc="10 0" size="85 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
    </group>
    <textmessage loc="10 0" size="95 20"
      style="PlainText"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
