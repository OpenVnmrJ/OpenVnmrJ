<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="705 245"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="400 135" size="270 65"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="NumberofpointsinF2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="155 25" size="80 20"
        style="PlainText"
        vq="fn"
        vc="fn=$VALUE "
        set="on('fn'):$on if $on=0 then $VALUE='n' else $VALUE=fn endif"
        editable="No"
        >
        <mlabel 
          label="not used"
          chval="n"
          />
        <mlabel 
          label="1k"
          chval="1024"
          />
        <mlabel 
          label="2k"
          chval="2048"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
        <mlabel 
          label="32k"
          chval="32768"
          />
      </menu>
      <menu loc="155 5" size="80 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE "
        set="$VALUE=np"
        editable="Yes"
        >
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1k"
          chval="1024"
          />
        <mlabel 
          label="2k"
          chval="2048"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
      </menu>
      <textmessage loc="10 45" size="115 20"
        style="PlainText"
        vq="np"
        set="$VALUE='' if (np&gt;16384) then $VALUE='Warning: np &gt; 16k' endif"
        show="if (np&gt;16384) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <textmessage loc="135 45" size="115 20"
        style="PlainText"
        vq="fn"
        set="$VALUE='' if (fn&gt;64k) then $VALUE='Warning: fn &gt; 64k' endif"
        show="if (fn&gt;64k) then $ENABLE=1 else $ENABLE=0 endif"
        />
      <label loc="10 5" size="140 20"
        style="Label1"
        label="Number of points in F2"
        justify="Left"
        />
      <label loc="10 25" size="130 20"
        style="Label1"
        label="Fourier Number in F2"
        justify="Left"
        />
    </group>
    <group loc="400 0" size="270 135"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <entry loc="155 110" size="60 20"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        />
      <label loc="10 110" size="140 20"
        style="Label1"
        label="Number of increments"
        justify="Left"
        />
      <menu loc="155 90" size="60 20"
        style="PlainText"
        label="menu"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        editable="No"
        >
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="4"
          chval="4"
          />
        <mlabel 
          label="8"
          chval="8"
          />
        <mlabel 
          label="16"
          chval="16"
          />
        <mlabel 
          label="32"
          chval="32"
          />
      </menu>
      <label loc="10 90" size="140 20"
        style="Label1"
        label="Steady state scans"
        justify="Left"
        />
      <menu loc="155 70" size="60 20"
        style="PlainText"
        label="menu"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        editable="No"
        >
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="4"
          chval="4"
          />
        <mlabel 
          label="8"
          chval="8"
          />
        <mlabel 
          label="16"
          chval="16"
          />
        <mlabel 
          label="32"
          chval="32"
          />
      </menu>
      <label loc="10 70" size="140 20"
        style="Label1"
        label="Scans per increment"
        justify="Left"
        />
      <label loc="215 50" size="30 20"
        style="Label1"
        label="sec"
        justify="Left"
        />
      <entry loc="155 50" size="60 20"
        style="PlainText"
        vq="d1"
        vc="d1=$VALUE"
        set="$VALUE=d1"
        />
      <label loc="10 50" size="140 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
      <label loc="10 25" size="140 20"
        style="Label1"
        label="C13 pulse width"
        justify="Left"
        />
      <label loc="215 25" size="20 20"
        style="Label1"
        label="us"
        justify="Left"
        />
      <entry loc="155 25" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE "
        set="$VALUE=pw"
        digits="2"
        />
      <entry loc="155 5" size="60 20"
        style="PlainText"
        vq="pp"
        vc="pp=$VALUE "
        set="$VALUE=pp"
        digits="2"
        />
      <label loc="215 5" size="20 20"
        style="Label1"
        label="us"
        justify="Left"
        />
      <label loc="10 5" size="140 20"
        style="Label1"
        label="H1 pulse width"
        justify="Left"
        />
    </group>
    <group loc="0 0" size="380 200"
      style="Heading1"
      label="hetcorHT"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HadamardEncodingOptions"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <group loc="20 150" size="345 40"
        vq="file"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <button loc="180 10" size="160 25"
          style="Heading2"
          label="Edit HT frequencies"
          vc="editht('show')"
          bg=""
          decor1=""
          />
        <button loc="0 10" size="160 25"
          style="Heading2"
          label="Local HT frequencies"
          vc="localHTfreqs('H1')"
	  show="localHTfreqs('H1'):$ENABLE"
          bg=""
          decor1=""
          />
      </group>
      <label loc="60 25" size="265 25"
        style="Label1"
        label="Hadamard Frequency List Options"
        justify="Left"
        />
      <label loc="245 125" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="185 125" size="60 20"
        style="PlainText"
        vq="htbw1"
        vc="htbw1=$VALUE"
        set="$VALUE=htbw1"
        />
      <label loc="90 125" size="95 20"
        style="Label1"
        label="HT bandwidth"
        justify="Left"
        />
      <textmessage loc="120 95" size="115 20"
        style="PlainText"
        vq="htfrq1"
        set="$s='' $x=size('htfrq1') if($x&gt;1) then  format($x,1,0):$s $VALUE=$s+' lines found' else $VALUE=' ' endif"
        show="$x=size('htfrq1') if($x&gt;1) then $ENABLE=1 else $ENABLE=0 endif $VALUE=$ENABLE"
        />
      <entry loc="265 60" size="45 25"
        style="PlainText"
        vq="r7"
        vc="r7=$VALUE importHT(r7)"
        set="$VALUE=r7"
        />
      <button loc="45 60" size="220 25"
        style="Heading2"
        label="Import HT frequencies from exp"
        vq="r7"
        vc="importHT(r7)"
        bg=""
        decor1=""
        />
    </group>
  </group>
</template>
