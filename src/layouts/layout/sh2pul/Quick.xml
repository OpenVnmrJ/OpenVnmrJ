<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="730 210"
    style="Heading1"
    label="Defaults"
    vq="tn"
    show="$SHOW=(tn&lt;&gt;'H1' and tn&lt;&gt;'C13' and tn&lt;&gt;'F19' and tn&lt;&gt;'P31')"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <parameter loc="70 160" size="180 20"
      style="Heading1"
      label="Spin"
      vq="spin"
      bg=""
      tab="no"
      vq2="spin"
      enabled="vnmrinfo('get','spinExpControl'):$ENABLE"
      checkvalue="$VALUE=0 on('spin'):$e if (spin&gt;0 and $e&gt;0.5) then $VALUE=1 endif"
      checkcmd="on('spin') if spin=0 then spin=20 endif"
      checkcmd2="spin=0"
      entryvalue="$VALUE=spin"
      entrycmd="spin=$VALUE"
      entrysize="70"
      unitsenabled="Label"
      unitssize="30"
      unitslabel="Hz"
      enable="no"
      reference="parameter"
      useref="no"
      disable="Grayed out"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="0 0" size="60 20"
        style="Heading1"
        label="Spin"
        vq="spin spin"
        subtype="parameter"
        />
      <entry loc="80 0" size="70 20"
        style="PlainText"
        vq="spin spin"
        vc="spin=$VALUE"
        set="$VALUE=spin"
        show="vnmrinfo('get','spinExpControl'):$ENABLE"
        disable="Grayed out"
        subtype="parameter"
        />
      <check loc="60 0" size="20 20"
        style="Label1"
        vq="spin spin"
        vc="on('spin') if spin=0 then spin=20 endif"
        vc2="spin=0"
        set="$VALUE=0 on('spin'):$e if (spin&gt;0 and $e&gt;0.5) then $VALUE=1 endif"
        show="vnmrinfo('get','spinExpControl'):$ENABLE"
        subtype="parameter"
        />
      <label loc="150 0" size="30 20"
        style="Label1"
        label="Hz"
        vq="spin spin"
        key="units"
        keyval="Hz"
        subtype="parameter"
        />
    </parameter>
    <group loc="10 180" size="240 20"
      vq="atune panellevel"
      show="$SHOW=(atune='y' and panellevel&gt;29)"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="50 0" size="90 20"
        style="Heading1"
        label="Tune method   "
        justify="Right"
        />
      <filemenu loc="140 0" size="100 20"
        style="PlainText"
        vq="tunemethod"
        vc="$v='$VALUE' if $v='none' then tunemethod='' else tunemethod=$v endif xmtune('filemenu')"
        set="$n='' $e=0 xmtune('methodname'):$n,$e if $e=1 then $VALUE=$n else $VALUE='none' endif"
        file="$VALUE=userdir+'/persistence/wtune'"
        type="file"
        bg=""
        editable="No"
        />
    </group>
    <group loc="480 30" size="240 40"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="120 20" size="120 20"
        style="PlainText"
        label="menu"
        vq="lb"
        vc="lb=$VALUE"
        set="on('lb'):$on if ($on=0) then $VALUE='n' elseif (lb=1/at) then $VALUE='1/at' else $VALUE=lb endif"
        editable="No"
        >
        <mlabel 
          label="None"
          chval="n"
          />
        <mlabel 
          label="1/at"
          chval="1/at"
          />
        <mlabel 
          label="0.1"
          chval="0.1"
          />
        <mlabel 
          label="0.2"
          chval="0.2"
          />
        <mlabel 
          label="0.5"
          chval="0.5"
          />
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="5"
          chval="5"
          />
        <mlabel 
          label="10"
          chval="10"
          />
      </menu>
      <menu loc="120 0" size="120 20"
        style="PlainText"
        label="menu"
        vq="fn"
        vc="format($VALUE,'isreal'):$r if $r then fn=$VALUE*2 else fn=$VALUE endif"
        set="on('fn'):$on if($on&gt;0.5) then $VALUE=fn/2 else $VALUE='n' endif"
        editable="No"
        >
        <mlabel 
          label="Not used"
          chval="n"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
        <mlabel 
          label="32k"
          chval="32768"
          />
        <mlabel 
          label="64k"
          chval="65536"
          />
        <mlabel 
          label="128k"
          chval="131072"
          />
      </menu>
      <label loc="0 20" size="110 20"
        style="Label1"
        label="Line Broadening"
        justify="Left"
        />
      <label loc="0 0" size="100 20"
        style="Label1"
        label="Transform size"
        justify="Left"
        />
    </group>
    <group loc="10 30" size="240 120"
      style="Heading1"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="140 100" size="100 20"
        style="PlainText"
        label="menu"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        editable="No"
        >
        <mlabel 
          label="16"
          chval="16"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1000"
          chval="1000"
          />
        <mlabel 
          label="2000"
          chval="2000"
          />
        <mlabel 
          label="5000"
          chval="5000"
          />
      </menu>
      <menu loc="140 80" size="100 20"
        style="PlainText"
        label="menu"
        vq="d1"
        vc="d1=$VALUE"
        set="$VALUE=d1"
        editable="No"
        >
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="5"
          chval="5"
          />
        <mlabel 
          label="25"
          chval="25"
          />
      </menu>
      <menu loc="140 40" size="100 20"
        style="PlainText"
        label="menu"
        vq="pw pw90"
        vc="pw=($VALUE/90)*pw90"
        set="$VALUE=trunc(90*pw/pw90+0.1)"
        show="$ENABLE=(pw90 &gt; 0)*2-1"
        editable="No"
        >
        <mlabel 
          label="15"
          chval="15"
          />
        <mlabel 
          label="30"
          chval="30"
          />
        <mlabel 
          label="45"
          chval="45"
          />
        <mlabel 
          label="60"
          chval="60"
          />
        <mlabel 
          label="90"
          chval="90"
          />
      </menu>
      <group loc="0 20" size="240 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="swstartend"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="10 0" size="70 20"
          style="Heading1"
          label="Downfield "
          justify="Right"
          />
        <entry loc="80 0" size="50 20"
          style="PlainText"
          vq="sw tof reffrq rfl rfp"
          vc="invsetsw(5):$f1,$f2 setsw($VALUE,$f2)"
          set="invsetsw(5):$f1,$f2 $VALUE=$f1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="180 0" size="50 20"
          style="PlainText"
          vq="sw tof reffrq rfl rfp"
          vc="invsetsw(5):$f1,$f2 setsw($f1,$VALUE)"
          set="invsetsw(5):$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
        <label loc="120 0" size="60 20"
          style="Label1"
          label="Upfield "
          justify="Right"
          />
      </group>
      <label loc="0 100" size="120 20"
        style="Label1"
        label="Number of Scans"
        justify="Left"
        />
      <label loc="0 80" size="130 20"
        style="Label1"
        label="Relaxation Delay [sec]"
        justify="Left"
        />
      <label loc="0 40" size="130 20"
        style="Label1"
        label="Pulse Width [degrees]"
        justify="Left"
        />
      <label loc="0 0" size="120 20"
        style="Label1"
        label="Spectral Width"
        justify="Left"
        />
      <group loc="0 60" size="210 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="20 0" size="110 20"
          style="Heading1"
          label="Enter pulse width  "
          justify="Right"
          />
        <entry loc="150 0" size="50 20"
          style="PlainText"
          vq="pw pw90"
          vc="pw($VALUE)"
          set="$VALUE=90*pw/pw90"
          show="$ENABLE=(pw90 &gt; 0)*2-1"
          digits="0"
          disable="Grayed out"
          />
      </group>
    </group>
    <group loc="480 80" size="240 100"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <textmessage loc="0 0" size="120 20"
        style="PlainText"
        label="Plotting turned off"
        vq="pltmod"
        set="if pltmod='off' then $VALUE='Plotting turned off' else $VALUE='Plot when done' endif"
        />
      <group loc="10 20" size="230 80"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <menu loc="110 60" size="120 20"
          style="PlainText"
          label="Integral"
          vq="intmod pltmod"
          vc="intmod='$VALUE'"
          set="$VALUE=intmod"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="Partial"
            chval="partial"
            />
          <mlabel 
            label="Full"
            chval="full"
            />
          <mlabel 
            label="Off"
            chval="off"
            />
        </menu>
        <menu loc="110 40" size="120 20"
          style="PlainText"
          label="Peak Pick"
          vq="pkpick pltmod"
          vc="pkpick='$VALUE'"
          set="$VALUE=pkpick"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="On Spectrum"
            chval="ppf"
            />
          <mlabel 
            label="List, at Left"
            chval="pll"
            />
          <mlabel 
            label="List, at Right"
            chval="pll(wc-50)"
            />
          <mlabel 
            label="None"
            />
        </menu>
        <label loc="0 40" size="110 20"
          style="Label1"
          label="Peak Values: "
          justify="Left"
          />
        <label loc="0 60" size="100 20"
          style="Label1"
          label="Integrals: "
          justify="Left"
          />
        <menu loc="110 20" size="120 20"
          style="PlainText"
          label="menu"
          vq="parstyle pltmod"
          vc="parstyle='$VALUE'"
          set="$VALUE=parstyle"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="Full, Top Left"
            chval="pap"
            />
          <mlabel 
            label="Full, Top Right"
            chval="pap(wc-50)"
            />
          <mlabel 
            label="Basic, Top Left"
            chval="ppa"
            />
          <mlabel 
            label="Basic, Top Right"
            chval="ppa(wc-50)"
            />
          <mlabel 
            label="Boxed"
            chval="bpa"
            />
          <mlabel 
            label="Text Only"
            chval="pltext"
            />
          <mlabel 
            label="None"
            />
        </menu>
        <menu loc="110 0" size="120 20"
          style="PlainText"
          label="Spectra Width"
          vq="pltmod"
          vc="pltmod='$VALUE'"
          set="$VALUE=pltmod"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="As Displayed"
            chval="fixed"
            />
          <mlabel 
            label="Full"
            chval="full"
            />
          <mlabel 
            label="From Region"
            chval="variable"
            />
          <mlabel 
            label="Off"
            chval="off"
            />
        </menu>
        <label loc="0 20" size="110 20"
          style="Label1"
          label="Parameters:"
          justify="Left"
          />
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Spectrum:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="260 80" size="210 63"
      vq="tn"
      show="if (tn&lt;&gt;'H1') and(tn&lt;&gt;'F19') then $SHOW=1 else $SHOW=0 endif"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="10 20" size="190 40"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="140 0" size="40 20"
          style="Heading1"
          label=" scans"
          justify="Left"
          />
        <label loc="0 20" size="110 20"
          style="Label1"
          label="Stop when S/N is "
          justify="Left"
          />
        <label loc="0 0" size="80 20"
          style="Label1"
          label="Block Size is "
          justify="Left"
          />
        <menu loc="80 0" size="60 20"
          style="PlainText"
          label="menu"
          vq="bs wbs"
          vc="bs=$VALUE"
          set="$VALUE=bs"
          editable="No"
          >
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
        </menu>
        <entry loc="110 20" size="40 20"
          style="PlainText"
          vq="sn wbs"
          vc="sn=$VALUE"
          set="$VALUE=sn"
          show="$ENABLE=(wbs='testsn')*2-1"
          disable="Grayed out"
          />
      </group>
      <check loc="0 0" size="210 20"
        style="Label1"
        label="Test for S/N at every Block Size"
        vq="wbs"
        vc="wbs='testsn'"
        vc2="wbs=''"
        set="$VALUE=(wbs='testsn')"
        />
    </group>
    <group loc="260 30" size="210 40"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="0 0" size="110 20"
        style="Heading1"
        label="Decoupling Mode:"
        justify="Left"
        />
      <menu loc="20 20" size="130 20"
        style="PlainText"
        label="menu"
        vq="dm"
        vc="dm='$VALUE'"
        set="$VALUE=dm"
        editable="No"
        >
        <mlabel 
          label="Decoupled+NOE"
          chval="yyy"
          />
        <mlabel 
          label="Decoupled-NOE"
          chval="nny"
          />
        <mlabel 
          label="Coupled+NOE"
          chval="ynn"
          />
        <mlabel 
          label="Coupled-NOE"
          chval="nnn"
          />
      </menu>
    </group>
    <textmessage loc="10 0" size="200 20"
      style="PlainText"
      label="sh2pul [N15]"
      vq="pslabel tn"
      set="$VALUE=pslabel+' ['+tn+']'"
      />
    <button loc="260 0" size="130 20"
      style="Heading2"
      label="Display Sequence"
      vc="dps"
      decor1=""
      />
  </group>
</template>
