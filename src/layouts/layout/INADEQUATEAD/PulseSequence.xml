<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="550 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 75" size="205 125"
      style="Heading1"
      label="Adiabatic Pulse"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 100" size="190 20"
        style="Label1"
        label="Xsense Probe (SP26 shape)"
        vq="xsens"
        vc="xsens='y'"
        vc2="xsens='n'"
        set="$VALUE=(xsens='y')"
        />
      <entry loc="120 80" size="55 20"
        style="PlainText"
        vq="adpwlvl adiabatic"
        vc="adpwlvl=$VALUE"
        set="$VALUE=adpwlvl"
        show="$ENABLE=(adiabatic='p')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="120 60" size="55 20"
        style="PlainText"
        vq="adpw adiabatic"
        vc="adpw=$VALUE"
        set="$VALUE=adpw"
        show="$ENABLE=(adiabatic='p')*2-1"
        disable="Grayed out"
        />
      <menu loc="120 20" size="55 20"
        style="PlainText"
        vq="adiabatic"
        vc="adiabatic='$VALUE'"
        set="$VALUE=adiabatic"
        bg=""
        editable="No"
        >
        <mlabel 
          label="Auto"
          chval="y"
          />
        <mlabel 
          label="User"
          chval="p"
          />
        <mlabel 
          label="No"
          chval="n"
          />
      </menu>
      <label loc="180 80" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="180 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="Adiabatic Pulse:"
        justify="Left"
        />
      <label loc="10 80" size="45 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 60" size="45 20"
        style="Label1"
        label="Length"
        justify="Left"
        />
      <entry loc="55 40" size="120 20"
        style="PlainText"
        vq="adshape adiabatic"
        vc="adshape='$VALUE'"
        set="$VALUE=adshape"
        show="$ENABLE=(adiabatic='p')*2-1"
        disable="Grayed out"
        />
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="INADEQUATEAD"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="215 25" size="170 130"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <check loc="10 100" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        show="$ENABLE=(PFGflg='n')*2-1"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="205 50"
      style="Heading1"
      label="C-C Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="120 20" size="55 20"
        style="PlainText"
        vq="jcc"
        vc="jcc=$VALUE"
        set="$VALUE=jcc"
        digits="0"
        disable="Grayed out"
        />
      <label loc="180 20" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="10 20" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
    </group>
  </group>
</template>
