<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="670 260"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 200"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <button loc="65 135" size="180 45"
        style="Heading2"
        label="Acquire Trial Spectra"
        vc="gmapsys('au')"
        bg=""
        decor1=""
        />
      <group loc="30 30" size="330 95"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="s2pulacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <menu loc="145 45" size="70 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="6"
            chval="6"
            />
          <mlabel 
            label="12"
            chval="12"
            />
        </menu>
        <menu loc="145 25" size="70 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <label loc="35 25" size="105 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <label loc="35 65" size="80 20"
          style="Label1"
          label="Pulse Angle"
          justify="Left"
          />
        <label loc="220 65" size="60 20"
          style="Label1"
          label="degrees"
          justify="Left"
          />
        <label loc="220 45" size="60 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="270 5" size="40 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <label loc="195 5" size="20 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <label loc="10 5" size="135 20"
          style="Label1"
          label="Spectral width -  from"
          justify="Left"
          />
        <menu loc="145 65" size="70 20"
          style="PlainText"
          label="menu"
          vq="pw pw90"
          vc="pw($VALUE)"
          set="$VALUE=pw*90/pw90"
          bg=""
          editable="No"
          >
          <mlabel 
            label="35"
            chval="35"
            />
          <mlabel 
            label="30"
            chval="30"
            />
          <mlabel 
            label="45"
            chval="45"
            />
          <mlabel 
            label="60"
            chval="60"
            />
          <mlabel 
            label="90"
            chval="90"
            />
        </menu>
        <label loc="35 45" size="105 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
        <entry loc="215 5" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq sw"
          vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
          set="invsetsw:$f1,$f2 $VALUE=$f1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="145 5" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq"
          vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
          set="invsetsw:$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
      </group>
    </group>
    <group loc="400 35" size="235 200"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="5 0" size="135 20"
        style="Heading1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="145 20" size="80 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
  </group>
</template>
