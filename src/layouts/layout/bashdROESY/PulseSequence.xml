        style="Label1"
        label="shape"
        justify="Left"
        />
      <entry loc="85 80" size="115 20"
        style="PlainText"
        vq="selshapeA"
        vc="selshapeA='$VALUE'"
        set="$VALUE=selshapeA"
        disable="Grayed out"
        />
      <label loc="20 60" size="55 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <label loc="20 40" size="55 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <entry loc="85 40" size="60 20"
        style="PlainText"
        vq="selpwA"
        vc="selpwA=$VALUE*1000"
        set="$VALUE=selpwA/1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="85 60" size="60 20"
        style="PlainText"
        vq="selpwrA"
        vc="selpwrA=$VALUE"
        set="$VALUE=selpwrA"
        disable="Grayed out"
        />
      <entry loc="160 60" size="60 20"
        style="PlainText"
        vq="selpwrB"
        vc="selpwrB=$VALUE"
        set="$VALUE=selpwrB"
        disable="Grayed out"
        />
      <label loc="225 60" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="225 40" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="160 40" size="60 20"
        style="PlainText"
        vq="selpwB"
        vc="selpwB=$VALUE*1000"
        set="$VALUE=selpwB/1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="160 20" size="60 20"
        style="Label1"
        label="Echo2"
        justify="Center"
        />
      <label loc="85 20" size="60 20"
        style="Label1"
        label="Echo1"
        justify="Center"
        />
      <label loc="10 20" size="55 20"
        style="Label1"
        label="Pulse"
        justify="Left"
        />
      <label loc="225 145" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="bashdROESY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
