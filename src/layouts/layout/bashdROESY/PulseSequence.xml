<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="675 270"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 155" size="200 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="170 60" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="110 40" size="55 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
    <group loc="5 25" size="200 130"
      style="Heading1"
      label="SpinLock"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SpinlockR"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="10 20" size="80 20"
        style="Heading1"
        label="Mixing time"
        justify="Left"
        />
      <label loc="10 40" size="95 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <button loc="70 60" size="40 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        bg=""
        decor1=""
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="170 100" size="30 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <entry loc="110 100" size="55 20"
        style="PlainText"
        vq="slpwR"
        vc="slpwR=$VALUE"
        set="$VALUE=slpwR"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="110 80" size="55 20"
        style="PlainText"
        vq="slpwrR"
        vc="getpower($VALUE,tn):spinlockR setpower(spinlockR,tn):slpwrR,slpwR spinlockR=spinlockR slpwR=slpwR"
        set="$VALUE=slpwrR"
        disable="Grayed out"
        />
      <label loc="170 80" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="170 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="spinlockR"
        vc="setpower($VALUE,tn):slpwrR,slpwR getpower(slpwrR,tn):spinlockR spinlockR=spinlockR slpwR=slpwR"
        set="$VALUE=spinlockR"
        disable="Grayed out"
        />
      <menu loc="110 40" size="75 20"
        style="PlainText"
        label="menu"
        vq="slpatR"
        vc="slpatR='$VALUE'"
        set="$VALUE=slpatR"
        bg=""
        editable="No"
        >
        <mlabel 
          label="cw"
          chval="cw"
          />
        <mlabel 
          label="troesy"
          chval="troesy"
          />
      </menu>
      <label loc="170 20" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="110 20" size="55 20"
        style="PlainText"
        vq="mixR"
        vc="mixR=$VALUE/1000"
        set="$VALUE=mixR*1000"
        disable="Grayed out"
        />
    </group>
    <group loc="210 25" size="195 215"
      style="Heading1"
      label="Z-filter"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="zfilter"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 85" size="180 105"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="150 85" size="30 20"
          style="Heading1"
          label="deg"
          justify="Left"
          />
        <label loc="150 45" size="30 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <label loc="150 65" size="30 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <entry loc="85 85" size="60 20"
          style="PlainText"
          vq="zfphinc flipback"
          vc="zfphinc=$VALUE"
          set="$VALUE=zfphinc"
          show="$ENABLE=(flipback='y')*2-1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="85 65" size="60 20"
          style="PlainText"
          vq="fbpw flipback"
          vc="fbpw=$VALUE*1000"
          set="$VALUE=fbpw/1000"
          show="$ENABLE=(flipback='y')*2-1"
          digits="3"
          disable="Grayed out"
          />
        <entry loc="85 45" size="60 20"
          style="PlainText"
          vq="fbpwr flipback"
          vc="fbpwr=$VALUE"
          set="$VALUE=fbpwr"
          show="$ENABLE=(flipback='y')*2-1"
          disable="Grayed out"
          />
        <entry loc="85 25" size="60 20"
          style="PlainText"
          vq="fbshp flipback"
          vc="fbshp='$VALUE'"
          set="$VALUE=fbshp"
          show="$ENABLE=(flipback='y')*2-1"
          disable="Grayed out"
          />
        <label loc="5 85" size="75 20"
          style="Label1"
          label="Phase shift"
          justify="Left"
          />
        <label loc="5 65" size="75 20"
          style="Label1"
          label="Duration"
          justify="Left"
          />
        <label loc="5 45" size="75 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <check loc="5 5" size="130 20"
          style="Label1"
          label="Flipback pulse"
          vq="flipback zfilt"
          vc="flipback='y'"
          vc2="flipback='n'"
          set="$VALUE=(flipback='y')"
          show="$ENABLE=(zfilt='y')*2-1"
          />
        <label loc="5 25" size="75 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
      <entry loc="90 60" size="60 20"
        style="PlainText"
        vq="gtz zfilt PFGflg"
        vc="gtz=$VALUE/1000"
        set="$VALUE=gtz*1000"
        show="$ENABLE=(zfilt='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <label loc="155 60" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="90 40" size="60 20"
        style="PlainText"
        vq="gzlvlz zfilt PFGflg"
        vc="gzlvlz=$VALUE"
        set="$VALUE=gzlvlz"
        show="$ENABLE=(zfilt='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="130 20"
        style="Label1"
        label="Z-filter"
        vq="zfilt PFGflg"
        vc="zfilt='y'"
        vc2="zfilt='n'"
        set="$VALUE=(zfilt='y' and PFGflg='y')"
        />
    </group>
    <group loc="410 25" size="250 215"
      style="Heading1"
      label="Double PFG spinecho"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="dpfgsegroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 185" size="195 20"
        style="Label1"
        label="Homonuclear Doupling (F1)"
        vq="homodec"
        vc="homodec='y'"
        vc2="homodec='n'"
        set="$VALUE=(homodec='y')"
        />
      <button loc="85 160" size="135 20"
        style="Heading2"
        label="make shapes"
        vq="selfrqc selfrqppm"
        vc="suselfrq"
        show="on('selfrqc'):$s1 on('selfrqppm'):$s2 $ENABLE=(($s1&gt;0) or ($s2&gt;0))"
        bg=""
        decor1=""
        />
      <label loc="10 140" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 120" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <entry loc="85 140" size="60 20"
        style="PlainText"
        vq="gtA"
        vc="gtA=$VALUE/1000"
        set="$VALUE=gtA*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 140" size="60 20"
        style="PlainText"
        vq="gtB"
        vc="gtB=$VALUE/1000"
        set="$VALUE=gtB*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 120" size="60 20"
        style="PlainText"
        vq="gzlvlB"
        vc="gzlvlB=$VALUE"
        set="$VALUE=gzlvlB"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="85 120" size="60 20"
        style="PlainText"
        vq="gzlvlA"
        vc="gzlvlA=$VALUE"
        set="$VALUE=gzlvlA"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 100" size="115 20"
        style="PlainText"
        vq="selshapeB"
        vc="selshapeB='$VALUE'"
        set="$VALUE=selshapeB"
        disable="Grayed out"
        />
