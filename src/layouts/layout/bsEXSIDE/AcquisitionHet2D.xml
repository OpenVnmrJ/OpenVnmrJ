<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="AcquisitionHet2D" element="pages" type="acquisition" >
  <group size="645 245"
    style="PlainText"
    label="Acquisition"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="AcquisitionHet2D"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="320 25" size="295 210"
      style="Heading1"
      label="Acquisition in F1"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitioninF1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="210 180" size="75 20"
        style="PlainText"
        vq="pwxlvl"
        vc="pwxlvl=$VALUE"
        set="$VALUE=pwxlvl"
        disable="Grayed out"
        tooltip="pwxlvl"
        />
      <menu loc="210 20" size="75 20"
        style="PlainText"
        label="menu"
        vq="sw1 ni"
        vc="parunits('set','sw1','$VALUE')"
        set="parunits('get','sw1'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm2"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <button loc="135 60" size="150 20"
        style="Heading2"
        label="Set LP1 and Weighting"
        vc="setLP1andWindow"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <group loc="10 80" size="265 60"
        style="PlainText"
        vq="ni phase sw1"
        show="if ni&gt;1 then $VALUE=1 else $VALUE=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="10 40" size="80 20"
          style="Label3"
          label="J-Resolution:"
          justify="Left"
          />
        <label loc="10 20" size="75 20"
          style="PlainText"
          label="Resolution:"
          justify="Left"
          />
        <label loc="140 40" size="75 20"
          style="Label3"
          label="Hz (jscale)"
          justify="Left"
          />
        <textmessage loc="90 20" size="50 20"
          style="PlainText"
          label="230.7"
          vq="ni sw1 phase"
          set="if ni&gt;1 then $VALUE=sw1/ni else $VALUE=0 endif"
          digits="1"
          />
        <label loc="140 20" size="45 20"
          style="PlainText"
          label="Hz, or"
          justify="Left"
          />
        <textmessage loc="185 20" size="45 20"
          style="PlainText"
          label="1.84"
          vq="ni sw1 phase reffrq1"
          set="if ni&gt;1 then $VALUE=(sw1/ni )/reffrq1 else $VALUE=0 endif"
          digits="2"
          />
        <label loc="230 20" size="30 20"
          style="PlainText"
          label="ppm"
          justify="Left"
          />
        <label loc="230 0" size="30 20"
          style="PlainText"
          label="s"
          justify="Left"
          />
        <textmessage loc="90 40" size="50 20"
          style="Label3"
          label="15.38"
          vq="ni sw1 phase"
          set="$sw1=sw1 on('selfrq1c'):$o if $o then $sw1=selfrq1w endif if ni&gt;1 then $VALUE=$sw1/ni/jscale else $VALUE=0 endif"
          digits="2"
          />
        <textmessage loc="140 0" size="75 20"
          style="PlainText"
          label="0.004334"
          vq="ni sw1"
          set="if ni&gt;1 then $VALUE=ni/sw1 else $VALUE=0 endif"
          digits="6"
          />
        <label loc="0 0" size="140 20"
          style="PlainText"
          label="Acquisition time (max.)"
          justify="Left"
          />
      </group>
      <entry loc="135 180" size="75 20"
        style="PlainText"
        vq="pwx"
        vc="pwx=$VALUE"
        set="$VALUE=pwx"
        show="if (pwx=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        tooltip="pwx"
        />
      <label loc="10 180" size="115 20"
        style="PlainText"
        label="X Pulse / at power"
        justify="Left"
        />
      <group loc="10 160" size="270 20"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <radio loc="140 0" size="90 20"
          style="PlainText"
          label="Full 2D"
          vq="ni"
          vc="if phase[1]&gt;0.5 then phase=1,2 endif if ni&lt;1.5 then ni=128 endif"
          set="$VALUE=(ni&gt;1.5)"
          />
        <radio loc="15 0" size="140 20"
          style="PlainText"
          label="First Increment"
          vq="ni"
          vc="ni=1 phase=phase[1]"
          set="$VALUE=(ni&lt;1.5)"
          />
      </group>
      <label loc="10 140" size="120 20"
        style="PlainText"
        label="Acquisition Mode:"
        justify="Left"
        />
      <group loc="130 140" size="140 20"
        vq="phase"
        show="$SHOW=(phase[1]&gt;=0.5)"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="0 0" size="130 20"
          style="PlainText"
          label="Hypercomplex 2D"
          justify="Left"
          />
      </group>
      <group loc="120 135" size="120 25"
        vq="phase"
        show="$SHOW=(phase[1]&lt;0.5)"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="10 5" size="110 20"
          style="PlainText"
          label="Absolute Value"
          justify="Left"
          />
      </group>
      <label loc="10 40" size="110 20"
        style="PlainText"
        label="Increments in t1"
        justify="Left"
        />
      <entry loc="135 40" size="75 20"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        tooltip="ni"
        />
      <entry loc="135 20" size="75 20"
        style="PlainText"
        vq="sw1 ni"
        vc="vnmrunits('set','sw1',$VALUE)"
        set="vnmrunits('get','sw1'):$VALUE"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        digits="1"
        disable="Grayed out"
        tooltip="sw1"
        />
      <label loc="10 20" size="100 20"
        style="PlainText"
        label="Spectral width"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="310 210"
      style="Heading1"
      label="Acquisition in F2"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <entry loc="170 180" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE"
        set="$VALUE=pw"
        show="if (pw=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        tooltip="pw"
        />
      <menu loc="230 160" size="65 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <menu loc="230 40" size="65 20"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <label loc="20 60" size="90 20"
        style="PlainText"
        label="Complex points"
        justify="Left"
        />
      <label loc="170 110" size="80 20"
        style="PlainText"
        label="Steady-State"
        justify="Left"
        />
      <entry loc="170 90" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        disable="Grayed out"
        tooltip="nt"
        />
      <menu loc="230 20" size="65 20"
        style="PlainText"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="250 110" size="45 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="if ss=0 then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        tooltip="ss"
        />
      <entry loc="230 180" size="65 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        disable="Grayed out"
        tooltip="tpwr"
        />
      <button loc="230 140" size="65 20"
        style="Heading2"
        label="Find gain"
        vc="gain2Dproc"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <label loc="60 110" size="70 20"
        style="PlainText"
        label="Completed"
        justify="Left"
        />
      <textmessage loc="130 110" size="40 20"
        style="Label1"
        label="0"
        vq="ct"
        set="$VALUE=ct"
        />
      <label loc="60 90" size="70 20"
        style="PlainText"
        label="Requested"
        justify="Left"
        />
      <label loc="10 90" size="50 20"
        style="PlainText"
        label="Scans:"
        justify="Left"
        />
      <entry loc="170 160" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="if (d1=0) then $VALUE=0 else $VALUE=1 endif"
        digits="3"
        disable="Grayed out"
        tooltip="d1"
        />
      <entry loc="170 140" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        disable="Grayed out"
        tooltip="gain"
        />
      <label loc="10 180" size="150 20"
        style="PlainText"
        label="Observe Pulse / at power"
        justify="Left"
        />
      <label loc="10 160" size="110 20"
        style="PlainText"
        label="Relaxation delay"
        justify="Left"
        />
      <label loc="10 140" size="90 20"
        style="PlainText"
        label="Receiver Gain"
        justify="Left"
        />
      <entry loc="170 60" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        disable="Grayed out"
        tooltip="np/2"
        />
      <entry loc="170 40" size="60 20"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        disable="Grayed out"
        tooltip="at"
        />
      <entry loc="170 20" size="60 20"
        style="PlainText"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        disable="Grayed out"
        tooltip="sw"
        />
      <label loc="10 40" size="100 20"
        style="PlainText"
        label="Acquisition time"
        justify="Left"
        />
      <label loc="10 20" size="100 20"
        style="PlainText"
        label="Spectral width"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="bsEXSIDE"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
