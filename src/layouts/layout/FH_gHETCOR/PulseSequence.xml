<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="625 250"
    style="Heading1"
    label="Pulse Sequence"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="220 165" size="200 70"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="man"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <button loc="50 20" size="110 30"
        style="Heading2"
        label="Manual"
        vc="clear man(seqfil) vnmrjcmd('setpage','Process','Text Output')"
        bg=""
        decor1=""
        />
    </group>
    <group loc="220 25" size="200 135"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="gradsel"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="3"
        disable="Grayed out"
        />
      <label loc="20 80" size="70 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <label loc="175 105" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="110 105" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="10 105" size="100 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="175 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="20 60" size="70 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 40" size="70 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <label loc="10 20" size="130 20"
        style="Label1"
        label="Encoding Gradient"
        justify="Left"
        />
      <entry loc="110 40" size="60 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="425 110" size="175 125"
      style="Heading1"
      label="Solvent Suppresion"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="peter_presat"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="10 100" size="60 20"
        style="Label1"
        label="Wet"
        vq="wet"
        vc="wet='y'"
        vc2="wet='n'"
        set="$VALUE=wet='y'"
        />
      <check loc="10 20" size="160 20"
        style="Label1"
        label="ON / OFF Status ('yn')"
        vq="satmode"
        vc="satmode='yn'"
        vc2="satmode='n'"
        set="$VALUE=(satmode='yn')"
        />
      <entry loc="85 40" size="60 20"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="$ENABLE=(satmode='yn')"
        digits="2"
        disable="Grayed out"
        />
      <label loc="150 40" size="25 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <label loc="150 60" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="85 80" size="60 20"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="$ENABLE=(satmode='yn')"
        disable="Grayed out"
        />
      <entry loc="85 60" size="60 20"
        style="PlainText"
        vq="satfrq satmode"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        show="$ENABLE=(satmode='yn')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Frequency"
        justify="Left"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
    </group>
    <group loc="425 25" size="175 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SteadyState"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="150 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="85 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y')"
        disable="Grayed out"
        />
      <entry loc="85 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y')"
        />
    </group>
    <group loc="5 100" size="210 75"
      style="Heading1"
      label="2nd Channel Pulse"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="2ndChannelPulse"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="185 45" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="185 25" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <entry loc="120 45" size="60 20"
        style="PlainText"
        vq="pwxlvl"
        vc="pwxlvl=$VALUE"
        set="$VALUE=pwxlvl"
        disable="Grayed out"
        />
      <entry loc="120 25" size="60 20"
        style="PlainText"
        vq="pwx"
        vc="pwx=$VALUE"
        set="$VALUE=pwx"
        disable="Grayed out"
        />
      <label loc="10 45" size="105 20"
        style="Label1"
        label="Power level"
        justify="Left"
        />
      <label loc="10 25" size="105 20"
        style="Label1"
        label="Pulse length"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="210 75"
      style="Heading1"
      label="HETCOR Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HETCORCoupling"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="10 45" size="110 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
      <textmessage loc="120 45" size="60 20"
        style="PlainText"
        vq="jFH"
        set="$VALUE=500/jFH"
        digits="1"
        />
      <label loc="185 45" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="185 25" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="120 25" size="60 20"
        style="PlainText"
        vq="jFH"
        vc="jFH=$VALUE"
        set="$VALUE=jFH"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="245 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
