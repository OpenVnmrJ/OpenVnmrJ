<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="550 190"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 100" size="230 70"
      style="Heading1"
      label="Second Echo"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="195 25" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <textmessage loc="130 45" size="60 20"
        style="PlainText"
        label="yny"
        vq="dm"
        set="$VALUE=dm"
        />
      <entry loc="130 25" size="60 20"
        style="PlainText"
        vq="d3"
        vc="d3=$VALUE/1000"
        set="$VALUE=d3*1000"
        disable="Grayed out"
        />
      <label loc="10 45" size="120 20"
        style="Label1"
        label="Decoupler status"
        justify="Left"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="Echo delay"
        justify="Left"
        />
    </group>
    <group loc="240 25" size="225 75"
      style="Heading1"
      label="One-Bond XH Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="190 45" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="190 25" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="10 45" size="115 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
      <textmessage loc="125 45" size="60 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <entry loc="125 25" size="60 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        disable="Grayed out"
        />
      <label loc="10 25" size="115 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="230 75"
      style="Heading1"
      label="Observe Pulses"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="195 45" size="30 20"
        style="Label1"
        label="μs"
        justify="Left"
        />
      <label loc="195 25" size="30 20"
        style="Label1"
        label="μs"
        justify="Left"
        />
      <entry loc="130 25" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE"
        set="$VALUE=pw"
        disable="Grayed out"
        />
      <entry loc="130 45" size="60 20"
        style="PlainText"
        vq="p1"
        vc="p1=$VALUE"
        set="$VALUE=p1"
        disable="Grayed out"
        />
      <label loc="10 45" size="120 20"
        style="Label1"
        label="Composite Pi pulse"
        justify="Left"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="First pulse"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="APT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
