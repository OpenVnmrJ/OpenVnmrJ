<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence2" element="pages" type="acquisition" >
  <group size="625 230"
    style="Heading1"
    label="Pulse Sequence.."
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence2"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 115" size="185 50"
      style="Heading1"
      label="C12-H1 suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tango"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 20" size="140 20"
        style="Label1"
        label="TANGO-Gradient"
        vq="nullflg"
        vc="nullflg='y'"
        vc2="nullflg='n'"
        set="$VALUE=(nullflg='y')"
        />
    </group>
    <group loc="5 25" size="185 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="160 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
