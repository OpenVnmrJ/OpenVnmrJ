<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="625 240"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="400 25" size="185 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="155 60" size="30 20"
        style="Heading1"
        label=" ms"
        justify="Left"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
    <group loc="400 115" size="185 115"
      style="Heading1"
      label="X-Inversion"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AD-Xinversion"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="95 85" size="60 20"
        style="PlainText"
        vq="pwxlvl180"
        vc="pwxlvl180=$VALUE"
        set="$VALUE=pwxlvl180"
        disable="Grayed out"
        />
      <entry loc="95 65" size="60 20"
        style="PlainText"
        vq="pwx180"
        vc="pwx180=$VALUE"
        set="$VALUE=pwx180"
        disable="Grayed out"
        />
      <label loc="155 65" size="30 20"
        style="Label1"
        label=" µs"
        justify="Left"
        />
      <label loc="155 85" size="25 20"
        style="Label1"
        label=" dB"
        justify="Left"
        />
      <label loc="10 85" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="60 45" size="115 20"
        style="PlainText"
        vq="pwx180adR"
        vc="pwx180adR='$VALUE'"
        set="$VALUE=pwx180adR"
        disable="Grayed out"
        />
      <label loc="10 45" size="50 20"
        style="Label1"
        label="ShapeR"
        justify="Left"
        />
      <label loc="10 65" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <entry loc="60 25" size="115 20"
        style="PlainText"
        vq="pwx180ad"
        vc="pwx180ad='$VALUE'"
        set="$VALUE=pwx180ad"
        disable="Grayed out"
        />
      <label loc="10 25" size="50 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
    </group>
    <group loc="195 180" size="200 50"
      style="Heading1"
      label="C12-H1 suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tango"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 20" size="140 20"
        style="Label1"
        label="TANGO-Gradient "
        vq="nullflg"
        vc="nullflg='y'"
        vc2="nullflg='n'"
        set="$VALUE=(nullflg='y')"
        />
    </group>
    <group loc="195 25" size="200 155"
      style="Heading1"
      label="SpinLock"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MLEV17c"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="110 65" size="60 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <entry loc="110 85" size="60 20"
        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <entry loc="110 105" size="60 20"
        style="PlainText"
        vq="slpwT"
        vc="slpwT=$VALUE"
        set="$VALUE=slpwT"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="110 125" size="60 20"
        style="PlainText"
        vq="trim"
        vc="trim=$VALUE/1000"
        set="$VALUE=trim*1000"
        disable="Grayed out"
        />
      <label loc="170 125" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <label loc="170 105" size="25 20"
        style="Label1"
        label=" µs"
        justify="Left"
        />
      <label loc="170 85" size="30 20"
        style="Label1"
        label=" dB"
        justify="Left"
        />
      <label loc="170 65" size="30 20"
        style="Label1"
        label=" Hz"
        justify="Left"
        />
      <entry loc="110 25" size="60 20"
        style="PlainText"
        vq="mixT"
        vc="mixT=$VALUE/1000"
        set="$VALUE=mixT*1000"
        disable="Grayed out"
        />
      <label loc="170 25" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <menu loc="65 45" size="105 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        editable="No"
        >
        <mlabel 
          label="mlev17"
          chval="mlev17"
          />
        <mlabel 
          label="clean mlev17"
          chval="mlev17c"
          />
      </menu>
      <button loc="65 65" size="45 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        bg=""
        decor1=""
        />
      <label loc="10 45" size="50 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <label loc="10 85" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 105" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 125" size="90 20"
        style="Label1"
        label="Trim pulse"
        justify="Left"
        />
      <label loc="10 65" size="55 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 25" size="80 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
    </group>
    <group loc="5 155" size="185 75"
      style="Heading1"
      label="One-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="105 45" size="50 20"
        style="PlainText"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <label loc="155 45" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <label loc="155 25" size="30 20"
        style="Label1"
        label=" Hz"
        justify="Left"
        />
      <entry loc="105 25" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 25" size="95 20"
        style="Label1"
        label="Coupling const."
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="185 130"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="EDggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="155 105" size="30 20"
        style="Heading1"
        label=" ms"
        justify="Left"
        />
      <label loc="155 65" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <entry loc="105 45" size="50 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 65" size="50 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="105 85" size="50 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="105 105" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 85" size="80 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <label loc="25 65" size="65 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="10 105" size="90 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="25 45" size="65 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <textmessage loc="10 25" size="170 20"
        style="PlainText"
        vq="gzlvlE invgcal probe"
        set="$gcm='' fixgrdR(gzlvlE):$ret,$gcm if ($gcm&lt;&gt;'') then $gcm='('+$gcm+' G/cm)' endif $VALUE='Encode Grad '+$gcm"
        />
    </group>
  </group>
</template>
