<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="QuickH1" element="pages" type="acquisition" >
  <group size="670 265"
    style="Heading1"
    label="Default H1"
    vq="pslabel"
    show="$SHOW=(pslabel='PROTON')"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="QuickH1"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <group loc="40 30" size="310 155"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="PROTONacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="0 5" size="140 20"
          style="Heading1"
          label="Spectral Width (select):"
          justify="Right"
          />
        <group loc="5 5" size="300 40"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="swentry"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <menu loc="140 0" size="120 20"
            style="PlainText"
            label="menu"
            vq="obsSW"
            vc="exec('$VALUE')"
            set="$VALUE=''"
            bg=""
            editable="No"
            >
            <mlabel 
              label="-2  →  14"
              chval="setsw(14,-2)"
              />
            <mlabel 
              label="-1  →  11"
              chval="setsw(11,-1)"
              />
            <mlabel 
              label="-2  →  16"
              chval="setsw(16,-2)"
              />
            <mlabel 
              label="-0.5  → 9.5"
              chval="setsw(9.5,-0.5)"
              />
            <mlabel 
              label="0.5  →  8.5"
              chval="setsw(8.5,0.5)"
              />
          </menu>
          <label loc="5 20" size="130 20"
            style="Label1"
            label="(...or enter):"
            justify="Right"
            />
          <label loc="190 20" size="20 20"
            style="Label1"
            label="to"
            justify="Center"
            />
          <label loc="265 20" size="35 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <label loc="265 0" size="35 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <entry loc="210 20" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq sw"
            vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
            set="invsetsw:$f1,$f2 $VALUE=$f1"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="140 20" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq"
            vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
            set="invsetsw:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
        </group>
        <label loc="225 85" size="60 20"
          style="Label1"
          label="degrees"
          justify="Left"
          />
        <label loc="225 65" size="50 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <menu loc="145 45" size="75 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
        </menu>
        <label loc="35 85" size="80 20"
          style="Label1"
          label="Pulse Angle"
          justify="Left"
          />
        <label loc="35 65" size="105 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
        <label loc="35 45" size="110 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <menu loc="145 65" size="75 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="10"
            chval="10"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <menu loc="145 85" size="75 20"
          style="PlainText"
          label="menu"
          vq="pw pw90"
          vc="pw($VALUE) setddrtc"
          set="$VALUE=pw*90/pw90"
          bg=""
          editable="No"
          >
          <mlabel 
            label="45"
            chval="45"
            />
          <mlabel 
            label="30"
            chval="30"
            />
          <mlabel 
            label="60"
            chval="60"
            />
          <mlabel 
            label="90"
            chval="90"
            />
        </menu>
        <group loc="25 105" size="215 40"
          vq="customflag"
          show="$SHOW=(customflag='y')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="minswshow"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="10 0" size="100 20"
            style="Heading1"
            label="Minimize SW"
            justify="Left"
            />
          <group loc="0 20" size="215 20"
            vq="procdim"
            show="$SHOW=(procdim&lt;1)"
            bg=""
            border="None"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
            <label loc="10 0" size="100 20"
              style="Heading1"
              label="Calibrate pw90"
              justify="Left"
              />
            <check loc="120 0" size="25 20"
              style="Label1"
              vq="wstart"
              vc="wstart='pw90proc'"
              vc2="wstart=''"
              set="$VALUE=(wstart&lt;&gt;'')"
              />
          </group>
          <menu loc="120 0" size="75 20"
            style="PlainText"
            vq="MinSW"
            vc="MinSW='$VALUE'"
            set="$VALUE=MinSW"
            bg=""
            editable="No"
            >
            <mlabel 
              label="Auto"
              chval="auto"
              />
            <mlabel 
              label="Manual"
              chval="manual"
              />
            <mlabel 
              label="Off"
              chval="off"
              />
            <mlabel 
              label="Skip"
              chval="skip"
              />
          </menu>
        </group>
      </group>
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="5 0" size="135 20"
        style="Heading1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="145 20" size="80 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 45" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <label loc="0 150" size="220 20"
          style="Heading1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="10 105" size="200 25"
          style="PlainText"
          label=" "
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          label="After CARBON acq:"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          label="Before CARBON acq:"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label=" "
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
  </group>
</template>
