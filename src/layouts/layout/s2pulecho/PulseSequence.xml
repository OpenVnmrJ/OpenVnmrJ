<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="670 235"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="260 25" size="200 175"
      style="Heading1"
      label="Echo"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="bipstuff"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 40" size="190 60"
        vq="sbbiflg"
        show="$SHOW=(sbbiflg='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="165 40" size="25 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <entry loc="55 0" size="130 20"
          style="PlainText"
          vq="tnbipshp sbbiflg"
          vc="tnbipshp='$VALUE'"
          set="$VALUE=tnbipshp"
          disable="Grayed out"
          />
        <entry loc="105 40" size="55 20"
          style="PlainText"
          vq="tnbippwr sbbiflg"
          vc="tnbippwr=$VALUE"
          set="$VALUE=tnbippwr"
          disable="Grayed out"
          />
        <entry loc="105 20" size="55 20"
          style="PlainText"
          vq="tnbippw sbbiflg"
          vc="tnbippw=$VALUE"
          set="$VALUE=tnbippw"
          disable="Grayed out"
          />
        <label loc="165 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <label loc="5 40" size="50 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="5 20" size="50 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
        <label loc="5 0" size="50 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
      <group loc="5 100" size="190 75"
        vq="sbbiflg PFGflg"
        show="$SHOW=((sbbiflg='y') and (PFGflg='y'))"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="gradstuff"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="165 45" size="25 20"
          style="Label1"
          label="ms"
          />
        <label loc="165 25" size="25 20"
          style="Label1"
          label="ms"
          />
        <label loc="5 5" size="55 20"
          style="Label1"
          label="Gradient"
          />
        <entry loc="105 46" size="55 20"
          style="PlainText"
          vq="segstab"
          vc="segstab=$VALUE/1e3"
          set="$VALUE=segstab*1e3"
          digits="2"
          />
        <entry loc="105 26" size="55 20"
          style="PlainText"
          vq="segt5"
          vc="segt5=$VALUE/1e3"
          set="$VALUE=segt5*1e3"
          digits="2"
          />
        <entry loc="105 6" size="55 20"
          style="PlainText"
          vq="gzlvl5"
          vc="gzlvl5=$VALUE"
          set="$VALUE=gzlvl5"
          digits="0"
          />
        <label loc="35 46" size="65 20"
          style="Label1"
          label="recovery"
          justify="Right"
          />
        <label loc="35 26" size="65 20"
          style="Label1"
          label="duration"
          justify="Right"
          />
        <label loc="50 6" size="50 20"
          style="Label1"
          label="level"
          justify="Right"
          />
      </group>
      <check loc="10 20" size="185 20"
        style="Label1"
        label="BB double echo"
        vq="sbbiflg"
        vc="sbbiflg='y' proc='ft' ddrpm='e' setddrtc"
        vc2="sbbiflg='n' proc='ft' ddrpm='p' setddrtc"
        set="$VALUE=(sbbiflg='y')"
        />
    </group>
    <group loc="5 140" size="250 60"
      style="Heading1"
      label="Homospoil"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="185 25" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="120 25" size="60 20"
        style="PlainText"
        vq="hst hs"
        vc="hst=$VALUE*.001"
        set="$VALUE=hst*1000"
        show="$ENABLE=(hs='y' or hs='yy' or hs='ny' or hs='yn')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 25" size="40 20"
        style="Label1"
        label="Time"
        justify="Left"
        />
    </group>
    <textmessage loc="10 0" size="170 25"
      style="PlainText"
      label="CARBON"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
    <group loc="5 25" size="250 115"
      style="Heading1"
      label="Excitation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Excitation"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <menu loc="180 45" size="60 20"
        style="PlainText"
        label="menu"
        vq="p1"
        vc="parunits('set','p1','$VALUE')"
        set="parunits('get','p1'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 65" size="60 20"
        style="PlainText"
        vq="d2"
        vc="parunits('set','d2','$VALUE')"
        set="parunits('get','d2'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <label loc="10 65" size="110 20"
        style="Label1"
        label="Inter-pulse delay"
        justify="Left"
        />
      <label loc="10 85" size="110 20"
        style="Label1"
        label="Observe Pulse"
        justify="Left"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="First pulse"
        justify="Left"
        />
      <menu loc="180 25" size="60 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 85" size="60 20"
        style="PlainText"
        label="menu"
        vq="pw"
        vc="parunits('set','pw','$VALUE')"
        set="parunits('get','pw'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="120 85" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="$ENABLE=(pw[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 65" size="60 20"
        style="PlainText"
        vq="d2"
        vc="vnmrunits('set','d2',$VALUE)"
        set="vnmrunits('get','d2'):$VALUE"
        show="$ENABLE=(d2[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="120 45" size="60 20"
        style="PlainText"
        vq="p1"
        vc="vnmrunits('set','p1',$VALUE)"
        set="vnmrunits('get','p1'):$VALUE"
        show="$ENABLE=(p1[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 25" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="$ENABLE=(d1[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
    </group>
  </group>
</template>
