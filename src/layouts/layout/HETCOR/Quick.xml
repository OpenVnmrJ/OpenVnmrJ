<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="675 260"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="360 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="375 0" size="240 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
    </group>
    <group loc="5 35" size="395 200"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="30 55" size="335 125"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="HETCORacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="20 25" size="110 20"
          style="Heading1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="20 5" size="140 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
        <menu loc="190 25" size="90 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1 gaussian"
          set="$VALUE=ni"
          bg=""
          editable="No"
          >
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <menu loc="190 5" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
        </menu>
      </group>
      <group loc="50 30" size="300 25"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="swentry"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <label loc="190 0" size="20 20"
          style="Heading1"
          label="to"
          justify="Center"
          />
        <entry loc="210 0" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq sw"
          vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
          set="invsetsw:$f1,$f2 $VALUE=$f1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="140 0" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq"
          vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
          set="invsetsw:$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
        <label loc="0 0" size="140 20"
          style="Label1"
          label="Spectral Width"
          justify="Left"
          />
        <label loc="265 0" size="35 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
      </group>
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="5 25" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
  </group>
</template>
