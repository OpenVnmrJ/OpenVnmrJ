<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="670 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="410 140" size="185 65"
      style="Heading1"
      label="Misc"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tangog"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 30" size="175 20"
        style="Label1"
        label="Start F1 with 1/dwell time"
        vq="f1180"
        vc="f1180='y'"
        vc2="f1180='n'"
        set="$VALUE=(f1180='y')"
        />
    </group>
    <group loc="410 25" size="185 115"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="90 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="90 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <label loc="155 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="210 25" size="195 180"
      style="Heading1"
      label="Gradients"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="xhggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="170 145" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="170 120" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="170 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="170 40" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="105 20" size="60 20"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 40" size="60 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="105 60" size="60 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 80" size="60 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="105 100" size="60 20"
        style="PlainText"
        vq="gzlvl3"
        vc="gzlvl3=$VALUE"
        set="$VALUE=gzlvl3"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 120" size="60 20"
        style="PlainText"
        vq="gt3"
        vc="gt3=$VALUE/1000"
        set="$VALUE=gt3*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="105 145" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 145" size="95 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="10 120" size="70 20"
        style="Label1"
        label="gt3"
        justify="Left"
        />
      <label loc="10 100" size="70 20"
        style="Label1"
        label="gzlvl3"
        justify="Left"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="gt2"
        justify="Left"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="gzlvl2"
        justify="Left"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="gt1"
        justify="Left"
        />
      <label loc="10 20" size="70 20"
        style="Label1"
        label="gzlvl1"
        justify="Left"
        />
    </group>
    <group loc="5 100" size="200 105"
      style="Heading1"
      label="C-C Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tangog"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="175 35" size="25 20"
        style="Heading1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 15" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <menu loc="10 75" size="160 20"
        style="PlainText"
        label="menu"
        vq="kappa"
        vc="kappa=$VALUE"
        set="$VALUE=kappa"
        editable="No"
        >
        <mlabel 
          label="SQ (chemical shifts)"
          chval="1"
          />
        <mlabel 
          label="DQ (traditional)"
          chval="0"
          />
      </menu>
      <label loc="10 55" size="110 20"
        style="Label1"
        label="C-C evolution:"
        justify="Left"
        />
      <entry loc="120 35" size="50 20"
        style="PlainText"
        vq="dofdec"
        vc="dofdec=$VALUE"
        set="$VALUE=dofdec"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 35" size="105 20"
        style="Label1"
        label="C13 dec offset"
        justify="Left"
        />
      <entry loc="120 15" size="50 20"
        style="PlainText"
        vq="jcc"
        vc="jcc=$VALUE"
        set="$VALUE=jcc"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 15" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="200 75"
      style="Heading1"
      label="One-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="175 25" size="25 20"
        style="Heading1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 45" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <textmessage loc="120 45" size="50 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <entry loc="120 25" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 25" size="95 20"
        style="Label1"
        label="Coupling const."
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="ADEQUATEAD"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
