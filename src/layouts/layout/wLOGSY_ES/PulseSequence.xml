<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="710 220"
    style="PlainText"
    label="Pulse Sequence"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="225 25" size="215 175"
      style="Heading1"
      label="Selective water refocusing"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="dpfgsegroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="120 60" size="60 20"
        style="PlainText"
        vq="wselpwr"
        vc="wselpwr=$VALUE"
        set="$VALUE=wselpwr"
        disable="Grayed out"
        />
      <entry loc="120 40" size="60 20"
        style="PlainText"
        vq="wselpw"
        vc="wselpw=$VALUE*1000"
        set="$VALUE=wselpw/1000"
        disable="Grayed out"
        />
      <label loc="10 120" size="110 20"
        style="PlainText"
        label="Gradient strength"
        justify="Left"
        />
      <button loc="10 100" size="170 20"
        style="Heading2"
        label="Recreate water shape"
        vc="make_wselshape"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <label loc="10 140" size="110 20"
        style="PlainText"
        label="Gradient time"
        justify="Left"
        />
      <label loc="185 140" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="120 120" size="60 20"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="120 140" size="60 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        disable="Grayed out"
        />
      <label loc="185 40" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="185 60" size="30 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <entry loc="80 80" size="100 20"
        style="PlainText"
        vq="wselshape"
        vc="wselshape='$VALUE'"
        set="$VALUE=wselshape"
        disable="Grayed out"
        />
      <label loc="30 40" size="60 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <label loc="30 60" size="60 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <label loc="30 80" size="50 20"
        style="Label1"
        label="shape"
        justify="Left"
        />
      <label loc="10 20" size="110 20"
        style="PlainText"
        label="Refocusing pulse"
        justify="Left"
        />
    </group>
    <group loc="5 110" size="215 90"
      style="Heading1"
      label="NOESY parameters"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="130 20" size="50 20"
        style="PlainText"
        vq="mixN"
        vc="mixN=$VALUE"
        set="$VALUE=mixN"
        disable="Grayed out"
        />
      <label loc="10 40" size="115 20"
        style="PlainText"
        label="Grad. stab. delay"
        justify="Left"
        />
      <label loc="10 60" size="120 20"
        style="PlainText"
        label="Homospoil strength"
        justify="Left"
        />
      <entry loc="130 60" size="50 20"
        style="PlainText"
        vq="gzlvlhs"
        vc="gzlvlhs=$VALUE"
        set="$VALUE=gzlvlhs"
        digits="0"
        disable="Grayed out"
        />
      <label loc="185 20" size="20 20"
        style="PlainText"
        label="s"
        justify="Left"
        />
      <entry loc="130 40" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        disable="Grayed out"
        />
      <label loc="185 40" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="10 20" size="100 20"
        style="PlainText"
        label="Mixing time"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="215 85"
      style="Heading1"
      label="Steady State Pulse"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="185 60" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="120 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul "
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' )"
        disable="Grayed out"
        />
      <entry loc="120 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' )"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="110 20"
        style="PlainText"
        label="Gradient time"
        justify="Left"
        />
      <label loc="10 40" size="110 20"
        style="PlainText"
        label="Gradient strength"
        justify="Left"
        />
      <check loc="10 20" size="145 20"
        style="PlainText"
        label="Steady State on/off"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' )"
        />
    </group>
    <textmessage loc="10 0" size="240 25"
      style="PlainText"
      label="wLOGSY_ES"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
