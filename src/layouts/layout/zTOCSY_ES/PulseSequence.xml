<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="655 230"
    style="PlainText"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 155" size="210 70"
      style="Heading2"
      label="Crusher Gradients"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="80 20" size="50 20"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        digits="0"
        />
      <entry loc="130 20" size="50 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        digits="0"
        />
      <entry loc="130 40" size="50 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="2"
        />
      <entry loc="80 40" size="50 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        digits="2"
        />
      <label loc="185 40" size="25 20"
        style="Label1"
        label="ms"
        />
      <label loc="5 40" size="75 20"
        style="PlainText"
        label="G-durations"
        justify="Left"
        />
      <label loc="5 20" size="60 20"
        style="PlainText"
        label="G-levels"
        justify="Left"
        />
    </group>
    <group loc="220 25" size="230 150"
      style="Heading2"
      label="ZQ Filter"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ZQfilter2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="205 100" size="25 20"
        style="Label1"
        label="dB"
        />
      <label loc="205 80" size="25 20"
        style="Label1"
        label="ms"
        />
      <entry loc="140 80" size="60 20"
        style="PlainText"
        vq="zqfpw2 Gzqfilt"
        vc="zqfpw2=$VALUE*1000"
        set="$VALUE=zqfpw2/1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="1"
        />
      <entry loc="140 100" size="60 20"
        style="PlainText"
        vq="zqfpwr2 Gzqfilt"
        vc="zqfpwr2=$VALUE"
        set="$VALUE=zqfpwr2"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="140 120" size="60 20"
        style="PlainText"
        vq="gzlvlzq2 Gzqfilt"
        vc="gzlvlzq2=$VALUE"
        set="$VALUE=gzlvlzq2"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="75 120" size="60 20"
        style="PlainText"
        vq="gzlvlzq1 Gzqfilt"
        vc="gzlvlzq1=$VALUE"
        set="$VALUE=gzlvlzq1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="75 100" size="60 20"
        style="PlainText"
        vq="zqfpwr1 Gzqfilt"
        vc="zqfpwr1=$VALUE"
        set="$VALUE=zqfpwr1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="75 80" size="60 20"
        style="PlainText"
        vq="zqfpw1 Gzqfilt"
        vc="zqfpw1=$VALUE*1000"
        set="$VALUE=zqfpw1/1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="1"
        />
      <entry loc="90 60" size="110 20"
        style="PlainText"
        vq="zqfpat2 Gzqfilt"
        vc="zqfpat2='$VALUE'"
        set="$VALUE=zqfpat2"
        show="$ENABLE=(Gzqfilt='y')"
        />
      <entry loc="75 40" size="110 20"
        style="PlainText"
        vq="zqfpat1 Gzqfilt"
        vc="zqfpat1='$VALUE'"
        set="$VALUE=zqfpat1"
        show="$ENABLE=(Gzqfilt='y')"
        />
      <label loc="10 120" size="60 20"
        style="PlainText"
        label="G-levels"
        justify="Left"
        />
      <label loc="10 100" size="50 20"
        style="PlainText"
        label="Powers"
        justify="Left"
        />
      <label loc="10 80" size="50 20"
        style="PlainText"
        label="Widths"
        justify="Left"
        />
      <label loc="10 40" size="50 20"
        style="PlainText"
        label="Shapes"
        justify="Left"
        />
      <check loc="10 20" size="130 20"
        style="Label1"
        label="ZQ Filter"
        vq="Gzqfilt"
        vc="Gzqfilt='y'"
        vc2="Gzqfilt='n'"
        set="$VALUE=(Gzqfilt='y')"
        />
    </group>
    <group loc="5 25" size="210 130"
      style="Heading2"
      label="SpinLock"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MLEV17c"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <button loc="90 60" size="40 20"
        style="Label3"
        label="Set"
        vc="setslgh2"
        bg="transparent"
        decor1="yes"
        />
      <entry loc="130 20" size="50 20"
        style="PlainText"
        vq="mixT"
        vc="mixT=$VALUE/1000"
        set="$VALUE=mixT*1000"
        disable="Grayed out"
        />
      <entry loc="130 60" size="50 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <entry loc="130 80" size="50 20"
        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <entry loc="130 100" size="50 20"
        style="PlainText"
        vq="slpwT"
        vc="slpwT=$VALUE"
        set="$VALUE=slpwT"
        disable="Grayed out"
        />
      <menu loc="90 40" size="90 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="DIPSI2"
          chval="dipsi2"
          />
        <mlabel 
          label="DIPSI3"
          chval="dipsi3"
          />
      </menu>
      <label loc="185 20" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="185 60" size="25 20"
        style="PlainText"
        label="Hz"
        justify="Left"
        />
      <label loc="185 80" size="25 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <label loc="185 100" size="25 20"
        style="PlainText"
        label="Âµs"
        justify="Left"
        />
      <label loc="10 60" size="55 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 40" size="65 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="Heading3"
        label="zTOCSY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="455 25" size="170 110"
      style="Heading2"
      label="Steady State"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <label loc="145 80" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="PlainText"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="PlainText"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="PlainText"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="PlainText"
        label="Gradient -"
        justify="Left"
        />
    </group>
  </group>
</template>
