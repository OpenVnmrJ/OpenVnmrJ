<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="620 195"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="HSQC"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="5 25" size="185 75"
      style="Heading1"
      label="One-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="105 25" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        disable="Grayed out"
        />
      <label loc="160 45" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="160 25" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <textmessage loc="105 45" size="50 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <label loc="10 25" size="95 20"
        style="Label1"
        label="Coupling const."
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
    </group>
    <group loc="370 25" size="190 75"
      style="Heading1"
      label="C12-H1 suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="c12hnull"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 45" size="175 25"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="birdnull"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="155 0" size="20 20"
          style="Heading1"
          label="s"
          justify="Left"
          />
        <entry loc="90 0" size="60 20"
          style="PlainText"
          vq="null nullflg"
          vc="null=$VALUE"
          set="$VALUE=null"
          show="if (nullflg = 'n') then $ENABLE=1 else $ENABLE=0 endif"
          disable="Grayed out"
          />
        <check loc="0 0" size="140 20"
          style="Label1"
          label="BIRD-delay"
          vq="nullflg null"
          vc="nullflg='n' if (null = 0) then null=0.35 endif"
          vc2="null=0"
          set="if (nullflg='n') and (null &gt; 0) then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <check loc="10 20" size="140 20"
        style="Label1"
        label="TANGO-Gradient"
        vq="nullflg"
        vc="nullflg='y'"
        vc2="nullflg='n'"
        set="$VALUE=(nullflg='y')"
        />
    </group>
    <group loc="195 25" size="170 130"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <check loc="10 100" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        show="$ENABLE=(PFGflg='n')*2-1"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="5 100" size="185 55"
      style="Heading1"
      label="XH Multiplicity"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="XHMultiplicity"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="10 25" size="165 20"
        style="PlainText"
        vq="mult"
        vc="mult=$VALUE"
        set="$VALUE=mult"
        editable="No"
        >
        <mlabel 
          label="No Edit"
          chval="0"
          />
        <mlabel 
          label="XH ONLY"
          chval="1"
          />
        <mlabel 
          label="XH/XH3 up, XH2 down"
          chval="2"
          />
      </menu>
    </group>
  </group>
</template>
