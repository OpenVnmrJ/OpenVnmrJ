<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="670 230"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <button loc="375 0" size="130 20"
      style="Heading2"
      label="Make Frequency List"
      vc="$cmd=seqfil lb=20 wft(1) peak:$a th=$a/6.0 htll lb='n' wft(1) {$cmd} "
      bg=""
      decor1=""
      />
    <entry loc="320 0" size="50 20"
      style="PlainText"
      vq="r7"
      vc="r7=$VALUE"
      set="$VALUE=r7"
      digits="0"
      disable="Grayed out"
      />
    <button loc="515 0" size="145 20"
      style="Heading2"
      label="Display Pulse Sequence"
      vc="dps"
      bg=""
      decor1=""
      />
    <button loc="110 0" size="210 20"
      style="Heading2"
      label="Import HT frequency list from exp"
      vc="importHT(r7)"
      bg=""
      decor1=""
      />
    <group loc="10 0" size="100 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="90 20"
        style="PlainText"
        label="gcosyHT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="230 120" size="205 105"
      style="Heading1"
      label="Solvent Suppression"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventSuppression"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="100 20" size="65 20"
        style="Label1"
        label="Presat"
        vq="satmode wet"
        vc="satmode='ynn' wet='n'"
        vc2="satmode='nnn'"
        set="$s='' substr(satmode,1,1):$s $VALUE=($s='y')"
        toolTip="Solvent presaturation ON / OFF Status"
        />
      <check loc="10 20" size="65 20"
        style="Label1"
        label="Wet"
        vq="satmode wet"
        vc="satmode='nnn' wet='y'"
        vc2="wet='n'"
        set="if(wet='y') then $VALUE=1 else $VALUE=0 endif"
        toolTip="WET Solvent suppression"
        />
      <label loc="160 80" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="160 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="160 40" size="30 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <entry loc="100 80" size="60 20"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="satfrq satmode"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Frequency"
        justify="Left"
        />
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        disable="Grayed out"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
    </group>
    <group loc="435 25" size="230 125"
      style="Heading1"
      label="Gradients"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 100" size="60 20"
        style="Label1"
        label="sspul"
        vq="sspul"
        vc="sspul='y' "
        vc2="sspul='n'"
        set="if( sspul = 'y') then $VALUE=1 else $VALUE=0 endif"
        toolTip="Suppress steady state magnetization"
        />
      <label loc="190 80" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 80" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="3"
        />
      <label loc="100 80" size="40 20"
        style="Label1"
        label="gstab"
        />
      <label loc="190 60" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 60" size="50 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="3"
        />
      <label loc="115 60" size="30 20"
        style="Label1"
        label="gt2"
        />
      <label loc="190 40" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 40" size="50 20"
        style="PlainText"
        vq="gt0"
        vc="gt0=$VALUE/1000"
        set="$VALUE=gt0*1000"
        digits="3"
        />
      <label loc="140 20" size="50 20"
        style="Label1"
        label="Duration"
        />
      <label loc="115 40" size="30 20"
        style="Label1"
        label="gt0"
        />
      <label loc="50 20" size="50 20"
        style="Label1"
        label="Level"
        />
      <label loc="5 40" size="45 20"
        style="Label1"
        label="gzlvl0"
        />
      <entry loc="50 40" size="60 20"
        style="PlainText"
        vq="gzlvl0"
        vc="gzlvl0=$VALUE"
        set="$VALUE=gzlvl0"
        />
      <entry loc="50 60" size="60 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        />
      <label loc="5 60" size="45 20"
        style="Label1"
        label="gzlvl2"
        />
    </group>
    <group loc="230 25" size="205 95"
      style="Heading1"
      label="Observe pulse"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventSuppression"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="160 20" size="30 20"
        style="Heading1"
        label="us"
        justify="Left"
        />
      <entry loc="95 20" size="60 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE"
        set="$VALUE=pw"
        digits="2"
        disable="Grayed out"
        />
      <label loc="10 20" size="70 20"
        style="Label1"
        label="Pulse width"
        justify="Left"
        />
      <entry loc="140 65" size="60 20"
        style="PlainText"
        vq="compH"
        vc="compH=$VALUE"
        set="$VALUE=compH"
        digits="4"
        disable="Grayed out"
        />
      <label loc="10 65" size="125 20"
        style="Label1"
        label="Ampl Compression"
        justify="Left"
        />
      <label loc="160 41" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="10 41" size="70 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="95 41" size="60 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        digits="0"
        disable="Grayed out"
        />
    </group>
    <group loc="5 25" size="225 200"
      style="Heading1"
      label="Hadamard Encoding"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventSuppression"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 110" size="140 20"
        style="Label1"
        label="Show Pbox reports"
        vq="repflg"
        vc="repflg='y' "
        vc2="repflg='h'"
        set="if( repflg = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <button loc="35 145" size="140 20"
        style="Heading2"
        label="Edit HT Frequencies"
        vc="editht('show')"
        bg=""
        decor1=""
        />
      <check loc="5 90" size="190 20"
        style="Label1"
        label="Bloch-Siegert Correction"
        vq="bscor"
        vc="bscor='y' "
        vc2="bscor='n'"
        set="if( bscor = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="140 65" size="50 20"
        style="PlainText"
        vq="htofs1"
        vc="htofs1=$VALUE"
        set="$VALUE=htofs1"
        digits="0"
        />
      <label loc="5 65" size="130 20"
        style="Label1"
        label="Skip first increments:"
        />
      <entry loc="100 45" size="50 20"
        style="PlainText"
        vq="htbw1"
        vc="htbw1=$VALUE"
        set="$VALUE=htbw1"
        digits="0"
        />
      <label loc="5 45" size="70 20"
        style="Label1"
        label="Bandwidth"
        />
      <menu loc="100 20" size="100 20"
        style="PlainText"
        label="menu"
        vq="pshape"
        vc="pshape='$VALUE'"
        set="$VALUE=pshape"
        editable="No"
        >
        <mlabel 
          label="sinc90"
          chval="sinc90"
          />
        <mlabel 
          label="gaus90"
          chval="gaus90"
          />
        <mlabel 
          label="square90"
          chval="square90"
          />
      </menu>
      <label loc="155 45" size="30 20"
        style="Label1"
        label="Hz"
        />
      <label loc="5 20" size="110 20"
        style="Label1"
        label="HT Waveform"
        />
    </group>
  </group>
</template>
