<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="550 165"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <textmessage loc="10 0" size="170 25"
      style="PlainText"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
    <group loc="260 25" size="140 65"
      style="Heading1"
      label="Homospoil"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="105 30" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="50 30" size="50 20"
        style="PlainText"
        vq="hst hs"
        vc="hst=$VALUE*.001"
        set="$VALUE=hst*1000"
        show="$ENABLE=(hs='y' or hs='yy' or hs='ny' or hs='yn')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 30" size="40 20"
        style="Label1"
        label="Time"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="250 115"
      style="Heading1"
      label="Excitation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Excitation"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="180 45" size="60 20"
        style="PlainText"
        label="menu"
        vq="p1"
        vc="parunits('set','p1','$VALUE')"
        set="parunits('get','p1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 65" size="60 20"
        style="PlainText"
        vq="d2"
        vc="parunits('set','d2','$VALUE')"
        set="parunits('get','d2'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <label loc="10 65" size="110 20"
        style="Label1"
        label="Inter-pulse delay"
        justify="Left"
        />
      <label loc="10 85" size="110 20"
        style="Label1"
        label="Observe Pulse"
        justify="Left"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="First pulse"
        justify="Left"
        />
      <menu loc="180 25" size="60 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 85" size="60 20"
        style="PlainText"
        label="menu"
        vq="pw"
        vc="parunits('set','pw','$VALUE')"
        set="parunits('get','pw'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="120 85" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="$ENABLE=(pw[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 65" size="60 20"
        style="PlainText"
        vq="d2"
        vc="vnmrunits('set','d2',$VALUE)"
        set="vnmrunits('get','d2'):$VALUE"
        show="$ENABLE=(d2[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="120 45" size="60 20"
        style="PlainText"
        vq="p1"
        vc="vnmrunits('set','p1',$VALUE)"
        set="vnmrunits('get','p1'):$VALUE"
        show="$ENABLE=(p1[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 25" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="$ENABLE=(d1[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
    </group>
  </group>
</template>
