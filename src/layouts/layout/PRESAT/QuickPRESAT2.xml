<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="QuickPRESAT2" element="pages" type="acquisition" >
  <group size="675 310"
    style="Heading1"
    label="default"
    vq="panellevel"
    show="$SHOW=(panellevel&lt;30)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="QuickPRESAT2"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <label loc="75 20" size="70 20"
        style="Heading1"
        label="Autogain:"
        justify="Right"
        />
      <label loc="10 0" size="135 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Right"
        />
      <check loc="150 20" size="20 20"
        style="Label1"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="150 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <group loc="10 40" size="215 180"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="0 155" size="215 20"
          style="Heading1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <textmessage loc="15 105" size="190 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <check loc="125 25" size="85 20"
          style="Label1"
          label=" Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="125 5" size="85 20"
          style="Label1"
          label=" Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="5 5" size="125 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq :'"
          />
        <textmessage loc="5 25" size="115 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq :'"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
    <group loc="5 35" size="395 265"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="5 0" size="385 260"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="PRESATacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <group loc="5 5" size="375 100"
          vq="wds"
          show="$SHOW=(wds='')"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="PRESATacq"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <group loc="25 0" size="305 40"
            bg=""
            border="None"
            tab="no"
            enable="no"
            reference="swentry"
            useref="no"
            subtype="Convenience"
            expanded="yes"
            >
            <menu loc="145 0" size="120 20"
              style="PlainText"
              label="menu"
              vq="obsSW tn"
              vc="exec('$VALUE')"
              set="$VALUE=''"
              show="$ENABLE=(tn='H1')*2-1"
              editable="No"
              >
              <mlabel 
                label="-2  -&gt;  14"
                chval="setsw(14,-2)"
                />
              <mlabel 
                label="-1  -&gt;  11"
                chval="setsw(11,-1)"
                />
              <mlabel 
                label="-2  -&gt;  16"
                chval="setsw(16,-2)"
                />
              <mlabel 
                label="-0.5  -&gt;  9.5"
                chval="setsw(9.5,-0.5)"
                />
              <mlabel 
                label="0.5  -&gt;  8.5"
                chval="setsw(8.5,0.5)"
                />
            </menu>
            <label loc="270 20" size="35 20"
              style="Label1"
              label="ppm"
              justify="Left"
              />
            <label loc="270 0" size="35 20"
              style="Label1"
              label="ppm"
              justify="Left"
              />
            <entry loc="215 20" size="50 20"
              style="PlainText"
              vq="rfl rfp reffrq sw"
              vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
              set="invsetsw:$f1,$f2 $VALUE=$f1"
              digits="1"
              disable="Grayed out"
              />
            <label loc="195 20" size="20 20"
              style="Label1"
              label="to:"
              justify="Left"
              />
            <entry loc="145 20" size="50 20"
              style="PlainText"
              vq="rfl rfp reffrq"
              vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
              set="invsetsw:$f1,$f2 $VALUE=$f2"
              digits="1"
              disable="Grayed out"
              />
            <label loc="55 0" size="90 20"
              style="Label1"
              label="PROTON sw:"
              justify="Right"
              />
            <label loc="12 20" size="130 20"
              style="Label1"
              label="(...or enter):"
              justify="Right"
              />
          </group>
          <group loc="5 42" size="275 20"
            vq="doscout"
            bg=""
            border="None"
            tab="no"
            enable="no"
            useref="no"
            subtype="Convenience"
            >
            <check loc="180 0" size="90 20"
              style="Label1"
              label="Select Later"
              vq="doscout customflag wqinit"
              vc="doscout=1 psatsolv=''"
              vc2="doscout=0"
              set="$VALUE=doscout"
              show="on('doscout'):$ison if (customflag='y') and ((wqinit&lt;&gt;'acq') or (wqinit&lt;&gt;'addq')) then $ENABLE=-1 else if ($ison&gt;0) then $ENABLE=1 else $ENABLE=-1 endif endif "
              />
            <menu loc="85 0" size="90 20"
              style="PlainText"
              label="menu"
              vq="psatsolv  doscout  tn wetpeaks"
              vc="if ((doscout=0) and (tn='H1')) then psatsolv='$VALUE' endif"
              set="$VALUE=psatsolv"
              show="on('doscout'):$ison on('wetpeaks'):$weton if ($ison&gt;0) and ($weton=0) then $ENABLE=((doscout=0) and (tn='H1'))*2-1 else $ENABLE=-1 endif"
              editable="No"
              >
              <mlabel 
                label="HDO/D2O"
                chval="d2o"
                />
              <mlabel 
                label="wet DMSO"
                chval="dmsowet"
                />
              <mlabel 
                label="90% H2O"
                chval="h2o"
                />
            </menu>
            <label loc="15 0" size="70 20"
              style="Label1"
              label="Saturate?:"
              justify="Left"
              />
          </group>
          <check loc="285 42" size="85 20"
            style="Label1"
            label="Do Scout"
            vq="doscout"
            vc="on('doscout'):$ison if ($ison &gt; -1) then on('doscout') procdim=0 wstart='PRESATproc(`wstart`)' endif"
            vc2="on('doscout'):$ison if ($ison &gt; -1) then off('doscout') wstart='' endif"
            set="on('doscout'):$ison if ($ison&gt;0) then $VALUE=1 else $VALUE=0 endif"
            show="on('doscout'):$ison if ($ison&gt; -1) then $ENABLE=1 else $ENABLE=0 endif"
            />
          <group loc="55 62" size="295 20"
            vq="doscout"
            show="on('doscout'):$ison if ($ison) then $SHOW=(doscout=0) else $SHOW=0 endif"
            bg=""
            border="None"
            tab="no"
            enable="no"
            reference="wetpeaks"
            useref="no"
            subtype="Convenience"
            expanded="yes"
            >
            <label loc="165 0" size="115 20"
              style="Heading1"
              label="tallest peaks"
              />
            <label loc="5 0" size="30 20"
              style="Label1"
              label="OR"
              />
            <entry loc="132 0" size="30 20"
              style="PlainText"
              vq="wetpeaks"
              vc="on('wetpeaks'):$ison if ($ison) then wetpeaks=$VALUE else wetpeaks=wetpeaks off('wetpeaks') endif"
              set="$VALUE=wetpeaks"
              show="on('wetpeaks'):$ENABLE"
              digits="0"
              />
            <check loc="37 0" size="90 20"
              style="Label1"
              label="   Suppress.."
              vq="wetpeaks"
              vc="on('wetpeaks')"
              vc2="wetpeaks=0 off('wetpeaks')"
              set="on('wetpeaks'):$VALUE"
              />
          </group>
        </group>
        <group loc="55 85" size="215 25"
          vq="customflag"
          show="$SHOW=(customflag='y')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="minswshow"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <menu loc="120 0" size="85 20"
            style="PlainText"
            vq="MinSW"
            vc="MinSW='$VALUE'"
            set="$VALUE=MinSW"
            editable="No"
            >
            <mlabel 
              label="Auto"
              chval="auto"
              />
            <mlabel 
              label="Manual"
              chval="manual"
              />
            <mlabel 
              label="Off"
              chval="off"
              />
          </menu>
          <label loc="10 0" size="100 20"
            style="Label1"
            label="Minimize SW?"
            />
        </group>
        <label loc="75 145" size="65 20"
          style="Label1"
          label="PURGE"
          justify="Right"
          />
        <menu loc="175 145" size="70 20"
          style="PlainText"
          vq="prgflg prgcycle"
          vc="prgcycle=$VALUE"
          set="$VALUE=prgcycle"
          show="$ENABLE=(prgflg='y')*2-1"
          editable="No"
          >
          <mlabel 
            label="2-step"
            chval="2"
            />
          <mlabel 
            label="4-step"
            chval="4"
            />
        </menu>
        <check loc="150 145" size="30 20"
          style="Label1"
          vq="prgflg"
          vc="prgflg='y'"
          vc2="prgflg='n'"
          set="$VALUE=(prgflg='y')"
          />
        <group loc="25 170" size="345 85"
          vq="procdim wds"
          vc="crmode='b' ds"
          vc2="clear"
          show="$SHOW=((wds&lt;&gt;'') and (procdim=1))"
          bg=""
          border="LoweredBevel"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="ds_selfrq"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <button loc="45 60" size="80 20"
            style="Heading2"
            label="CLEAR"
            vc="selfrqc=0 selfrqw=0 off('selfrqc') ds"
            decor1=""
            />
          <button loc="135 60" size="80 20"
            style="Heading2"
            label="Select"
            vc="setselfrqc"
            decor1=""
            />
          <button loc="225 60" size="80 20"
            style="Heading2"
            label="ShowList"
            vq="selfrqc"
            vc="showfreq('satfrq')"
            show="on('selfrqc'):$ENABLE"
            decor1=""
            />
          <check loc="190 21" size="135 20"
            style="Label1"
            label="Multi-Freq PRESAT"
            vq="slpsat rfwg"
            vc="isrfwg:$rfwg if ($rfwg&gt;0)  then slpsat='y' selfrqc=0 off('selfrqc') satfrq=tof else slpsat='n' endif"
            vc2="slpsat='n' "
            set="$VALUE=(slpsat='y')"
            show="isrfwg:$rfwg $ENABLE=$rfwg"
            />
          <check loc="20 21" size="135 20"
            style="Label1"
            label="Single-Freq PRESAT"
            vq="slpsat"
            vc="slpsat='n'"
            vc2="isrfwg:$rfwg if ($rfwg&gt;0) then slpsat='y' selfrqc=0 off('selfrqc') satfrq=tof else slpsat='n' endif"
            set="$VALUE=(slpsat='n')"
            />
          <label loc="45 1" size="245 20"
            style="Label1"
            label="Select Presaturation Band  using  cusor(s)"
            justify="Center"
            />
          <button loc="225 41" size="80 20"
            style="Heading2"
            label="Nearest Line"
            vc="nl"
            decor1=""
            />
          <button loc="135 41" size="80 20"
            style="Heading2"
            label="Expand/Full"
            vc="ds('expand')"
            decor1=""
            />
          <button loc="45 41" size="80 20"
            style="Heading2"
            label="Box/Cursor"
            vc="ds('toggle')"
            decor1=""
            />
        </group>
        <label loc="60 105" size="115 20"
          style="Label1"
          label="Number of scans:"
          justify="Left"
          />
        <menu loc="175 105" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
        </menu>
        <label loc="245 125" size="60 20"
          style="Label1"
          label="sec"
          justify="Left"
          />
        <menu loc="175 125" size="70 20"
          style="PlainText"
          label="menu"
          vq="satdly"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          editable="No"
          >
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="1.5"
            chval="1.5"
            />
          <mlabel 
            label="2.5"
            chval="2.5"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="10"
            chval="10"
            />
        </menu>
        <label loc="45 125" size="120 20"
          style="Label1"
          label="Presaturation Delay:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 5" size="610 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="5 0" size="310 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="330 0" size="270 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
  </group>
</template>
