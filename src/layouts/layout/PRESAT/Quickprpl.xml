<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quickprpl" element="pages" type="acquisition" >
  <group size="685 240"
    style="Heading1"
    label="ProcPlotAdv"
    vq="customflag panellevel"
    show="$SHOW=((customflag='y') and (panellevel&gt;29))"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quickprpl"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="445 165"
      vq="arraydim"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="prpl2d-hoph"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="0 5" size="445 115"
        vq="arraydim"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="prpl1D"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="5 5" size="215 95"
          style="Heading1"
          label="Process Options"
          bg=""
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="proc1d-ez"
          useref="no"
          subtype="Titled"
          expanded="yes"
          >
          <menu loc="105 40" size="100 20"
            style="PlainText"
            label="menu"
            vc="exec('$VALUE')"
            set="$VALUE=''"
            bg=""
            editable="No"
            >
            <mlabel 
              label="none"
              chval="lb='n' gf='n' gfs='n' sb='n' sbs='n' awc='n'"
              />
            <mlabel 
              label="exponential"
              chval="expfilt('acq')"
              />
            <mlabel 
              label="gaussian"
              chval="gaussian('acq')"
              />
            <mlabel 
              label="cosine"
              chval="cosine('acq')"
              />
            <mlabel 
              label="sq-cosine"
              chval="sqcosine('acq')"
              />
            <mlabel 
              label="res-enhance"
              chval="resenhance('acq')"
              />
          </menu>
          <menu loc="105 20" size="100 20"
            style="PlainText"
            label="menu"
            vq="fn"
            vc="fn=$VALUE"
            set="on('fn'):$on if($on&gt;0.5) then $VALUE=fn else $VALUE='n' endif"
            bg=""
            editable="No"
            >
            <mlabel 
              label="Default"
              chval="n"
              />
            <mlabel 
              label="16k"
              chval="16384"
              />
            <mlabel 
              label="32k"
              chval="32768"
              />
            <mlabel 
              label="64k"
              chval="65536"
              />
            <mlabel 
              label="128k"
              chval="131072"
              />
            <mlabel 
              label="256k"
              chval="262144"
              />
          </menu>
          <label loc="10 40" size="95 20"
            style="Label1"
            label="Weighting"
            justify="Left"
            />
          <label loc="10 20" size="95 20"
            style="Label1"
            label="Transform size"
            justify="Left"
            />
        </group>
        <group loc="225 5" size="215 95"
          style="Heading1"
          label="Plot Options"
          bg=""
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="plot1d-ez"
          useref="no"
          subtype="Titled"
          expanded="yes"
          >
          <menu loc="105 60" size="100 20"
            style="PlainText"
            label="Peak Pick"
            vq="pkpick"
	    vc="$pkpick='$VALUE' if ($pkpick='none') then pkpick='' dpf('off') else pkpick=$pkpick endif"
            set="$VALUE=pkpick"
            bg=""
            editable="No"
            >
            <mlabel 
              label="On Peaks"
              chval="ppf"
              />
            <mlabel 
              label="List, at Left"
              chval="pll"
              />
            <mlabel 
              label="List, at Right"
              chval="pll(wc-50)"
              />
            <mlabel 
              label="None"
              chval="none"
              />
          </menu>
          <menu loc="105 40" size="100 20"
            style="PlainText"
            label="Integral"
            vq="intmod"
            vc="intmod='$VALUE'"
            set="$VALUE=intmod"
            bg=""
            editable="No"
            >
            <mlabel 
              label="Partial"
              chval="partial"
              />
            <mlabel 
              label="Full"
              chval="full"
              />
            <mlabel 
              label="Off"
              chval="off"
              />
          </menu>
          <menu loc="105 20" size="100 20"
            style="PlainText"
            label="menu"
            vq="parstyle"
            vc="parstyle='$VALUE'"
            set="$VALUE=parstyle"
            bg=""
            editable="No"
            >
            <mlabel 
              label="Full"
              chval="pap"
              />
            <mlabel 
              label="Basic"
              chval="ppa"
              />
            <mlabel 
              label="Text only"
              chval="pltext"
              />
          </menu>
          <label loc="10 60" size="80 20"
            style="Label1"
            label="Peak Values"
            justify="Left"
            />
          <label loc="10 40" size="70 20"
            style="Label1"
            label="Integrals"
            justify="Left"
            />
          <label loc="10 20" size="100 20"
            style="Label1"
            label="Plot Parameters"
            justify="Left"
            />
        </group>
      </group>
    </group>
    <group loc="450 35" size="215 190"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar6"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="0 5" size="215 175"
        style="Heading1"
        label="Advanced Options"
	vq="seqfil pslabel customflag"
	show="$SHOW=((seqfil&lt;&gt;'s2pul') and (seqfil&lt;&gt;'APT') and (seqfil&lt;&gt;'DEPT') and (seqfil&lt;&gt;'s2pulecho'))"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <button loc="60 145" size="145 20"
          style="Heading2"
          label="Update curpar"
          vq="chreqexp"
          vc="customizepar('update')"
          show="$ENABLE=(chreqexp&gt;0)*2-1"
          bg=""
          decor1=""
          />
        <filemenu loc="10 105" size="195 20"
          style="PlainText"
          vq="strtpar curexp chreqexp"
          vc="strtpar='$VALUE' customizepar('resetreqexp')"
          set="$VALUE=strtpar"
          show="$ENABLE=(chreqexp&gt;0)*2-1"
          file="$VALUE=curexp+'/curFIDlist'"
          type="file"
          bg=""
          editable="No"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <label loc="10 65" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <label loc="10 25" size="130 20"
          style="Label1"
          label="Receiver Gain (dB):"
          justify="Left"
          />
        <textmessage loc="140 25" size="55 20"
          style="PlainText"
          label="30"
          vq="gain"
          set="$VALUE=gain"
          digits="0"
          />
	<group loc="10 45" size="190 20"
          vq="pslabel"
          show="$SHOW=(pslabel='PRESAT')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <label loc="0 0" size="130 20"
            style="Heading1"
            label="Presat gammaH2:"
            justify="Left"
            />
          <entry loc="130 0" size="35 20"
            style="PlainText"
            vq="presatgh2"
	    vc="presatgh2=$VALUE setpower(presatgh2,tn):satpwr"
            set="$VALUE=presatgh2"
            digits="0"
            />
          <label loc="170 0" size="20 20"
            style="Label1"
            label="Hz"
            justify="Left"
            />
	</group>
        <check loc="100 65" size="70 20"
          style="Label1"
          label="Change"
          vq="chreqexp"
          vc="chreqexp=1"
          vc2="chreqexp=0"
          set="$VALUE = chreqexp"
          />
        <textmessage loc="10 85" size="200 20"
          style="PlainText"
          label="wet1D PRESAT PROTON"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
        <group loc="5 125" size="205 20"
          vq="chreqexp REQexp strtpar"
          show="$SHOW=(chreqexp&gt;1)"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <entry loc="55 0" size="145 20"
            style="PlainText"
            vq="REQexp chreqexp"
            vc="REQexp='$VALUE' customizepar('resetreqexp')"
            set="$VALUE=REQexp"
            />
          <label loc="5 0" size="50 20"
            style="Label1"
            label="Other:"
            />
        </group>
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="homcordisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="345 25"
        style="PlainText"
        label="Experiment: NOESY1D   Solvent: dmso"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="360 0" size="255 25"
        style="PlainText"
        label="H1-H1 Correlation     [ Decoupler: C13 ]"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
  </group>
</template>
