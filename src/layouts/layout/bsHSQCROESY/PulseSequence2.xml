<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence2" element="pages" type="acquisition" >
  <group size="625 235"
    style="Heading1"
    label="Pulse Sequence.."
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence2"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="195 115" size="185 50"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="impress"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 10" size="85 30"
        style="Label1"
        label="IMPRESS"
        vq="impress"
        vc="impress=1"
        vc2="impress=0 imphase=1"
        set="$VALUE=impress"
        show="$ENABLE=-1"
        />
    </group>
    <group loc="195 25" size="185 85"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="160 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
    <group loc="5 70" size="185 95"
      style="Heading1"
      label="Z Filter"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ZQfilter"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="10 60" size="60 20"
        style="Heading1"
        label="G- level"
        justify="Left"
        />
      <label loc="135 40" size="30 20"
        style="Label1"
        label="ms"
        />
      <entry loc="70 60" size="60 20"
        style="PlainText"
        vq="gzlvlz"
        vc="gzlvlz=$VALUE"
        set="$VALUE=gzlvlz"
        show="$ENABLE=(zfilt='y')"
        digits="0"
        />
      <entry loc="70 40" size="60 20"
        style="PlainText"
        vq="gtz"
        vc="gtz=$VALUE/1000"
        set="$VALUE=gtz*1000"
        show="$ENABLE=(zfilt='y')"
        />
      <label loc="10 40" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <check loc="10 20" size="115 20"
        style="Label1"
        label="PFG Purge"
        vq="zfilt"
        vc="zfilt='y'"
        vc2="zfilt='n'"
        set="$VALUE=(zfilt='y')"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="bsHSQCROESY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="5 25" size="185 45"
      style="Heading1"
      label="C12-H1 suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="c12hnull"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 20" size="140 20"
        style="Label1"
        label="TANGO-Gradient"
        vq="nullflg"
        vc="nullflg='y'"
        vc2="nullflg='n'"
        set="$VALUE=(nullflg='y')"
        />
    </group>
  </group>
</template>
