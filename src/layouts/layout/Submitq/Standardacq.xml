<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standardacq" element="pages" type="sample" >
  <group size="878 293"
    style="Heading1"
    label="Standard"
    vq="customflag wqinit clustermod"
    show="$VALUE=((wqinit='acq') and (clustermod='no'))"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standardacq"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="375 257" size="438 30"
      vq="wshim"
      show="$SHOW=(wshim&lt;&gt;'n')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <filemenu loc="130 5" size="295 20"
        style="PlainText"
        vq="method Gmapname"
        vc="exec('$VALUE')"
        set="$VALUE=`method='`+method+`' Gmapname='`+Gmapname+`'`"
        file="exists('shimmenuInfo','templates/vnmrj/choicefiles'):$r,$VALUE"
        type="file"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 5" size="115 20"
        style="Label1"
        label="Select shim menu"
        />
    </group>
    <group loc="10 30" size="620 220"
      style="Heading1"
      label="Sample information"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SampleInfo"
      useref="yes"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      >
    </group>
    <group loc="642 139" size="210 81"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <menu loc="50 25" size="140 20"
        style="PlainText"
        label="menu"
        vq="alock"
        vc="$alock='$VALUE' if (Console='mercury') and ($alock='u') then $alock='n' endif alock=$alock"
        set="if (alock&lt;&gt;'n') and (alock&lt;&gt;'u') then $VALUE='s' else $VALUE=alock endif"
        editable="No"
        >
        <mlabel 
          label="Yes (alock&lt;&gt;n)"
          chval="s"
          />
        <mlabel 
          label="No (alock=n)"
          chval="n"
          />
        <mlabel 
          label="Unlocked (alock=u)"
          chval="u"
          />
      </menu>
      <label loc="5 25" size="45 20"
        style="Label1"
        label="Lock?"
        justify="Left"
        />
      <label loc="25 5" size="160 20"
        style="Label1"
        label="Before First Exp"
        justify="Center"
        />
      <check loc="30 46" size="60 20"
        style="Label1"
        label="Shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="$VALUE=(wshim&lt;&gt;'n')"
        />
      <check loc="120 46" size="60 20"
        style="Label1"
        label="Tune"
        vq="ptuneatsample wtune atune tunemethod"
        vc="strstr(ptuneatsample,'auto'):$wk if ($wk) then ptuneatsample='autowkup' else ptuneatsample='wkup' endif"
        vc2="strstr(ptuneatsample,'auto'):$wk if ($wk) then ptuneatsample='auto' else ptuneatsample='no' endif"
        set="strstr(ptuneatsample,'wkup'):$ato $VALUE=($ato&gt;0)"
        show="$ok='no' checkprotune('global'):$ok $ENABLE=($ok='yes')*2-1"
        />
    </group>
    <group loc="642 38" size="210 96"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="90 8" size="102 20"
        style="Label1"
        label="Autoplot"
        vq="pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <label loc="5 7" size="80 20"
        style="Label1"
        label="After EXP"
        justify="Left"
        />
      <label loc="5 34" size="50 20"
        style="Heading1"
        label="plotter"
        justify="Left"
        />
      <filemenu loc="60 34" size="140 20"
        style="PlainText"
        vq="userplotter userdir"
        vc="userplotter='$VALUE'"
        set="$VALUE=userplotter"
        file="$VALUE=userdir+'/persistence/plotters'"
        type="file"
        bg="VJBackground"
        editable="No"
        vq2="userplotter"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <group loc="1 63" size="200 20"
        vq="emailflag"
        show="$SHOW=(emailflag='y')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="BeforeFirstExperiment"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <check loc="90 0" size="90 20"
          style="Label1"
          label="e-message"
          vq="emessage"
          vc="emessage='y'"
          vc2="emessage='n'"
          set="$VALUE=(emessage='y')"
          />
        <label loc="5 0" size="80 20"
          style="Label1"
          label="After Queue"
          justify="Left"
          />
      </group>
    </group>
    <textmessage loc="9 258" size="354 20"
      style="PlainText"
      vq="daytime nighttime"
      set="$VALUE='Study time: '+daytime"
      />
    <group loc="10 5" size="300 20"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="180 20"
        style="PlainText"
        vq="operator"
        set="$VALUE='      Operator : '+operator"
        />
    </group>
  </group>
</template>
