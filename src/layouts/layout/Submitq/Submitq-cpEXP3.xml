  <group size="810 280"
    style="Heading1"
    label="EXPnew"
    vq="PFGflg panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Submitq-cpEXP3"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 200" size="545 50"
      bg=""
      border="Etched"
      side="Top"
      vq="sqflag appmode"
      show="$SHOW=((appmode='walkup') and (sqflag='n'))"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampqinfo"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="10 5" size="525 20"
        style="Heading1"
        label="Use RebuildSQ button to display/update/use studyQ"
        justify="Center"
        />
      <label loc="10 25" size="525 20"
        style="Label1"
        label="[The studyQ can be used to customize/delete experiments or rearrange queue]"
        justify="Center"
        />
    </group>
    <group loc="10 210" size="545 55"
      bg=""
      border="Etched"
      side="Top"
      vq="appmode"
      show="$SHOW=(appmode&lt;&gt;'walkup')"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampqinfo"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
