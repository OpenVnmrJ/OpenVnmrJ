          />
        <check loc="10 25" size="90 20"
          style="Label1"
          label="Short/Day Q"
          vq="longQ"
          vc="longQ='n' seqfil=seqfil"
          vc2="longQ='y' seqfil=seqfil"
          set="$VALUE=(longQ='n')"
          />
        <check loc="110 25" size="95 20"
          style="Label1"
          label="Long/Night Q"
          vq="longQ"
          vc="longQ='y' seqfil=seqfil"
          vc2="longQ='n' seqfil=seqfil"
          set="$VALUE=(longQ='y')"
          />
      </group>
    </group>
    <group loc="10 5" size="400 30"
