<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standardcluster" element="pages" type="sample" >
  <group size="840 305"
    style="Heading1"
    label="cluster"
    vq="customflag wqinit clustermod"
    show="$VALUE=(clustermod='yes')"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standardcluster"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <textmessage loc="35 5" size="200 20"
      style="PlainText"
      label="Operator : vnmr1"
      vq="operator"
      set="$VALUE='     Operator : '+operator"
      />
    <group loc="5 30" size="370 215"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampinfo"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="100 45" size="110 20"
        style="PlainText"
        vq=" notebook"
        vc=" notebook='$VALUE'"
        set="$VALUE=notebook"
        disable="Grayed out"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="Notebook:"
        justify="Left"
        />
      <textfile loc="5 120" size="360 90"
        style="PlainText"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <entry loc="100 65" size="260 20"
        style="PlainText"
        vq="emailaddr "
        vc="emailaddr='$VALUE'"
        set="$VALUE=emailaddr"
        disable="Grayed out"
        />
      <label loc="10 65" size="90 20"
        style="Label1"
        label="e-Address"
        justify="Left"
        />
      <entry loc="255 45" size="105 20"
        style="PlainText"
        vq=" page"
        vc=" page='$VALUE'"
        set="$VALUE=page"
        disable="Grayed out"
        />
      <label loc="215 45" size="40 20"
        style="Label1"
        label="Page:"
        justify="Left"
        />
      <group loc="235 5" size="130 20"
        vq="userremembrance"
        show="$SHOW=(userremembrance='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="userrem"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <button loc="2 0" size="120 20"
          style="Heading2"
          label="Use My Prefs"
          vc="loaduserprefs"
          show="isnewexp:$isnew $ENABLE=($isnew&gt;0)*2-1"
          bg=""
          decor1=""
          />
      </group>
      <entry loc="100 5" size="130 20"
        style="PlainText"
        vq=" samplename"
        vc=" samplename='$VALUE' setsamplename"
        set="$VALUE=samplename"
        disable="Grayed out"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="ClusterDir:"
        justify="Left"
        />
      <textmessage loc="100 25" size="260 20"
        style="PlainText"
        vq="sample sdirtmplt adirtmplt samplename"
        set="if (sample='') then $as='' $s='' Svfname(adirtmplt+'/'+sdirtmplt,''):$as shell('basename '+$as):$s $VALUE='('+$s+')' else $VALUE=sample endif"
        />
      <label loc="5 95" size="100 20"
        style="Label1"
        label="Comment"
        justify="Left"
        />
      <label loc="10 5" size="90 20"
        style="Label1"
        label="Cluster Name:"
        justify="Left"
        />
    </group>
  </group>
</template>
