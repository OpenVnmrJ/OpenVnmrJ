<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standardauto" element="pages" type="sample" >
  <group size="1075 290"
    style="Heading1"
    label="standard"
    vq="wqinit clustermod"
    show="$VALUE=((wqinit&lt;&gt;'acq') and (clustermod='no'))"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standardauto"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 30" size="620 220"
      style="Heading1"
      label="Sample information"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SampleInfo"
      useref="yes"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      >
    </group>
    <group loc="840 5" size="230 215"
	style="Heading1"
      vq="wqinit"
      bg="transparent"
      border="Etched"
	label="Status"
      tab="no"
      enable="no"
      reference="daynight"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textfile loc="5 90" size="220 120"
        style="Info"
        vq="userdir NextLoc"
        set="$VALUE=userdir+'/persistence/autotimeinfo'"
        show="$ENABLE=0"
        editable="no"
        wrap="no"
        units="char"
        />
      <group loc="5 5" size="220 40"
        vq="wqinit"
        show="$SHOW=(wqinit='enter')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <textmessage loc="20 25" size="190 20"
          style="Info"
          label=""
          vq="globalenter"
          set="$VALUE='' shell('basename '+globalenter):$VALUE"
          />
        <label loc="5 10" size="90 20"
          style="Label1"
          label="AutoFile"
          justify="Left"
          />
      </group>
      <label loc="5 60" size="115 25"
        style="Label1"
        label="Current status"
        />
      <button loc="110 60" size="95 25"
        style="Heading2"
        label="Update"
        vc="wrautoinfo(wqinit)"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <group loc="5 5" size="215 40"
        vq="wqinit"
        show="$SHOW=(wqinit='auto')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <textmessage loc="25 25" size="185 20"
          style="Info"
          label="auto_20130122_01"
          vq="autodir globalauto"
          set="$VALUE='' shell('basename '+autodir):$VALUE"
          />
        <label loc="5 10" size="95 20"
          style="Label1"
          label="AutoRun"
          justify="Left"
          />
      </group>
    </group>
    <group loc="640 150" size="195 70"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <menu loc="50 25" size="140 20"
        style="PlainText"
        vq="alock"
        vc="$alock='$VALUE' if (Console='mercury') and ($alock='u') then $alock='n' endif alock=$alock"
        set="if (alock&lt;&gt;'n') and (alock&lt;&gt;'u') then $VALUE='s' else $VALUE=alock endif"
        editable="No"
        >
        <mlabel 
          label="Yes (alock&lt;&gt;n)"
          chval="s"
          />
        <mlabel 
          label="No (alock=n)"
          chval="n"
          />
        <mlabel 
          label="Unlocked (alock=u)"
          chval="u"
          />
      </menu>
      <label loc="5 25" size="45 20"
        style="Label1"
        label="Lock"
        justify="Left"
        />
      <label loc="5 5" size="185 20"
        style="Label1"
        label="Before first EXP (day/night)"
        justify="Left"
        />
      <check loc="120 45" size="65 20"
        style="Label1"
        label="Tune"
        vq="ptuneatsample atune wtune tunemethod"
        vc="strstr(ptuneatsample,'wkup'):$wk if ($wk) then ptuneatsample='autowkup' else ptuneatsample='auto' endif"
        vc2="strstr(ptuneatsample,'wkup'):$wk if ($wk) then ptuneatsample='wkup' else ptuneatsample='no' endif"
        set="strstr(ptuneatsample,'auto'):$ato $VALUE=((ptuneatsample='yes') or ($ato&gt;0))"
        show="$ok2='no' checkprotune('global'):$ok2 $ENABLE=($ok2='yes')*2-1"
        />
      <check loc="50 45" size="55 20"
        style="Label1"
        label="Shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="$VALUE=(wshim&lt;&gt;'n')"
        />
    </group>
    <group loc="640 80" size="195 65"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="90 5" size="80 20"
        style="Label1"
        label="Autoplot"
        vq="pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <label loc="5 5" size="75 20"
        style="Label1"
        label="After EXP"
        justify="Left"
        />
      <filemenu loc="50 25" size="140 20"
        style="PlainText"
        vq="userdir userplotter"
        vc="userplotter='$VALUE'"
        set="$VALUE=userplotter"
        file="$VALUE=userdir+'/persistence/plotters'"
        type="file"
        bg="VJBackground"
        editable="No"
        vq2="userplotter"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 25" size="45 20"
        style="Label1"
        label="Plotter"
        justify="Left"
        />
      <group loc="0 45" size="190 20"
        vq="emailflag"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="BeforeFirstExperiment"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <check loc="90 0" size="100 20"
          style="Label1"
          label="Email"
          vq="emessage emailflag"
          vc="emessage='y'"
          vc2="emessage='n'"
          set="$VALUE=(emessage='y')"
          show="$ENABLE=(emailflag='y')*2-1"
          />
        <label loc="5 0" size="80 20"
          style="Label1"
          label="After Queue"
          justify="Left"
          />
      </group>
    </group>
    <group loc="640 5" size="195 70"
      style="Heading1"
      bg="transparent"
      border="Etched"
	label="Sample preparation"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampstat"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 25" size="185 45"
        vq="panellevel"
        show="$SHOW=(panellevel&gt;29)"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="5 5" size="95 20"
          style="Label1"
          label="Equilibrate for"
          justify="Left"
          />
        <entry loc="95 5" size="50 20"
          style="PlainText"
          vq="pad"
          vc="pad=$VALUE walkupQ('time')"
          set="$VALUE=pad"
          digits="1"
          disable="Grayed out"
          />
        <group loc="0 25" size="175 20"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <entry loc="95 0" size="50 20"
            style="PlainText"
            vq="temp"
            vc="temp=$VALUE checktemplimit"
            set="$VALUE=temp"
            show="vnmrinfo('get','tempExpControl'):$tex getautoglobaltemp(wqinit):$tc2 operights('candovtinautomation'):$ok $ENABLE=($tex&gt;0 and $tc2&gt;0 and $ok&gt;0)*2-1"
            digits="1"
            disable="Grayed out"
            />
          <label loc="5 0" size="95 20"
            style="Label1"
            label="Run sample at"
            justify="Left"
            />
          <label loc="150 0" size="20 20"
            style="Label1"
            label="C"
            />
        </group>
        <label loc="150 5" size="25 20"
          style="Label1"
          label="sec"
          />
      </group>
    </group>
    <textmessage loc="10 260" size="445 20"
      style="Info"
      label="Study time: Day -"
      vq="daytime nighttime"
      set="$VALUE='Study time: Day - '+daytime if (nighttime&lt;&gt;'') then $VALUE=$VALUE+'; Night - '+nighttime endif"
      />
    <group loc="10 5" size="300 20"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="180 20"
        style="PlainText"
        label="Operator : PaulB"
        vq="operator"
        set="$VALUE='      Operator : '+operator"
        />
    </group>
    <group loc="640 226" size="431 30"
      vq="wshim"
      show="$SHOW=(wshim&lt;&gt;'n')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <filemenu loc="130 5" size="295 20"
        style="PlainText"
        vq="method Gmapname"
        vc="exec('$VALUE')"
        set="$VALUE=`method='`+method+`' Gmapname='`+Gmapname+`'`"
        file="exists('shimmenuInfo','templates/vnmrj/choicefiles'):$r,$VALUE"
        type="file"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 5" size="115 20"
        style="Label1"
        label="Select shim menu"
        />
    </group>
  </group>
</template>
