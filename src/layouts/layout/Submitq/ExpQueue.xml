<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ExpQueue" element="pages" type="sample" >
  <group size="675 255"
    style="Heading1"
    label="ExpQueue"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="ExpQueue"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="265 30" size="380 135"
      vq="appmode sqflag"
      show="$SHOW=((appmode&lt;&gt;'walkup') or (sqflag='n'))"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampqinfo"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="14 10" size="160 20"
        style="Heading1"
        label="Customize/Delete:"
        />
      <label loc="234 35" size="90 20"
        style="Label1"
        label="Night Q"
        justify="Center"
        />
      <label loc="69 35" size="90 20"
        style="Label1"
        label="Day Q"
        justify="Center"
        />
      <button loc="89 95" size="100 30"
        style="Heading2"
        label="Customize"
        vq="DEname"
        vc="walkupQ('DelEdit','edit')"
        show="$ENABLE=((DEname&lt;&gt;'') and (DEname&lt;&gt;'NULL'))*2-1"
        bg=""
        decor1=""
        />
      <button loc="224 95" size="100 30"
        style="Heading2"
        label="Delete"
        vq="DEname"
        vc="walkupQ('DelEdit','delete')"
        show="$ENABLE=((DEname&lt;&gt;'') and (DEname&lt;&gt;'NULL'))*2-1"
        bg=""
        decor1=""
        />
      <filemenu loc="204 60" size="160 20"
        style="PlainText"
        vq="DEname longQ cursqexp curexp"
        vc="DEname='$VALUE'"
        set="$VALUE=DEname"
        show="$ENABLE=(longQ='y')*2-1"
        file="$VALUE=cursqexp+'/LONGdeledit'"
        type="file"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <filemenu loc="39 60" size="160 20"
        style="PlainText"
        vq="DEname longQ cursqexp curexp"
        vc="DEname='$VALUE'"
        set="$VALUE=DEname"
        show="$ENABLE=(longQ='n')*2-1"
        file="$VALUE=cursqexp+'/EXPdeledit'"
        type="file"
        editable="No"
        vq2="cursqexp"
        statshow="yes"
        elastic="no"
        display="yes"
        />
    </group>
    <group loc="25 5" size="230 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="daynight"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="5 0" size="220 50"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="daynight"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="15 5" size="195 20"
          style="Heading1"
          label="Submit next selection to:"
          justify="Center"
          />
        <check loc="10 25" size="90 20"
          style="Label1"
          label="Short/Day Q"
          vq="longQ"
          vc="longQ='n' seqfil=seqfil"
          vc2="longQ='y' seqfil=seqfil"
          set="$VALUE=(longQ='n')"
          />
        <check loc="110 25" size="95 20"
          style="Label1"
          label="Long/Night Q"
          vq="longQ"
          vc="longQ='y' seqfil=seqfil"
          vc2="longQ='n' seqfil=seqfil"
          set="$VALUE=(longQ='y')"
          />
      </group>
      <textfile loc="5 55" size="220 160"
        style="PlainText"
        vq="cursqexp curexp"
        set="$VALUE=cursqexp+'/EXPTIME'"
        show="$ENABLE=0"
        editable="no"
        wrap="no"
        units="char"
        />
    </group>
  </group>
</template>
