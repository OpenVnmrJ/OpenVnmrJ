<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standard" element="pages" type="sample" >
  <group size="840 305"
    style="Heading1"
    label="Standard"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standard"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="380 75" size="195 65"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="90 5" size="70 20"
        style="Label1"
        label="Autoplot"
        vq="pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <label loc="5 5" size="75 20"
        style="Label1"
        label="After EXP"
        justify="Left"
        />
      <filemenu loc="50 25" size="140 20"
        style="PlainText"
        vq="userdir userplotter"
        vc="userplotter='$VALUE'"
        set="$VALUE=userplotter"
        file="$VALUE=userdir+'/persistence/plotters'"
        type="file"
        editable="No"
        vq2="userplotter"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 25" size="45 20"
        style="Label1"
        label="plotter"
        justify="Left"
        />
      <group loc="0 45" size="190 20"
        vq="emailflag"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="BeforeFirstExperiment"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <check loc="90 0" size="90 20"
          style="Label1"
          label="e-message"
          vq="emessage emailflag"
          vc="emessage='y'"
          vc2="emessage='n'"
          set="$VALUE=(emessage='y')"
          show="$ENABLE=(emailflag='y')*2-1"
          />
        <label loc="5 0" size="80 20"
          style="Label1"
          label="After Queue"
          justify="Left"
          />
      </group>
    </group>
    <group loc="380 5" size="195 70"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampstat"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="5 25" size="185 45"
        vq="panellevel"
        show="$SHOW=(panellevel&gt;29)"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="10 5" size="95 20"
          style="Heading1"
          label="Equilibrate for:"
          justify="Left"
          />
        <entry loc="105 5" size="45 20"
          style="PlainText"
          vq="pad"
          vc="pad=$VALUE walkupQ('time')"
          set="$VALUE=pad"
          digits="1"
          disable="Grayed out"
          />
        <group loc="0 25" size="175 20"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <entry loc="105 0" size="45 20"
            style="PlainText"
            vq="temp operator"
            vc="temp=$VALUE checktemplimit"
            set="$VALUE=temp"
            show="vnmrinfo('get','tempExpControl'):$tex getautoglobaltemp(wqinit):$tc2 operights('candovtinautomation'):$ok $ENABLE=($tex&gt;0 and $tc2&gt;0 and $ok&gt;0)*2-1"
            digits="1"
            disable="Grayed out"
            />
          <label loc="10 0" size="95 20"
            style="Label1"
            label="Run Sample at:"
            justify="Left"
            />
          <label loc="150 0" size="20 20"
            style="Label1"
            label="C"
            />
        </group>
        <label loc="150 5" size="25 20"
          style="Label1"
          label="sec"
          />
      </group>
      <label loc="15 5" size="165 20"
        style="Label1"
        label="Sample Preparation"
        justify="Center"
        />
    </group>
    <group loc="40 250" size="715 30"
      vq="sqflag appmode"
      show="$SHOW=((appmode='walkup') and (sqflag='n'))"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampqinfo2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="535 5" size="165 20"
        style="Heading2"
        label="Turn ON studyQ !"
        vq="panellevel"
        vc="sqflag='y' xmhaha('refreshSQ') seqfil=seqfil"
        show="$ENABLE=(panellevel&gt;29)"
        bg=""
        decor1=""
        />
      <label loc="10 5" size="525 20"
        style="Label1"
        label="Use StudyQ Action menu (Refresh Queue) to populate StudyQ .... OR"
        justify="Left"
        />
    </group>
    <group loc="380 140" size="195 70"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="50 25" size="140 20"
        style="PlainText"
        vq="alock"
        vc="$alock='$VALUE' if (Console='mercury') and ($alock='u') then $alock='n' endif alock=$alock"
        set="if (alock&lt;&gt;'n') and (alock&lt;&gt;'u') then $VALUE='s' else $VALUE=alock endif"
        editable="No"
        >
        <mlabel 
          label="Yes (alock&lt;&gt;n)"
          chval="s"
          />
        <mlabel 
          label="No (alock=n)"
          chval="n"
          />
        <mlabel 
          label="Unlocked (alock=u)"
          chval="u"
          />
      </menu>
      <label loc="5 25" size="45 20"
        style="Label1"
        label="Lock?"
        justify="Left"
        />
      <label loc="5 5" size="185 20"
        style="Label1"
        label="Before 1st EXP (day/night)"
        justify="Center"
        />
      <check loc="100 45" size="55 20"
        style="Label1"
        label="Tune"
        vq="ptuneatsample atune wtune tunemethod"
        vc="ptuneatsample='yes'"
        vc2="ptuneatsample='no'"
        set="$VALUE=(ptuneatsample='yes')"
        show="$ok2='no' checkprotune('global'):$ok2 $ENABLE=($ok2='yes')*2-1"
        />
      <check loc="30 45" size="55 20"
        style="Label1"
        label="Shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="$VALUE=(wshim&lt;&gt;'n')"
        />
    </group>
    <group loc="380 215" size="430 30"
      vq="wshim"
      show="$SHOW=(wshim&lt;&gt;'n')"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <filemenu loc="120 5" size="300 20"
        style="PlainText"
        vq="Gmap Gmapname operator"
        vc="Gmaplist('set','$VALUE')"
        set="if (Gmapname='') then $VALUE=Gmap else $VALUE='custom_'+Gmapname endif"
        file="$VALUE=userdir+'/persistence/'+operator+'_Gmaplist'"
        type="file"
        editable="No"
        vq2="Gmap Gmapname"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 5" size="105 20"
        style="Label1"
        label="Select shimmap:"
        />
    </group>
    <group loc="580 5" size="230 200"
      vq="wqinit"
      bg=""
      border="Etched"
      tab="no"
      enable="no"
      reference="daynight"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textfile loc="5 80" size="220 115"
        style="PlainText"
        vq="userdir NextLoc"
        set="$VALUE=userdir+'/persistence/autotimeinfo'"
        show="$ENABLE=0"
        editable="no"
        wrap="no"
        units="char"
        />
      <label loc="5 50" size="115 25"
        style="Label1"
        label="Current Status:"
        />
      <button loc="125 50" size="95 25"
        style="Heading2"
        label="Update"
        vc="wrautoinfo(wqinit)"
        bg=""
        decor1=""
        />
      <group loc="5 5" size="220 40"
        vq="wqinit"
        show="$SHOW=(wqinit='enter')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="20 20" size="190 20"
          style="PlainText"
          vq="globalenter"
          set="$VALUE='' substr(globalenter,'basename'):$VALUE"
          />
        <label loc="5 0" size="90 20"
          style="Label1"
          label="AutoFile:"
          justify="Left"
          />
      </group>
      <group loc="5 5" size="215 40"
        vq="wqinit"
        show="$SHOW=(wqinit='auto')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <textmessage loc="25 20" size="185 20"
          style="PlainText"
          label="auto_20061129_02"
          vq="autodir globalauto"
          set="$VALUE='' substr(autodir,'basename'):$VALUE"
          />
        <label loc="5 0" size="95 20"
          style="Label1"
          label="AutoRun:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="35 5" size="300 20"
      bg=""
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <textmessage loc="30 0" size="180 20"
        style="PlainText"
        label="Operator : cpwuser"
        vq="operator"
        set="$VALUE='      Operator : '+operator"
        />
      <button loc="215 0" size="80 20"
        style="Heading2"
        label="Logout"
        vc="walkupQ('logout') operatorlogout"
        bg=""
        decor1=""
        />
    </group>
    <group loc="5 30" size="370 215"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampinfo"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <filemenu loc="85 85" size="125 20"
        style="PlainText"
        vq="solvent"
        vc="solvent='$VALUE' setref"
        set="format(solvent,'lower'):$VALUE"
        file="exists('solventlist',''):$e,$VALUE"
        type="file"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <textfile loc="5 145" size="360 65"
        style="PlainText"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <entry loc="255 65" size="105 20"
        style="PlainText"
        vq="emailaddr "
        vc="emailaddr='$VALUE'"
        set="$VALUE=emailaddr"
        disable="Grayed out"
        />
      <label loc="215 65" size="45 20"
        style="Label1"
        label="Eaddr"
        justify="Left"
        />
      <entry loc="255 45" size="105 20"
        style="PlainText"
        vq=" page"
        vc=" page='$VALUE'"
        set="$VALUE=page"
        disable="Grayed out"
        />
      <label loc="215 45" size="40 20"
        style="Label1"
        label="Page:"
        justify="Left"
        />
      <group loc="235 5" size="130 20"
        vq="userremembrance"
        show="$SHOW=(userremembrance='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="userrem"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <button loc="2 0" size="120 20"
          style="Heading2"
          label="Use My Prefs"
          vc="loaduserprefs"
          show="isnewexp:$isnew $ENABLE=($isnew&gt;0)*2-1"
          bg=""
          decor1=""
          />
      </group>
      <entry loc="100 5" size="130 20"
        style="PlainText"
        vq=" samplename"
        vc=" samplename='$VALUE' setsamplename"
        set="$VALUE=samplename"
        disable="Grayed out"
        />
      <entry loc="100 45" size="110 20"
        style="PlainText"
        vq="lot"
        vc="lot='$VALUE'"
        set="$VALUE=lot"
        disable="Grayed out"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="Lot Number:"
        justify="Left"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="SampleDir:"
        justify="Left"
        />
      <textmessage loc="100 25" size="260 20"
        style="PlainText"
        label="(_20080415_01)"
        vq="sample sdirtmplt adirtmplt samplename"
        set="if (sample='') then $as='' $s='' Svfname(adirtmplt+'/'+sdirtmplt,''):$as substr($as,'basename'):$s $VALUE='('+$s+')' else $VALUE=sample endif"
        />
      <label loc="10 65" size="90 20"
        style="Label1"
        label="Notebook:"
        justify="Left"
        />
      <entry loc="100 65" size="110 20"
        style="PlainText"
        vq=" notebook"
        vc=" notebook='$VALUE'"
        set="$VALUE=notebook"
        disable="Grayed out"
        />
      <toggle loc="295 105" size="70 20"
        style="Heading2"
        label="Other"
        vq="solvent"
        vc="solvent=solvent"
        set="format(solvent,'lower'):$solv $VALUE=(($solv&lt;&gt;'d2o') and ($solv&lt;&gt;'dmso') and ($solv&lt;&gt;'cdcl3'))"
        radiobutton="yes"
        />
      <toggle loc="225 105" size="70 20"
        style="Heading2"
        label="D2O"
        vq="solvent"
        vc="solvent='d2o' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='d2o')"
        radiobutton="yes"
        />
      <toggle loc="155 105" size="70 20"
        style="Heading2"
        label="CDCl3"
        vq="solvent"
        vc="solvent='cdcl3' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='cdcl3')"
        radiobutton="yes"
        />
      <toggle loc="85 105" size="70 20"
        style="Heading2"
        label="DMSO"
        vq="solvent"
        vc="solvent='dmso' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='dmso')"
        radiobutton="yes"
        />
      <label loc="5 95" size="80 20"
        style="Label1"
        label="Solvent"
        justify="Left"
        />
      <label loc="5 125" size="100 20"
        style="Label1"
        label="Comment"
        justify="Left"
        />
      <label loc="10 5" size="90 20"
        style="Label1"
        label="Sample Name:"
        justify="Left"
        />
    </group>
  </group>
</template>
