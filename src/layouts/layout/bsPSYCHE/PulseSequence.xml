<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="800 245"
    style="Heading1"
    label="Pulse Sequence"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 25" size="270 220"
      style="Heading1"
      label="PSYCHE"
      border="Etched"
      tab="no"
      enable="no"
      reference="PSYCHEgrp"
      useref="no"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      />
    <group loc="280 25" size="270 170"
      style="Heading1"
      label="DPFGSE"
      border="Etched"
      tab="no"
      enable="no"
      reference="DPFGSEgrp"
      useref="no"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      />
    <group loc="280 200" size="270 45"
      style="Heading1"
      label="Steady State (G-90-G)"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="5 20" size="20 20"
        style="Label1"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="30 20" size="60 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <entry loc="90 20" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="155 20" size="60 20"
        style="Label1"
        label="time (ms)"
        justify="Left"
        />
      <entry loc="215 20" size="50 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
    </group>
    <group loc="10 0" size="390 25"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="PSYCHE"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
