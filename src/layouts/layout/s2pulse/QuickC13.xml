<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="QuickC13" element="pages" type="acquisition" >
  <group size="660 260"
    style="Heading1"
    label="Default C13"
    vq="tn"
    show="$SHOW=(pslabel='CARBON')"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="QuickC13"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptionsC"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="10 25" size="380 195"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="CARBONacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="60 140" size="315 45"
          vq="wbs"
          show="$ret=0 strstr(wbs,'testsn'):$ret $SHOW=($ret&gt;0)"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <entry loc="195 5" size="65 20"
            style="PlainText"
            vq="wbs"
            vc="$v1='' $v2='' tsnspwp('get'):$v1,$v2 tsnspwp('set',$v1,$VALUE)"
            set="$v3='' tsnspwp('get'):$v3,$VALUE"
            digits="1"
            disable="Grayed out"
            />
          <label loc="265 5" size="40 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <entry loc="90 5" size="70 20"
            style="PlainText"
            vq="wbs"
            vc="$v1='' $v2='' tsnspwp('get'):$v1,$v2 tsnspwp('set',$VALUE,$v2)"
            set="tsnspwp('get'):$VALUE"
            digits="1"
            disable="Grayed out"
            />
          <label loc="160 5" size="35 20"
            style="Label1"
            label="and"
            justify="Center"
            />
          <entry loc="265 25" size="45 20"
            style="PlainText"
            vq="sn"
            vc="sn=$VALUE"
            set="$VALUE=sn"
            digits="0"
            disable="Grayed out"
            />
          <label loc="160 25" size="105 20"
            style="Label1"
            label="scans &amp; stop at:"
            justify="Center"
            />
          <menu loc="90 25" size="70 20"
            style="PlainText"
            label="menu"
            vq="bs"
            vc="bs=$VALUE"
            set="$VALUE=bs"
            bg=""
            editable="No"
            >
            <mlabel 
              label="16"
              chval="16"
              />
            <mlabel 
              label="64"
              chval="64"
              />
            <mlabel 
              label="128"
              chval="128"
              />
            <mlabel 
              label="256"
              chval="256"
              />
            <mlabel 
              label="512"
              chval="512"
              />
            <mlabel 
              label="1024"
              chval="1024"
              />
          </menu>
          <label loc="30 5" size="60 20"
            style="Label1"
            label="between"
            justify="Left"
            />
          <label loc="30 25" size="60 20"
            style="Label1"
            label="at every"
            justify="Left"
            />
        </group>
        <label loc="0 5" size="145 20"
          style="Label1"
          label="Spectral Width (select):"
          justify="Right"
          />
        <menu loc="150 105" size="135 20"
          style="PlainText"
          label="menu"
          vq="dm"
          vc="dm='$VALUE'"
          set="$VALUE=dm"
          bg=""
          editable="No"
          >
          <mlabel 
            label="Decoupled + NOE"
            chval="yyy"
            />
          <mlabel 
            label="Decoupled - NOE"
            chval="nny"
            />
          <mlabel 
            label="Coupled + NOE"
            chval="ynn"
            />
          <mlabel 
            label="Coupled - NOE"
            chval="nnn"
            />
        </menu>
        <menu loc="150 45" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1000"
            chval="1000"
            />
          <mlabel 
            label="2000"
            chval="2000"
            />
          <mlabel 
            label="5000"
            chval="5000"
            />
          <mlabel 
            label="10000"
            chval="10000"
            />
        </menu>
        <menu loc="150 65" size="70 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="0"
            chval="0"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <menu loc="150 85" size="70 20"
          style="PlainText"
          label="menu"
          vq="pw pw90"
          vc="pw($VALUE)"
          set="$VALUE=pw*90/pw90"
          bg=""
          editable="No"
          >
          <mlabel 
            label="45"
            chval="45"
            />
          <mlabel 
            label="30"
            chval="30"
            />
          <mlabel 
            label="60"
            chval="60"
            />
          <mlabel 
            label="90"
            chval="90"
            />
        </menu>
        <label loc="40 125" size="75 20"
          style="Label1"
          label="Check S/N?"
          justify="Left"
          />
        <label loc="40 105" size="90 20"
          style="Label1"
          label="H1 decoupling"
          justify="Left"
          />
        <label loc="40 85" size="85 20"
          style="Label1"
          label="Pulse Angle"
          justify="Left"
          />
        <label loc="40 65" size="110 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
        <label loc="40 45" size="110 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <group loc="10 5" size="300 40"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="swentry"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="190 20" size="20 20"
            style="Heading1"
            label="to"
            justify="Center"
            />
          <label loc="5 20" size="130 20"
            style="Label1"
            label="(...or enter):"
            justify="Right"
            />
          <menu loc="140 0" size="120 20"
            style="PlainText"
            label="menu"
            vq="obsSW"
            vc="exec('$VALUE')"
            set="$VALUE=''"
            bg=""
            editable="No"
            >
            <mlabel 
              label="-15  →  235"
              chval="setsw(235,-15)"
              />
            <mlabel 
              label="-10  →  200"
              chval="setsw(200,-10)"
              />
            <mlabel 
              label="-25  →  250"
              chval="setsw(250,-25)"
              />
          </menu>
          <entry loc="140 20" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq"
            vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
            set="invsetsw:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
          <label loc="265 20" size="35 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <label loc="265 0" size="35 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <entry loc="210 20" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq sw"
            vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
            set="invsetsw:$f1,$f2 $VALUE=$f1"
            digits="1"
            disable="Grayed out"
            />
        </group>
        <label loc="224 65" size="60 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="224 85" size="60 20"
          style="Label1"
          label="degrees"
          justify="Left"
          />
        <radio loc="194 125" size="45 20"
          style="Label1"
          label="no"
          vq="wbs"
          vc="wbs=''"
          vc2="strstr(wbs,'testsn'):$ret1 if ($ret1=0) then wbs='testsn' else wbs=wbs endif"
          set="$ret=0 strstr(wbs,'testsn'):$ret $VALUE=($ret=0)"
          />
        <radio loc="149 125" size="45 20"
          style="Label1"
          label="yes"
          vq="wbs"
          vc="strstr(wbs,'testsn'):$ret1 if ($ret1=0) then wbs='testsn' else wbs=wbs endif"
          vc2="wbs=''"
          set="$ret=0 strstr(wbs,'testsn'):$ret $VALUE=($ret&gt;0)"
          />
      </group>
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="5 0" size="135 20"
        style="Heading1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="145 20" size="80 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 45" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
  </group>
</template>
