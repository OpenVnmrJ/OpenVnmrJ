<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="SolventSuppress" element="pages" type="acquisition" >
  <group size="645 240"
    style="Heading1"
    label="SolvSupp"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="SolventSuppress"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="390 35" size="220 190"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="presatgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="5 30" size="210 155"
        vq="satmode"
        show="$SHOW=(satmode='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <entry loc="80 85" size="125 20"
          style="PlainText"
          vq="pstof slpsat"
          vc="if (slpsat='y') then pstof=$VALUE endif pstof=pstof"
          set="array2csv(pstof,1):$VALUE"
          show="$ENABLE=(slpsat='y')"
          digits="1"
          disable="Grayed out"
          />
        <label loc="10 85" size="70 20"
          style="Label1"
          label="Offsets (Hz)"
          vq="slpsat"
          show="$ENABLE=(slpsat='y')"
          justify="Left"
          />
        <label loc="10 105" size="135 20"
          style="Label1"
          label="Chem. Shift(ppm):"
          vq="slpsat"
          show="$ENABLE=(slpsat='y')"
          justify="Left"
          />
        <textmessage loc="10 125" size="180 20"
          style="PlainText"
          label="6.00p"
          vq="slpsat psppm"
          set="$VALUE=psppm"
          />
        <check loc="10 65" size="165 20"
          style="Label1"
          label="Off-Resonance PRESAT"
          vq="slpsat"
          vc="slpsat='y' pstof=pstof"
          vc2="slpsat='n' pstof=pstof"
          set="$VALUE=(slpsat='y')"
          />
        <textmessage loc="135 45" size="50 20"
          style="PlainText"
          label="46 Hz"
          vq="satpwr"
          set="getpower(satpwr,tn):$gh2 $VALUE='' format($gh2,0,0):$VALUE $VALUE=$VALUE+' Hz'"
          />
        <entry loc="85 45" size="45 20"
          style="PlainText"
          vq="satpwr satmode"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          disable="Grayed out"
          />
        <label loc="10 45" size="70 20"
          style="Label1"
          label="Power (dB)"
          justify="Left"
          />
        <textmessage loc="150 25" size="50 20"
          style="PlainText"
          label="6.00p"
          vq="satfrq satmode slpsat"
          set="setsatppm:$VALUE"
          show="$ENABLE=(slpsat='n')"
          />
        <label loc="10 25" size="70 20"
          style="Label1"
          label="Offset (Hz)"
          justify="Left"
          />
        <label loc="145 5" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <entry loc="85 25" size="60 20"
          style="PlainText"
          vq="satfrq satmode slpsat"
          vc="if (slpsat='n') then satfrq=$VALUE else satfrq=satfrq endif"
          set="$VALUE=satfrq"
          show="$ENABLE=(slpsat='n')"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="85 5" size="60 20"
          style="PlainText"
          vq="satdly satmode"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          digits="2"
          disable="Grayed out"
          />
        <label loc="10 5" size="70 20"
          style="Label1"
          label="Delay"
          justify="Left"
          />
      </group>
      <check loc="35 5" size="160 20"
        style="Label1"
        label="Presaturation"
        vq="satmode"
        vc="satmode='y' if (wet='n') then seqfil='PRESAT' endif"
        vc2="satmode='n' if (wet='y') then seqfil='wet1D' else seqfil='s2pul' endif"
        set="$VALUE=(satmode='y')"
        />
    </group>
    <group loc="10 35" size="375 190"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="wetgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="6 24" size="365 163"
        vq="wet"
        show="if (wet='y') then $SHOW=1 else $SHOW=0 endif"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="129 1" size="210 20"
          style="PlainText"
          vq="wetshape "
          vc="wetshape='$VALUE'"
          set="$VALUE=wetshape"
          disable="Grayed out"
          />
        <label loc="24 1" size="100 20"
          style="Label1"
          label="wet Pulse shape:"
          justify="Left"
          />
        <textmessage loc="104 101" size="170 20"
          style="PlainText"
          label="C13  Decoupling during wet?"
          vq="dn"
          set="$VALUE=dn+'  Decoupling during wet?'"
          />
        <group loc="14 121" size="330 40"
          vq="c13wet"
          show="if (c13wet='y') then $SHOW=1 else $SHOW=0 endif"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <label loc="300 0" size="20 20"
            style="Heading1"
            label="us"
            justify="Left"
            />
          <entry loc="240 20" size="60 20"
            style="PlainText"
            vq="decwavewet "
            vc="decwavewet='$VALUE'"
            set="$VALUE=decwavewet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="240 0" size="60 20"
            style="PlainText"
            vq="dmfwet "
            vc="dmfwet=$VALUE/1e6"
            set="$VALUE=1e6/dmfwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="145 20" size="90 20"
            style="Label1"
            label="modulation"
            justify="Left"
            />
          <label loc="145 0" size="90 20"
            style="Label1"
            label="pi/2 Pulse width"
            justify="Left"
            />
          <entry loc="80 20" size="60 20"
            style="PlainText"
            vq="dpwrwet "
            vc="dpwrwet=$VALUE"
            set="$VALUE=dpwrwet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="80 0" size="60 20"
            style="PlainText"
            vq="dofwet "
            vc="dofwet=$VALUE"
            set="$VALUE=dofwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="5 20" size="75 20"
            style="Label1"
            label="power"
            justify="Left"
            />
          <label loc="5 0" size="75 20"
            style="Label1"
            label="Offset"
            justify="Left"
            />
        </group>
        <check loc="79 101" size="25 20"
          style="Label1"
          vq="c13wet"
          vc="c13wet='y'"
          vc2="c13wet='n'"
          set="if (c13wet='y') then $VALUE=1 else $VALUE=0 endif"
          />
        <group loc="5 80" size="355 20"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <entry loc="230 0" size="120 20"
            style="PlainText"
            vq="compshape composit"
            vc="compshape='$VALUE'"
            set="$VALUE=compshape"
            show="if (composit='y') then $ENABLE=1 else $ENABLE=0 endif"
            disable="Grayed out"
            />
          <label loc="180 0" size="50 20"
            style="Label1"
            label=" Shape:"
            justify="Left"
            />
          <check loc="0 0" size="175 20"
            style="Label1"
            label="Composite observe Pulse?"
            vq="composit"
            vc="composit='y'"
            vc2="composit='n'"
            set="if (composit='y') then $VALUE=1 else $VALUE=0 endif"
            />
        </group>
        <entry loc="275 60" size="50 20"
          style="PlainText"
          vq="gtw wet"
          vc="gtw=$VALUE/1000"
          set="$VALUE=gtw*1000"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="275 40" size="50 20"
          style="PlainText"
          vq="gzlvlw"
          vc="gzlvlw=$VALUE"
          set="$VALUE=gzlvlw"
          digits="0"
          disable="Grayed out"
          />
        <label loc="335 60" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 60" size="60 20"
          style="Label1"
          label=" time"
          justify="Left"
          />
        <label loc="215 40" size="60 20"
          style="Label1"
          label="strength"
          justify="Left"
          />
        <label loc="205 20" size="70 20"
          style="Label1"
          label="Gradient -"
          justify="Left"
          />
        <label loc="170 60" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="105 60" size="60 20"
          style="PlainText"
          vq="dz "
          vc="dz=$VALUE/1e6"
          set="$VALUE=dz*1e6"
          disable="Grayed out"
          />
        <label loc="5 60" size="95 20"
          style="Label1"
          label="delay after wet"
          justify="Left"
          />
        <label loc="5 40" size="75 20"
          style="Label1"
          label="Pulse power"
          justify="Left"
          />
        <entry loc="105 40" size="60 20"
          style="PlainText"
          vq="wetpwr "
          vc="wetpwr=$VALUE"
          set="$VALUE=wetpwr"
          digits="1"
          disable="Grayed out"
          />
        <label loc="170 20" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="105 20" size="60 20"
          style="PlainText"
          vq="pwwet "
          vc="pwwet=$VALUE"
          set="$VALUE=pwwet"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 20" size="75 20"
          style="Label1"
          label="Pulse width"
          justify="Left"
          />
      </group>
      <check loc="126 4" size="120 20"
        style="Label1"
        label="   WET"
        vq="wet"
        vc="wet='y' if (satmode='n') then seqfil='wet1D' endif"
        vc2="wet='n' if (satmode='y') then seqfil='PRESAT' else seqfil='s2pul' endif"
        set="if (wet='y') then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
    <button loc="440 0" size="90 20"
      style="Heading2"
      label="Arrays"
      vc="vnmrjcmd('array','popup')"
      decor1=""
      />
    <button loc="260 0" size="130 20"
      style="Heading2"
      label="Display Sequence"
      vc="dps"
      decor1=""
      />
    <textmessage loc="10 0" size="170 20"
      style="PlainText"
      label="PROTON"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
