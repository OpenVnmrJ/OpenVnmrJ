        style="Label1"
        label="X-decouple (garp1)"
        vq="dmct"
        vc="dmct='y'"
        vc2="dmct='n'"
        set="$VALUE=(dmct='y')"
        />
      <entry loc="85 90" size="50 20"
        style="PlainText"
        vq="dmfct dmct"
        vc="if (dmct='y') then dmfct=$VALUE else dmfct=dmfct endif"
        set="$VALUE=dmfct"
        show="$ENABLE=(dmct='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="85 70" size="50 20"
        style="PlainText"
        vq="dpwrct dmct"
        vc="if (dmct='y') then dpwrct=$VALUE else dpwrct=dpwrct endif"
        set="$VALUE=dpwrct"
        show="$ENABLE=(dmct='y')"
        digits="0"
        disable="Grayed out"
