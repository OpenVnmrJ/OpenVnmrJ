<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="670 270"
    style="Heading1"
    label="Defaults"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','default'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 5" size="620 25"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        label="Experiment: PS1D   Solvent: d2o"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        label="Observe: H1          Decoupler: C13"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="5 35" size="395 200"
      style="Heading1"
      label="Acquisition Options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <group loc="10 20" size="375 170"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="s2pulacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <entry loc="195 65" size="70 20"
          style="PlainText"
          vq="ss"
          vc="ss=$VALUE"
          set="$VALUE=ss"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="195 5" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq"
          vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
          set="invsetsw:$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
        <label loc="245 5" size="20 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <entry loc="265 5" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq sw"
          vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
          set="invsetsw:$f1,$f2 $VALUE=$f1"
          digits="1"
          disable="Grayed out"
          />
        <label loc="320 5" size="40 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <menu loc="195 25" size="70 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
          <mlabel 
            label="4096"
            chval="4096"
            />
        </menu>
        <label loc="270 45" size="60 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <menu loc="195 45" size="70 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="0"
            chval="0"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <group loc="10 85" size="350 85"
          border="None"
          tab="no"
          enable="no"
          reference="pureshift"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          rows="1"
          columns="1"
          >
          <textmessage loc="35 65" size="220 20"
            style="PlainText"
            label="Suitable for J values &lt; ~12.3 Hz"
            vq="tauPS"
            set="$j=(1/(10*tauPS)) $js='' format($j,0,1):$js $VALUE='Suitable for J values &lt; ~'+$js+' Hz'"
            />
          <entry loc="185 45" size="70 20"
            style="PlainText"
            vq="tauPS"
            vc="tauPS=$VALUE/1000"
            set="$VALUE=tauPS*1000"
            digits="1"
            disable="Grayed out"
            />
          <label loc="260 45" size="40 20"
            style="Label1"
            label="ms"
            justify="Left"
            />
          <label loc="35 45" size="130 20"
            style="Label1"
            label="&quot;PureShift tau delay&quot;"
            justify="Left"
            />
          <textmessage loc="35 25" size="285 20"
            style="PlainText"
            label="Slice selection pulse pattern: rsnob"
            vq="selwavePS"
            set="$VALUE='Slice selection pulse pattern: '+selwavePS"
            />
          <entry loc="185 5" size="70 20"
            style="PlainText"
            vq="selbwPS"
            vc="selbwPS=$VALUE"
            set="$VALUE=selbwPS"
            digits="1"
            disable="Grayed out"
            />
          <label loc="260 5" size="40 20"
            style="Label1"
            label="Hz"
            justify="Left"
            />
          <label loc="35 5" size="150 20"
            style="Label1"
            label="Slice Selection Bandwidth"
            justify="Left"
            />
        </group>
        <label loc="45 5" size="135 20"
          style="Label1"
          label="Spectral width -  from"
          justify="Left"
          />
        <label loc="45 25" size="130 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <label loc="45 45" size="125 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
        <label loc="45 65" size="125 20"
          style="Label1"
          label="Steady State"
          justify="Left"
          />
      </group>
    </group>
    <group loc="400 35" size="235 220"
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="20 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="145 0" size="35 20"
        style="PlainText"
        label="16"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
      <group loc="5 25" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          label="DOSYtest"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          label="After PS1D acq:"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          label="Before PS1D acq:"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label="DOSYtest_PROTON_01"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
  </group>
</template>
