<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="DPFGSE" element="pages" type="acquisition" >
  <group size="770 295"
    style="PlainText"
    label="DPFGSE"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="DPFGSE"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="235 25" size="260 265"
      style="Heading2"
      label="Excitation Sculpting (DPFGSE)"
      vq="ES_flg"
      vc="dps"
      vc2="dps"
      show="$SHOW=(ES_flg='y')"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ESgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="25 240" size="110 20"
        style="Label1"
        label="Lock gating option"
        justify="Left"
        />
      <check loc="10 240" size="20 20"
        style="PlainText"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y' )"
        tooltip="Turn lock control on/off during the sequence and acquisition (lkgate_flg)"
        />
      <check loc="150 220" size="20 20"
        style="PlainText"
        vq="sgyflg"
        vc="sgyflg='y'"
        vc2="sgyflg='n'"
        set="$VALUE=(sgyflg='y' )"
        tooltip="Turn SOGGY composite pulse on/off (sgyflg)"
        />
      <label loc="25 220" size="110 20"
        style="Label1"
        label="Gradient alteration"
        justify="Left"
        />
      <label loc="165 220" size="85 20"
        style="Label1"
        label="SOGGY option"
        justify="Left"
        />
      <check loc="10 220" size="20 20"
        style="PlainText"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y' )"
        tooltip="Change gradient sign on odd scans (alt_grd)"
        />
      <label loc="30 175" size="100 20"
        style="PlainText"
        label="Recovery time"
        justify="Left"
        />
      <label loc="200 175" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <label loc="200 155" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="140 175" size="60 20"
        style="PlainText"
        vq="esgstab"
        vc="esgstab=$VALUE/1000"
        set="$VALUE=esgstab*1000"
        digits="1"
        disable="Grayed out"
        tooltip="Gradient recovery delay ijn the DPFGSE block (esgstab)"
        />
      <entry loc="140 155" size="60 20"
        style="PlainText"
        vq="esgt"
        vc="esgt=$VALUE/1000"
        set="$VALUE=esgt*1000"
        digits="1"
        disable="Grayed out"
        tooltip="Gradient duration in the DPFGSE block (esgt)"
        />
      <label loc="30 155" size="60 20"
        style="PlainText"
        label="time (ms)"
        justify="Left"
        />
      <entry loc="140 135" size="60 20"
        style="PlainText"
        vq="esgzlvl"
        vc="esgzlvl=$VALUE"
        set="$VALUE=esgzlvl"
        digits="0"
        disable="Grayed out"
        tooltip="Gradient level in the DPFGSE block (esgzlvl)"
        />
      <label loc="30 135" size="60 20"
        style="PlainText"
        label="strength"
        justify="Left"
        />
      <label loc="10 120" size="70 20"
        style="PlainText"
        label="Gradient -"
        justify="Left"
        />
      <entry loc="100 100" size="145 20"
        style="PlainText"
        vq="esshape"
        vc="esshape='$VALUE'"
        set="$VALUE=esshape"
        disable="Grayed out"
        tooltip="Pattern of the Solvent Selective Pulse (esshape)"
        />
      <label loc="40 100" size="60 20"
        style="Label1"
        label="shape"
        justify="Left"
        />
      <entry loc="140 80" size="60 20"
        style="PlainText"
        vq="essoftpwrf"
        vc="essoftpwrf=$VALUE"
        set="$VALUE=essoftpwrf"
        digits="1"
        disable="Grayed out"
        tooltip="Fine Power Level of the Solvent Selective Pulse (essoftpwrf)"
        />
      <label loc="40 80" size="70 20"
        style="Label1"
        label="fine power"
        justify="Left"
        />
      <label loc="40 60" size="60 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <entry loc="140 60" size="60 20"
        style="PlainText"
        vq="essoftpwr"
        vc="essoftpwr=$VALUE"
        set="$VALUE=essoftpwr"
        digits="1"
        disable="Grayed out"
        tooltip="Coarse Power Level of the Solvent Selective Pulse (essoftpwr)"
        />
      <label loc="200 60" size="30 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <label loc="200 40" size="30 20"
        style="PlainText"
        label="us"
        justify="Left"
        />
      <entry loc="140 40" size="60 20"
        style="PlainText"
        vq="essoftpw"
        vc="essoftpw=$VALUE"
        set="$VALUE=essoftpw"
        digits="1"
        disable="Grayed out"
        tooltip="Solvent Selective Pulse Width (essoftpw) "
        />
      <label loc="40 40" size="60 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <label loc="10 20" size="140 20"
        style="Label1"
        label="Solvent Selective Pulse"
        justify="Left"
        />
      <button loc="30 200" size="200 20"
        style="Label3"
        label="Recreate water refocusing shape"
        vc="make_es_shape"
        bg="VJBackground"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="30 80" size="190 95"
      style="Heading2"
      label="T1rho filter"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="PURGEgroup_ddrtc"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="55 20" size="110 20"
        style="Label1"
        label="Purge Pulse"
        justify="Left"
        />
      <check loc="35 20" size="20 20"
        style="PlainText"
        vq="prg_flg"
        vc="prg_flg='y' ddrtc=ddrtc-2*pw/3.14"
        vc2="prg_flg='n' ddrtc=ddrtc+2*pw/3.14"
        set="$VALUE=(prg_flg='y' )"
        tooltip="Turn purge pulse on/off (prg_flg)"
        />
      <entry loc="100 40" size="50 20"
        style="PlainText"
        vq="prgtime prg_flg"
        vc="prgtime=$VALUE/1000"
        set="$VALUE=prgtime*1000"
        show="$ENABLE=(prg_flg='y' )"
        digits="1"
        disable="Grayed out"
        tooltip="Purge Pulse length (prgtime) in milliseconds"
        />
      <entry loc="100 60" size="50 20"
        style="PlainText"
        vq="prgpwr prg_flg"
        vc="prgpwr=$VALUE"
        set="$VALUE=prgpwr"
        show="$ENABLE=(prg_flg='y' )"
        digits="1"
        disable="Grayed out"
        tooltip="Power level for the Purge Pulse (prgpwr)"
        />
      <label loc="10 60" size="90 20"
        style="Label1"
        label="at power"
        justify="Left"
        />
      <label loc="10 40" size="90 20"
        style="Label1"
        label="pulse duration"
        justify="Left"
        />
      <label loc="150 40" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
    </group>
    <radio loc="270 0" size="230 25"
      style="PlainText"
      label="Turn Excitation Sculpting on/off"
      vq="ES_flg"
      vc="ES_flg='y' dps"
      vc2="ES_flg='n' dps"
      set="$VALUE=(ES_flg='y')"
      />
    <textmessage loc="10 0" size="240 25"
      style="Heading3"
      label="STD_ES"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
