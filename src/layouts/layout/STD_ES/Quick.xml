<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="670 260"
    style="Heading2"
    label="Defaults"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <group loc="30 35" size="330 155"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="s2pulacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <group loc="15 60" size="290 80"
          vq="procdim wstart"
          show="$SHOW=((procdim&lt;1) and (wstart&lt;&gt;''))"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          reference="Optimizepowers"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          rows="1"
          columns="1"
          >
          <check loc="130 40" size="65 20"
            style="PlainText"
            vq="doscout ESpwrcalflg"
            vc="ESpwrcalflg='y'"
            vc2="ESpwrcalflg='n'"
            set="$VALUE=(ESpwrcalflg='y')"
            show="$ENABLE=(doscout=1)*2-1"
            />
          <label loc="20 40" size="110 20"
            style="Label1"
            label="Optimize powers"
            justify="Left"
            />
          <entry loc="130 20" size="70 20"
            style="PlainText"
            vq="tofh2o doscout"
            vc="tofh2o=$VALUE tof=tofh2o"
            set="$VALUE=tofh2o"
            show="$ENABLE=(doscout=0)*2-1"
            digits="1"
            disable="Grayed out"
            />
          <label loc="50 20" size="80 20"
            style="Label1"
            label="Water offset"
            justify="Left"
            />
          <check loc="130 0" size="65 20"
            style="PlainText"
            vq="dscout"
            vc="doscout=1"
            vc2="doscout=0"
            set="$VALUE=(doscout=1)"
            />
          <label loc="0 0" size="130 20"
            style="Label1"
            label="Do scout experiment"
            justify="Left"
            />
        </group>
        <entry loc="145 0" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq"
          vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
          set="invsetsw:$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="215 0" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq sw"
          vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
          set="invsetsw:$f1,$f2 $VALUE=$f1"
          digits="1"
          disable="Grayed out"
          />
        <label loc="35 40" size="105 20"
          style="Label1"
          label="Relaxation delay"
          justify="Left"
          />
        <menu loc="145 40" size="70 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="0"
            chval="0"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <menu loc="145 20" size="70 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
          <mlabel 
            label="4096"
            chval="4096"
            />
        </menu>
        <label loc="10 0" size="135 20"
          style="Label1"
          label="Spectral width -  from"
          justify="Left"
          />
        <label loc="195 0" size="20 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <label loc="270 0" size="40 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <label loc="220 40" size="60 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="35 20" size="105 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 5" size="620 25"
      border="RaisedBevel"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="10 0" size="355 25"
        style="Info"
        label="Experiment: STD_ES   Solvent: cdcl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="Info"
        label="Observe: H1          Decoupler: C13"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="400 35" size="235 220"
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="5 0" size="135 20"
        style="Label1"
        label="Receiver gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="145 20" size="80 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 45" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <label loc="0 150" size="220 20"
          style="Info"
          label="More options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="10 105" size="200 25"
          style="Info"
          label=" "
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 25" size="130 20"
          style="Label1"
          label="After STD_ES acq"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq'"
          />
        <textmessage loc="5 5" size="130 20"
          style="Label1"
          label="Before STD_ES acq"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="Info"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Sample name:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="Start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="Info"
          label=" "
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
  </group>
</template>
