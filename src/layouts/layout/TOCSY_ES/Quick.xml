<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="650 260"
    style="Heading2"
    label="Defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 35" size="395 200"
      style="Heading2"
      label="Acquisition Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="60 30" size="305 120"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="TOCSYacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <menu loc="160 5" size="70 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg="VJBackground"
          editable="No"
          >
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <menu loc="160 25" size="70 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg="VJBackground"
          editable="No"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <group loc="10 65" size="285 50"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          reference="MixXselect"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <label loc="5 0" size="115 20"
            style="Label1"
            label="Spinlock Duration"
            justify="Left"
            />
          <group loc="130 0" size="135 20"
            bg="transparent"
            border="None"
            tab="no"
            enable="no"
            useref="no"
            subtype="Convenience"
            >
            <menu loc="20 0" size="70 20"
              style="PlainText"
              label="menu"
              vq="mixT"
              vc="mixT=$VALUE"
              set="$VALUE=mixT"
              bg="VJBackground"
              editable="No"
              >
              <mlabel 
                label="30"
                chval="0.03"
                />
              <mlabel 
                label="40"
                chval="0.04"
                />
              <mlabel 
                label="50"
                chval="0.05"
                />
              <mlabel 
                label="60"
                chval="0.06"
                />
              <mlabel 
                label="70"
                chval="0.07"
                />
              <mlabel 
                label="80"
                chval="0.08"
                />
              <mlabel 
                label="90"
                chval="0.09"
                />
              <mlabel 
                label="100"
                chval="0.1"
                />
              <mlabel 
                label="150"
                chval="0.15"
                />
            </menu>
            <label loc="95 0" size="30 20"
              style="Label1"
              label="ms"
              justify="Left"
              />
          </group>
        </group>
        <label loc="15 45" size="125 20"
          style="Label1"
          label="Spinlock Pattern"
          justify="Left"
          />
        <label loc="15 25" size="110 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="15 5" size="145 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
        <textmessage loc="160 45" size="70 20"
          style="PlainText"
          label="mlev17"
          vq="slpatT"
          set="$VALUE=slpatT"
          />
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="homcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="345 25"
        style="Heading3"
        label="Experiment: TOCSY   Solvent: CDCl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="360 0" size="255 25"
        style="Label3"
        label="H1-H1 Correlation     [ Decoupler: C13 ]"
        vq="tn dn"
        set="$VALUE=tn+'-'+tn+' Correlation     [ Decoupler: '+dn+' ]'"
        />
    </group>
    <group loc="400 35" size="235 220"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="5 25" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label4"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="Label3"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="Label3"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="PlainText"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="PlainText"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="Label1"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Heading4"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label3"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label3"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        label="30"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
  </group>
</template>
