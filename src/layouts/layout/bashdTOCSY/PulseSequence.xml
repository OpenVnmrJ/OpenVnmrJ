        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <button loc="75 60" size="35 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        bg=""
        decor1=""
        />
      <menu loc="75 40" size="90 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        bg=""
        editable="No"
        >
        <mlabel 
          label="dipsi2"
          chval="dipsi2"
          />
        <mlabel 
          label="dipsi3"
          chval="dipsi3"
          />
      </menu>
      <label loc="10 40" size="65 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
