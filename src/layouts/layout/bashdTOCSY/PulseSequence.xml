        vq="zqfpwr2 Gzqfilt"
        vc="zqfpwr2=$VALUE"
        set="$VALUE=zqfpwr2"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="155 120" size="60 20"
        style="PlainText"
        vq="gzlvlzq2 Gzqfilt"
        vc="gzlvlzq2=$VALUE"
        set="$VALUE=gzlvlzq2"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="90 120" size="60 20"
        style="PlainText"
        vq="gzlvlzq1 Gzqfilt"
        vc="gzlvlzq1=$VALUE"
        set="$VALUE=gzlvlzq1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <label loc="10 120" size="55 20"
        style="Label1"
        label="G-levels"
        justify="Left"
