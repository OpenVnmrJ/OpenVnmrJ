        style="PlainText"
        vq="gt2 Gzqfilt"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="2"
        />
      <entry loc="155 165" size="60 20"
        style="PlainText"
        vq="gzlvl2 Gzqfilt"
        vc="gzlvl2=$VALUE"
