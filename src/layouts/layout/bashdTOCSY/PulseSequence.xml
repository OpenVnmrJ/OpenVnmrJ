<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="725 275"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="455 25" size="250 215"
      style="Heading1"
      label="Double PFG spinecho"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="dpfgsegroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 185" size="195 20"
        style="Label1"
        label="Homonuclear Doupling (F1)"
        vq="homodec"
        vc="homodec='y'"
        vc2="homodec='n'"
        set="$VALUE=(homodec='y')"
        />
      <button loc="85 160" size="135 20"
        style="Heading2"
        label="make shapes"
        vq="selfrqc selfrqppm"
        vc="suselfrq"
        show="on('selfrqc'):$s1 on('selfrqppm'):$s2 $ENABLE=(($s1&gt;0) or ($s2&gt;0))"
        bg=""
        decor1=""
        />
      <label loc="10 140" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 120" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
