      <entry loc="105 60" size="110 20"
        style="PlainText"
        vq="zqfpat2 Gzqfilt"
        vc="zqfpat2='$VALUE'"
        set="$VALUE=zqfpat2"
        show="$ENABLE=(Gzqfilt='y')"
        />
      <entry loc="155 80" size="60 20"
        style="PlainText"
        vq="zqfpw2 Gzqfilt"
        vc="zqfpw2=$VALUE*1000"
        set="$VALUE=zqfpw2/1000"
        show="$ENABLE=(Gzqfilt='y')"
