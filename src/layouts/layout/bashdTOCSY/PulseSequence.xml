        style="PlainText"
        vq="gtA"
        vc="gtA=$VALUE/1000"
        set="$VALUE=gtA*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 140" size="60 20"
        style="PlainText"
        vq="gtB"
        vc="gtB=$VALUE/1000"
        set="$VALUE=gtB*1000"
