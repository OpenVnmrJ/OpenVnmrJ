<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Acquisition" element="pages" type="acquisition" >
  <group size="700 225"
    style="Heading1"
    label="Acquisition"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Acquisition"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="265 150" size="365 75"
      style="Heading1"
      label="Receiver"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <entry loc="225 45" size="45 20"
        style="PlainText"
        vq="alfa"
        vc="alfa=$VALUE"
        set="$VALUE=alfa"
        digits="1"
        disable="Grayed out"
        />
      <label loc="190 45" size="35 20"
        style="Label1"
        label="alfa"
        justify="Left"
        />
      <entry loc="125 45" size="60 20"
        style="PlainText"
        vq="rof2"
        vc="rof2=$VALUE"
        set="$VALUE=rof2"
        digits="2"
        disable="Grayed out"
        />
      <label loc="85 45" size="40 20"
        style="Label1"
        label="rof2"
        justify="Right"
        />
      <label loc="10 45" size="75 20"
        style="Label1"
        label="Timing (µs)"
        justify="Left"
        />
      <group loc="270 45" size="95 20"
        style="Heading1"
        label="Calibration"
        vq="Console"
        show="$SHOW=(Console='vnmrs')"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="40 0" size="45 20"
          style="PlainText"
          vq="ddrtc"
          vc="ddrtc=$VALUE"
          set="$VALUE=ddrtc"
          digits="1"
          disable="Grayed out"
          />
        <button loc="0 0" size="40 20"
          style="Heading2"
          label="ddrtc"
          vc="setddrtc"
          />
      </group>
      <check loc="190 20" size="60 20"
        style="Label1"
        label="Auto"
        vq="gain"
        vc="gain='n'"
        vc2="gain='y'"
        set="off('gain'):$VALUE"
        />
      <entry loc="125 20" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        disable="Grayed out"
        />
      <label loc="10 20" size="90 20"
        style="Label1"
        label="Receiver gain"
        justify="Left"
        />
    </group>
    <group loc="265 25" size="365 125"
      style="Heading1"
      label="Excitation"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Excitation"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="245 80" size="20 20"
        style="Label1"
        label="or"
        justify="Right"
        />
      <label loc="245 40" size="20 20"
        style="Label1"
        label="or"
        justify="Right"
        />
      <entry loc="270 40" size="35 20"
        style="PlainText"
        vq="p1 pw90"
        vc="p1($VALUE)"
        set="$VALUE=90*p1[1]/pw90"
        show="$VALUE=(pw90&gt;0)*2-1 if ($VALUE&gt;-0.5) then $VALUE=(p1[1]&gt;0) endif"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="270 80" size="35 20"
        style="PlainText"
        vq="pw pw90"
        vc="pw($VALUE)"
        set="$VALUE=90*pw[1]/pw90"
        show="$VALUE=(pw90 &gt; 0)*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="310 80" size="55 20"
        style="Label1"
        label="degrees"
        justify="Left"
        />
      <label loc="310 40" size="55 20"
        style="Label1"
        label="degrees"
        justify="Left"
        />
      <menu loc="185 80" size="55 20"
        style="PlainText"
        label="menu"
        vq="pw"
        vc="parunits('set','pw','$VALUE')"
        set="parunits('get','pw'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="125 20" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="$ENABLE=(d1[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="125 40" size="60 20"
        style="PlainText"
        vq="p1"
        vc="vnmrunits('set','p1',$VALUE)"
        set="vnmrunits('get','p1'):$VALUE"
        show="$ENABLE=(p1[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="125 60" size="60 20"
        style="PlainText"
        vq="d2"
        vc="vnmrunits('set','d2',$VALUE)"
        set="vnmrunits('get','d2'):$VALUE"
        show="$ENABLE=(d2[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="125 80" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="$ENABLE=(pw[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <menu loc="185 60" size="55 20"
        style="PlainText"
        vq="d2"
        vc="parunits('set','d2','$VALUE')"
        set="parunits('get','d2'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="185 40" size="55 20"
        style="PlainText"
        label="menu"
        vq="p1"
        vc="parunits('set','p1','$VALUE')"
        set="parunits('get','p1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="185 20" size="55 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <group loc="10 100" size="355 20"
        style="Heading1"
        label="Calibration"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="195 0" size="60 20"
          style="Label1"
          label="at power"
          justify="Right"
          />
        <entry loc="260 0" size="35 20"
          style="PlainText"
          vq="tpwr"
          vc="tpwr=$VALUE"
          set="$VALUE=tpwr"
          digits="0"
          disable="Grayed out"
          />
        <label loc="300 0" size="25 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <label loc="180 0" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <entry loc="115 0" size="60 20"
          style="PlainText"
          vq="pw90"
          vc="pw90=$VALUE"
          set="$VALUE=pw90"
          digits="2"
          disable="Grayed out"
          />
        <label loc="70 0" size="40 20"
          style="Label1"
          label="pw90"
          justify="Right"
          />
        <label loc="0 0" size="75 20"
          style="Label1"
          label="Calibration"
          justify="Left"
          />
      </group>
      <label loc="10 60" size="110 20"
        style="Label1"
        label="Inter-pulse delay"
        justify="Left"
        />
      <label loc="10 80" size="110 20"
        style="Label1"
        label="Observe pulse"
        justify="Left"
        />
      <label loc="10 40" size="90 20"
        style="Label1"
        label="First pulse"
        justify="Left"
        />
      <label loc="10 20" size="110 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
    </group>
    <group loc="5 115" size="255 110"
      style="Heading1"
      label="Scans"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Scans"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 60" size="100 20"
        style="Label1"
        label="Steady state"
        vq="ss"
        vc="ss='y'"
        vc2="ss='n'"
        set="on('ss'):$v $VALUE=($v and ss&lt;&gt;0)"
        />
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="bs"
        vc="bs=$VALUE"
        set="$VALUE=bs"
        disable="Grayed out"
        />
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="on('ss'):$VALUE if (ss=0) then $VALUE=0 endif"
        disable="Grayed out"
        />
      <textmessage loc="110 40" size="60 20"
        style="Info"
        label="0"
        vq="ct"
        set="$VALUE=ct"
        />
      <entry loc="110 20" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        disable="Grayed out"
        />
      <check loc="175 80" size="80 20"
        style="Label1"
        label="Interleave"
        vq="il"
        vc="il='y'"
        vc2="il='n'"
        set="if(il = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 80" size="90 20"
        style="Label1"
        label="Block size"
        vq="bs"
        vc="bs=y"
        vc2="bs=n"
        set="on('bs'):$VALUE"
        />
      <label loc="10 40" size="90 20"
        style="Label1"
        label="Completed"
        justify="Left"
        />
      <label loc="10 20" size="90 20"
        style="Label1"
        label="Requested"
        justify="Left"
        />
    </group>
    <textmessage loc="10 0" size="240 25"
      style="Info"
      label="PROTON"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
    <group loc="5 25" size="255 90"
      style="Heading1"
      label="Data"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="190 40" size="55 20"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="190 20" size="55 20"
        style="PlainText"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <label loc="10 60" size="120 20"
        style="Label1"
        label="Complex points"
        justify="Left"
        />
      <label loc="10 40" size="120 20"
        style="Label1"
        label="Acquisition time"
        justify="Left"
        />
      <label loc="10 20" size="120 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
      <entry loc="130 60" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        disable="Grayed out"
        />
      <entry loc="130 40" size="60 20"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="130 20" size="60 20"
        style="PlainText"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
    </group>
  </group>
</template>
