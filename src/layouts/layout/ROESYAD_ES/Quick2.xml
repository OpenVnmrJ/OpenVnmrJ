<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick2" element="pages" type="acquisition" >
  <group size="670 260"
    style="Heading2"
    label="defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&lt;30)"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick2"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 35" size="395 200"
      style="Heading2"
      label="Acquisition Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="25 30" size="355 140"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="ROESYacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <menu loc="185 5" size="95 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg="VJBackground"
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
        </menu>
        <label loc="285 45" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="310 65" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <menu loc="185 25" size="95 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg="VJBackground"
          editable="No"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <textmessage loc="120 65" size="65 20"
          style="Label3"
          vq="slpatR"
          set="$VALUE=slpatR"
          />
        <menu loc="185 45" size="95 20"
          style="PlainText"
          label="menu"
          vq="d1 satmode satdly"
          vc="satdly=$VALUE d1=$VALUE"
          set="if (satmode='y') then $VALUE=satdly else $VALUE=d1 endif"
          bg="VJBackground"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="1.5"
            chval="1.5"
            />
          <mlabel 
            label="3"
            chval="3"
            />
        </menu>
        <label loc="185 65" size="60 20"
          style="Label1"
          label="Strength:"
          justify="Left"
          />
        <textmessage loc="245 65" size="65 20"
          style="Label3"
          vq="spinlockR"
          set="$VALUE=spinlockR"
          />
        <label loc="15 65" size="105 20"
          style="Label1"
          label="Spinlock pattern"
          justify="Left"
          />
        <label loc="15 25" size="140 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="15 5" size="145 20"
          style="Label1"
          label="Scans per t1 increment"
          justify="Left"
          />
        <group loc="15 85" size="330 50"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          reference="MixXselect"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <group loc="145 0" size="180 25"
            bg="transparent"
            border="None"
            tab="no"
            enable="no"
            useref="no"
            subtype="Convenience"
            >
            <menu loc="25 0" size="95 20"
              style="PlainText"
              label="menu"
              vq="mixR"
              vc="mixR=$VALUE"
              set="$VALUE=mixR"
              bg="VJBackground"
              editable="No"
              >
              <mlabel 
                label="100"
                chval="0.1"
                />
              <mlabel 
                label="200"
                chval="0.2"
                />
              <mlabel 
                label="300"
                chval="0.3"
                />
              <mlabel 
                label="400"
                chval="0.4"
                />
              <mlabel 
                label="500"
                chval="0.5"
                />
            </menu>
            <label loc="125 0" size="25 20"
              style="Label1"
              label="ms"
              justify="Left"
              />
          </group>
          <label loc="0 0" size="130 20"
            style="Label1"
            label="Spinlock Mixing time"
            justify="Left"
            />
        </group>
        <group loc="5 45" size="140 25"
          bg="transparent"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="satdlyd1group"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <group loc="10 0" size="125 20"
            vq="satmode"
            show="$satmode1='' substr(satmode,1,1):$satmode if ($satmode='y') then $SHOW=1 else $SHOW=0 endif"
            bg="transparent"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
            <label loc="0 0" size="115 20"
              style="Label1"
              label="Relaxation time"
              justify="Left"
              />
          </group>
          <group loc="0 0" size="110 20"
            vq="satmode"
            show="$satmode1='' substr(satmode,1,1):$satmode if ($satmode='n') then $SHOW=1 else $SHOW=0 endif"
            bg="transparent"
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
            <label loc="10 0" size="100 20"
              style="Label1"
              label="Relaxation time"
              justify="Left"
              />
          </group>
        </group>
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="homcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="345 25"
        style="Heading3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="360 0" size="255 25"
        style="Label3"
        vq="tn dn"
        set="$VALUE=tn+'-'+tn+' Correlation     [ Decoupler: '+dn+' ]'"
        />
    </group>
    <group loc="400 35" size="235 220"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="5 25" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label4"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="Label3"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="Label3"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="PlainText"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="PlainText"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="Label1"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Heading4"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label3"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label3"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
  </group>
</template>
