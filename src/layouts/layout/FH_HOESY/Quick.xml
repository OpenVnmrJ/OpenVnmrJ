<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="640 250"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="400 35" size="230 205"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 25" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          label=" "
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          label="After FH_HOESY acq:"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          label="Before FH_HOESY acq:"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label="FLUORINE"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        label="30"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
    <group loc="5 35" size="395 200"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="30 75" size="335 100"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="HETCORacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <menu loc="200 25" size="90 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg=""
          editable="No"
          >
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <check loc="20 70" size="270 20"
          style="Label1"
          label="Make coherence selection with gradients"
          vq="grad_sel"
          vc="grad_sel='y' f1coef='1 0 1 0 0 -1 0 1'"
          vc2="grad_sel='n' f1coef=''"
          set="$VALUE=(grad_sel='y')"
          />
        <group loc="20 45" size="300 20"
          style="Heading1"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <menu loc="180 0" size="90 20"
            style="PlainText"
            vq="mix"
            vc="mix=$VALUE/1000"
            set="$VALUE=mix*1000"
            bg=""
            editable="No"
            >
            <mlabel 
              label="30"
              chval="30"
              />
            <mlabel 
              label="60"
              chval="60"
              />
            <mlabel 
              label="100"
              chval="100"
              />
            <mlabel 
              label="200"
              chval="200"
              />
            <mlabel 
              label="300"
              chval="300"
              />
            <mlabel 
              label="400"
              chval="400"
              />
            <mlabel 
              label="500"
              chval="500"
              />
            <mlabel 
              label="700"
              chval="700"
              />
          </menu>
          <label loc="275 0" size="25 20"
            style="Label1"
            label="ms"
            justify="Left"
            />
          <label loc="0 0" size="120 20"
            style="Label1"
            label="HOESY mixing time"
            justify="Left"
            />
        </group>
        <menu loc="200 5" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
        </menu>
        <label loc="20 25" size="110 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="20 5" size="140 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
      </group>
      <menu loc="230 30" size="90 20"
        style="PlainText"
        vq="sw1 dof reffrq1 rfl1 rfp1"
        vc="setsw1(dn,$VALUE):dof"
        set="$s1='' $s2='' invsetsw1:$f1,$f2 format($f1,1,0):$s1 format($f2,1,0):$s2 $VALUE=$s1+','+$s2"
        bg=""
        editable="No"
        >
        <mlabel 
          label="14  →  -2"
          chval="14,-2"
          />
        <mlabel 
          label="12  →  -1"
          chval="12,-1"
          />
        <mlabel 
          label="10  →  0"
          chval="10,0"
          />
        <mlabel 
          label="8  →  1"
          chval="8,1"
          />
      </menu>
      <group loc="85 50" size="240 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="swstartend"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="15 0" size="90 20"
          style="Heading1"
          label="(...or enter):"
          justify="Right"
          />
        <entry loc="110 0" size="50 20"
          style="PlainText"
          vq="sw1 dof reffrq1 rfl1 rfp1"
          vc="invsetsw1:$f1,$f2 setsw1(dn,$VALUE,$f2):dof"
          set="invsetsw1:$f1,$f2 $VALUE=$f1"
          digits="0"
          disable="Grayed out"
          />
        <label loc="160 0" size="25 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <entry loc="185 0" size="50 20"
          style="PlainText"
          vq="sw1 dof reffrq1 rfl1 rfp1"
          vc="invsetsw1:$f1,$f2 setsw1(dn,$f1,$VALUE):dof"
          set="invsetsw1:$f1,$f2 $VALUE=$f2"
          digits="0"
          disable="Grayed out"
          />
      </group>
      <label loc="325 50" size="35 20"
        style="Label1"
        label="ppm"
        justify="Left"
        />
      <label loc="50 30" size="180 20"
        style="Label1"
        label="H1 Spectral Width [ppm] [F1]"
        justify="Left"
        />
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="360 25"
        style="PlainText"
        label="Experiment: FH_HOESY   Solvent: cdcl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="375 0" size="240 25"
        style="PlainText"
        label="Observe: F19       F19-H1 Correlation"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
    </group>
  </group>
</template>
