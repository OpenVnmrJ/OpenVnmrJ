<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="650 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 25" size="220 190"
      style="Heading1"
      label="SpinLock"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MLEV17c"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="170 100" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <entry loc="110 100" size="55 20"
        style="PlainText"
        vq="slpwT"
        vc="slpwT=$VALUE"
        set="$VALUE=slpwT"
        digits="3"
        disable="Grayed out"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="110 80" size="55 20"
        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <label loc="170 80" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="170 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <button loc="70 60" size="40 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        bg=""
        decor1=""
        />
      <label loc="10 60" size="55 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 40" size="60 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <menu loc="70 40" size="135 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        editable="No"
        >
        <mlabel 
          label="MLEV17"
          chval="mlev17"
          />
        <mlabel 
          label="clean MLEV17"
          chval="mlev17c"
          />
      </menu>
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Spinlock time"
        justify="Left"
        />
      <entry loc="110 20" size="55 20"
        style="PlainText"
        vq="d2"
        vc="d2=$VALUE/1000"
        set="$VALUE=d2*1000"
        disable="Grayed out"
        />
      <label loc="170 20" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="T1Rmeas"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
