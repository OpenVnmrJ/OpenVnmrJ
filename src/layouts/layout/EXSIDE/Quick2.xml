<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick2" element="pages" type="acquisition" >
  <group size="655 275"
    style="Heading1"
    label="defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&lt;30)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick2"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="400 35" size="235 215"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 25" size="220 185"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
    <group loc="5 35" size="395 210"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 25" size="380 180"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="gHSQCADacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="5 80" size="370 100"
          vq="customflag procdim"
          show="$SHOW=((procdim&lt;1) and (customflag='y'))"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="customizepar-sppmXY"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <group loc="0 0" size="335 30"
            vq="customflag"
            show="$SHOW=(customflag='y')"
            bg=""
            border="None"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
            <check loc="110 5" size="55 20"
              style="Label1"
              label="Later"
              vq="wds"
              vc="rmpar('Array') selppm2frq('init',0) wds='wds=`suselfrq`'"
              vc2="wds=''"
              set="strstr(wds,'wds'):$is if ($is&gt;0) then $VALUE=1 else $VALUE=0 endif"
              />
            <label loc="10 5" size="95 20"
              style="Label1"
              label="Define Bands"
              />
            <check loc="200 5" size="60 20"
              style="Label1"
              label="Now"
              vq="wds"
              vc="mkpar('integer','Array',0) selppm2frq('init',Array) wds='wstart=`suselfrq`'"
              vc2="rmpar('Array') selppm2frq('init',0) wds=''"
              set="strstr(wds,'wstart'):$is if ($is&gt;0) then $VALUE=1 else $VALUE=0 endif"
              />
          </group>
          <group loc="0 25" size="370 75"
            vq="wds"
            show="strstr(wds,'wds'):$is if ($is&lt;1) then $SHOW=1 else $SHOW=0 endif"
            bg=""
            border="None"
            tab="no"
            enable="no"
            reference="customizepar-sppm"
            useref="no"
            subtype="Untitled"
            expanded="yes"
            >
            <entry loc="245 5" size="40 20"
              style="PlainText"
              vq="Array"
              vc="mkpar('integer','Array',0) Array=$VALUE if (Array &gt; 5) then Array=5 endif  selppm2frq('init',Array)"
              set="$VALUE=Array"
              />
            <label loc="10 5" size="225 20"
              style="Label1"
              label="No. of Selective Freqencies (max: 5)"
              />
            <label loc="335 50" size="30 20"
              style="Label1"
              label="Hz"
              />
            <label loc="335 30" size="30 20"
              style="Label1"
              label="ppm"
              />
            <label loc="10 50" size="100 20"
              style="Label1"
              label="Selective Width"
              />
            <entry loc="110 50" size="40 20"
              style="PlainText"
              vq="selfrqw"
              vc="if (Array &gt; 0) then selfrqw[1]=$VALUE  endif"
              set="$sps=size('selfrqw') if ($sps &gt; 0) then $VALUE=selfrqw[1] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 0)"
              digits="1"
              />
            <entry loc="155 50" size="40 20"
              style="PlainText"
              vq="selfrqw"
              vc="if (Array &gt; 1) then selfrqw[2]=$VALUE  endif"
              set="$sps=size('selfrqw') if ($sps &gt; 1) then $VALUE=selfrqw[2] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 1)"
              digits="1"
              />
            <entry loc="200 50" size="40 20"
              style="PlainText"
              vq="selfrqw"
              vc="if (Array &gt; 2) then selfrqw[3]=$VALUE  endif"
              set="$sps=size('selfrqw') if ($sps &gt; 2) then $VALUE=selfrqw[3] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 2)"
              digits="1"
              />
            <entry loc="245 50" size="40 20"
              style="PlainText"
              vq="selfrqw"
              vc="if (Array &gt; 3) then selfrqw[4]=$VALUE  endif"
              set="$sps=size('selfrqw') if ($sps &gt; 3) then $VALUE=selfrqw[4] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 3)"
              digits="1"
              />
            <entry loc="290 50" size="40 20"
              style="PlainText"
              vq="selfrqw"
              vc="if (Array &gt; 4) then selfrqw[5]=$VALUE  endif"
              set="$sps=size('selfrqw') if ($sps &gt; 4) then $VALUE=selfrqw[5] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 4)"
              digits="1"
              />
            <entry loc="290 30" size="40 20"
              style="PlainText"
              vq="selfrqppm "
              vc="if (Array &gt; 4) then selfrqppm[5]=$VALUE  endif"
              set="$sps=size('selfrqppm') if ($sps &gt; 4) then $VALUE=selfrqppm[5] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 4)"
              digits="2"
              />
            <entry loc="245 30" size="40 20"
              style="PlainText"
              vq="selfrqppm "
              vc="if (Array &gt; 3) then selfrqppm[4]=$VALUE  endif"
              set="$sps=size('selfrqppm') if ($sps &gt; 3) then $VALUE=selfrqppm[4] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 3)"
              digits="2"
              />
            <entry loc="200 30" size="40 20"
              style="PlainText"
              vq="selfrqppm "
              vc="if (Array &gt; 2) then selfrqppm[3]=$VALUE  endif"
              set="$sps=size('selfrqppm') if ($sps &gt; 2) then $VALUE=selfrqppm[3] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 2)"
              digits="2"
              />
            <entry loc="155 30" size="40 20"
              style="PlainText"
              vq="selfrqppm "
              vc="if (Array &gt; 1) then selfrqppm[2]=$VALUE  endif"
              set="$sps=size('selfrqppm') if ($sps &gt; 1) then $VALUE=selfrqppm[2] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 1)"
              digits="2"
              />
            <entry loc="110 30" size="40 20"
              style="PlainText"
              vq="selfrqppm "
              vc="if (Array &gt; 0) then selfrqppm[1]=$VALUE  endif"
              set="$sps=size('selfrqppm') if ($sps &gt; 0) then $VALUE=selfrqppm[1] else $VALUE=0 endif"
              show="$ENABLE=(Array &gt; 0)"
              digits="2"
              />
            <label loc="10 30" size="100 20"
              style="Label1"
              label="Selective Freq."
              />
          </group>
        </group>
        <group loc="50 95" size="275 75"
          vq="procdim wds"
          vc="crmode='b' ds"
          vc2="clear(2)"
          show="if (procdim&gt;0) and (wds&lt;&gt;'') then $SHOW=1 else $SHOW=0 endif"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="ds_selfrq"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="15 5" size="245 20"
            style="Heading1"
            label="Select Excitation Bands using cursors"
            justify="Center"
            />
          <button loc="55 50" size="80 20"
            style="Heading2"
            label="CLEAR"
            vc="selfrqc=0 selfrqw=0 selshapeA=selshapeA[1] off('selfrqc') clear(2) ds"
            bg=""
            decor1=""
            />
          <button loc="145 50" size="80 20"
            style="Heading2"
            label="ShowList"
            vq="selfrqc"
            vc="showfreq('selfrqc')"
            show="on('selfrqc'):$ENABLE"
            bg=""
            decor1=""
            />
          <button loc="185 25" size="80 20"
            style="Heading2"
            label="Select"
            vq="procdim"
            vc="if crmode='c' then write('error','Use 2 cursors') else setselfrqc endif ds('restart')"
            bg=""
            decor1=""
            />
          <button loc="100 25" size="80 20"
            style="Heading2"
            label="Expand/Full"
            vq="procdim"
            vc="if (procdim=1) then ds('expand') else dconi('expand') endif"
            bg=""
            decor1=""
            />
          <button loc="10 25" size="80 20"
            style="Heading2"
            label="Box/Cursor"
            vq="procdim"
            vc="if (procdim=1) then ds('toggle') else dconi('toggle') endif"
            bg=""
            decor1=""
            />
        </group>
        <label loc="180 60" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <entry loc="120 60" size="55 20"
          style="PlainText"
          vq="jnxh"
          vc="jnxh=$VALUE"
          set="$VALUE=jnxh"
          digits="1"
          disable="Grayed out"
          />
        <label loc="20 60" size="85 20"
          style="Label1"
          label="n-Bond Jxh"
          justify="Left"
          />
        <label loc="20 35" size="95 20"
          style="Label1"
          label="Scans per incr."
          justify="Left"
          />
        <menu loc="120 35" size="55 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <label loc="210 60" size="50 20"
          style="Label1"
          label="J-scale"
          justify="Left"
          />
        <entry loc="295 60" size="55 20"
          style="PlainText"
          vq="jscale"
          vc="jscale=$VALUE"
          set="$VALUE=jscale"
          digits="0"
          disable="Grayed out"
          />
        <label loc="210 35" size="85 20"
          style="Label1"
          label="t1 increments"
          justify="Left"
          />
        <menu loc="295 35" size="55 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg=""
          editable="No"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <label loc="260 10" size="35 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <entry loc="295 10" size="55 20"
          style="PlainText"
          vc="invsetsw1:$f1,$f2 setsw1(dn,$VALUE,$f2)"
          set="invsetsw1:$f1,$f2 $VALUE=$f1"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="205 10" size="55 20"
          style="PlainText"
          vc="invsetsw1:$f1,$f2 setsw1(dn,$f1,$VALUE)"
          set="invsetsw1:$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
        <textmessage loc="20 10" size="170 20"
          style="PlainText"
          vq="dn"
          set="$VALUE=dn+' Spectral Width'"
          />
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="360 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="375 0" size="240 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
    </group>
  </group>
</template>
