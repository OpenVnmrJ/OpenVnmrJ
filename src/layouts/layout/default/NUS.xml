    <check loc="10 0" size="220 25"
      style="Label1"
      label="Enable non-uniform sampling"
      vq="sampling"
      vc="cpsetNUS('on')"
      vc2="cpsetNUS('off')"
      set="if sampling='sparse' then $VALUE=1 else $VALUE=0 endif"
      />
  </group>
</template>
