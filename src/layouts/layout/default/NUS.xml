            label="shift"
            justify="Center"
            />
          <check loc="220 78" size="20 20"
            style="Label1"
            vq="sbs1"
            vc="sbs1='y'"
            vc2="sbs1='n'"
            set="on('sbs1'):$VALUE"
            />
          <entry loc="170 78" size="50 20"
            style="PlainText"
            vq="sb1 sbs1"
            vc="sbs1=$VALUE"
            set="$VALUE=sbs1"
            show="onparams('sb1','sbs1'):$VALUE"
            disable="Grayed out"
            />
          <label loc="135 78" size="35 20"
            style="Label1"
            label="shift"
            justify="Center"
            />
          <check loc="115 98" size="20 20"
            style="Label1"
            vq="gf1 gfs1"
