            justify="Left"
            />
          <check loc="220 98" size="20 20"
            style="Label1"
            vq="gfs1"
            vc="gfs1='y'"
            vc2="gfs1='n'"
            set="on('gfs1'):$VALUE"
            />
          <entry loc="170 98" size="50 20"
            style="PlainText"
