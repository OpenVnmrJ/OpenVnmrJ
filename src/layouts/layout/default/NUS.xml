            set="$VALUE=lb1"
            show="on('lb1'):$VALUE"
            disable="Grayed out"
            />
          <label loc="10 58" size="55 20"
            style="Label1"
            label="Line bd."
            justify="Left"
            />
        </group>
        <radio loc="230 20" size="55 25"
          style="PlainText"
          label="Pbox"
          vq="CSinp CSwt"
          vc="CSinp='' CSwt='y'"
          vc2="CSinp='n'"
          set="if (CSinp='' and CSwt='y') then $VALUE=1 else $VALUE=0 endif"
          />
        <radio loc="25 20" size="75 25"
          style="PlainText"
          label="None"
          vq="CSwt CStype"
          vc="CSwt='n'"
          vc2="CSwt='y'"
          set="if CSwt='n' then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
