            set="on('gf1'):$VALUE"
            />
          <entry loc="65 98" size="50 20"
            style="PlainText"
            vq="gf1"
            vc="gf1=$VALUE"
            set="$VALUE=gf1"
            show="on('gf1'):$VALUE"
            disable="Grayed out"
            />
          <label loc="10 98" size="55 20"
            style="Label1"
            label="Gaussian"
            justify="Left"
            />
          <check loc="115 78" size="20 20"
            style="Label1"
            vq="sb1 sbs1"
            vc="sb1='y'"
            vc2="sb1='n'"
            set="on('sb1'):$VALUE"
            />
          <check loc="115 58" size="20 20"
            style="Label1"
            vq="lb1"
            vc="lb1='y'"
            vc2="lb1='n'"
            set="on('lb1'):$VALUE"
            />
          <entry loc="65 78" size="50 20"
            style="PlainText"
            vq="sb1"
            vc="sb1=$VALUE"
            set="$VALUE=sb1"
            show="on('sb1'):$VALUE"
            disable="Grayed out"
            />
          <entry loc="65 58" size="50 20"
            style="PlainText"
            vq="lb1"
