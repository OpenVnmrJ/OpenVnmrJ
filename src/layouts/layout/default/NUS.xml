              />
            <mlabel 
              label="cos2"
              chval="gcos2"
              />
            <mlabel 
              label="ramp"
              chval="grampd"
              />
            <mlabel 
              label="sin(J)"
              chval="jsin"
              />
            <mlabel 
              label="cos(J)"
              chval="jcos"
              />
            <mlabel 
              label="Pbox"
              chval="Pbox"
              />
          </selmenu>
        </group>
        <group loc="10 45" size="275 125"
          vq="CSinp CStype CSwt"
          show="if (CSwt='y' and CSinp='n' and CStype='i') then $SHOW=1 else $SHOW=0 endif"
          border="None"
          tab="no"
          enable="no"
          reference="Sinebell"
