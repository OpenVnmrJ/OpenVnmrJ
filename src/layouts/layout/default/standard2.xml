<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="standard2" element="pages" type="sample" >
  <group size="815 260"
    style="Heading1"
    label="standard2"
    vq="cppauseflg customflag"
    show="$SHOW=((cppauseflg='y') or (customflag='y'))"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="standard2"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="380 5" size="195 215"
      vq="wqinit panellevel"
      show="$SHOW=(panellevel&gt;29)"
      border="Etched"
      tab="no"
      enable="no"
      reference="Equilibrateagainfor:"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="10 5" size="170 20"
        style="Label1"
        label="Experiment:"
        justify="Left"
        />
      <textmessage loc="10 25" size="180 20"
        style="Info"
        label="CARBON_01_day"
        vq="pslabel layoutargs wqinit"
        set="if (layoutargs[2]='long') then $long='_night' elseif (wqinit='acq') then $long='' else $long='_day' endif $VALUE=layoutargs[1]+$long"
        />
      <group loc="5 55" size="185 140"
        vq="panellevel"
        show="vnmrinfo('get','tempExpControl'):$tc getautoglobaltemp(wqinit):$tc2 $SHOW=($tc&gt;0 and $tc2&gt;0)"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <entry loc="90 70" size="45 20"
          style="PlainText"
          vq="temp operator"
          vc="temp=$VALUE checktemplimit"
          set="$VALUE=temp"
          show="operights('candovtinautomation'):$ok $ENABLE=($ok)*2-1"
          digits="1"
          />
        <entry loc="90 115" size="45 20"
          style="PlainText"
          vq="pad"
          vc="pad=$VALUE"
          set="$VALUE=pad"
          digits="1"
          />
        <label loc="140 115" size="25 20"
          style="Label1"
          label="sec"
          />
        <label loc="140 70" size="45 20"
          style="Label1"
          label="C"
          />
        <label loc="5 95" size="175 20"
          style="Label1"
          label="Equilibrate again for"
          justify="Left"
          />
        <label loc="5 50" size="170 20"
          style="Label1"
          label="Acquire this experiment at"
          justify="Left"
          />
        <textmessage loc="5 25" size="175 20"
          style="Info"
          vq="SQtemp"
          set="$st='' format(SQtemp,0,1):$st $VALUE='   will be at '+$st+' C'"
          />
        <textmessage loc="5 5" size="175 20"
          style="Info"
          vq="SQtemp"
          set="$VALUE='By default, this sample'"
          />
      </group>
    </group>
    <label loc="5 220" size="370 20"
      style="Info"
      label="CANNOT edit Sampleinfo during customization!"
      justify="Center"
      />
    <group loc="5 5" size="370 215"
      style="Heading1"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampinfo"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textfile loc="10 135" size="350 70"
        style="StdPar"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <label loc="10 115" size="100 20"
        style="Label1"
        label="Comment"
        justify="Left"
        />
      <entry loc="100 5" size="260 20"
        style="PlainText"
        vq=" samplename"
        vc=" samplename=samplename"
        set="$VALUE=samplename"
        show="$ENABLE=0"
        disable="Grayed out"
        />
      <entry loc="255 45" size="105 20"
        style="PlainText"
        vq=" page"
        vc=" page=page"
        set="$VALUE=page"
        show="$ENABLE=0"
        disable="Grayed out"
        />
      <entry loc="255 65" size="105 20"
        style="PlainText"
        vq="emailaddr "
        vc="emailaddr=emailaddr"
        set="$VALUE=emailaddr"
        show="$ENABLE=0"
        disable="Grayed out"
        />
      <label loc="215 65" size="45 20"
        style="Label1"
        label="Email"
        justify="Left"
        />
      <label loc="215 45" size="45 20"
        style="Label1"
        label="Page"
        justify="Left"
        />
      <entry loc="100 45" size="110 20"
        style="PlainText"
        vq="lot"
        vc=" lot=lot"
        set="$VALUE=lot"
        show="$ENABLE=0"
        disable="Grayed out"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="Lot number"
        justify="Left"
        />
      <entry loc="100 65" size="110 20"
        style="PlainText"
        vq=" notebook"
        vc=" notebook=notebook"
        set="$VALUE=notebook"
        show="$ENABLE=0"
        disable="Grayed out"
        />
      <label loc="10 65" size="90 20"
        style="Label1"
        label="Notebook"
        justify="Left"
        />
      <textmessage loc="100 90" size="230 20"
        style="Info"
        label="dmso"
        vq="solvent"
        set="$VALUE=solvent"
        />
      <label loc="10 90" size="90 20"
        style="Label1"
        label="Solvent"
        justify="Left"
        />
      <textmessage loc="100 25" size="260 20"
        style="Info"
        label="(data)"
        vq="sample sdirtmplt adirtmplt samplename"
        set="if (sample='') then $as='' $s='' Svfname(adirtmplt+'/'+sdirtmplt,''):$as substr($as,'basename'):$s $VALUE='('+$s+')' else $VALUE=sample endif"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="SampleDir"
        justify="Left"
        />
      <label loc="10 5" size="90 20"
        style="Label1"
        label="Sample name"
        justify="Left"
        />
    </group>
    <group loc="580 5" size="230 215"
      vq="wqinit"
      show="$SHOW=((wqinit='auto') or (wqinit='enter'))"
      border="Etched"
      tab="no"
      enable="no"
      reference="autoinfo"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 5" size="215 45"
        vq="wqinit"
        show="$SHOW=(wqinit='auto')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="25 20" size="190 20"
          style="Info"
          label="auto_20100408_01"
          vq="autodir globalauto"
          set="$VALUE='' substr(autodir,'basename'):$VALUE"
          />
        <label loc="5 0" size="95 20"
          style="Label1"
          label="AutoRun"
          justify="Left"
          />
      </group>
      <group loc="5 5" size="220 40"
        vq="wqinit"
        show="$SHOW=(wqinit='enter')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="20 20" size="195 20"
          style="Info"
          vq="globalenter"
          set="$VALUE='' substr(globalenter,'basename'):$VALUE"
          />
        <label loc="5 0" size="90 20"
          style="Label1"
          label="AutoFile"
          justify="Left"
          />
      </group>
      <textfile loc="10 80" size="210 125"
        style="StdPar"
        vq="userdir NextLoc"
        set="$VALUE=userdir+'/persistence/autotimeinfo'"
        show="$ENABLE=0"
        editable="no"
        wrap="no"
        units="char"
        />
      <label loc="10 50" size="115 25"
        style="Label1"
        label="Current status"
        />
      <button loc="125 50" size="95 25"
        style="Heading2"
        label="Update"
        vc="wrautoinfo(wqinit)"
        />
    </group>
  </group>
</template>
