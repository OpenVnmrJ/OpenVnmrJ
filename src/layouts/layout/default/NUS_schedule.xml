<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="NUS_schedule" element="pages" type="processing" >
  <group size="655 315"
    label="NUS Schedule"
    vq="sampling"
    vc="CSpars"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="NUS_schedule"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="0 5" size="295 270"
      style="Heading2"
      label="Schedule Builder"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="ScheduleBuilder"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="15 200" size="265 50"
        label="Profile"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <check loc="200 15" size="60 25"
          style="PlainText"
          label="Auto"
          vq="CSshowsch  CStype"
          vc="CSshowsch='y'"
          vc2="CSshowsch='n'"
          set="if CSshowsch='y' then $VALUE=1 else $VALUE=0 endif"
          tooltip="Autoshow schedule after building"
          />
        <button loc="130 15" size="65 25"
          style="PlainText"
          label="Show"
          vc="CSdsch"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="70 15" size="60 25"
          style="Label2"
          label="Delete"
          vc="CSrmsch"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 15" size="65 25"
          style="Menu4"
          label="Build"
          vc="CSmksch"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <group loc="125 35" size="50 115"
        vq="ni"
        show="on('ni'):$e if $e&gt;=0 then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="false"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="5 55" size="40 25"
          style="PlainText"
          vq="CSdensity ni nimax"
          vc="on('nimax'):$e if ($e&gt;0) then ni=nimax*$VALUE/100.0 else on('CSdensity'):$e  if ($e&gt;0) then CSdensity=$VALUE endif endif"
          set="$VALUE=0 on('nimax'):$e if ($e&gt;0) then if nimax&gt;0 then $VALUE=100*ni/nimax endif else on('CSdensity'):$e  if ($e&gt;0) then $VALUE=CSdensity endif  endif"
          show="on('ni'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="5 30" size="40 25"
          style="PlainText"
          vq="ni nimax"
          vc="exists('nimax','parameter'):$e if ($e=1) then nimax=$VALUE else exists('ni','parameter'):$e if ($e=1) then ni=$VALUE endif endif"
          set="$VALUE=0 on('nimax'):$e if ($e&gt;=0) then $VALUE=nimax else on('ni'):$e  if ($e&gt;=0) then $VALUE=ni endif  endif"
          show="on('ni'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          disable="Grayed out"
          />
        <check loc="5 0" size="40 30"
          style="PlainText"
          label="ni"
          vq="ni"
          vc="ni='y'"
          vc2="ni='n'"
          set="on('ni'):$e if($e=1) then $VALUE=1 else $VALUE=0 endif"
          />
        <check loc="10 85" size="25 25"
          style="PlainText"
          vq="phase"
          vc="exists('phase','parameter'):$e if $e=1 then phase=1,2 endif"
          vc2="exists('phase','parameter'):$e if $e=1 then phase=1 endif"
          set="$VALUE=0 exists('phase','parameter'):$e if $e=1 then $s=size('phase') if($s&gt;1) then $VALUE=1 endif endif"
          />
      </group>
      <label loc="10 95" size="115 25"
        style="Label1"
        label="Sampling Density"
        />
      <label loc="25 120" size="100 25"
        style="Label1"
        label="Hypercomplex"
        />
      <label loc="40 70" size="85 25"
        style="PlainText"
        label="Domain Size"
        justify="Left"
        />
      <label loc="60 40" size="60 25"
        style="Menu4"
        label="Domain"
        justify="Left"
        />
      <group loc="175 35" size="55 115"
        vq="ni2"
        show="on('ni2'):$e if $e&gt;=0 then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="false"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="5 55" size="40 25"
          style="PlainText"
          vq="ni2 ni2max CSdensity2"
          vc="on('ni2max'):$e if ($e&gt;0) then ni2=ni2max*$VALUE/100.0 else on('CSdensity2'):$e  if ($e&gt;0) then CSdensity2=$VALUE endif endif"
          set="$VALUE=0 on('ni2max'):$e if ($e&gt;0) then if ni2max&gt;0 then $VALUE=100*ni2/ni2max endif else on('CSdensity2'):$e  if ($e&gt;0) then $VALUE=CSdensity2 endif  endif"
          show="on('ni2'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="5 30" size="40 25"
          style="PlainText"
          vq="ni2 ni2max"
          vc="exists('ni2max','parameter'):$e if ($e=1) then ni2max=$VALUE else exists('ni2','parameter'):$e if ($e=1) then ni2=$VALUE endif endif"
          set="$VALUE=0 on('ni2max'):$e if ($e&gt;=0) then $VALUE=ni2max else on('ni2'):$e  if ($e&gt;=0) then $VALUE=ni2 endif  endif"
          show="on('ni2'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          disable="Grayed out"
          />
        <check loc="10 0" size="45 30"
          style="PlainText"
          label="ni2"
          vq="ni2"
          vc="ni2='y'"
          vc2="ni2='n'"
          set="on('ni2'):$e if($e=1) then $VALUE=1 else $VALUE=0 endif"
          />
        <check loc="10 85" size="25 25"
          style="PlainText"
          vq="phase2"
          vc="exists('phase2','parameter'):$e if $e=1 then phase2=1,2 endif"
          vc2="exists('phase2','parameter'):$e if $e=1 then phase2=1 endif"
          set="$VALUE=0 exists('phase2','parameter'):$e if $e=1 then $s=size('phase2') if($s&gt;1) then $VALUE=1 endif endif"
          />
      </group>
      <group loc="235 35" size="50 115"
        vq="ni3"
        show="on('ni3'):$e if $e&gt;=0 then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="false"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="5 55" size="40 25"
          style="PlainText"
          vq="ni3 ni3max CSdensity3"
          vc="on('ni3max'):$e if ($e&gt;0) then ni3=ni3max*$VALUE/100.0 else on('CSdensity3'):$e  if ($e&gt;0) then CSdensity3=$VALUE endif endif"
          set="$VALUE=0 on('ni3max'):$e if ($e&gt;0) then $VALUE=100*ni3/ni3max else on('CSdensity3'):$e  if ($e&gt;0) then $VALUE=CSdensity3 endif  endif"
          show="on('ni3'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="5 30" size="40 25"
          style="PlainText"
          vq="ni3 ni3max"
          vc="exists('ni3max','parameter'):$e if ($e=1) then ni3max=$VALUE else exists('ni3','parameter'):$e if ($e=1) then ni3=$VALUE endif endif"
          set="$VALUE=0 on('ni3max'):$e if ($e&gt;=0) then $VALUE=ni3max else on('ni3'):$e  if ($e&gt;=0) then $VALUE=ni3 endif  endif"
          show="on('ni3'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          disable="Grayed out"
          />
        <check loc="5 0" size="45 30"
          style="PlainText"
          label="ni3"
          vq="ni3"
          vc="ni3='y'"
          vc2="ni3='n'"
          set="on('ni3'):$e if($e=1) then $VALUE=1 else $VALUE=0 endif"
          />
        <check loc="10 85" size="25 25"
          style="PlainText"
          vq="phase3"
          vc="exists('phase3','parameter'):$e if $e=1 then phase3=1,2 endif"
          vc2="exists('phase3','parameter'):$e if $e=1 then phase3=1 endif"
          set="$VALUE=0 exists('phase3','parameter'):$e if $e=1 then $s=size('phase3') if($s&gt;1) then $VALUE=1 else $VALUE=0 endif endif"
          />
      </group>
      <check loc="60 160" size="90 25"
        style="PlainText"
        label="Random"
        vq="CSseed"
        vc="CSseed=-CSseed"
        vc2="CSseed=-CSseed"
        set="$VALUE=0 on('CSseed'):$e if $e &gt;0 then if CSseed &lt;0  then $VALUE=1 else $VALUE=0 endif endif"
        show="$VALUE=0 on('CSseed'):$e if $e &gt;0 then $VALUE=1 else $VALUE = -1 endif"
        />
      <check loc="170 160" size="95 25"
        style="PlainText"
        label="Weighted"
        vq="CSwt CStype"
        vc="CSwt='y'"
        vc2="CSwt='n'"
        set="if  CSwt='y' then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
    <group loc="295 5" size="300 270"
      style="Heading2"
      label="Profile Builder"
      vq="CSwt"
      show="if CSwt='y' then $SHOW=1 else $SHOW=0 endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="NUSProfileBuilder2"
      useref="no"
      subtype="Major"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="215 35" size="50 25"
        style="PlainText"
        vq="CSdomain CSprosize"
        set="$VALUE=CSprosize[CSdomain]"
        show="$VALUE=0"
        />
      <entry loc="150 35" size="60 25"
        style="PlainText"
        vq="CSdomain CSprofile"
        set="$VALUE=CSprofile[CSdomain]"
        show="$VALUE=0"
        />
      <selmenu loc="95 35" size="50 25"
        style="Heading3"
        vq="CSdomain"
        vc="CSdomain=$VALUE"
        set="$VALUE=CSdomain"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="ni"
          chval="1"
          />
        <mlabel 
          label="ni2"
          chval="2"
          />
        <mlabel 
          label="ni3"
          chval="3"
          />
      </selmenu>
      <label loc="25 35" size="70 25"
        style="Menu4"
        label="Domain:"
        justify="Left"
        />
      <radio loc="110 80" size="165 25"
        style="PlainText"
        label="Use Window Functions"
        vq="CSinp"
        vc="CSinp='n'"
        vc2="CSinp=''"
        />
      <radio loc="25 80" size="60 25"
        style="PlainText"
        label="Pbox"
        vq="CSinp"
        vc="CSinp=''"
        vc2="CSinp='n'"
        set="if (CSinp='') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="20 130" size="270 55"
        vq="CSinp"
        show="$SHOW=0 if(CSinp='n') then $SHOW=1 endif"
        bg="transparent"
        border="Etched"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="5 0" size="245 25"
          style="PlainText"
          label="Set lb, sb, sbs, gf and gfs in the selected"
          vq="dimension"
          justify="Left"
          />
        <label loc="5 25" size="240 25"
          style="PlainText"
          label="domain. Then click &quot;Build&quot; button"
          vq="dimension"
          justify="Left"
          />
      </group>
      <group loc="20 120" size="265 60"
        vq="CSinp"
        show="$SHOW=1 if(CSinp='n') then $SHOW=0 endif"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <selmenu loc="5 0" size="90 25"
          style="PlainText"
          vq="CSshape"
          vc="CSshape='$VALUE'"
          set="$VALUE=CSshape"
          bg="VJBackground"
          editable="Yes"
          >
          <mlabel 
            label="exp05"
            chval="gexp-0.5"
            />
          <mlabel 
            label="exp1"
            chval="gexp"
            />
          <mlabel 
            label="exp2"
            chval="gexp-2"
            />
          <mlabel 
            label="hgaus"
            chval="ghgaus"
            />
          <mlabel 
            label="cos"
            chval="gcos"
            />
          <mlabel 
            label="cos2"
            chval="gcos2"
            />
          <mlabel 
            label="ramp"
            chval="grampd"
            />
          <mlabel 
            label="sin(J)"
            chval="jsin"
            />
          <mlabel 
            label="cos(J)"
            chval="jcos"
            />
          <mlabel 
            label="Pbox"
            chval="Pbox"
            />
        </selmenu>
        <group loc="0 25" size="225 30"
          vq="CSshape"
          show="$SHOW=0 if(CSshape='Pbox') then $SHOW=1 endif"
          bg="transparent"
          border="Etched"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <entry loc="125 0" size="85 25"
            style="PlainText"
            vq="CSinp"
            vc="CSinp='$VALUE'"
            set="$VALUE=CSinp"
            show="$ENABLE=1"
            disable="Grayed out"
            />
          <label loc="0 0" size="125 25"
            style="PlainText"
            label="Pbox input filename:"
            justify="Left"
            />
        </group>
        <group loc="105 0" size="120 25"
          vq="CSshape"
          show="if(CSshape='jsin') or (CSshape='jcos') then $SHOW=1 else $SHOW=0 endif"
          bg="transparent"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <entry loc="30 0" size="45 25"
            style="PlainText"
            vq="r7"
            vc="r7=$VALUE"
            set="$VALUE=r7"
            />
          <label loc="75 0" size="30 25"
            style="PlainText"
            label="(Hz)"
            vq="dimension"
            justify="Left"
            />
          <label loc="5 0" size="25 25"
            style="PlainText"
            label="J ="
            justify="Left"
            />
        </group>
      </group>
      <group loc="20 200" size="270 50"
        label="Profile"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <check loc="201 15" size="60 25"
          style="PlainText"
          label="Auto"
          vq="CSshowpro"
          vc="CSshowpro='y'"
          vc2="CSshowpro='n'"
          set="$VALUE=(CSshowpro='y')"
          />
        <button loc="130 15" size="65 25"
          style="PlainText"
          label="Show"
          vc="CSdpro"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="70 15" size="60 25"
          style="Label2"
          label="Delete"
          vq="CSdomain"
          vc="CSrmpro(CSdomain)"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 15" size="65 25"
          style="Menu4"
          label="Build"
          vc="CSmkpro"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
    </group>
  </group>
</template>
