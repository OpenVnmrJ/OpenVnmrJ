<template name="DOSYdefault" element="pages" type="processing" >
  <group size="730 230"
    style="PlainText"
    label="Default"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;9)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="DOSYdefault"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="420 130" size="140 80"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <label loc="0 0" size="140 20"
        style="Label1"
        label="1D baseline correct"
        justify="Left"
        />
      <button loc="0 20" size="120 20"
        style="Heading2"
        label="DC correct"
        vc="dc"
        />
      <button loc="0 40" size="120 20"
        style="Heading2"
        label="Find integrals"
        vc="intmod='partial' region"
        />
      <button loc="0 60" size="120 20"
        style="Heading2"
        label="BC correct"
        vc="exists('bcorder','parameter'):$e if $e&lt;0.5 then bc else bc(bcorder) endif"
        />
    </group>
    <button loc="570 200" size="130 20"
      style="Heading4"
      label="Plot"
      vc="plot"
      />
    <button loc="10 5" size="190 25"
      style="Heading4"
      label="Transform all 1Ds"
      vc="wft"
      />
    <group loc="230 0" size="130 140"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Adjust"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="0 80" size="130 20"
        style="Heading2"
        label="Full spectrum"
        vc="f"
        />
      <button loc="0 120" size="130 20"
        style="Heading2"
        label="Find nearest line"
        vc="nl dres"
        />
      <button loc="0 100" size="130 20"
        style="Heading2"
        label="Display text"
        vc="dtext"
        />
      <button loc="0 40" size="130 20"
        style="Heading2"
        label="Autophase"
        vc="aph"
        />
      <button loc="0 60" size="130 20"
        style="Heading2"
        label="Full screen"
        vc="full ds"
        />
      <button loc="0 20" size="130 20"
        style="Heading2"
        label="Autoscale"
        vc="vsadj ds"
        />
      <label loc="0 0" size="130 20"
        style="Label1"
        label="Display"
        justify="Left"
        />
    </group>
    <group loc="570 70" size="140 120"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <entry loc="80 80" size="50 20"
        style="PlainText"
        vq="ins"
        vc="ins=$VALUE"
        set="$VALUE=ins"
        show="if (ins=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        />
      <button loc="0 80" size="80 20"
        style="Heading2"
        label="Set norm to"
        vc="ds_setint"
        />
      <button loc="0 60" size="130 20"
        style="Heading2"
        label="Normalized values"
        vc="if (vp&lt;24) then vp=24 endif ds dpirn"
        />
      <button loc="0 40" size="130 20"
        style="Heading2"
        label="Integral values"
        vc="if (vp&lt;24) then vp=24 endif ds dpir"
        />
      <button loc="0 100" size="130 20"
        style="Heading2"
        label="Clear integrals"
        vc="cz ds"
        />
      <button loc="0 20" size="130 20"
        style="Heading2"
        label="Autoscale"
        vc="isadj"
        />
      <label loc="0 0" size="130 20"
        style="Label1"
        label="1D integration"
        justify="Left"
        />
    </group>
    <group loc="570 0" size="130 60"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <button loc="0 20" size="120 20"
        style="Heading2"
        label="Peak threshold"
        vc="thadj ds ds('thresh')"
        />
      <button loc="0 40" size="120 20"
        style="Heading2"
        label="Find peaks"
        vc="dpf"
        />
      <label loc="0 0" size="120 20"
        style="Label1"
        label="1D peak picking"
        justify="Left"
        />
    </group>
    <group loc="220 150" size="180 60"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <label loc="0 40" size="85 20"
        style="Label1"
        label="Display mode"
        justify="Left"
        />
      <menu loc="80 40" size="95 20"
        style="PlainText"
        label="menu"
        vq="dmg"
        vc="$VALUE"
        set="$VALUE=dmg"
        >
        <mlabel 
          label="phased"
          chval="ph"
          />
        <mlabel 
          label="abs value"
          chval="av"
          />
        <mlabel 
          label="powerMode"
          chval="pwr"
          />
      </menu>
      <label loc="0 20" size="60 20"
        style="Label1"
        label="Axis"
        justify="Left"
        />
      <label loc="0 0" size="60 20"
        style="Label1"
        label="Vert scale"
        justify="Left"
        />
      <menu loc="80 20" size="95 20"
        style="PlainText"
        label="menu"
        vq="axis"
        vc="substr(axis,1,1,'$VALUE'):axis"
        set="substr(axis,1,1):$VALUE"
        >
        <mlabel 
          label="PPM"
          chval="p"
          />
        <mlabel 
          label="Hz"
          chval="h"
          />
        <mlabel 
          label="kHz"
          chval="k"
          />
      </menu>
      <menu loc="80 0" size="95 20"
        style="PlainText"
        label="menu"
        vq="aig"
        vc="$VALUE"
        set="$VALUE=aig"
        >
        <mlabel 
          label="Normalized"
          chval="nm"
          />
        <mlabel 
          label="Absolute"
          chval="ai"
          />
      </menu>
    </group>
    <group loc="420 0" size="120 110"
      style="PlainText"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="0 0" size="100 20"
        style="Label1"
        label="Reference"
        justify="Left"
        />
      <label loc="0 70" size="120 20"
        style="Label1"
        label="Reference cursor to"
        justify="Left"
        />
      <menu loc="50 90" size="60 20"
        style="PlainText"
        label="menu"
        vq="ref"
        vc="parunits('set','ref','$VALUE')"
        set="parunits('get','ref'):$VALUE"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="0 90" size="50 20"
        style="PlainText"
        vq="ref"
        vc="vnmrunits('set','ref',$VALUE) rl(ref) ds"
        set="vnmrunits('get','ref'):$VALUE"
        show="on('ref'):$VALUE"
        digits="2"
        disable="Grayed out"
        />
      <button loc="0 40" size="100 20"
        style="Heading2"
        label="By TMS"
        vc="setref tmsref sp=sp-rfl ds"
        />
      <button loc="0 20" size="100 20"
        style="Heading2"
        label="By solvent"
        vc="setref sp=sp-rfl ds"
        />
    </group>
    <button loc="140 160" size="60 20"
      style="Heading2"
      label="Auto LP"
      vc="parlp proc='lp' lpopt='b' strtlp=4 lpext=3 strtext=3 lpfilt=32 lpnupts=np if lpnupts&gt;256 then lpnupts=256 endif"
      />
    <check loc="10 180" size="140 20"
      style="Label1"
      label="Solvent subtraction"
      vq="ssfilter"
      vc="parfidss ssfilter='y'"
      vc2="ssfilter='n'"
      set="exists('ssfilter','parameter'):$v1 if $v1&lt;0.5 then $VALUE=0 else on('ssfilter'):$VALUE endif"
      />
    <check loc="10 160" size="130 20"
      style="Label1"
      label="Linear prediction"
      vq="proc"
      vc="parlp proc='lp'"
      vc2="proc='ft'"
      set="if (proc = 'lp') then $VALUE=1 else $VALUE=0 endif"
      />
    <group loc="10 110" size="190 40"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="130 0" size="60 20"
        style="PlainText"
        label="menu"
        vq="fn"
        vc="fn=$VALUE*2"
        set="$VALUE=fn/2"
        show="on('fn'):$VALUE"
        >
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1k"
          chval="1024"
          />
        <mlabel 
          label="2k"
          chval="2048"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
        <mlabel 
          label="32k"
          chval="32768"
          />
        <mlabel 
          label="64k"
          chval="65536"
          />
        <mlabel 
          label="128k"
          chval="131072"
          />
      </menu>
      <check loc="0 0" size="120 20"
        style="Label1"
        label="Transform size"
        vq="fn"
        vc="on('fn')"
        vc2="off('fn')"
        set="on('fn'):$VALUE"
        />
      <textmessage loc="130 20" size="60 20"
        style="Info"
        label="15,008"
        vq="np"
        set="$VALUE=np/2"
        digits="0"
        />
      <label loc="25 20" size="100 20"
        style="Label1"
        label="Acquired points"
        justify="Left"
        />
    </group>
    <group loc="10 60" size="190 40"
      style="PlainText"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="0 20" size="190 20"
        style="PlainText"
        label="menu"
        vq="lb gf gfs sb sbs awc"
        vc="cqwtmenu('1D','set','$VALUE')"
        set="cqwtmenu('1D','get'):$VALUE"
        >
        <mlabel 
          label="exponential"
          chval="exp"
          />
        <mlabel 
          label="gaussian"
          chval="gauss"
          />
        <mlabel 
          label="sinebell"
          chval="sine"
          />
        <mlabel 
          label="sinebell squared"
          chval="sqsine"
          />
        <mlabel 
          label="cosine"
          chval="cosine"
          />
        <mlabel 
          label="cosine squared"
          chval="sqcosine"
          />
        <mlabel 
          label="pseudo"
          chval="pseudo"
          />
        <mlabel 
          label="none"
          chval="none"
          />
      </menu>
      <label loc="0 0" size="80 20"
        style="Label1"
        label="Weighting"
        justify="Left"
        />
      <button loc="110 0" size="80 20"
        style="Heading2"
        label="Interactive"
        vc="wti"
        />
    </group>
    <entry loc="150 30" size="50 20"
      style="PlainText"
      vq="ftproc"
      vc="wft($VALUE)"
      set="select:$VALUE"
      digits="0"
      disable="Grayed out"
      />
    <label loc="35 30" size="130 20"
      style="Label1"
      label="Transform FID #"
      justify="Center"
      />
  </group>
</template>
