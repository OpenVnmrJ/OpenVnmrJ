<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standard" element="pages" type="sample" >
  <group size="640 270"
    style="Heading1"
    label="Sample Info"
    vq="personaType cppauseflg customflag"
    show="$S=1 PersonaManager('showInPanel','start','standard'):$S $SHOW=((cppauseflg='n') and (customflag='n') and ($S))"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standard"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="10 40" size="620 220"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="SampleInfo"
      useref="yes"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      >
      </group>
    <group loc="10 5" size="595 35"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="oplogout"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="370 5" size="110 30"
        vq="userremembrance"
        show="$SHOW=(userremembrance='y')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        reference="userrem"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="5 0" size="100 25"
          style="Heading2"
          label="Use My Prefs"
          vc="loaduserprefs"
          show="isnewexp:$isnew $ENABLE=($isnew&gt;0)*2-1"
          bg="VJBackground"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
      </group>
      <textmessage loc="5 5" size="180 25"
        style="PlainText"
        label="Operator: PaulB"
        vq="operator"
        set="$VALUE='      Operator: '+operator"
        />
    </group>
  </group>
</template>
