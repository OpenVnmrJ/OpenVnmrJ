<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Proc1Dbasic" element="pages" type="sample" >
  <group size="750 295"
    style="Heading1"
    label="BasicProc"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Proc1Dbasic"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="205 250" size="315 25"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <button loc="20 0" size="275 25"
        style="Heading2"
        label="Save current process/display parameters"
        vq="procdim"
        vc="svpdp"
        show="isnewexp:$isnew if (($isnew=0) and (procdim&gt;0)) then $ENABLE=1 else $ENABLE=-1 endif"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="5 35" size="215 90"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <check loc="195 20" size="20 20"
        style="Label1"
        vq="lb"
        vc="on('lb')"
        vc2="off('lb')"
        set="on('lb'):$ison $VALUE=$ison"
        />
      <button loc="10 20" size="180 20"
        style="Heading2"
        label="Exponential weighting"
        vc="expfilt('acq')"
        bg="transparent"
        halignment="Center"
        valignment="Center"
        />
      <label loc="10 0" size="100 20"
        style="Info"
        label="Process options:"
        justify="Left"
        />
      <group loc="10 45" size="180 40"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <menu loc="120 0" size="60 20"
          style="PlainText"
          label="menu"
          vq="fn"
          vc="fn=$VALUE*2"
          set="$VALUE=fn/2"
          show="on('fn'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1k"
            chval="1024"
            />
          <mlabel 
            label="2k"
            chval="2048"
            />
          <mlabel 
            label="4k"
            chval="4096"
            />
          <mlabel 
            label="8k"
            chval="8192"
            />
          <mlabel 
            label="16k"
            chval="16384"
            />
          <mlabel 
            label="32k"
            chval="32768"
            />
          <mlabel 
            label="64k"
            chval="65536"
            />
          <mlabel 
            label="128k"
            chval="131072"
            />
        </menu>
        <check loc="5 0" size="120 20"
          style="Label1"
          label="Transform size"
          vq="fn"
          vc="on('fn')"
          vc2="off('fn')"
          set="on('fn'):$VALUE"
          />
        <textmessage loc="120 20" size="60 20"
          style="Info"
          vq="np"
          set="$VALUE=np/2"
          digits="0"
          />
        <label loc="20 20" size="100 20"
          style="Label1"
          label="Acquired points"
          justify="Left"
          />
      </group>
    </group>
    <group loc="225 35" size="270 205"
      bg="transparent"
      border="Etched"
      tab="no"
      enable="no"
      reference="dispbasic"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="5 0" size="130 20"
        style="Info"
        label="Display options:"
        justify="Left"
        />
      <group loc="145 125" size="120 70"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="10 45" size="100 20"
          style="Heading2"
          label="Show peaks"
          vc="dpf"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="10 25" size="100 20"
          style="Heading2"
          label="Set threshold"
          vc="thadj ds ds('thresh')"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <label loc="10 5" size="100 20"
          style="Label1"
          label="Peak pick"
          justify="Center"
          />
      </group>
      <group loc="5 25" size="140 125"
        style="Heading1"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="5 20" size="130 20"
          style="Heading2"
          label="Integrate"
          vc="intmod='partial' region"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 40" size="130 20"
          style="Heading2"
          label="Autoscale"
          vc="isadj"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 60" size="130 20"
          style="Heading2"
          label="Normalized values"
          vc="if (vp&lt;24) then vp=24 endif ds dpirn"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <entry loc="85 80" size="50 20"
          style="PlainText"
          vq="ins"
          vc="ins=$VALUE"
          set="$VALUE=ins"
          show="if (ins=0) then $VALUE=0 else $VALUE=1 endif"
          digits="2"
          disable="Grayed out"
          />
        <button loc="5 80" size="80 20"
          style="Heading2"
          label="Set norm to"
          vc="ds_setint"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="5 100" size="130 20"
          style="Heading2"
          label="Clear integrals"
          vc="cz ds"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <label loc="0 0" size="130 20"
          style="Label1"
          label="Integration"
          justify="Center"
          />
      </group>
      <group loc="145 25" size="120 100"
        style="Heading1"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <menu loc="50 80" size="60 20"
          style="PlainText"
          label="menu"
          vq="ref"
          vc="parunits('set','ref','$VALUE')"
          set="parunits('get','ref'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="ppm"
            chval="ppm1"
            />
          <mlabel 
            label="Hz"
            chval="Hz"
            />
        </menu>
        <button loc="10 40" size="100 20"
          style="Heading2"
          label="By TMS"
          vc="setref tmsref sp=sp-rfl ds"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="10 20" size="100 20"
          style="Heading2"
          label="By solvent"
          vc="setref sp=sp-rfl ds"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <label loc="10 0" size="100 20"
          style="Label1"
          label="Reference"
          justify="Center"
          />
        <entry loc="0 80" size="50 20"
          style="PlainText"
          vq="ref"
          vc="vnmrunits('set','ref',$VALUE) rl(ref) ds"
          set="vnmrunits('get','ref'):$VALUE"
          show="on('ref'):$VALUE"
          digits="2"
          disable="Grayed out"
          />
        <label loc="0 60" size="120 20"
          style="Label1"
          label="Reference cursor to"
          justify="Left"
          />
      </group>
    </group>
    <button loc="525 10" size="160 20"
      style="Heading2"
      label="Auto plot preview"
      vq="PDFpreview"
      vc="pageview('auto')"
      show="$ENABLE=(PDFpreview='y')*2-1"
      bg="transparent"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="355 10" size="160 20"
      style="Heading2"
      label="Automatic plot"
      vc="plot"
      bg="transparent"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="185 10" size="160 20"
      style="Heading2"
      label="Display"
      vc="ds"
      bg="transparent"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <group loc="500 35" size="190 205"
      border="Etched"
      tab="no"
      enable="no"
      reference="plottemp"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="10 90" size="50 20"
        style="Label1"
        label="Peaks"
        justify="Left"
        />
      <label loc="10 110" size="75 20"
        style="Label1"
        label="Parameters"
        justify="Left"
        />
      <menu loc="85 110" size="100 20"
        style="PlainText"
        label="menu"
        vq="parstyle"
        vc="parstyle='$VALUE'"
        set="$VALUE=parstyle"
        editable="No"
        >
        <mlabel 
          label="Basic (left)"
          chval="ppa"
          />
        <mlabel 
          label="Basic (right)"
          chval="ppa(wc-50)"
          />
        <mlabel 
          label="Full (left)"
          chval="pap"
          />
        <mlabel 
          label="Full (right)"
          chval="pap(wc-50)"
          />
        <mlabel 
          label="None"
          />
      </menu>
      <menu loc="85 90" size="100 20"
        style="PlainText"
        label="menu"
        vq="pkpick"
        vc="pkpick='$VALUE'"
        set="$VALUE=pkpick"
        editable="No"
        >
        <mlabel 
          label="On peaks"
          chval="ppf"
          />
        <mlabel 
          label="List (left)"
          chval="pll"
          />
        <mlabel 
          label="List (right)"
          chval="pll(wc-50)"
          />
        <mlabel 
          label="None"
          />
      </menu>
      <label loc="5 0" size="130 20"
        style="Info"
        label="Manual plot:"
        justify="Left"
        />
      <button loc="15 135" size="160 20"
        style="Heading2"
        label="Send to plotter"
        vc="exec(parstyle) exec(pkpick)  page "
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="95 45" size="90 20"
        style="Heading2"
        label="Scale"
        vc="pscale"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="15 155" size="160 20"
        style="Heading2"
        label="Clear"
        vc="page('clear')"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="15 175" size="160 20"
        style="Heading2"
        label="Preview plot"
        vq="PDFpreview"
        vc="exec(parstyle) exec(pkpick)  pageview"
        show="$ENABLE=(PDFpreview='y')*2-1"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="95 65" size="90 20"
        style="Heading2"
        label="Logo"
        vc="pllogo"
        show="is_logo:$ret $ENABLE=($ret&gt;0.5)*2-1"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="5 65" size="90 20"
        style="Heading2"
        label="Molecule"
        vc="imagefile('plotall')"
        show="exists('/vnmr/java/jchempaint.jar','file'):$jchem $ENABLE=($jchem&gt;0.5)*2-1"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="5 45" size="90 20"
        style="Heading2"
        label="Text"
        vc="pltext"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="95 25" size="90 20"
        style="Heading2"
        label="Array"
        vq="arraydim "
        vc="pl('all')"
        show="getdim:$dim $ENABLE=((arraydim&gt;1) or ($dim&gt;1))*2-1"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="5 25" size="90 20"
        style="Heading2"
        label="Spectrum"
        vc="pl"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <button loc="15 10" size="160 20"
      style="Heading2"
      label="Process"
      vc="CPdndfid_process"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <group loc="5 130" size="215 110"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Adjust"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="10 0" size="130 20"
        style="Info"
        label="Display options:"
        justify="Left"
        />
      <label loc="20 65" size="30 20"
        style="Label1"
        label="axis"
        justify="Left"
        />
      <radio loc="110 65" size="60 20"
        style="Label1"
        label="Hz"
        vq="axis"
        vc="axis='h'"
        set="$VALUE=(axis='h')"
        />
      <radio loc="50 65" size="60 20"
        style="Label1"
        label="ppm"
        vq="axis"
        vc="axis='p'"
        set="$VALUE=(axis='p')"
        />
      <button loc="110 85" size="100 20"
        style="Heading2"
        label="DC Correct"
        vc="dc"
        bg="transparent"
        halignment="Center"
        valignment="Center"
        />
      <button loc="110 25" size="50 20"
        style="Heading2"
        label="+"
        vc="vs=vs*2 ds"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="160 25" size="50 20"
        style="Heading2"
        label="-"
        vc="vs=vs/2 ds"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="110 45" size="100 20"
        style="Heading2"
        label="Full spectrum"
        vc="f"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="15 85" size="90 20"
        style="Heading2"
        label="Autophase"
        vc="aph"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="15 45" size="90 20"
        style="Heading2"
        label="Full screen"
        vc="full ds"
        bg="transparent"
        halignment="Center"
        valignment="Center"
        />
      <button loc="15 25" size="90 20"
        style="Heading2"
        label="Autoscale"
        vc="vsadj ds"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
  </group>
</template>
