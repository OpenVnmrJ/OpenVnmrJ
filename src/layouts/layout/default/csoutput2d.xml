<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="csoutput2d" element="pages" type="processing" >
  <group size="730 250"
    style="Heading1"
    label="Csoutput2D"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;9)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="csoutput2d"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="520 10" size="230 190"
      style="Heading1"
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <button loc="140 170" size="70 20"
        style="Heading2"
        label="Combine"
        vc="csoutput2d('combine')"
        />
      <button loc="70 170" size="70 20"
        style="Heading2"
        label="Add"
        vc="csoutput2d('mark')"
        />
      <button loc="0 170" size="70 20"
        style="Heading2"
        label="Delete"
        vc="csoutput2d('unmark')"
        />
      <label loc="0 140" size="60 20"
        style="Label1"
        label="Edit:"
        />
      <button loc="145 65" size="50 20"
        style="Heading2"
        label="Left"
        vc="csoutput2d('leftbox')"
        />
      <button loc="15 65" size="50 20"
        style="Heading2"
        label="Right"
        vc="csoutput2d('rightbox')"
        />
      <button loc="70 55" size="70 40"
        style="Heading2"
        label="Redisplay"
        vc="csoutput2d('display')"
        />
      <button loc="80 100" size="50 20"
        style="Heading2"
        label="Down"
        vc="csoutput2d('downbox')"
        />
      <button loc="80 30" size="50 20"
        style="Heading2"
        label="Up"
        vc="csoutput2d('upbox')"
        />
    </group>
    <group loc="270 10" size="235 190"
      style="Heading1"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <button loc="5 120" size="230 20"
        style="Heading2"
        label="Display current chem. shift list"
        vc="csoutput2d('cslist')"
        />
      <button loc="5 90" size="230 20"
        style="Heading2"
        label="Email list"
        vc="csoutput2d('email')"
        />
      <button loc="5 60" size="230 20"
        style="Heading2"
        label="Done with editing"
        vc="csoutput2d('newlist')"
        />
      <button loc="5 30" size="230 20"
        style="Heading2"
        label="Edit current list"
        vc="csoutput2d('massage')"
        />
    </group>
    <group loc="10 10" size="260 240"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Adjust"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="10 200" size="230 20"
        style="Heading2"
        label="Peak pick current display"
        vc="csoutput2d('peakpick')"
        />
      <button loc="10 170" size="230 20"
        style="Heading2"
        label="Clear current peak list"
        vc="csoutput2d('startmanual')"
        />
      <label loc="10 140" size="60 20"
        style="Label1"
        label="Manual:"
        />
      <button loc="10 90" size="230 20"
        style="Heading2"
        label="Sort current peak list"
        vc="csoutput2d('fullsort')"
        />
      <button loc="10 60" size="230 20"
        style="Heading2"
        label="Do automatic 2D peakpick ONLY"
        vc="csoutput2d('autopeakpick')"
        />
      <button loc="10 30" size="230 20"
        style="Heading2"
        label="Do automatic 2D peakpick - sort"
        vc="csoutput2d('auto')"
        />
      <label loc="10 0" size="80 20"
        style="Label1"
        label="Automatic:"
        />
    </group>
  </group>
</template>
