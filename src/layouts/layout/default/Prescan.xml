<template name="Prescan" element="pages" type="acquisition" >
  <group size="580 200"
    style="Heading1"
    label="Prescan"
    vq="Hprescan tn activestudy"
    show="exists('Hprescan','parameter'):$e $SHOW=(tn='H1' and activestudy[1]&lt;&gt;'null' and $e&gt;0.5)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Prescan"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="20 60" size="253 84"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="0 0" size="90 83"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <group loc="0 60" size="90 20"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <radio loc="0 0" size="80 20"
            style="Label1"
            label="Wet1d"
            vq="Hprescan"
            vc="Hprescan[3]='wet1D' xmHprescan"
            vc2="Hprescan[3]='off' xmHprescan"
            set="$VALUE=(Hprescan[3]='wet1D')"
            />
        </group>
        <group loc="0 40" size="90 20"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <radio loc="0 0" size="80 20"
            style="Label1"
            label="Presat"
            vq="Hprescan"
            vc="Hprescan[3]='presat' xmHprescan"
            vc2="Hprescan[3]='off' xmHprescan"
            set="$VALUE=(Hprescan[3]='presat')"
            />
        </group>
        <group loc="0 0" size="80 20"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <radio loc="0 0" size="70 20"
            style="Label1"
            label="Proton"
            vq="Hprescan"
            vc="Hprescan[3]='off' xmHprescan"
            vc2="Hprescan[3]='off' xmHprescan"
            set="$VALUE=(Hprescan[3]='off')"
            />
        </group>
        <group loc="0 20" size="90 20"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <radio loc="0 0" size="80 20"
            style="Label1"
            label="MinSW"
            vq="Hprescan"
            vc="Hprescan[1]='auto' xmHprescan('setupminsw')"
            vc2="Hprescan[1]='off' xmHprescan('setupminsw')"
            set="$VALUE=(Hprescan[1]&lt;&gt;'off')"
            />
        </group>
      </group>
      <group loc="90 40" size="160 30"
        vq="prescan Hprescan"
        show="$SHOW=(Hprescan[3]='presat' and Hprescan[2]='auto')"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="Presatsolvent"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <menu loc="60 0" size="100 20"
          style="PlainText"
          label="menu"
          vq="presatgh2 Hprescan"
          vc="presatgh2=$VALUE setpower(presatgh2,tn):$p satpwr=$p if presatgh2&gt;40 then Gmap='H1' else Gmap='lk' endif"
          set="$VALUE=presatgh2"
          show="if Hprescan[3]='presat' then $ENABLE=1 else $ENABLE=-1 endif"
          >
          <mlabel 
            label="HDO"
            chval="10"
            />
          <mlabel 
            label="wet DMSO"
            chval="11"
            />
          <mlabel 
            label="90% H2O"
            chval="70"
            />
        </menu>
        <label loc="0 0" size="60 20"
          style="Label1"
          label="Solvent:"
          />
      </group>
    </group>
    <group loc="280 60" size="240 80"
      vq="Hprescan"
      show="$SHOW=(Hprescan[3]='wet1D' and Hprescan[2]='auto')"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="175 0" size="60 20"
        vq="Console"
        show="$SHOW=(Console&lt;&gt;'mercury')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="0 0" size="50 20"
          style="PlainText"
          label="menu"
          vq="wetpeaks"
          vc="wetpeaks=$VALUE"
          set="$VALUE=wetpeaks"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="6"
            chval="6"
            />
          <mlabel 
            label="7"
            chval="7"
            />
        </menu>
      </group>
      <entry loc="80 20" size="90 20"
        style="PlainText"
        vq="wetshape"
        vc="wetshape='$VALUE'"
        set="$VALUE=wetshape"
        />
      <label loc="130 60" size="30 20"
        style="Label1"
        label="ms"
        />
      <label loc="130 40" size="30 20"
        style="Label1"
        label="dB"
        />
      <check loc="170 40" size="70 20"
        style="Label1"
        label="C13wet"
        vq="c13wet"
        vc="c13wet='y' setwetpar('new')"
        vc2="c13wet='n'"
        set="$VALUE=(c13wet='y')"
        />
      <textmessage loc="80 40" size="50 20"
        style="Info"
        label="0"
        vq="wetpwr"
        set="$VALUE=wetpwr"
        />
      <textmessage loc="80 60" size="50 20"
        style="Info"
        label="15.0"
        vq="pwwet"
        set="$VALUE=pwwet*1e-3"
        digits="1"
        />
      <group loc="170 0" size="60 20"
        vq="Console"
        show="$SHOW=(Console='mercury')"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <menu loc="10 0" size="50 20"
          style="PlainText"
          label="menu"
          vq="wetpeaks"
          vc="wetpeaks=$VALUE"
          set="$VALUE=wetpeaks"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
        </menu>
      </group>
      <label loc="0 60" size="70 20"
        style="Label1"
        label="wet pulse"
        />
      <label loc="0 40" size="70 20"
        style="Label1"
        label="wet power"
        />
      <label loc="0 20" size="70 20"
        style="Label1"
        label="wet shape"
        />
      <label loc="0 0" size="190 20"
        style="Label1"
        label="Number of peaks to suppress"
        />
    </group>
    <check loc="110 40" size="130 20"
      style="Label1"
      label="Save scout FIDs"
      vq="prescan"
      vc="prescan[9]='on'"
      vc2="prescan[9]='off'"
      set="$VALUE=(prescan[9]='on')"
      />
    <label loc="10 35" size="70 20"
      style="Heading1"
      label="Prescan"
      justify="Left"
      />
    <group loc="10 0" size="390 20"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay1"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 20"
        style="Info"
        label="Presat"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
      <button loc="250 0" size="130 20"
        style="Heading2"
        label="Display sequence"
        vc="dps"
        />
    </group>
  </group>
</template>
