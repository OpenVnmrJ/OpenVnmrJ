    <button loc="625 215" size="80 20"
      style="Heading2"
      label="Clear"
      vc="clear"
      />
    <button loc="625 185" size="80 20"
      style="Heading2"
      label="Manual"
      vc="man(seqfil)"
      />
    <button loc="625 155" size="80 20"
      style="Heading2"
      label="Shims"
      vc="dgs"
      />
    <button loc="625 125" size="80 20"
      style="Heading2"
