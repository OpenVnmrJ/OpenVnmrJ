      vc="dglp"
      />
    <button loc="625 95" size="80 20"
      style="Heading2"
      label="Channels"
      vc="dg2"
      />
    <button loc="625 65" size="80 20"
      style="Heading2"
      label="Array"
      vc="da"
      />
    <button loc="625 35" size="80 20"
      style="Heading2"
      label="Basic"
      vc="dg"
      />
    <button loc="625 5" size="80 20"
      style="Heading2"
      label="Text"
      vc="clear text"
      />
