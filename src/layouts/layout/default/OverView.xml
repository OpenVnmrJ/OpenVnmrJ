      vq="wstart"
      show="$SHOW=(wstart='studywstart')"
      border="None"
      tab="no"
      enable="no"
      reference="oplogout"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <button loc="0 5" size="110 20"
        style="Heading2"
        label="Setup 1st exp"
        vc="studywstart('setup')"
        />
