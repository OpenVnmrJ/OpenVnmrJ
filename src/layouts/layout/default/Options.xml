<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="0 0" size="850 350"
    style="Heading1"
    label="Options"
    vq="panellevel"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Options"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="565 200" size="270 70"
      style="Heading1"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="CPMGgrp"
      useref="no"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      />
    <group loc="5 10" size="270 305"
      style="Heading1"
      border="Etched"
      >
      <check loc="15 5" size="100 20"
        vq="WGmode"
        vc="WGmode='y'"
        vc2="WGmode='n'"
        set="$VALUE=(WGmode='y')"
        label="WATERGATE"
        style="Label1"
        />
      <group loc="0 30" size="270 270"
        style="Heading2"
        bg="transparent"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="WGecho"
        useref="yes"
        expanded="no"
        rows="1"
        columns="1"
        />
      </group>
    <group loc="285 10" size="270 260"
      style="Heading1"
      border="Etched"
      >
      <check loc="15 5" size="250 20"
        vq="ESmode"
        vc="ESmode='y'"
        vc2="ESmode='n'"
        set="$VALUE=(ESmode='y')"
        label="Excitation Sculpting"
        style="Label1"
        />
      <group loc="0 30" size="270 230"
        style="Heading2"
        bg="transparent"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="ExSculpt"
        useref="yes"
        expanded="no"
        rows="1"
        columns="1"
        />
      </group>
    <group loc="285 275" size="550 40"
      style="Heading1"
      border="Etched"
      vq="WGmode ESmode"
      >
     <label loc="20 10" size="100 20"
        style="Heading1"
        label="Misc. options:"
        />
     <label loc="165 10" size="150 20"
        style="Label1"
        label="Lock gating"
        justify="Left"
        />
      <check loc="140 10" size="20 20"
        style="PlainText"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y' )"
        tooltip="Turn lock control on/off during the sequence and acquisition (lkgate_flg)"
        />
      <group loc="250 10" size="270 20"
        vq="WGmode ESmode"
        show="$SHOW=((WGmode='y') or (ESmode='y'))"
        >
        <label loc="65 0" size="150 20"
          style="Label1"
          label="Gradient alteration"
          justify="Left"
          />
        <check loc="40 0" size="20 20"
          style="PlainText"
          vq="alt_grd"
          vc="alt_grd='y'"
          vc2="alt_grd='n'"
          set="$VALUE=(alt_grd='y' )"
          tooltip="Change gradient sign on odd scans (alt_grd)"
        />
        </group>

      </group>
    <group loc="565 10" size="270 185"
      style="Heading1"
      border="Etched"
      >
      <check loc="15 5" size="250 20"
        vq="ESmode"
        vc="flipback='y'"
        vc2="flipback='n'"
        set="$VALUE=(flipback='y')"
        label="Flipback"
        style="Label1"
        />
      <group loc="0 30" size="270 155"
        style="Heading2"
        bg="transparent"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="FlipBack"
        useref="yes"
        expanded="no"
        rows="1"
        columns="1"
        />
      </group>
  </group>
</template>
