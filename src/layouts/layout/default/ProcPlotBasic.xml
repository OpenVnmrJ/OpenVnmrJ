<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ProcPlotBasic" element="pages" type="processing" >
  <group size="730 265"
    style="Heading1"
    label="Basic"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','process','basic'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="ProcPlotBasic"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="490 0" size="215 225"
      style="Heading1"
      label="Plot options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="proc1d-ez"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 25" size="205 195"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="40 160" size="120 25"
          style="Heading2"
          label="Auto preview"
          vq="PDFpreview procdim"
          vc="pageview('auto')"
          show="$ENABLE=((PDFpreview='y') and (procdim&gt;0))*2-1"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="40 135" size="120 25"
          style="Heading2"
          label="Auto plot"
          vq="procdim"
          vc="plot"
          show="$ENABLE=(procdim&gt;0)*2-1"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <group loc="5 75" size="195 45"
          vq="procdim"
          show="$SHOW=(procdim&gt;1)"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <menu loc="85 20" size="110 20"
            style="PlainText"
            label="menu"
            vq="plt2Darg"
            vc="plt2Darg[5]='$VALUE'"
            set="$VALUE=plt2Darg[5]"
            editable="No"
            >
            <mlabel 
              label="spectrum"
              chval="f1"
              />
            <mlabel 
              label="projection"
              chval="projection"
              />
            <mlabel 
              label="None"
              chval="noside"
              />
          </menu>
          <label loc="45 20" size="30 20"
            style="Label1"
            label="(F1)"
            justify="Right"
            />
          <menu loc="85 0" size="110 20"
            style="PlainText"
            label="menu"
            vq="plt2Darg"
            vc="plt2Darg[4]='$VALUE'"
            set="$VALUE=plt2Darg[4]"
            editable="No"
            >
            <mlabel 
              label="spectrum"
              chval="f2"
              />
            <mlabel 
              label="projection"
              chval="projection"
              />
            <mlabel 
              label="none"
              chval="notop"
              />
          </menu>
          <label loc="45 0" size="30 20"
            style="Label1"
            label="(F2)"
            justify="Right"
            />
          <label loc="0 0" size="45 20"
            style="Label1"
            label="Plot 1D"
            justify="Left"
            />
        </group>
        <group loc="5 25" size="195 40"
          vq="procdim"
          show="$SHOW=(procdim=1)"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <menu loc="75 20" size="120 20"
            style="PlainText"
            label="Peak Pick"
            vq="pkpick"
            vc="$pkpick='$VALUE' if ($pkpick='none') then pkpick='' dpf('off') else pkpick=$pkpick endif"
            set="if pkpick='' then $VALUE='none' else $VALUE=pkpick endif"
            editable="No"
            >
            <mlabel 
              label="Peaks Hz"
              chval="ppf('axish')"
              />
            <mlabel 
              label="Peaks ppm"
              chval="ppf('axisp')"
              />
            <mlabel 
              label="Peaks Hz,  top"
              chval="ppf('axish','top')"
              />
            <mlabel 
              label="Peaks ppm, top"
              chval="ppf('axisp','top')"
              />
            <mlabel 
              label="List, at left"
              chval="pll"
              />
            <mlabel 
              label="List, at right"
              chval="pll(wc-50)"
              />
            <mlabel 
              label="None"
              chval="none"
              />
          </menu>
          <label loc="0 20" size="75 20"
            style="Label1"
            label="Peak Values"
            justify="Left"
            />
          <menu loc="75 0" size="120 20"
            style="PlainText"
            label="Integral"
            vq="intmod"
            vc="intmod='$VALUE'"
            set="$VALUE=intmod"
            editable="No"
            >
            <mlabel 
              label="Partial"
              chval="partial"
              />
            <mlabel 
              label="Full"
              chval="full"
              />
            <mlabel 
              label="Off"
              chval="off"
              />
          </menu>
          <label loc="0 0" size="70 20"
            style="Label1"
            label="Integrals"
            justify="Left"
            />
        </group>
        <menu loc="80 5" size="120 20"
          style="PlainText"
          label="menu"
          vq="parstyle"
          vc="parstyle='$VALUE'"
          set="$VALUE=parstyle"
          editable="No"
          >
          <mlabel 
            label="Full"
            chval="pap"
            />
          <mlabel 
            label="Basic"
            chval="ppa"
            />
          <mlabel 
            label="Text only"
            chval="pltext"
            />
        </menu>
        <label loc="5 5" size="80 20"
          style="Label1"
          label="Parameters"
          justify="Left"
          />
      </group>
    </group>
    <group loc="275 0" size="210 225"
      style="Heading1"
      label="Process options"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="proc1d-ez"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 110" size="195 110"
        style="Heading1"
        label="Non-uniform sampling"
        vq="sampling"
        show="$SHOW=0 exists('sampling','parameter'):$e if $e=1 then if sampling='sparse' then $SHOW=1  endif  endif"
        border="Etched"
        tab="no"
        enable="no"
        reference="NUSprocessing"
        useref="no"
        subtype="Titled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <button loc="30 75" size="140 25"
          style="Heading2"
          label="Restore raw data"
          vq="curexp"
          vc="if (file=curexp+'/procdir/cln2d' or file=curexp+'/procdir/cln2d.fid') then $file=curexp+'/procdir/orig' else $file=file endif rt($file) phfid=0 write('line3','Raw NUS data restored') wft(1)"
          bg="transparent"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <button loc="30 50" size="140 25"
          style="Heading2"
          label="Process"
          vc="NUSproc"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <label loc="10 20" size="120 20"
          style="Label1"
          label="Processing method"
          />
        <textmessage loc="122 20" size="60 20"
          style="Info"
          vq="CS_dpars CSreconmethod"
          set="exists('CSreconmethod','parameter'):$pex if $pex then $VALUE=CSreconmethod else $VALUE='CLEAN' endif"
	  />

      </group>
        <group loc="35 155" size="155 35"
          vq="sampling"
          show="$SHOW=1 exists('sampling','parameter'):$e if $e then if sampling='sparse' then $SHOW=0 endif endif"
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <button loc="5 5" size="130 25"
            style="Heading2"
            label="Process"
            vc="CPdndfid_process"
            decor1="yes"
            halignment="Center"
            valignment="Center"
            />
        </group>

      <group loc="10 25" size="195 85"
        show="getdim:$dim if ($dim=2) then $SHOW=1 else $SHOW=0 endif"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <menu loc="95 60" size="95 20"
          style="PlainText"
          label="menu"
          vq="lb1 sb1 sbs1 gf1 gfs1 awc1"
          vc="CPwtmenu('f1','set','$VALUE')"
          set="CPwtmenu('f1','get'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="gaussian"
            chval="gaussian"
            />
          <mlabel 
            label="sqcosine"
            chval="sqcosine"
            />
          <mlabel 
            label="cosine"
            chval="cosine"
            />
          <mlabel 
            label="sinebell"
            chval="sinebell"
            />
          <mlabel 
            label="sqsinebell"
            chval="sqsinebell"
            />
        </menu>
        <menu loc="95 40" size="95 20"
          style="PlainText"
          label="menu"
          vq="lb sb sbs gf gfs awc"
          vc="CPwtmenu('acq','set','$VALUE')"
          set="CPwtmenu('acq','get'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="gaussian"
            chval="gaussian"
            />
          <mlabel 
            label="sqcosine"
            chval="sqcosine"
            />
          <mlabel 
            label="cosine"
            chval="cosine"
            />
          <mlabel 
            label="sinebell"
            chval="sinebell"
            />
          <mlabel 
            label="sqsinebell"
            chval="sqsinebell"
            />
        </menu>
        <menu loc="95 0" size="95 20"
          style="PlainText"
          label="menu"
          vq="fn fn1"
          vc="fnfn1('set',$VALUE)"
          set="fnfn1('get'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="256 x 256"
            chval="256,256"
            />
          <mlabel 
            label="512 x 512"
            chval="512,512"
            />
          <mlabel 
            label="1k x 1k"
            chval="1024,1024"
            />
          <mlabel 
            label="2k x 1k"
            chval="2048,1024"
            />
          <mlabel 
            label="2k x 2k"
            chval="2048,2048"
            />
          <mlabel 
            label="4k x 2k"
            chval="4096,2048"
            />
          <mlabel 
            label="2k x 4k"
            chval="2048,4096"
            />
          <mlabel 
            label="4k x 4k"
            chval="4096,4096"
            />
        </menu>
        <menu loc="120 20" size="70 20"
          style="PlainText"
          vq="proc1 strtext1 lpext1 dmg1 ni"
          vc="setLP1($VALUE*ni) if (dmg1='ph1') then gaussian('f1') else sqsinebell('f1') endif"
          set="$VALUE=(strtext1+lpext1-1)/ni"
          show="$ENABLE=(proc1='lp')"
          editable="No"
          >
          <mlabel 
            label="2*ni"
            chval="2"
            />
          <mlabel 
            label="4*ni"
            chval="4"
            />
          <mlabel 
            label="none"
            chval="1"
            />
        </menu>
        <check loc="0 20" size="115 20"
          style="Label1"
          label="F1 linear pred."
          vq="proc1"
          vc="parlp(1) proc1='lp' if (dmg1='ph1') then gaussian('f1') else sqsinebell('f1') endif"
          vc2="proc1='ft' if (dmg1='ph1') then gaussian('f1') else sqsinebell('f1') endif"
          set="$VALUE=(proc1='lp')"
          />
        <label loc="0 0" size="95 20"
          style="Label1"
          label="FT data size"
          justify="Left"
          />
        <label loc="0 62" size="95 20"
          style="Label1"
          label="Weighting (F1)"
          justify="Left"
          />
        <label loc="0 42" size="95 20"
          style="Label1"
          label="Weighting (F2)"
          justify="Left"
          />
      </group>
      <group loc="10 25" size="195 45"
        show="getdim:$dim if ($dim=1) then $SHOW=1 else $SHOW=0 endif"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <menu loc="95 5" size="95 20"
          style="PlainText"
          label="menu"
          vq="fn"
          vc="if ($VALUE=0) then off('fn') else fn=$VALUE*2 endif"
          set="on('fn'):$on if($on&gt;0.5) then $VALUE=fn/2 else $VALUE=0 endif"
          editable="No"
          >
          <mlabel 
            label="Default"
            chval="0"
            />
          <mlabel 
            label="2k"
            chval="2048"
            />
          <mlabel 
            label="4k"
            chval="4096"
            />
          <mlabel 
            label="8k"
            chval="8192"
            />
          <mlabel 
            label="16k"
            chval="16384"
            />
          <mlabel 
            label="32k"
            chval="32768"
            />
          <mlabel 
            label="64k"
            chval="65536"
            />
          <mlabel 
            label="128k"
            chval="131072"
            />
          <mlabel 
            label="256k"
            chval="262144"
            />
        </menu>
        <label loc="160 25" size="30 20"
          style="Label1"
          label="Hz"
          />
        <entry loc="95 25" size="60 20"
          style="PlainText"
          vq="lb"
          vc="on('lb'):$ison if ($ison) then lb=$VALUE else lb=lb off('lb') endif"
          set="$VALUE=lb"
          show="on('lb'):$ison if ($ison) then $ENABLE=1 else $ENABLE=0 endif"
          digits="1"
          />
        <check loc="0 25" size="95 20"
          style="Label1"
          label="LineBroaden"
          vq="lb"
          vc="on('lb')"
          vc2="off('lb')"
          set="on('lb'):$ison if ($ison) then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="0 5" size="95 20"
          style="Label1"
          label="FT data size"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 0" size="265 225"
      style="Heading1"
      label="Sample information"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="proc1d-ez"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textfile loc="10 120" size="245 90"
        style="PlainText"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <textmessage loc="105 65" size="150 20"
        style="Info"
        label=""
        vq="studyowner sampleowner"
        set="$s=sampleowner if ($s='') then $s=studyowner endif $VALUE=$s"
        />
      <textmessage loc="105 45" size="150 20"
        style="Info"
        label="cdcl3"
        vq="solvent"
        set="$VALUE=solvent"
        />
      <textmessage loc="105 25" size="150 20"
        style="Info"
        label="Ethylindanone"
        vq="samplename"
        set="$VALUE=samplename"
        />
      <label loc="10 65" size="90 20"
        style="Label1"
        label="Sample owner"
        />
      <label loc="10 45" size="50 20"
        style="Label1"
        label="Solvent"
        />
      <label loc="10 25" size="50 20"
        style="Label1"
        label="Sample"
        />
      <label loc="10 100" size="90 20"
        style="Label1"
        label="Comments:"
        />
    </group>
    <group loc="5 230" size="700 35"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="proc1d-ez"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <button loc="315 5" size="335 25"
        style="Heading2"
        label="Save current process/display parameters"
        vq="procdim file"
        vc="svpdp"
        show="isnewexp:$isnew if (($isnew=0) and (procdim&gt;0)) then $ENABLE=1 else $ENABLE=-1 endif"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="40 5" size="170 25"
        style="Heading2"
        label="Display spectrum"
        vq="procdim"
        vc="if (procdim=1) then ds elseif procdim=2 then dconi endif"
        show="$ENABLE=(procdim&gt;0)*2-1"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
  </group>
</template>
