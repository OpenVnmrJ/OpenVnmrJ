<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ProcPlotBasic" element="pages" type="processing" >
  <group loc="0 0" size="730 255"
    style="Heading1"
    label="Default"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','process','default'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Proc1Ddefault"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="555 215" size="150 35"
      style="Heading1"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <button loc="10 5" size="130 25"
        style="Heading2"
        label="Auto plot"
        vc="plot"
        bg="transparent"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="5 215" size="545 35"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Untitled"
      rows="1"
      columns="1"
      >
      <label loc="385 5" size="110 25"
        style="Label1"
        label="in FID directory"
        />
      <button loc="10 5" size="370 25"
        style="Heading2"
        label="Save current process / Display parameters"
        vq="procdim"
        vc="svpdp"
        show="isnewexp:$isnew if (($isnew=0) and (procdim&gt;0)) then $ENABLE=1 else $ENABLE=-1 endif"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="555 70" size="150 140"
      style="Heading1"
      label="Integration"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <button loc="10 110" size="130 20"
        style="Heading2"
        label="Clear integrals"
        vc="cz ds"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <entry loc="90 80" size="50 20"
        style="PlainText"
        vq="ins"
        vc="ins=$VALUE"
        set="$VALUE=ins"
        show="if (ins=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        />
      <button loc="10 80" size="80 20"
        style="Heading2"
        label="Set norm to"
        vc="ds_setint"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 60" size="130 20"
        style="Heading2"
        label="Normalized values"
        vc="if (vp&lt;24) then vp=24 endif ds dpirn"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 40" size="130 20"
        style="Heading2"
        label="Integral values"
        vc="if (vp&lt;24) then vp=24 endif ds dpir"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="130 20"
        style="Heading2"
        label="Autoscale"
        vc="isadj"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="555 0" size="150 70"
      style="Heading1"
      label="Peak picking"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <button loc="10 40" size="130 20"
        style="Heading2"
        label="Find peaks"
        vc="dpf"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="130 20"
        style="Heading2"
        label="Peak threshold"
        vc="thadj ds ds('thresh')"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="5 0" size="200 210"
      style="Heading1"
      label="Fourier transform"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="FourierTransform"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <entry loc="130 45" size="60 20"
        style="PlainText"
        vq="ftproc"
        vc="wft($VALUE)"
        set="select:$VALUE"
        digits="0"
        disable="Grayed out"
        />
      <button loc="130 175" size="60 20"
        style="Heading2"
        label="Auto LP"
        vc="setLP"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="180 20"
        style="Heading2"
        label="Transform all"
        vc="wft"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <check loc="10 175" size="125 20"
        style="Label1"
        label="Lin. prediction"
        vq="proc"
        vc="parlp proc='lp'"
        vc2="proc='ft'"
        set="if (proc = 'lp') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="10 125" size="190 40"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <textmessage loc="120 20" size="60 20"
          style="Info"
          vq="np"
          set="$VALUE=np/2"
          digits="0"
          />
        <menu loc="120 0" size="60 20"
          style="PlainText"
          label="menu"
          vq="fn"
          vc="fn=$VALUE*2"
          set="$VALUE=fn/2"
          show="on('fn'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1k"
            chval="1024"
            />
          <mlabel 
            label="2k"
            chval="2048"
            />
          <mlabel 
            label="4k"
            chval="4096"
            />
          <mlabel 
            label="8k"
            chval="8192"
            />
          <mlabel 
            label="16k"
            chval="16384"
            />
          <mlabel 
            label="32k"
            chval="32768"
            />
          <mlabel 
            label="64k"
            chval="65536"
            />
          <mlabel 
            label="128k"
            chval="131072"
            />
        </menu>
        <check loc="0 0" size="120 20"
          style="Label1"
          label="Transform size"
          vq="fn"
          vc="on('fn')"
          vc2="off('fn')"
          set="on('fn'):$VALUE"
          />
        <label loc="0 20" size="100 20"
          style="Label1"
          label="Acquired points"
          justify="Left"
          />
      </group>
      <group loc="10 70" size="190 40"
        style="Heading1"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <menu loc="0 20" size="180 20"
          style="PlainText"
          label="menu"
          vq="lb gf gfs sb sbs awc"
          vc="CPwtmenu('acq','set','$VALUE')"
          set="CPwtmenu('acq','get'):$VALUE"
          editable="No"
          >
          <mlabel 
            label="exponential"
            chval="expfilt"
            />
          <mlabel 
            label="gaussian"
            chval="gaussian"
            />
          <mlabel 
            label="sinebell"
            chval="sinebell"
            />
          <mlabel 
            label="sinebell squared"
            chval="sqsinebell"
            />
          <mlabel 
            label="cosine"
            chval="cosine"
            />
          <mlabel 
            label="cosine squared"
            chval="sqcosine"
            />
          <mlabel 
            label="pseudo echo"
            chval="pseudoecho"
            />
          <mlabel 
            label="pi/4 shifted Sq.Bell"
            chval="pi4ssbsq"
            />
          <mlabel 
            label="pi/3 shifted Sq.Bell"
            chval="pi3ssbsq"
            />
          <mlabel 
            label="none"
            chval="none"
            />
        </menu>
        <button loc="100 0" size="80 20"
          style="Heading2"
          label="Interactive"
          vc="wti"
          decor1="yes"
          halignment="Center"
          valignment="Center"
          />
        <label loc="0 0" size="70 20"
          style="Label1"
          label="Weighting"
          justify="Left"
          />
      </group>
      <label loc="10 45" size="110 20"
        style="Label1"
        label="Transform FID #"
        justify="Left"
        />
    </group>
    <group loc="400 0" size="150 120"
      style="Heading1"
      label="Reference"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Reference"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <menu loc="75 90" size="65 20"
        style="PlainText"
        label="menu"
        vq="ref"
        vc="parunits('set','ref','$VALUE')"
        set="parunits('get','ref'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <label loc="10 70" size="130 20"
        style="Label1"
        label="Reference cursor to"
        justify="Left"
        />
      <entry loc="10 90" size="65 20"
        style="PlainText"
        vq="ref"
        vc="vnmrunits('set','ref',$VALUE) rl(ref) ds"
        set="vnmrunits('get','ref'):$VALUE"
        show="on('ref'):$VALUE"
        digits="2"
        disable="Grayed out"
        />
      <button loc="10 40" size="130 20"
        style="Heading2"
        label="By TMS"
        vc="setref tmsref sp=sp-rfl ds"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="130 20"
        style="Heading2"
        label="By solvent"
        vc="setref sp=sp-rfl ds"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="400 120" size="150 90"
      style="Heading1"
      label="Baseline correct"
      border="Etched"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <button loc="10 60" size="130 20"
        style="Heading2"
        label="BC correct"
        vc="exists('bcorder','parameter'):$e if $e&lt;0.5 then bc else bc(bcorder) endif"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 40" size="130 20"
        style="Heading2"
        label="Find integrals"
        vc="intmod='partial' region"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="130 20"
        style="Heading2"
        label="DC correct"
        vc="dc"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <group loc="210 0" size="185 210"
      style="Heading1"
      label="Display"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Adjust"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 145" size="170 60"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <menu loc="70 40" size="95 20"
          style="PlainText"
          label="menu"
          vq="dmg"
          vc="$VALUE"
          set="$VALUE=dmg"
          editable="No"
          >
          <mlabel 
            label="Phased"
            chval="ph"
            />
          <mlabel 
            label="Abs Value"
            chval="av"
            />
          <mlabel 
            label="Power"
            chval="pwr"
            />
        </menu>
        <label loc="0 40" size="75 20"
          style="Label1"
          label="Disp. mode"
          justify="Left"
          />
        <label loc="0 0" size="65 20"
          style="Label1"
          label="Vert scale"
          justify="Left"
          />
        <menu loc="70 20" size="95 20"
          style="PlainText"
          label="menu"
          vq="axis"
          vc="axis='$VALUE'"
          set="$VALUE=axis"
          editable="No"
          >
          <mlabel 
            label="PPM"
            chval="p"
            />
          <mlabel 
            label="Hz"
            chval="h"
            />
          <mlabel 
            label="kHz"
            chval="k"
            />
        </menu>
        <menu loc="70 0" size="95 20"
          style="PlainText"
          label="menu"
          vq="aig"
          vc="$VALUE"
          set="$VALUE=aig"
          editable="No"
          >
          <mlabel 
            label="Normalized"
            chval="nm"
            />
          <mlabel 
            label="Absolute"
            chval="ai"
            />
        </menu>
        <label loc="0 20" size="60 20"
          style="Label1"
          label="Axis"
          justify="Left"
          />
      </group>
      <button loc="25 120" size="135 20"
        style="Heading2"
        label="Find nearest line"
        vc="nl dres"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="25 100" size="135 20"
        style="Heading2"
        label="Display text"
        vc="dtext"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="25 80" size="135 20"
        style="Heading2"
        label="Full spectrum"
        vc="f"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="25 60" size="135 20"
        style="Heading2"
        label="Full screen"
        vc="full ds"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="25 40" size="135 20"
        style="Heading2"
        label="Autophase"
        vc="aph"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
      <button loc="25 20" size="135 20"
        style="Heading2"
        label="Autoscale"
        vc="vsadj ds"
        decor1="yes"
        halignment="Center"
        valignment="Center"
        />
    </group>
  </group>
</template>
