<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="650 240"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="210 95" size="185 120"
      style="Heading1"
      label="X-Inversion"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AD-Xinversion"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="105 65" size="50 20"
        style="PlainText"
        vq="pwx180"
        vc="pwx180=$VALUE"
        set="$VALUE=pwx180"
        disable="Grayed out"
        />
      <entry loc="105 85" size="50 20"
        style="PlainText"
        vq="pwxlvl180"
        vc="pwxlvl180=$VALUE"
        set="$VALUE=pwxlvl180"
        disable="Grayed out"
        />
      <label loc="155 65" size="30 20"
        style="Label1"
        label=" µs"
        justify="Left"
        />
      <label loc="155 85" size="25 20"
        style="Label1"
        label=" dB"
        justify="Left"
        />
      <label loc="10 85" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="60 45" size="115 20"
        style="PlainText"
        vq="pwx180adR"
        vc="pwx180adR='$VALUE'"
        set="$VALUE=pwx180adR"
        disable="Grayed out"
        />
      <label loc="10 45" size="50 20"
        style="Label1"
        label="ShapeR"
        justify="Left"
        />
      <label loc="10 65" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <entry loc="60 25" size="115 20"
        style="PlainText"
        vq="pwx180ad"
        vc="pwx180ad='$VALUE'"
        set="$VALUE=pwx180ad"
        disable="Grayed out"
        />
      <label loc="10 25" size="50 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
    </group>
    <group loc="210 25" size="185 70"
      style="Heading1"
      label="One-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="105 45" size="50 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <label loc="155 45" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <label loc="155 25" size="30 20"
        style="Label1"
        label=" Hz"
        justify="Left"
        />
      <entry loc="105 25" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 25" size="95 20"
        style="Label1"
        label="Coupling const."
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
    </group>
    <group loc="5 170" size="200 45"
      style="Heading1"
      label="XH Direct Correlation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="XHDirectCorrelation"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="20 20" size="80 20"
        style="Label1"
        label="In-phase"
        vq="mult"
        vc="mult=0"
        vc2="mult=2"
        set="if (mult &gt; 0) then $VALUE=0 else $VALUE=1 endif"
        />
      <check loc="110 20" size="80 20"
        style="Label1"
        label="Invert"
        vq="mult"
        vc="mult=2"
        vc2="mult=1"
        set="if (mult &gt; 0) then $VALUE=1 else $VALUE=0 endif"
        />
      </group>
    <group loc="5 25" size="200 145"
      style="Heading1"
      label="SpinLock"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MLEV17c"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <entry loc="110 100" size="60 20"
        style="PlainText"
        vq="slpwT"
        vc="slpwT=$VALUE"
        set="$VALUE=slpwT"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="110 120" size="60 20"
        style="PlainText"
        vq="trim"
        vc="trim=$VALUE/1000"
        set="$VALUE=trim*1000"
        disable="Grayed out"
        />
      <label loc="170 120" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <label loc="170 100" size="25 20"
        style="Label1"
        label=" µs"
        justify="Left"
        />
      <label loc="170 80" size="30 20"
        style="Label1"
        label=" dB"
        justify="Left"
        />
      <label loc="170 60" size="30 20"
        style="Label1"
        label=" Hz"
        justify="Left"
        />
      <entry loc="110 20" size="60 20"
        style="PlainText"
        vq="mixT"
        vc="mixT=$VALUE/1000"
        set="$VALUE=mixT*1000"
        disable="Grayed out"
        />
      <label loc="170 20" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <menu loc="65 40" size="105 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        editable="No"
        >
        <mlabel 
          label="mlev17"
          chval="mlev17"
          />
        <mlabel 
          label="clean mlev17"
          chval="mlev17c"
          />
      </menu>
      <button loc="65 60" size="45 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        bg=""
        decor1=""
        />
      <label loc="10 40" size="50 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 120" size="90 20"
        style="Label1"
        label="Trim pulse"
        justify="Left"
        />
      <label loc="10 60" size="55 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
    </group>
    <group loc="400 25" size="185 70"
      style="Heading1"
      label="C12-H1 suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="c12hnull"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 45" size="175 25"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="birdnull"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="145 0" size="25 20"
          style="Heading1"
          label=" s"
          justify="Left"
          />
        <entry loc="90 0" size="55 20"
          style="PlainText"
          vq="null nullflg"
          vc="null=$VALUE"
          set="$VALUE=null"
          show="if (nullflg = 'n') then $ENABLE=1 else $ENABLE=0 endif"
          digits="3"
          disable="Grayed out"
          />
        <check loc="0 0" size="140 20"
          style="Label1"
          label="BIRD-delay"
          vq="nullflg null"
          vc="nullflg='n' if (null = 0) then null=0.35 endif"
          vc2="null=0"
          set="if (nullflg='n') and (null &gt; 0) then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <check loc="10 20" size="140 20"
        style="Label1"
        label="TANGO-Gradient "
        vq="nullflg"
        vc="nullflg='y'"
        vc2="nullflg='n'"
        set="$VALUE=(nullflg='y')"
        />
    </group>
    <group loc="400 95" size="185 120"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="155 60" size="30 20"
        style="Heading1"
        label=" ms"
        justify="Left"
        />
      <entry loc="100 60" size="55 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="100 40" size="55 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <check loc="10 80" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        show="$ENABLE=(PFGflg='n')*2-1"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
  </group>
</template>
