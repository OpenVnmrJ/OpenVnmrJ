<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence2" element="pages" type="acquisition" >
  <group size="650 265"
    style="Heading1"
    label="Pulse Sequence.."
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence2"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="220 40" size="200 125"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="136 85" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="11 1" size="130 20"
        style="Label1"
        label="Steady State"
        justify="Left"
        />
      <check loc="11 23" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <check loc="11 103" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        />
      <label loc="21 81" size="60 20"
        style="Label1"
        label=" time"
        justify="Left"
        />
      <label loc="21 61" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <label loc="11 41" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
      <entry loc="81 81" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="81 61" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
    </group>
    <group loc="15 40" size="200 130"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="c12hnull"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="5 95" size="190 25"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="birdnull"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="160 0" size="20 20"
          style="Heading1"
          label="s"
          justify="Left"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="null nullflg"
          vc="null=$VALUE"
          set="$VALUE=null"
          show="if (nullflg = 'n') then $ENABLE=1 else $ENABLE=0 endif"
          digits="3"
          disable="Grayed out"
          />
        <check loc="0 0" size="140 20"
          style="Label1"
          label="BIRD-delay"
          vq="nullflg null"
          vc="nullflg='n' if (null = 0) then null=0.35 endif"
          vc2="null=0"
          set="if (nullflg='n') and (null &gt; 0) then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <group loc="5 25" size="190 70"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="tangog"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="160 40" size="30 20"
          style="Heading1"
          label="ms"
          justify="Left"
          />
        <entry loc="100 40" size="60 20"
          style="PlainText"
          vq="hsgt nullflg"
          vc="hsgt=$VALUE/1000"
          set="$VALUE=hsgt*1000"
          show="$ENABLE=(nullflg='y')"
          digits="0"
          disable="Grayed out"
          />
        <label loc="20 40" size="70 20"
          style="Label1"
          label="G Duration"
          justify="Left"
          />
        <entry loc="100 20" size="60 20"
          style="PlainText"
          vq="hsglvl nullflg"
          vc="hsglvl=$VALUE"
          set="$VALUE=hsglvl"
          show="$ENABLE=(nullflg='y')"
          digits="0"
          disable="Grayed out"
          />
        <label loc="20 20" size="70 20"
          style="Label1"
          label="G strength:"
          justify="Left"
          />
        <check loc="0 0" size="140 20"
          style="Label1"
          label="TANGO-Gradient "
          vq="nullflg"
          vc="nullflg='y'"
          vc2="nullflg='n'"
          set="$VALUE=(nullflg='y')"
          />
      </group>
      <label loc="15 5" size="180 20"
        style="Label1"
        label="C12-H1 suppression"
        justify="Center"
        />
    </group>
    <button loc="440 0" size="90 20"
      style="Heading2"
      label="Arrays"
      vc="vnmrjcmd('array','popup')"
      decor1=""
      />
    <group loc="10 0" size="390 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 20"
        style="PlainText"
        label="bsHSQCAD"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
      <button loc="250 0" size="130 20"
        style="Heading2"
        label="Display Sequence"
        vc="dps"
        decor1=""
        />
    </group>
  </group>
</template>
