<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quickprpl" element="pages" type="acquisition" >
  <group size="685 240"
    style="Heading1"
    label="ProcPlotAdv"
    vq="customflag panellevel"
    show="$SHOW=((customflag='y') and (panellevel&gt;29))"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quickprpl"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="445 165"
      vq="arraydim"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="prpl2d-hoph"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 10" size="435 130"
        vq="arraydim"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="prpl1D"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="215 5" size="215 110"
          style="Heading1"
          label="Plot Options"
          bg=""
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="plot2d-ez"
          useref="no"
          subtype="Titled"
          expanded="yes"
          >
          <menu loc="115 40" size="90 20"
            style="PlainText"
            label="menu"
            vq="plt2Darg"
            vc="plt2Darg[4]='$VALUE'"
            set="$VALUE=plt2Darg[4]"
            bg=""
            editable="No"
            >
            <mlabel 
              label="spectrum"
              chval="f2"
              />
            <mlabel 
              label="projection"
              chval="projection"
              />
            <mlabel 
              label="none"
              chval="notop"
              />
          </menu>
          <menu loc="115 20" size="90 20"
            style="PlainText"
            label="menu"
            vq="parstyle"
            vc="parstyle='$VALUE'"
            set="$VALUE=parstyle"
            bg=""
            editable="No"
            >
            <mlabel 
              label="Full"
              chval="pap"
              />
            <mlabel 
              label="Basic"
              chval="ppa"
              />
            <mlabel 
              label="Text only"
              chval="pltext"
              />
          </menu>
          <label loc="60 60" size="55 20"
            style="Label1"
            label="along F1"
            justify="Left"
            />
          <menu loc="115 60" size="90 20"
            style="PlainText"
            label="menu"
            vq="plt2Darg"
            vc="plt2Darg[5]='$VALUE'"
            set="$VALUE=plt2Darg[5]"
            bg=""
            editable="No"
            >
            <mlabel 
              label="spectrum"
              chval="f1"
              />
            <mlabel 
              label="projection"
              chval="projection"
              />
            <mlabel 
              label="None"
              chval="noside"
              />
          </menu>
          <label loc="60 40" size="55 20"
            style="Label1"
            label="along F2"
            justify="Left"
            />
          <label loc="10 40" size="50 20"
            style="Label1"
            label="Plot 1D"
            justify="Left"
            />
          <label loc="10 20" size="100 20"
            style="Label1"
            label="Plot Parameters"
            justify="Left"
            />
        </group>
        <group loc="0 5" size="215 110"
          style="Heading1"
          label="Process Options"
          bg=""
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="proc2d-ez"
          useref="no"
          subtype="Titled"
          expanded="yes"
          >
          <menu loc="115 20" size="90 20"
            style="PlainText"
            label="menu"
            vq="fn fn1"
            vc="fnfn1('set',$VALUE)"
            set="fnfn1('get'):$VALUE"
            bg=""
            editable="No"
            >
            <mlabel 
              label="1k x 1k"
              chval="1024,1024"
              />
            <mlabel 
              label="2k x 1k"
              chval="2048,1024"
              />
            <mlabel 
              label="2k x 2k"
              chval="2048,2048"
              />
            <mlabel 
              label="4k x 2k"
              chval="4096,2048"
              />
            <mlabel 
              label="2k x 4k"
              chval="2048,4096"
              />
            <mlabel 
              label="4k x 4k"
              chval="4096,4096"
              />
          </menu>
          <menu loc="115 40" size="90 20"
            style="PlainText"
            vq="dmg1"
            vc="setLP1($VALUE) if (dmg1='av1') then sinebell('f1') else gaussian('f1') endif"
            set="getLP1:$VALUE"
            bg=""
            editable="No"
            >
            <mlabel 
              label="2*ni"
              chval="2*ni"
              />
            <mlabel 
              label="4*ni"
              chval="4*ni"
              />
            <mlabel 
              label="none"
              chval="0"
              />
          </menu>
          <group loc="110 60" size="95 20"
            vq="dmg"
            show="$SHOW=(dmg&lt;&gt;'av')"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            reference="prpl2d-hoph"
            useref="no"
            subtype="Untitled"
            expanded="yes"
            >
            <menu loc="5 0" size="90 20"
              style="PlainText"
              label="menu"
              vc="exec('$VALUE')"
              set="$VALUE=''"
              bg=""
              editable="No"
              >
              <mlabel 
                label="gaussian"
                chval="gaussian('acq')"
                />
              <mlabel 
                label="sq Cosine"
                chval="sqcosine('acq')"
                />
              <mlabel 
                label="cosine"
                chval="cosine('acq')"
                />
              <mlabel 
                label="pi/3 ssbsq"
                chval="pi3ssbsq('acq')"
                />
              <mlabel 
                label="pi/4 ssbsq"
                chval="pi4ssbsq('acq')"
                />
            </menu>
          </group>
          <group loc="115 60" size="95 20"
            vq="dmg"
            show="$SHOW=(dmg='av')"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            reference="prpl2d-hoph"
            useref="no"
            subtype="Untitled"
            expanded="yes"
            >
            <menu loc="0 0" size="90 20"
              style="PlainText"
              label="menu"
              vc="exec('$VALUE')"
              set="$VALUE=''"
              bg=""
              editable="No"
              >
              <mlabel 
                label="sinebell"
                chval="sinebell('acq')"
                />
              <mlabel 
                label="sqsinebell"
                chval="sqsinebell('acq')"
                />
            </menu>
          </group>
          <group loc="115 80" size="95 20"
            vq="dmg1"
            show="$SHOW=(dmg1='av1')"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            reference="prpl2d-hoph"
            useref="no"
            subtype="Untitled"
            expanded="yes"
            >
            <menu loc="0 0" size="90 20"
              style="PlainText"
              label="menu"
              vc="exec('$VALUE')"
              set="$VALUE=''"
              bg=""
              editable="No"
              >
              <mlabel 
                label="sinebell"
                chval="sinebell('f1')"
                />
              <mlabel 
                label="sqsinebell"
                chval="sqsinebell('f1')"
                />
            </menu>
          </group>
          <group loc="110 80" size="95 20"
            vq="dmg1"
            show="$SHOW=(dmg1&lt;&gt;'av1')"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            reference="prpl2d-hoph"
            useref="no"
            subtype="Untitled"
            expanded="yes"
            >
            <menu loc="5 0" size="90 20"
              style="PlainText"
              label="menu"
              vc="exec('$VALUE')"
              set="$VALUE=''"
              bg=""
              editable="No"
              >
              <mlabel 
                label="gaussian"
                chval="gaussian('f1')"
                />
              <mlabel 
                label="sq Cosine"
                chval="sqcosine('f1')"
                />
              <mlabel 
                label="cosine"
                chval="cosine('f1')"
                />
              <mlabel 
                label="pi/3 ssbsq"
                chval="pi3ssbsq('f1')"
                />
              <mlabel 
                label="pi/4 ssbsq"
                chval="pi4ssbsq('f1')"
                />
            </menu>
          </group>
          <label loc="10 80" size="95 20"
            style="Label1"
            label="Weighting (F1)"
            justify="Left"
            />
          <label loc="10 60" size="95 20"
            style="Label1"
            label="Weighting (F2)"
            justify="Left"
            />
          <label loc="10 40" size="105 20"
            style="Label1"
            label="Linear Pred. (F1)"
            justify="Left"
            />
          <label loc="10 20" size="90 20"
            style="Label1"
            label="fn x fn1 size"
            justify="Left"
            />
        </group>
      </group>
    </group>
    <group loc="450 35" size="215 190"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar6"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="0 5" size="215 175"
        style="Heading1"
        label="Advanced Options"
	vq="seqfil pslabel"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <button loc="60 145" size="145 20"
          style="Heading2"
          label="Update curpar"
          vq="chreqexp"
          vc="customizepar('update')"
          show="$ENABLE=(chreqexp&gt;0)*2-1"
          bg=""
          decor1=""
          />
        <filemenu loc="10 105" size="195 20"
          style="PlainText"
          vq="strtpar curexp chreqexp"
          vc="strtpar='$VALUE' customizepar('resetreqexp')"
          set="$VALUE=strtpar"
          show="$ENABLE=(chreqexp&gt;0)*2-1"
          file="$VALUE=curexp+'/curFIDlist'"
          type="file"
          bg=""
          editable="No"
          statshow="yes"
          elastic="no"
          display="yes"
          />
        <label loc="10 65" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <group loc="0 45" size="210 20"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <check loc="10 0" size="195 20"
            style="Label1"
            label="Autogain with First Increment"
            vq="wstart"
            vc="wstart='gain2Dproc'"
            vc2="wstart=''"
            set="$VALUE=(wstart='gain2Dproc')"
            show="$ENABLE=((wstart='') or (wstart='gain2Dproc'))*2-1"
            />
        </group>
        <label loc="10 25" size="130 20"
          style="Label1"
          label="Receiver Gain (dB):"
          justify="Left"
          />
        <textmessage loc="140 25" size="55 20"
          style="PlainText"
          label="30"
          vq="gain"
          set="$VALUE=gain"
          digits="0"
          />
        <check loc="100 65" size="70 20"
          style="Label1"
          label="Change"
          vq="chreqexp"
          vc="chreqexp=1"
          vc2="chreqexp=0"
          set="$VALUE = chreqexp"
          />
        <textmessage loc="10 85" size="200 20"
          style="PlainText"
          label="wet1D PRESAT PROTON"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
        <group loc="5 125" size="205 20"
          vq="chreqexp REQexp strtpar"
          show="$SHOW=(chreqexp&gt;1)"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <entry loc="55 0" size="145 20"
            style="PlainText"
            vq="REQexp chreqexp"
            vc="REQexp='$VALUE' customizepar('resetreqexp')"
            set="$VALUE=REQexp"
            />
          <label loc="5 0" size="50 20"
            style="Label1"
            label="Other:"
            />
        </group>
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="homcordisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="345 25"
        style="PlainText"
        label="Experiment: NOESY1D   Solvent: dmso"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="360 0" size="255 25"
        style="PlainText"
        label="H1-H1 Correlation     [ Decoupler: C13 ]"
        vq="tn dn refsource1"
        set="if (refsource1='dfrq') then $VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation' elseif (refsource1='sfrq') then $VALUE=tn+'-'+tn+' Correlation         [ Decoupler: '+dn+' ]' else $VALUE='Observe: '+tn+'          Decoupler: '+dn endif"
        />
    </group>
  </group>
</template>
