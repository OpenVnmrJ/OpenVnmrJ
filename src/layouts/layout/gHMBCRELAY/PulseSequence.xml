<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="625 235"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="190 165" size="200 40"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SteadyState"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <check loc="10 10" size="185 20"
        style="Label1"
        label="Adiabatic X 180 pulses"
        vq="shapedpulse"
        vc="shapedpulse='y'"
        vc2="shapedpulse='n'"
        set="$VALUE=(shapedpulse='y')"
        />
    </group>
    <group loc="190 25" size="200 135"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="gradsel"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="10 80" size="70 20"
        style="Heading1"
        label="E/D ratio"
        justify="Left"
        />
      <entry loc="110 80" size="60 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE/1000"
        set="$VALUE=EDratio*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="175 105" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="110 105" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="10 105" size="100 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="175 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="20 60" size="70 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 40" size="70 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <label loc="10 20" size="130 20"
        style="Label1"
        label="Encoding Gradient"
        justify="Left"
        />
      <entry loc="110 40" size="60 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="110 60" size="60 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="5 115" size="180 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SteadyState"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="85 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y')"
        disable="Grayed out"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <label loc="150 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="85 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y')"
        />
    </group>
    <group loc="5 25" size="180 90"
      style="Heading1"
      label="Coupling Constants"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SteadyState"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="85 60" size="60 20"
        style="PlainText"
        vq="jhh"
        vc="jhh=$VALUE"
        set="$VALUE=jhh"
        disable="Grayed out"
        />
      <entry loc="85 40" size="60 20"
        style="PlainText"
        vq="jnxh"
        vc="jnxh=$VALUE"
        set="$VALUE=jnxh"
        disable="Grayed out"
        />
      <label loc="150 40" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="150 20" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="150 60" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="85 20" size="60 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        disable="Grayed out"
        />
      <label loc="10 20" size="70 20"
        style="Label1"
        label="J1(XH)"
        justify="Left"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="Jn(HH)"
        justify="Left"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Jn(XH)"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="gHMBCRELAY"
        vq="seqfil"
        set="$VALUE=seqfil"
        />
    </group>
  </group>
</template>
