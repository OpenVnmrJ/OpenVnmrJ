<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="660 245"
    style="Heading1"
    label="Defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="360 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="375 0" size="240 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
    </group>
    <group loc="5 35" size="395 200"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="20 25" size="375 155"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="gHSQCacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <menu loc="190 80" size="90 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg=""
          editable="No"
          >
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="96"
            chval="96"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <menu loc="190 60" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <check loc="190 125" size="90 20"
          style="Label1"
          label="ASAP"
          vq="asap"
          vc="asap='y'"
          vc2="asap='n'"
          set="$VALUE=(asap='y')"
          />
        <label loc="25 100" size="105 20"
          style="Label1"
          label="One-Bond J1xh"
          justify="Left"
          />
        <label loc="25 80" size="110 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="25 60" size="140 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
        <textmessage loc="25 40" size="165 20"
          style="PlainText"
          vq="dn"
          set="$VALUE=dn+' Spectral Width (ppm)'"
          />
        <label loc="285 100" size="30 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <group loc="170 40" size="185 20"
          vq="dn"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <label loc="65 0" size="20 20"
            style="Heading1"
            label="to"
            justify="Center"
            />
          <entry loc="85 0" size="45 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$VALUE,$f2)"
            set="invsetsw1:$f1,$f2 $VALUE=$f1"
            digits="0"
            disable="Grayed out"
            />
          <entry loc="20 0" size="45 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$f1,$VALUE)"
            set="invsetsw1:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
        </group>
        <entry loc="190 100" size="90 20"
          style="PlainText"
          vq="j1xh"
          vc="j1xh=$VALUE"
          set="$VALUE=j1xh"
          digits="1"
          disable="Grayed out"
          />
        <group loc="250 10" size="120 20"
          vq="dn"
          show="$SHOW=(dn='N15')"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <textmessage loc="5 0" size="110 20"
            style="PlainText"
            vq="dn"
            set="$ref='' N15ref:$ref if ($ref='') then $ref='CH3NO2' endif $VALUE='['+$ref+' (0 ppm)]'"
            />
        </group>
        <group loc="25 5" size="220 30"
          bg=""
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="F1Nucleus:"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <group loc="165 0" size="50 25"
            vq="dn"
            show="if (dn='C13') or (dn='N15') then $SHOW=0 else $SHOW=1 endif"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
            <entry loc="0 5" size="45 20"
              style="PlainText"
              vq="dn"
              vc="dn='$VALUE' setdecpars('update') reff1:$ret"
              set="$VALUE=dn"
              disable="Grayed out"
              />
          </group>
          <menu loc="75 5" size="85 20"
            style="PlainText"
            label="menu"
            vq="dn"
            vc="$dn='$VALUE' if ($dn = 'other') then dn='P31' else dn=$dn endif setdecpars('update') reff1:$ret"
            set="$VALUE=dn"
            bg=""
            editable="No"
            >
            <mlabel 
              label="C13"
              chval="C13"
              />
            <mlabel 
              label="N15"
              chval="N15"
              />
            <mlabel 
              label="Other"
              chval="other"
              />
          </menu>
          <label loc="5 5" size="75 20"
            style="Label1"
            label="F1 Nucleus"
            justify="Left"
            />
        </group>
      </group>
    </group>
    <label loc="430 35" size="120 20"
      style="Label1"
      label="Receiver Gain (dB):"
      justify="Left"
      />
    <textmessage loc="560 35" size="65 20"
      style="PlainText"
      vq="gain"
      set="$VALUE=gain"
      />
    <group loc="405 60" size="220 175"
      vq="customflag"
      show="$SHOW=(customflag='y')"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="customizepar2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 105" size="200 20"
        style="PlainText"
        vq="samplename"
        set="$VALUE=samplename"
        />
      <label loc="0 150" size="220 20"
        style="Label1"
        label="More Options: PlotProcAdv page"
        justify="Center"
        />
      <textmessage loc="5 25" size="130 20"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='After '+pslabel+' acq:'"
        />
      <textmessage loc="5 5" size="130 20"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Before '+pslabel+' acq:'"
        />
      <check loc="140 25" size="75 20"
        style="Label1"
        label="Autoplot"
        vq="pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <check loc="140 5" size="75 20"
        style="Label1"
        label="Re-shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
        />
      <textmessage loc="140 125" size="65 20"
        style="PlainText"
        vq="SQalock SQwshim"
        set="$VALUE=SQalock+' / '+SQwshim"
        />
      <label loc="5 85" size="125 20"
        style="Label1"
        label="Samplename:"
        />
      <label loc="5 125" size="130 20"
        style="Label1"
        label="start of Q lock/shim?"
        />
      <label loc="5 45" size="85 20"
        style="Label1"
        label="Starting with:"
        />
      <textmessage loc="10 65" size="200 20"
        style="PlainText"
        vq="strtpar REQexp chreqexp"
        set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
        />
    </group>
  </group>
</template>
