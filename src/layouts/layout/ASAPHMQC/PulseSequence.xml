<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="635 190"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="425 25" size="205 100"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 70" size="95 20"
        style="Label1"
        label="Lock gating"
        vq="lkgate_flg"
        vc="lkgate_flg='y'"
        vc2="lkgate_flg='n'"
        set="$VALUE=(lkgate_flg='y')"
        />
      <label loc="10 45" size="110 20"
        style="Label1"
        label="Gradient time"
        justify="Left"
        />
      <entry loc="120 45" size="55 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <label loc="180 45" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="120 25" size="55 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Gradient strength"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="205 155"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="xhggroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="115 105" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 105" size="100 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="10 85" size="80 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <textmessage loc="10 25" size="180 20"
        style="PlainText"
        label="Encode Grad (10.0 G/cm)"
        vq="gzlvlE invgcal probe"
        set="$gcm='' fixgrdR(gzlvlE):$ret,$gcm if ($gcm&lt;&gt;'') then $gcm='('+$gcm+' G/cm)' endif $VALUE='Encode Grad '+$gcm"
        />
      <label loc="180 65" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="25 45" size="70 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <label loc="25 65" size="70 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <entry loc="115 85" size="60 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="115 65" size="60 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="115 45" size="60 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <label loc="180 105" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="ASAPHMQC"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="215 95" size="205 85"
      style="Heading1"
      label="ASAP"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="c12hnull"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="10 60" size="90 20"
        style="Heading1"
        label="Mixing time"
        justify="Left"
        />
      <check loc="10 40" size="135 20"
        style="Label1"
        label="Adiabatic pulses"
        vq="adiabatic"
        vc="adiabatic='y'"
        vc2="adiabatic='n'"
        set="$VALUE=(adiabatic='y')"
        />
      <label loc="175 60" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="120 60" size="50 20"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        disable="Grayed out"
        />
      <check loc="10 20" size="95 20"
        style="Label1"
        label="ASAP on/off"
        vq="asap"
        vc="asap='y'"
        vc2="asap='n'"
        set="$VALUE=(asap='y')"
        />
    </group>
    <group loc="215 25" size="205 70"
      style="Heading1"
      label="One-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="175 25" size="30 20"
        style="Heading1"
        label="Hz"
        justify="Left"
        />
      <label loc="175 45" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
      <textmessage loc="120 45" size="50 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <entry loc="120 25" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        disable="Grayed out"
        />
    </group>
  </group>
</template>
