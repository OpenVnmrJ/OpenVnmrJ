<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="628 242"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="HMBC"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="420 25" size="170 145"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <check loc="10 100" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        show="$ENABLE=(PFGflg='n')*2-1"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="210 25" size="205 145"
      style="Heading1"
      label="One-Bond suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="onebondsuppress"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="5 40" size="195 100"
        vq="jfilter PFGflg"
        show="$SHOW=(jfilter='y' and PFGflg='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <label loc="170 80" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="170 20" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <label loc="170 0" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <label loc="5 0" size="110 20"
          style="Label1"
          label="Coupling 1 (high)"
          justify="Left"
          />
        <label loc="40 80" size="60 20"
          style="Label1"
          label="time"
          justify="Left"
          />
        <entry loc="115 80" size="50 20"
          style="PlainText"
          vc="gt0=$VALUE/1000"
          set="$VALUE=gt0*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="115 60" size="50 20"
          style="PlainText"
          vq="gzlvl0 jfilter"
          vc="gzlvl0=$VALUE"
          set="$VALUE=gzlvl0"
          digits="0"
          disable="Grayed out"
          />
        <label loc="40 60" size="60 20"
          style="Label1"
          label="strength"
          justify="Left"
          />
        <label loc="5 40" size="110 20"
          style="Label1"
          label="Crusher Gradient:"
          justify="Left"
          />
        <entry loc="115 20" size="50 20"
          style="PlainText"
          vq="j1min jfilter"
          vc="j1min=$VALUE"
          set="$VALUE=j1min"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="115 0" size="50 20"
          style="PlainText"
          vq="j1max jfilter"
          vc="j1max=$VALUE"
          set="$VALUE=j1max"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 20" size="110 20"
          style="Label1"
          label="Coupling 2 (low)"
          justify="Left"
          />
      </group>
      <group loc="10 40" size="195 65"
        vq="jfilter PFGflg"
        show="$SHOW=(jfilter='y' and PFGflg='n')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="110 40" size="50 20"
          style="PlainText"
          vq="j1max j1min"
          set="$VALUE=1000/(j1max+j1min)"
          digits="1"
          />
        <label loc="165 40" size="30 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="165 20" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <label loc="165 0" size="30 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <entry loc="110 20" size="50 20"
          style="PlainText"
          vq="j1min jfilter"
          vc="j1min=$VALUE"
          set="$VALUE=j1min"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="110 0" size="50 20"
          style="PlainText"
          vq="j1max jfilter"
          vc="j1max=$VALUE"
          set="$VALUE=j1max"
          digits="1"
          disable="Grayed out"
          />
        <label loc="0 40" size="110 20"
          style="Label1"
          label="Suppression delay"
          justify="Left"
          />
        <label loc="0 20" size="110 20"
          style="Label1"
          label="Coupling(min)"
          justify="Left"
          />
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Coupling(max)"
          justify="Left"
          />
      </group>
      <check loc="10 20" size="165 20"
        style="Label1"
        label="One-Bond suppression"
        vq="jfilter"
        vc="jfilter='y'"
        vc2="jfilter='n'"
        set="$VALUE=(jfilter='y')"
        />
    </group>
    <group loc="5 25" size="200 75"
      style="Heading1"
      label="Multiple-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="jnxhgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="175 45" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="175 25" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <textmessage loc="120 45" size="50 20"
        style="PlainText"
        label="62.5"
        vq="jnxh"
        set="$VALUE=500/jnxh"
        digits="1"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
      <entry loc="120 25" size="50 20"
        style="PlainText"
        vq="jnxh"
        vc="jnxh=$VALUE"
        set="$VALUE=jnxh"
        disable="Grayed out"
        />
    </group>
  </group>
</template>
