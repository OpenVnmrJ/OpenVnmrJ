<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick2" element="pages" type="acquisition" >
  <group size="660 260"
    style="Heading1"
    label="defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&lt;30)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick2"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="360 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="375 0" size="240 25"
        style="PlainText"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
    </group>
    <group loc="10 35" size="395 200"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 25" size="375 140"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="gHMBCacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <group loc="175 100" size="180 25"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <label loc="85 0" size="25 20"
            style="Heading1"
            label="Hz"
            justify="Left"
            />
          <menu loc="15 0" size="65 20"
            style="PlainText"
            label="menu"
            vq="jnxh"
            vc="jnxh=$VALUE"
            set="$VALUE=jnxh"
            bg=""
            editable="No"
            >
            <mlabel 
              label="8"
              chval="8"
              />
            <mlabel 
              label="5"
              chval="5"
              />
            <mlabel 
              label="3"
              chval="3"
              />
            <mlabel 
              label="10"
              chval="10"
              />
            <mlabel 
              label="12"
              chval="12"
              />
          </menu>
        </group>
        <menu loc="190 40" size="65 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
        </menu>
        <menu loc="190 20" size="90 20"
          style="PlainText"
          label="menu"
          vq="f1SW"
          vc="exec('$VALUE')"
          set="$VALUE=f1SW"
          bg=""
          editable="No"
          >
          <mlabel 
            label="-15 to 225"
            chval="setsw1(dn,225,-15)"
            />
          <mlabel 
            label="-10 to 190"
            chval="setsw1(dn,190,-10)"
            />
          <mlabel 
            label="-10 to 160"
            chval="setsw1(dn,160,-10)"
            />
          <mlabel 
            label="-20 to 240"
            chval="setsw1(dn,240,-20)"
            />
        </menu>
        <group loc="40 80" size="235 20"
          vq="PFGflg jfilter"
          show="$SHOW=(jfilter='y' and PFGflg='n')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <label loc="0 0" size="115 20"
            style="Heading1"
            label="1-step J1xh filter:"
            justify="Left"
            />
          <textmessage loc="150 0" size="65 20"
            style="PlainText"
            vq="jfilter"
            set="if (jfilter='y') then $VALUE='YES' else $VALUE='NO' endif"
            />
        </group>
        <group loc="30 80" size="235 20"
          vq="jfilter PFGflg"
          show="$SHOW=(jfilter='y' and PFGflg='y')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          >
          <textmessage loc="160 0" size="65 20"
            style="PlainText"
            vq="jfilter"
            set="if (jfilter='y') then $VALUE='YES' else $VALUE='NO' endif"
            />
          <label loc="10 0" size="115 20"
            style="Label1"
            label="2-step J1xh filter:"
            justify="Left"
            />
        </group>
        <label loc="40 100" size="125 20"
          style="Label1"
          label="Multiple-Bond Jnxh"
          justify="Left"
          />
        <menu loc="190 60" size="65 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg=""
          editable="No"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <label loc="40 60" size="110 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="40 40" size="150 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
        <textmessage loc="40 20" size="135 20"
          style="PlainText"
          vq="dn"
          set="$VALUE=dn+' Spec Width (ppm)'"
          />
      </group>
    </group>
    <group loc="405 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="5 25" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
  </group>
</template>
