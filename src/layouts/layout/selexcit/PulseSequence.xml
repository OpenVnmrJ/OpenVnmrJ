<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="580 245"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 25" size="250 195"
      style="Heading1"
      label="Double PFG spinecho"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="dpfgsegroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="225 45" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="225 65" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="225 165" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="225 145" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="20 85" size="55 20"
        style="Label1"
        label="shape"
        justify="Left"
        />
      <label loc="20 65" size="55 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <label loc="20 45" size="55 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <label loc="10 25" size="55 20"
        style="Label1"
        label="Pulse"
        justify="Left"
        />
      <label loc="10 165" size="125 20"
        style="Label1"
        label="G- recovery time"
        justify="Left"
        />
      <label loc="10 145" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 125" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <entry loc="105 105" size="115 20"
        style="PlainText"
        vq="selshapeB"
        vc="selshapeB='$VALUE'"
        set="$VALUE=selshapeB"
        disable="Grayed out"
        />
      <entry loc="85 85" size="115 20"
        style="PlainText"
        vq="selshapeA"
        vc="selshapeA='$VALUE'"
        set="$VALUE=selshapeA"
        disable="Grayed out"
        />
      <entry loc="85 145" size="60 20"
        style="PlainText"
        vq="gtA"
        vc="gtA=$VALUE/1000"
        set="$VALUE=gtA*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="85 125" size="60 20"
        style="PlainText"
        vq="gzlvlA"
        vc="gzlvlA=$VALUE"
        set="$VALUE=gzlvlA"
        digits="0"
        disable="Grayed out"
        />
      <label loc="160 25" size="60 20"
        style="Label1"
        label="Echo2"
        justify="Center"
        />
      <label loc="85 25" size="60 20"
        style="Label1"
        label="Echo1"
        justify="Center"
        />
      <entry loc="85 45" size="60 20"
        style="PlainText"
        vq="selpwA"
        vc="selpwA=$VALUE*1000"
        set="$VALUE=selpwA/1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="85 65" size="60 20"
        style="PlainText"
        vq="selpwrA"
        vc="selpwrA=$VALUE"
        set="$VALUE=selpwrA"
        disable="Grayed out"
        />
      <entry loc="160 165" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 145" size="60 20"
        style="PlainText"
        vq="gtB"
        vc="gtB=$VALUE/1000"
        set="$VALUE=gtB*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 125" size="60 20"
        style="PlainText"
        vq="gzlvlB"
        vc="gzlvlB=$VALUE"
        set="$VALUE=gzlvlB"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="160 65" size="60 20"
        style="PlainText"
        vq="selpwrB"
        vc="selpwrB=$VALUE"
        set="$VALUE=selpwrB"
        disable="Grayed out"
        />
      <entry loc="160 45" size="60 20"
        style="PlainText"
        vq="selpwB"
        vc="selpwB=$VALUE*1000"
        set="$VALUE=selpwB/1000"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <textmessage loc="10 0" size="170 25"
      style="PlainText"
      label="selexcit"
      vq="seqfil"
      set="$VALUE=seqfil"
      />
    <group loc="260 25" size="190 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="160 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
  </group>
</template>
