<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="635 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="215 100" size="200 110"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 25" size="160 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <label loc="175 65" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="120 65" size="50 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        disable="Grayed out"
        />
      <entry loc="120 45" size="50 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 65" size="110 20"
        style="Label1"
        label="Gradient time"
        justify="Left"
        />
      <label loc="10 45" size="110 20"
        style="Label1"
        label="Gradient strength"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="205 130"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="xhggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="10 100" size="100 20"
        style="Heading1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="10 80" size="80 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <label loc="25 60" size="70 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="25 40" size="70 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <textmessage loc="10 20" size="180 20"
        style="PlainText"
        label="Encode Grad (10.1 G/cm)"
        vq="gzlvlE invgcal probe"
        set="$gcm='' fixgrdR(gzlvlE):$ret,$gcm if ($gcm&lt;&gt;'') then $gcm='('+$gcm+' G/cm)' endif $VALUE='Encode Grad '+$gcm"
        />
      <label loc="180 100" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="180 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="115 80" size="60 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="115 41" size="60 20"
        style="PlainText"
        vq="gzlvlE EDratio"
        vc="gzlvlE=$VALUE/2/EDratio"
        set="$VALUE=gzlvlE*EDratio*2"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="115 61" size="60 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/500"
        set="$VALUE=gtE*500"
        digits="1"
        disable="Grayed out"
        />
    </group>
    <group loc="215 25" size="200 75"
      style="Heading1"
      label="One-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="10 45" size="95 20"
        style="Heading1"
        label="Transfer delay"
        justify="Left"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
      <label loc="175 45" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="175 25" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="120 25" size="50 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        disable="Grayed out"
        />
      <textmessage loc="120 45" size="50 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
    </group>
    <group loc="5 160" size="205 50"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="10 25" size="190 20"
        style="Heading1"
        label="HH Decoupled:          YES"
        justify="Left"
        />
      <label loc="10 5" size="190 20"
        style="Label1"
        label="XH Multiplicity Edit:  YES"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="gHETCOR"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
