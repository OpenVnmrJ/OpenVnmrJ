<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="660 230"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="20 170" size="250 20"
      vq="atune panellevel"
      show="$SHOW=(atune='y' and panellevel&gt;29)"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <filemenu loc="110 0" size="140 20"
        style="PlainText"
        vq="tunemethod"
        vc="$v='$VALUE' if $v='none' then tunemethod='' else tunemethod=$v endif xmtune('filemenu')"
        set="$n='' $e=0 xmtune('methodname'):$n,$e if $e=1 then $VALUE=$n else $VALUE='none' endif"
        file="$VALUE=userdir+'/persistence/wtune'"
        type="file"
        bg=""
        editable="No"
        />
      <label loc="0 0" size="90 20"
        style="Label1"
        label="Tune method"
        justify="Left"
        />
    </group>
    <group loc="20 50" size="260 120"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="0 90" size="250 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="150 0" size="80 20"
          style="PlainText"
          vq="ni"
          vc="ni=$VALUE"
          set="$VALUE=ni"
          />
        <label loc="0 0" size="150 20"
          style="Label1"
          label="Number of increments"
          justify="Left"
          />
      </group>
      <group loc="0 70" size="250 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="0 0" size="140 20"
          style="Label1"
          label="Scans per increment"
          justify="Left"
          />
        <menu loc="150 0" size="100 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
      </group>
      <group loc="0 40" size="250 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="150 0" size="80 20"
          style="PlainText"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          />
        <label loc="0 0" size="150 20"
          style="Label1"
          label="Relaxation delay [s]"
          justify="Left"
          />
      </group>
      <group loc="0 0" size="250 40"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="h1sw"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="0 0" size="140 20"
          style="Label1"
          label="Spectral Width [ppm]"
          justify="Left"
          />
        <group loc="0 20" size="240 20"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="swstartend"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <label loc="10 0" size="70 20"
            style="Label1"
            label="Downfield "
            justify="Right"
            />
          <entry loc="80 0" size="50 20"
            style="PlainText"
            vq="sw tof reffrq rfl rfp"
            vc="invsetsw:$f1,$f2 setsw($VALUE,$f2) sw1=sw setref1(tn,'setup')"
            set="invsetsw:$f1,$f2 $VALUE=$f1"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="180 0" size="50 20"
            style="PlainText"
            vq="sw tof reffrq rfl rfp"
            vc="invsetsw:$f1,$f2 setsw($f1,$VALUE) sw1=sw setref1(tn,'setup')"
            set="invsetsw:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
          <label loc="120 0" size="60 20"
            style="Label1"
            label="Upfield "
            justify="Right"
            />
        </group>
        <menu loc="150 0" size="100 20"
          style="PlainText"
          vq="sw tof reffrq rfl rfp"
          vc="setsw($VALUE) sw1=sw setref1(tn,'setup')"
          set="$s1='' $s2='' invsetsw:$f1,$f2 format($f1,1,1):$s1 format($f2,1,1):$s2 $VALUE=$s1+','+$s2"
          editable="No"
          >
          <mlabel 
            label="14  -&gt;  -2"
            chval="14.0,-2.0"
            />
          <mlabel 
            label="11  -&gt;  -1"
            chval="11.0,-1.0"
            />
          <mlabel 
            label="9.5  -&gt;  -0.5"
            chval="9.5,-0.5"
            />
          <mlabel 
            label="8.5  -&gt;  0.5"
            chval="8.5,0.5"
            />
        </menu>
      </group>
    </group>
    <group loc="370 110" size="260 100"
      style="Heading1"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="def2dplot"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 20" size="250 80"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="plt2dhom"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="0 0" size="90 20"
          style="Label1"
          label="Parameters:"
          justify="Left"
          />
        <label loc="90 40" size="50 20"
          style="Label1"
          label="  F1"
          justify="Left"
          />
        <label loc="90 60" size="50 20"
          style="Label1"
          label="  F2"
          justify="Left"
          />
        <menu loc="140 60" size="110 20"
          style="PlainText"
          label="menu"
          vq="plt2Darg pltmod"
          vc="plt2Darg[5]='$VALUE'"
          set="$VALUE=plt2Darg[5]"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="spectrum"
            chval="f2"
            />
          <mlabel 
            label="projection"
            chval="projection"
            />
          <mlabel 
            label="none"
            chval="none"
            />
        </menu>
        <menu loc="140 40" size="110 20"
          style="PlainText"
          label="menu"
          vq="plt2Darg pltmod"
          vc="plt2Darg[4]='$VALUE'"
          set="$VALUE=plt2Darg[4]"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="spectrum"
            chval="f1"
            />
          <mlabel 
            label="projection"
            chval="projection"
            />
          <mlabel 
            label="none"
            chval="none"
            />
        </menu>
        <menu loc="90 20" size="160 20"
          style="PlainText"
          label="menu"
          vq="plt2Darg pltmod"
          vc="plt2Darg[1]='$VALUE'"
          set="$VALUE=plt2Darg[1]"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="Positive &amp; Negative"
            chval="both"
            />
          <mlabel 
            label="Positive Only"
            chval="pos"
            />
          <mlabel 
            label="Negative Only"
            chval="neg"
            />
        </menu>
        <menu loc="90 0" size="160 20"
          style="PlainText"
          label="menu"
          vq="parstyle pltmod"
          vc="parstyle='$VALUE'"
          set="$VALUE=parstyle"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="Full, Top Left"
            chval="pap"
            />
          <mlabel 
            label="Full, Top Right"
            chval="pap(wc-50)"
            />
          <mlabel 
            label="Basic, Top Left"
            chval="ppa"
            />
          <mlabel 
            label="Basic, Top Right"
            chval="ppa(wc-50)"
            />
          <mlabel 
            label="Boxed"
            chval="bpa"
            />
          <mlabel 
            label="Text Only"
            chval="pltext"
            />
        </menu>
        <label loc="0 20" size="90 20"
          style="Label1"
          label="Plot contours:"
          justify="Left"
          />
        <label loc="0 40" size="60 20"
          style="Label1"
          label="Plot 1D:"
          justify="Left"
          />
      </group>
      <textmessage loc="0 0" size="120 20"
        style="PlainText"
        label="Plotting turned off"
        vq="pltmod"
        set="if pltmod='off' then $VALUE='Plotting turned off' else $VALUE='Plot when done' endif"
        />
    </group>
    <group loc="370 30" size="260 60"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="0 20" size="150 20"
        style="Label1"
        label="Fourier Number in F2"
        justify="Left"
        />
      <label loc="0 40" size="130 20"
        style="Label1"
        label="Window functions:"
        justify="Left"
        />
      <menu loc="160 40" size="100 20"
        style="PlainText"
        vq="lb sb sbs gf gfs awc lb1 sb1 sbs1 gf1 gfs1 awc1"
        vc="exec('$VALUE')"
        set="$VALUE=''"
        editable="No"
        >
        <mlabel 
          label="gaussian"
          chval="gaussian"
          />
        <mlabel 
          label="sinebell"
          chval="sinebell"
          />
        <mlabel 
          label="sq cosine"
          chval="sqcosine"
          />
        <mlabel 
          label="pi/3 ssbsq"
          chval="pi3ssbsq"
          />
        <mlabel 
          label="pi/4 ssbsq"
          chval="pi4ssbsq"
          />
      </menu>
      <menu loc="160 20" size="100 20"
        style="PlainText"
        vq="fn"
        vc="fn=$VALUE on('fn'):$on if $on=1 then fn=fn*2 endif"
        set="on('fn'):$on if $on=0 then $VALUE='n' else $VALUE=fn/2 endif"
        editable="No"
        >
        <mlabel 
          label="not used"
          chval="n"
          />
        <mlabel 
          label="1k"
          chval="1024"
          />
        <mlabel 
          label="2k"
          chval="2048"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
        <mlabel 
          label="32k"
          chval="32768"
          />
        <mlabel 
          label="64k"
          chval="65536"
          />
        <mlabel 
          label="128k"
          chval="131072"
          />
      </menu>
      <group loc="0 0" size="260 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="0 0" size="150 20"
          style="Label1"
          label="Fourier Number in F1"
          justify="Left"
          />
        <menu loc="160 0" size="100 20"
          style="PlainText"
          vq="fn1"
          vc="fn1=$VALUE on('fn1'):$on if $on=1 then fn1=fn1*2 endif"
          set="on('fn1'):$on if $on=0 then $VALUE='n' else $VALUE=fn1/2 endif"
          editable="No"
          >
          <mlabel 
            label="not used"
            chval="n"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
          <mlabel 
            label="2048"
            chval="2048"
            />
          <mlabel 
            label="4096"
            chval="4096"
            />
        </menu>
      </group>
    </group>
    <label loc="0 0" size="120 30"
      style="Label1"
      label="Pulse Sequence:"
      justify="Left"
      />
    <textmessage loc="120 0" size="110 30"
      style="PlainText"
      label="selexHT"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
    <button loc="260 0" size="130 20"
      style="Heading2"
      label="Display Sequence"
      vc="dps"
      decor1=""
      />
  </group>
</template>
