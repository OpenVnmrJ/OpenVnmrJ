<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="720 240"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="220 30" size="220 190"
      label="HT Excitation"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Major"
      >
      <check loc="10 120" size="180 20"
        style="Label1"
        label="Bloch-Seigert Correction"
        vq="bscor"
        vc="bscor='y' "
        vc2="bscor='n'"
        set="if( bscor = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="130 100" size="50 20"
        style="PlainText"
        vq="niofs"
        vc="niofs=$VALUE"
        set="$VALUE=niofs"
        />
      <entry loc="130 80" size="90 20"
        style="PlainText"
        vq="compH"
        vc="compH=$VALUE"
        set="$VALUE=compH"
        digits="3"
        />
      <entry loc="130 40" size="90 20"
        style="PlainText"
        vq="pshape ptype"
        vc="pshape='$VALUE'"
        set="$VALUE=pshape"
        />
      <menu loc="120 20" size="100 20"
        style="PlainText"
        label="menu"
        vq="httype pshape gt1 gt2 gzlvl1 gzlvl2"
        vc="httype='$VALUE' HTchoice"
        set="$VALUE=httype"
        >
        <mlabel 
          label="Refocusing"
          chval="r"
          />
        <mlabel 
          label="Inversion"
          chval="i"
          />
        <mlabel 
          label="Excitation"
          chval="e"
          />
      </menu>
      <entry loc="130 60" size="50 20"
        style="PlainText"
        vq="htbw1"
        vc="htbw1=$VALUE"
        set="$VALUE=htbw1"
        digits="0"
        />
      <label loc="180 60" size="30 20"
        style="Label1"
        label="Hz"
        />
      <menu loc="140 140" size="60 20"
        style="PlainText"
        label="Hadamard Dimension"
        vq="Hdim"
        vc="Hdim=$VALUE if $VALUE=0 then write('line3', 'ixHT must be arrayed between 1 and niHT') endif"
        set="$VALUE=Hdim"
        >
        <mlabel 
          label="F1"
          chval="1"
          />
        <mlabel 
          label="0"
          chval="0"
          />
      </menu>
      <label loc="10 140" size="140 20"
        style="Label1"
        label="Hadamard Dimension"
        />
      <label loc="10 100" size="120 20"
        style="Label1"
        label="Skip First increments"
        />
      <label loc="10 80" size="120 20"
        style="Label1"
        label="H1 Amp Compres."
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Bandwidth"
        />
      <label loc="10 40" size="110 20"
        style="Label1"
        label="HT Shaped Pulse"
        />
      <label loc="10 20" size="110 20"
        style="Label1"
        label="Selection Method"
        />
    </group>
    <group loc="450 30" size="250 190"
      label="Gradients"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="200 80" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="200 100" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="200 120" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="200 140" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 140" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        />
      <entry loc="140 120" size="60 20"
        style="PlainText"
        vq="gt3"
        vc="gt3=$VALUE/1000"
        set="$VALUE=gt3*1000"
        />
      <entry loc="140 100" size="60 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        />
      <entry loc="140 80" size="60 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        />
      <entry loc="140 40" size="60 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        />
      <entry loc="140 60" size="60 20"
        style="PlainText"
        vq="gzlvl3"
        vc="gzlvl3=$VALUE"
        set="$VALUE=gzlvl3"
        />
      <entry loc="140 20" size="60 20"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        />
      <label loc="60 120" size="50 20"
        style="Label1"
        label="gt3"
        />
      <label loc="60 100" size="50 20"
        style="Label1"
        label="gt2"
        />
      <label loc="60 80" size="50 20"
        style="Label1"
        label="gt1"
        />
      <label loc="10 80" size="50 20"
        style="Label1"
        label="Delay"
        />
      <label loc="10 20" size="50 20"
        style="Label1"
        label="Level"
        />
      <label loc="60 140" size="60 20"
        style="Label1"
        label="gstab"
        />
      <label loc="60 60" size="60 20"
        style="Label1"
        label="gzlvl3"
        />
      <label loc="60 40" size="60 20"
        style="Label1"
        label="gzlvl2"
        />
      <label loc="60 20" size="60 20"
        style="Label1"
        label="gzlvl1"
        />
    </group>
    <group loc="0 150" size="210 70"
      label="HT Optimization"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HTOptimization"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="60 40" size="80 20"
        style="PlainText"
        vq="htpwr1"
        vc="htpwr1=$VALUE"
        set="$VALUE=htpwr1"
        />
      <label loc="10 40" size="50 20"
        style="Label1"
        label="htpwr1"
        />
      <entry loc="60 20" size="80 20"
        style="PlainText"
        vq="htcal1"
        vc="htcal1=$VALUE"
        set="$VALUE=htcal1"
        />
      <label loc="10 20" size="50 20"
        style="Label1"
        label="htcal1"
        />
    </group>
    <group loc="0 30" size="210 120"
      style="Heading1"
      label="Solvent Suppression"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventSuppression"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="0 20" size="160 20"
        style="Label1"
        label="Presat ON / OFF Status"
        vq="satmode wet"
        vc="satmode='ynn' wet='n'"
        vc2="satmode='nnn'"
        set="$s='' substr(satmode,1,1):$s $VALUE=($s='y')"
        />
      <check loc="0 100" size="160 20"
        style="Label1"
        label="WET ON / OFF Status"
        vq="satmode wet"
        vc="wet='y' satmode='nnn'"
        vc2="wet='n'"
        set="$s='' substr(wet,1,1):$s $VALUE=($s='y')"
        />
      <entry loc="100 80" size="60 20"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="satfrq satmode"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Frequency"
        justify="Left"
        />
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        disable="Grayed out"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
      <label loc="160 73" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="160 53" size="30 20"
        style="Label1"
        label="s"
        justify="Left"
        />
    </group>
    <button loc="440 0" size="90 20"
      style="Heading2"
      label="Arrays"
      vc="vnmrjcmd('array','popup')"
      decor1=""
      />
    <group loc="10 0" size="390 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 20"
        style="PlainText"
        label="selexHT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
      <button loc="250 0" size="130 20"
        style="Heading2"
        label="Display Sequence"
        vc="dps"
        decor1=""
        />
    </group>
  </group>
</template>
