<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="630 220"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="310 100" size="263 103"
      style="Heading1"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="def2dplot"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="120 20"
        style="PlainText"
        label="Plot when done"
        vq="pltmod"
        set="if pltmod='off' then $VALUE='Plotting turned off' else $VALUE='Plot when done' endif"
        />
      <group loc="10 20" size="250 80"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="plt2ds"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="90 60" size="50 20"
          style="Label1"
          label=" F2 (H)"
          justify="Left"
          />
        <label loc="90 40" size="50 20"
          style="Label1"
          label=" F1 (X)"
          justify="Left"
          />
        <menu loc="140 60" size="110 20"
          style="PlainText"
          label="menu"
          vq="plt2Darg pltmod"
          vc="plt2Darg[5]='$VALUE'"
          set="$VALUE=plt2Darg[5]"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          >
          <mlabel 
            label="spectrum"
            chval="f2"
            />
          <mlabel 
            label="projection"
            chval="projection"
            />
          <mlabel 
            label="none"
            chval="none"
            />
        </menu>
        <menu loc="140 40" size="110 20"
          style="PlainText"
          label="menu"
          vq="plt2Darg pltmod"
          vc="plt2Darg[4]='$VALUE'"
          set="$VALUE=plt2Darg[4]"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          >
          <mlabel 
            label="spectrum"
            chval="f1"
            />
          <mlabel 
            label="projection"
            chval="projection"
            />
          <mlabel 
            label="none"
            chval="none"
            />
        </menu>
        <menu loc="90 20" size="160 20"
          style="PlainText"
          label="menu"
          vq="plt2Darg pltmod"
          vc="plt2Darg[1]='$VALUE'"
          set="$VALUE=plt2Darg[1]"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          >
          <mlabel 
            label="Positive &amp; Negative"
            chval="both"
            />
          <mlabel 
            label="Positive Only"
            chval="pos"
            />
          <mlabel 
            label="Negative Only"
            chval="neg"
            />
        </menu>
        <menu loc="90 0" size="160 20"
          style="PlainText"
          label="menu"
          vq="parstyle pltmod"
          vc="parstyle='$VALUE'"
          set="$VALUE=parstyle"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          >
          <mlabel 
            label="Full, Top Left"
            chval="pap"
            />
          <mlabel 
            label="Full, Top Right"
            chval="pap(wc-50)"
            />
          <mlabel 
            label="Basic, Top Left"
            chval="ppa"
            />
          <mlabel 
            label="Basic, Top Right"
            chval="ppa(wc-50)"
            />
          <mlabel 
            label="Boxed"
            chval="bpa"
            />
          <mlabel 
            label="Text Only"
            chval="pltext"
            />
        </menu>
        <label loc="0 0" size="80 20"
          style="Label1"
          label="Parameters:"
          justify="Left"
          />
        <label loc="0 20" size="90 20"
          style="Label1"
          label="Plot contours:"
          justify="Left"
          />
        <label loc="0 40" size="60 20"
          style="Label1"
          label="Plot 1D:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="310 30" size="270 60"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="180 40" size="90 20"
        style="PlainText"
        vq="lb1 sb1 sbs1 gf1 gfs1 awc1"
        vc="exec('$VALUE')"
        set="$VALUE=''"
        >
        <mlabel 
          label="sq sinebell"
          chval="sqsinebell"
          />
        <mlabel 
          label="sinebell"
          chval="sinebell"
          />
        <mlabel 
          label="gaussian"
          chval="gaussian"
          />
      </menu>
      <menu loc="180 20" size="90 20"
        style="PlainText"
        vq="proc1 lpopt1 lpext1 strtext1 lpnupts1 strtlp1 lpfilt1"
        vc="if '$VALUE'='none' then proc1='ft' elseif '$VALUE'='def' then setLP1 else setLP1($VALUE) endif"
        set="getLP1:$VALUE"
        >
        <mlabel 
          label="default"
          chval="def"
          />
        <mlabel 
          label="ni * 2"
          chval="2*ni"
          />
        <mlabel 
          label="ni * 4"
          chval="4*ni"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
        <mlabel 
          label="none"
          chval="none"
          />
      </menu>
      <menu loc="180 0" size="90 20"
        style="PlainText"
        vq="fn fn1"
        vc="fnfn1('set',$VALUE) gaussian"
        set="fnfn1('get'):$VALUE"
        >
        <mlabel 
          label="not used"
          chval="`n`,`n`"
          />
        <mlabel 
          label=".5k x .5k"
          chval="512,512"
          />
        <mlabel 
          label="1k x .5k"
          chval="1024,512"
          />
        <mlabel 
          label="1k x 1k"
          chval="1024,1024"
          />
        <mlabel 
          label="2k x 1k"
          chval="2048,1024"
          />
        <mlabel 
          label="2k x 2k"
          chval="2048,2048"
          />
      </menu>
      <label loc="0 40" size="120 20"
        style="Label1"
        label="Window functions:"
        justify="Left"
        />
      <label loc="0 20" size="140 20"
        style="Label1"
        label="Linear Prediction in t1:"
        justify="Left"
        />
      <label loc="0 0" size="160 20"
        style="Label1"
        label="Fourier Number in F2 &amp; F1:"
        justify="Left"
        />
    </group>
    <group loc="20 50" size="253 110"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="def2dacq"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="0 80" size="250 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <menu loc="150 0" size="100 20"
          style="PlainText"
          vq="jnxh"
          vc="jnxh=$VALUE"
          set="$VALUE=jnxh"
          >
          <mlabel 
            label="3 Hz"
            chval="3"
            />
          <mlabel 
            label="5 Hz"
            chval="5"
            />
          <mlabel 
            label="8 Hz"
            chval="8"
            />
          <mlabel 
            label="10 Hz"
            chval="10"
            />
        </menu>
        <label loc="0 0" size="130 20"
          style="Label1"
          label="Coupling constant"
          justify="Left"
          />
      </group>
      <group loc="0 0" size="250 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <menu loc="150 0" size="100 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
        <label loc="0 0" size="130 20"
          style="Label1"
          label="Scans per increment"
          justify="Left"
          />
      </group>
      <group loc="0 20" size="250 20"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <menu loc="150 0" size="100 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <label loc="0 0" size="130 20"
          style="Label1"
          label="Number of increments"
          justify="Left"
          />
      </group>
      <group loc="0 40" size="250 43"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="c13sw"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <group loc="0 20" size="240 20"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="swstartend"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <label loc="10 0" size="70 20"
            style="Label1"
            label="Downfield "
            justify="Right"
            />
          <entry loc="80 0" size="50 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$VALUE,$f2)"
            set="invsetsw1:$f1,$f2 $VALUE=$f1"
            digits="0"
            disable="Grayed out"
            />
          <label loc="120 0" size="60 20"
            style="Label1"
            label="Upfield "
            justify="Right"
            />
          <entry loc="180 0" size="50 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$f1,$VALUE)"
            set="invsetsw1:$f1,$f2 $VALUE=$f2"
            digits="0"
            disable="Grayed out"
            />
        </group>
        <menu loc="150 0" size="100 20"
          style="PlainText"
          vq="dn"
          vc="setsw1(dn,$VALUE)"
          set="$s1='' $s2='' invsetsw1:$f1,$f2 format($f1,1,0):$s1 format($f2,1,0):$s2 $VALUE=$s1+','+$s2"
          >
          <mlabel 
            label="160  -&gt;  -10"
            chval="160,-10"
            />
          <mlabel 
            label="190  -&gt;  -10"
            chval="190,-10"
            />
          <mlabel 
            label="225  -&gt;  -15"
            chval="225,-15"
            />
          <mlabel 
            label="240  -&gt;  -20"
            chval="240,-20"
            />
        </menu>
        <label loc="0 0" size="150 20"
          style="Label1"
          label="C13 Spectral Width [ppm]"
          justify="Left"
          />
      </group>
    </group>
    <group loc="100 0" size="120 30"
      vq="PFGflg"
      show="$SHOW=(PFGflg='y')"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="seqwet"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="0 0" size="110 30"
        style="PlainText"
        label="menu"
        vq="pslabel"
        vc="$VALUE"
        set="$VALUE=pslabel"
        >
        <mlabel 
          label="HMBC"
          chval="Hmbc"
          />
        <mlabel 
          label="gHMBC"
          chval="Ghmbc"
          />
      </menu>
    </group>
    <group loc="100 0" size="80 40"
      vq="PFGflg"
      show="$SHOW=(PFGflg='n')"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <textmessage loc="0 0" size="80 30"
        style="PlainText"
        label="gHMBC"
        vq="seqfil"
        set="$VALUE=seqfil"
        />
    </group>
    <label loc="0 0" size="100 30"
      style="Label1"
      label="Pulse Sequence:"
      justify="Left"
      />
    <button loc="260 0" size="130 20"
      style="Heading2"
      label="Display Sequence"
      vc="dps"
      decor1=""
      />
  </group>
</template>
