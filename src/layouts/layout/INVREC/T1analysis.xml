<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="T1analysis" element="pages" type="processing" >
  <group size="820 280"
    style="Heading1"
    label="T1 Analysis"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','process','relaxanalysis'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="T1analysis"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 0" size="230 270"
      style="Heading1"
      label="T1 processing"
      border="Etched"
      tab="no"
      enable="no"
      reference="T1processing"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <button loc="10 165" size="135 20"
        style="Heading2"
        label="Display Fit Summary"
        vc="ds dpf dll fp t1 t1mod('tableonly')"
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <button loc="145 165" size="75 20"
        style="Heading2"
        label="...in Editor"
        vq="alphatext"
        vc="ds dpf dll fp t1 t1mod('tableonly') alphatextname:$f shell('gedit '+$f+' &amp;')"
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <button loc="145 105" size="75 20"
        style="Heading2"
        label="...in Editor"
        vq="alphatext"
        vc="ds dpf dll fp t1 t1mod alphatextname:$f shell('gedit '+$f+' &amp;')"
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <button loc="10 240" size="210 20"
        style="Heading2"
        label="Clear"
        vc="clear"
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <button loc="10 215" size="210 20"
        style="Heading2"
        label="Print Fit Summary"
        vc="t1 printon t1mod('tableonly') printoff "
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <entry loc="150 195" size="70 20"
        style="PlainText"
        vq="ftproc"
        vc="pexpl($VALUE) page"
        set="$VALUE=$peak_no"
        />
      <entry loc="150 145" size="70 20"
        style="PlainText"
        vq="ftproc"
        vc="expl($VALUE)"
        set="$VALUE=$peak_no"
        />
      <button loc="10 125" size="210 20"
        style="Heading2"
        label="Display All Fits"
        vc="expl "
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <button loc="10 85" size="210 20"
        style="Heading2"
        label="Display Last Spectrum"
        vc="wft  ds(arraydim) f full intmod='off'"
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <button loc="10 105" size="135 20"
        style="Heading2"
        label="Do T1 Analysis"
        vc="ds dpf dll fp t1 t1mod"
        bg="transparent"
        justify="Center"
        decor1="yes"
        />
      <label loc="10 195" size="130 20"
        style="Label1"
        label="Plot Selected Fits"
        />
      <label loc="10 145" size="130 20"
        style="Label1"
        label="Display Selected Fits"
        />
      <label loc="10 20" size="210 20"
        style="Label1"
        label="On last spectrum, expand"
        />
      <label loc="10 40" size="210 20"
        style="Label1"
        label="region and set threshhold"
        />
      <label loc="10 60" size="210 20"
        style="Label1"
        label="for T1 Analysis."
        />
    </group>
    <textfile loc="245 0" size="500 270"
      style="PlainText"
      vq="alphatext"
      set="alphatextname:$VALUE"
      editable="no"
      wrap="no"
      units="char"
      />
  </group>
</template>
