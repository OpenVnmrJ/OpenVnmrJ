<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="660 270"
    style="Heading1"
    label="Defaults"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','default'):$S $SHOW=$S"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="20 20" size="370 190"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="CARBONacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="225 45" size="30 20"
          style="Heading1"
          label="s"
          justify="Left"
          />
        <group loc="40 90" size="310 100"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="MinT1"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="185 60" size="40 20"
            style="Heading1"
            label="h"
            />
          <label loc="185 40" size="30 20"
            style="Label1"
            label="s"
            />
          <label loc="185 20" size="30 20"
            style="Label1"
            label="s"
            />
          <button loc="110 80" size="155 20"
            style="Heading2"
            label="Array Relaxation Delay"
            vc="T1setup(T1min,T1max,T1exptime,T1invrec)"
            bg=""
            decor1=""
            />
          <menu loc="110 0" size="155 20"
            style="PlainText"
            vq="T1invrec"
            vc="T1invrec='$VALUE' if T1invrec='y' then p1=2*pw90 else p1=pw90 endif"
            set="$VALUE=T1invrec"
            bg=""
            editable="No"
            >
            <mlabel 
              label="Inversion Recovery"
              chval="y"
              />
            <mlabel 
              label="Progressive Saturation"
              chval="n"
              />
          </menu>
          <entry loc="110 60" size="70 20"
            style="PlainText"
            vq="T1exptime"
            vc="T1exptime=$VALUE"
            set="$VALUE=T1exptime"
            />
          <entry loc="110 40" size="70 20"
            style="PlainText"
            vq="T1max"
            vc="T1max=$VALUE"
            set="$VALUE=T1max"
            />
          <entry loc="110 20" size="70 20"
            style="PlainText"
            vq="T1min"
            vc="T1min=$VALUE"
            set="$VALUE=T1min"
            />
          <label loc="0 60" size="100 20"
            style="Label1"
            label="Total Exp Time"
            />
          <label loc="0 40" size="90 20"
            style="Label1"
            label="Max T1"
            />
          <label loc="0 20" size="90 20"
            style="Label1"
            label="Min T1"
            />
          <label loc="0 0" size="70 20"
            style="Label1"
            label="T1 Mode"
            />
        </group>
        <menu loc="150 65" size="155 20"
          style="PlainText"
          label="menu"
          vq="dm"
          vc="dm='$VALUE'"
          set="$VALUE=dm"
          bg=""
          editable="No"
          >
          <mlabel 
            label="Decoupled + NOE"
            chval="yyy"
            />
          <mlabel 
            label="Decoupled - NOE"
            chval="nny"
            />
          <mlabel 
            label="Coupled + NOE"
            chval="ynn"
            />
          <mlabel 
            label="Coupled - NOE"
            chval="nnn"
            />
        </menu>
        <label loc="40 65" size="90 20"
          style="Label1"
          label="Decoupling"
          justify="Left"
          />
        <label loc="40 45" size="110 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
        <menu loc="150 45" size="70 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="0"
            chval="0"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="10"
            chval="10"
            />
          <mlabel 
            label="20"
            chval="20"
            />
        </menu>
        <menu loc="150 25" size="70 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1000"
            chval="1000"
            />
          <mlabel 
            label="2000"
            chval="2000"
            />
          <mlabel 
            label="5000"
            chval="5000"
            />
          <mlabel 
            label="10000"
            chval="10000"
            />
        </menu>
        <label loc="40 25" size="110 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <group loc="20 5" size="300 20"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="swentry"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="255 0" size="35 20"
            style="Heading1"
            label="ppm"
            justify="Left"
            />
          <entry loc="200 0" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq sw"
            vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
            set="invsetsw:$f1,$f2 $VALUE=$f1"
            digits="1"
            disable="Grayed out"
            />
          <label loc="180 0" size="20 20"
            style="Label1"
            label="to"
            justify="Center"
            />
          <entry loc="130 0" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq"
            vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
            set="invsetsw:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
          <label loc="20 0" size="110 20"
            style="Label1"
            label="Spectral Width:"
            justify="Left"
            />
        </group>
      </group>
    </group>
    <group loc="400 35" size="235 230"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 45" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <label loc="0 150" size="220 20"
          style="Heading1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="10 105" size="200 25"
          style="PlainText"
          label="BertQtest4a"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          label="After INVREC acq:"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          label="Before INVREC acq:"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label="PROTON_01"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="5 0" size="135 20"
        style="Label1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="145 20" size="75 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        label="Experiment: INVREC   Solvent: dmso"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        label="Observe: H1          Decoupler: C13"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
  </group>
</template>
