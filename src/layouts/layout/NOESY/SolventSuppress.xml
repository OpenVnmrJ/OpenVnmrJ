<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="SolventSuppress" element="pages" type="acquisition" >
  <group size="660 285"
    style="Heading1"
    label="SolvSupp"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="SolventSuppress"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="395 30" size="220 235"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="presatgroup2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="5 60" size="210 170"
        vq="satmode"
        show="if (satmode='') or (satmode='n') or (satmode='nn') then $SHOW=0 else $SHOW=1 endif"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <check loc="10 85" size="165 20"
          style="Label1"
          label="Off-Resonance PRESAT"
          vq="slpsat"
          vc="slpsat='y' pstof=pstof"
          vc2="slpsat='n' pstof=pstof"
          set="$VALUE=(slpsat='y')"
          />
        <label loc="10 105" size="70 20"
          style="Label1"
          label="Offsets (Hz)"
          vq="slpsat"
          show="$ENABLE=(slpsat='y')"
          justify="Left"
          />
        <entry loc="80 105" size="125 20"
          style="PlainText"
          vq="pstof slpsat"
          vc="if (slpsat='y') then pstof=$VALUE endif pstof=pstof"
          set="array2csv(pstof,1):$VALUE"
          show="$ENABLE=(slpsat='y')"
          digits="1"
          disable="Grayed out"
          />
        <label loc="10 125" size="135 20"
          style="Label1"
          label="Chem. Shift(ppm):"
          vq="slpsat"
          show="$ENABLE=(slpsat='y')"
          justify="Left"
          />
        <textmessage loc="10 145" size="180 20"
          style="PlainText"
          label="6.00p"
          vq="slpsat psppm"
          set="$VALUE=psppm"
          />
        <entry loc="85 5" size="60 20"
          style="PlainText"
          vq="satdly satmode"
          vc="$sm='' substr(satmode,1,1):$sm if ($sm='y') then satdly=$VALUE else satdly=satdly endif"
          set="$VALUE=satdly"
          show="$sm='' substr(satmode,1,1):$sm $ENABLE=($sm='y')"
          digits="2"
          disable="Grayed out"
          />
        <label loc="150 25" size="30 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <textmessage loc="85 25" size="65 20"
          style="PlainText"
          label="200.0"
          vq="mixN"
          set="$VALUE=mixN*1000"
          digits="1"
          />
        <label loc="10 25" size="70 20"
          style="Label1"
          label="Mixing time"
          justify="Left"
          />
        <textmessage loc="150 45" size="50 20"
          style="PlainText"
          label="5.96p"
          vq="satfrq satmode slpsat"
          set="setsatppm:$VALUE"
          show="$ENABLE=(slpsat='n')"
          />
        <entry loc="85 45" size="60 20"
          style="PlainText"
          vq="satfrq satmode slpsat"
          vc="if (slpsat='n') then satfrq=$VALUE else satfrq=satfrq endif"
          set="$VALUE=satfrq"
          show="$ENABLE=(slpsat='n')"
          digits="1"
          disable="Grayed out"
          />
        <label loc="10 45" size="70 20"
          style="Label1"
          label="Offset (Hz)"
          justify="Left"
          />
        <textmessage loc="135 65" size="50 20"
          style="PlainText"
          label="46 Hz"
          vq="satpwr"
          set="getpower(satpwr,tn):$gh2 $VALUE='' format($gh2,0,0):$VALUE $VALUE=$VALUE+' Hz'"
          />
        <entry loc="85 65" size="45 20"
          style="PlainText"
          vq="satpwr satmode"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          disable="Grayed out"
          />
        <label loc="10 65" size="70 20"
          style="Label1"
          label="Power (dB)"
          justify="Left"
          />
        <label loc="145 5" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="10 5" size="70 20"
          style="Label1"
          label="Delay"
          justify="Left"
          />
      </group>
      <check loc="5 40" size="200 20"
        style="Label1"
        label="Relaxation and Mixing"
        vq="satmode"
        vc="satmode='yy'"
        vc2="satmode='nn'"
        set="if (satmode='yy') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="110 20" size="75 20"
        style="Label1"
        label="Mixing"
        vq="satmode"
        vc="satmode='ny'"
        vc2="satmode='nn'"
        set="if (satmode='ny') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="5 20" size="105 20"
        style="Label1"
        label="Relaxation"
        vq="satmode"
        vc="satmode='yn'"
        vc2="satmode='nn'"
        set="if (satmode='y') or (satmode='yn') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="5 0" size="160 20"
        style="Label1"
        label="Presaturation"
        justify="Left"
        />
    </group>
    <group loc="15 30" size="375 190"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="wetgroup2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="190 5" size="155 20"
        style="Label1"
        label="after Evolution/Mixing"
        vq="wet"
        vc="$wet1='n' substr(wet,1,1):$wet1 wet=$wet1+'y'"
        vc2="$wet1='n' substr(wet,1,1):$wet1 wet=$wet1+'n'"
        set="$wet3='' substr(wet,2,1):$wet3 if ($wet3='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="60 5" size="135 20"
        style="Label1"
        label="before Preparation"
        vq="wet"
        vc="$wet3='n' substr(wet,2,1):$wet3 wet='y'+$wet3"
        vc2="$wet3='n' substr(wet,2,1):$wet3 wet='n'+$wet3"
        set="$wet1='' substr(wet,1,1):$wet1 if ($wet1='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="15 5" size="45 20"
        style="Label1"
        label="WET:"
        justify="Left"
        />
      <group loc="6 24" size="365 163"
        vq="wet"
        show="if (wet='y') or (wet='yn') or (wet='ny') or (wet='yy') then $SHOW=1 else $SHOW=0 endif"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="129 1" size="210 20"
          style="PlainText"
          vq="wetshape "
          vc="wetshape='$VALUE'"
          set="$VALUE=wetshape"
          disable="Grayed out"
          />
        <label loc="24 1" size="100 20"
          style="Label1"
          label="wet Pulse shape:"
          justify="Left"
          />
        <textmessage loc="104 101" size="170 20"
          style="PlainText"
          label="C13  Decoupling during wet?"
          vq="dn"
          set="$VALUE=dn+'  Decoupling during wet?'"
          />
        <group loc="14 121" size="330 40"
          vq="c13wet"
          show="if (c13wet='y') then $SHOW=1 else $SHOW=0 endif"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <label loc="300 0" size="20 20"
            style="Heading1"
            label="us"
            justify="Left"
            />
          <entry loc="240 20" size="60 20"
            style="PlainText"
            vq="decwavewet "
            vc="decwavewet='$VALUE'"
            set="$VALUE=decwavewet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="240 0" size="60 20"
            style="PlainText"
            vq="dmfwet "
            vc="dmfwet=$VALUE/1e6"
            set="$VALUE=1e6/dmfwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="145 20" size="90 20"
            style="Label1"
            label="modulation"
            justify="Left"
            />
          <label loc="145 0" size="90 20"
            style="Label1"
            label="pi/2 Pulse width"
            justify="Left"
            />
          <entry loc="80 20" size="60 20"
            style="PlainText"
            vq="dpwrwet "
            vc="dpwrwet=$VALUE"
            set="$VALUE=dpwrwet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="80 0" size="60 20"
            style="PlainText"
            vq="dofwet "
            vc="dofwet=$VALUE"
            set="$VALUE=dofwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="5 20" size="75 20"
            style="Label1"
            label="power"
            justify="Left"
            />
          <label loc="5 0" size="75 20"
            style="Label1"
            label="Offset"
            justify="Left"
            />
        </group>
        <check loc="79 101" size="25 20"
          style="Label1"
          vq="c13wet"
          vc="c13wet='y'"
          vc2="c13wet='n'"
          set="if (c13wet='y') then $VALUE=1 else $VALUE=0 endif"
          />
        <group loc="5 80" size="355 20"
          vq="wet"
          show="$wet3='' substr(wet,2,1):$wet3 if ($wet3='y') then $SHOW=1 else $SHOW=0 endif"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <entry loc="230 0" size="120 20"
            style="PlainText"
            vq="compshape composit"
            vc="compshape='$VALUE'"
            set="$VALUE=compshape"
            show="if (composit='y') then $ENABLE=1 else $ENABLE=0 endif"
            disable="Grayed out"
            />
          <label loc="180 0" size="50 20"
            style="Label1"
            label=" Shape:"
            justify="Left"
            />
          <check loc="0 0" size="175 20"
            style="Label1"
            label="Composite observe Pulse?"
            vq="composit"
            vc="composit='y'"
            vc2="composit='n'"
            set="if (composit='y') then $VALUE=1 else $VALUE=0 endif"
            />
        </group>
        <entry loc="275 60" size="50 20"
          style="PlainText"
          vq="gtw wet"
          vc="gtw=$VALUE/1000"
          set="$VALUE=gtw*1000"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="275 40" size="50 20"
          style="PlainText"
          vq="gzlvlw"
          vc="gzlvlw=$VALUE"
          set="$VALUE=gzlvlw"
          digits="0"
          disable="Grayed out"
          />
        <label loc="335 60" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="215 60" size="60 20"
          style="Label1"
          label=" time"
          justify="Left"
          />
        <label loc="215 40" size="60 20"
          style="Label1"
          label="strength"
          justify="Left"
          />
        <label loc="205 20" size="70 20"
          style="Label1"
          label="Gradient -"
          justify="Left"
          />
        <label loc="170 60" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="105 60" size="60 20"
          style="PlainText"
          vq="dz "
          vc="dz=$VALUE/1e6"
          set="$VALUE=dz*1e6"
          disable="Grayed out"
          />
        <label loc="5 60" size="95 20"
          style="Label1"
          label="delay after wet"
          justify="Left"
          />
        <label loc="5 40" size="75 20"
          style="Label1"
          label="Pulse power"
          justify="Left"
          />
        <entry loc="105 40" size="60 20"
          style="PlainText"
          vq="wetpwr "
          vc="wetpwr=$VALUE"
          set="$VALUE=wetpwr"
          digits="1"
          disable="Grayed out"
          />
        <label loc="170 20" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="105 20" size="60 20"
          style="PlainText"
          vq="pwwet "
          vc="pwwet=$VALUE"
          set="$VALUE=pwwet"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 20" size="75 20"
          style="Label1"
          label="Pulse width"
          justify="Left"
          />
      </group>
    </group>
    <button loc="440 0" size="90 20"
      style="Heading2"
      label="Arrays"
      vc="vnmrjcmd('array','popup')"
      decor1=""
      />
    <group loc="10 0" size="390 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 20"
        style="PlainText"
        label="NOESY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
      <button loc="250 0" size="130 20"
        style="Heading2"
        label="Display Sequence"
        vc="dps"
        decor1=""
        />
    </group>
  </group>
</template>
