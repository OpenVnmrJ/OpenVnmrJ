<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="635 255"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="400 25" size="200 135"
      style="Heading1"
      label="One-Bond suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="onebondsuppress"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 20" size="190 20"
        style="Label1"
        label="One-Bond suppression"
        vq="jfilter"
        vc="jfilter='y'"
        vc2="jfilter='n'"
        set="$VALUE=(jfilter='y')"
        />
      <group loc="5 40" size="195 85"
        vq="jfilter"
        show="$SHOW=(jfilter='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="170 0" size="25 20"
          style="Heading1"
          label="Hz"
          justify="Left"
          />
        <label loc="170 20" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <label loc="170 65" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <entry loc="115 65" size="50 20"
          style="PlainText"
          vc="gt0=$VALUE/1000"
          set="$VALUE=gt0*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="115 45" size="50 20"
          style="PlainText"
          vq="gzlvl0 jfilter"
          vc="gzlvl0=$VALUE"
          set="$VALUE=gzlvl0"
          digits="0"
          disable="Grayed out"
          />
        <label loc="5 65" size="60 20"
          style="Label1"
          label="G- time"
          justify="Left"
          />
        <label loc="5 45" size="80 20"
          style="Label1"
          label="G- strength"
          justify="Left"
          />
        <entry loc="115 20" size="50 20"
          style="PlainText"
          vq="j1min jfilter"
          vc="j1min=$VALUE"
          set="$VALUE=j1min"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="115 0" size="50 20"
          style="PlainText"
          vq="j1max jfilter"
          vc="j1max=$VALUE"
          set="$VALUE=j1max"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 20" size="110 20"
          style="Label1"
          label="Coupling 2 (low)"
          justify="Left"
          />
        <label loc="5 0" size="110 20"
          style="Label1"
          label="Coupling 1 (high)"
          justify="Left"
          />
      </group>
    </group>
    <group loc="400 160" size="200 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="175 65" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="120 45" size="50 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="120 65" size="50 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        disable="Grayed out"
        />
      <label loc="10 65" size="80 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 45" size="80 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 25" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
    </group>
    <button loc="5 170" size="80 20"
      style="Heading2"
      label="gHMBC-ct"
      vq="jscaleU jscaleD accord"
      vc="accord='n' jscaleU=0 jscaleD=0"
      bg=""
      decor1=""
      />
    <button loc="5 200" size="80 20"
      style="Heading2"
      label="CIGAR"
      vq="jscaleU jscaleD accord"
      vc="accord='y' jscaleU=0 jscaleD=0"
      bg=""
      decor1=""
      />
    <button loc="5 230" size="80 20"
      style="Heading2"
      label="gHMBC"
      vq="jscaleU jscaleD accord"
      vc="accord='n' jscaleU=1 jscaleD=0"
      bg=""
      decor1=""
      />
    <button loc="95 230" size="90 20"
      style="Heading2"
      label="AccordMBC"
      vq="jscaleU jscaleD accord"
      vc="accord='y' jscaleU=1 jscaleD=0"
      bg=""
      decor1=""
      />
    <button loc="95 200" size="90 20"
      style="Heading2"
      label="CIGAR2j3j"
      vq="jscaleU jscaleD accord"
      vc="accord='y' jscaleU=0 jscaleD=10"
      bg=""
      decor1=""
      />
    <button loc="95 170" size="90 20"
      style="Heading2"
      label="gHMBC-jscale"
      vq="accord jscaleU jscaleD"
      vc="accord='n' jscaleU=25 jscaleD=0"
      bg=""
      decor1=""
      />
    <group loc="5 25" size="180 135"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="EDggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="155 105" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="155 65" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="100 45" size="50 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="100 65" size="50 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="100 85" size="50 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="100 105" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 85" size="80 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <label loc="25 65" size="65 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="10 105" size="90 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="25 45" size="65 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <textmessage loc="10 25" size="170 20"
        style="PlainText"
        label="Encode Grad (10.1 G/cm)"
        vq="gzlvlE invgcal probe"
        set="$gcm='' fixgrdR(gzlvlE):$ret,$gcm if ($gcm&lt;&gt;'') then $gcm='('+$gcm+' G/cm)' endif $VALUE='Encode Grad '+$gcm"
        />
    </group>
    <group loc="190 25" size="205 135"
      style="Heading1"
      label="Multiple-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="cigar"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="180 25" size="25 20"
        style="Heading1"
        label="Hz"
        justify="Left"
        />
      <label loc="180 45" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <check loc="10 110" size="60 20"
        style="Label1"
        label="accord"
        vq="accord"
        vc="accord='y'"
        vc2="accord='n'"
        set="$VALUE=(accord='y')"
        />
      <label loc="10 90" size="110 20"
        style="Label1"
        label="Uniform Scaling"
        justify="Left"
        />
      <entry loc="125 90" size="50 20"
        style="PlainText"
        vq="jscaleU"
        vc="jscaleU=$VALUE"
        set="$VALUE=jscaleU"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="125 70" size="50 20"
        style="PlainText"
        vq="jscaleD"
        vc="jscaleD=$VALUE"
        set="$VALUE=jscaleD"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 70" size="115 20"
        style="Label1"
        label="Differential Scaling"
        justify="Left"
        />
      <label loc="10 45" size="110 20"
        style="Label1"
        label="Coupling (min)"
        justify="Left"
        />
      <entry loc="125 45" size="50 20"
        style="PlainText"
        vq="jnmin"
        vc="jnmin=$VALUE"
        set="$VALUE=jnmin"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="125 25" size="50 20"
        style="PlainText"
        vq="jnmax"
        vc="jnmax=$VALUE"
        set="$VALUE=jnmax"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Coupling (max)"
        justify="Left"
        />
    </group>
    <group loc="190 160" size="205 90"
      style="Heading1"
      label="S6 refocusing Pulse"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="180 45" size="25 20"
        style="Heading1"
        label="dB"
        justify="Left"
        />
      <label loc="180 25" size="25 20"
        style="Label1"
        label="µs"
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Power level"
        justify="Left"
        />
      <entry loc="125 45" size="50 20"
        style="PlainText"
        vq="pwxlvlS6"
        vc="pwxlvlS6=$VALUE"
        set="$VALUE=pwxlvlS6"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="125 25" size="50 20"
        style="PlainText"
        vq="pwxS6"
        vc="pwxS6=$VALUE"
        set="$VALUE=pwxS6"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 25" size="105 20"
        style="Label1"
        label="90 deg. pulse"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="CIGAR"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
