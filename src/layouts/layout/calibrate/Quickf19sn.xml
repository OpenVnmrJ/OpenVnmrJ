<template name="Quickf19sn" element="pages" type="acquisition" >
  <group size="670 260"
    style="Heading1"
    label="F19SN"
    vq="pslabel tn"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quickf19sn"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="10 20" size="280 25"
        style="Heading1"
        label="Use the F19 S/N sample"
        />

<!-- F19 pw90 group -->
      <check loc="10 45" size="115 20"
        style="Label1"
        label="F19 Observe"
        vq="caliblist"
        vc="xmhaha_calib('add','Fobs')"
        vc2="xmhaha_calib('delete','Fobs')"
        set="xmhaha_calib('','Fobs'):$VALUE"
        />
      <label loc="120 45" size="120 20"
        style="Label1"
        label="[pw90 Target (µs)]"
        />
      <entry loc="243 45" size="50 20"
        style="PlainText"
        vq="F19pw90 caliblist"
        vc="F19pw90=$VALUE"
        set="$VALUE=F19pw90"
        show="xmhaha_calib('','Fobs'):$ok $ENABLE=$ok*2-1"
        disable="Grayed out"
        />
      <group loc="300 45" size="90 20"
        vq="probeSpec_F19"
        show="probeid_target('check','probeSpec_F19'):$SHOW"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <textmessage loc="0 0" size="80 20"
          style="PlainText"
          vq="probeSpec_F19"
          set="probeid_target('get',probeSpec_F19):$VALUE"
          />
      </group>

