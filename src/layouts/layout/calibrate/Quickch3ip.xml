<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quickch3iP" element="pages" type="acquisition" >
  <group size="670 260"
    style="PlainText"
    label="TMP"
    vq="pslabel tn"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quickch3ip"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 220"
      style="Heading2"
      label="Acquisition Options"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <label loc="10 20" size="280 25"
        style="Heading1"
        label="Use the ID#1 (CH3I+TMP) sample"
        />

<!-- P31 pwx group -->
      <check loc="10 45" size="115 20"
        style="PlainText"
        label="P31 Decouple"
        vq="caliblist"
        vc="xmhaha_calib('add','Pdec')"
        vc2="xmhaha_calib('delete','Pdec')"
        set="xmhaha_calib('','Pdec'):$VALUE"
        />
      <label loc="120 45" size="120 20"
        style="PlainText"
        label="[pwx90 Target (µs)]"
        />
      <entry loc="243 45" size="50 20"
        style="PlainText"
        vq="P31pwx caliblist"
        vc="P31pwx=$VALUE"
        set="$VALUE=P31pwx"
        show="xmhaha_calib('','Pdec'):$ok $ENABLE=$ok*2-1"
        disable="Grayed out"
        />

      <group loc="300 45" size="90 20"
        vq="probeSpec_P31"
        show="probeid_target('check','probeSpec_P31'):$SHOW"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <textmessage loc="0 0" size="80 20"
          style="PlainText"
          vq="probeSpec_P31"
          set="probeid_target('get',probeSpec_P31):$VALUE"
          />
      </group>

<!-- H1P31 Gradient Ratio group -->
      <check loc="10 65" size="300 20"
        style="PlainText"
        label="P/H gradient ratio"
        vq="caliblist"
        vc="xmhaha_calib('add','Pgrad')"
        vc2="xmhaha_calib('delete','Pgrad')"
        set="xmhaha_calib('','Pgrad'):$VALUE"
        />
<!-- P31 pw90 group -->
      <check loc="10 85" size="115 20"
        style="PlainText"
        label="P31 Observe"
        vq="caliblist"
        vc="xmhaha_calib('add','Pobs')"
        vc2="xmhaha_calib('delete','Pobs')"
        set="xmhaha_calib('','Pobs'):$VALUE"
        />
      <label loc="120 85" size="120 20"
        style="PlainText"
        label="[pw90 Target (µs)]"
          />
      <entry loc="243 85" size="50 20"
        style="PlainText"
        vq="P31pw90 caliblist"
        vc="P31pw90=$VALUE"
        set="$VALUE=P31pw90"
        show="xmhaha_calib('','Pobs'):$ok $ENABLE=$ok*2-1"
        disable="Grayed out"
        />
      <group loc="300 85" size="90 20"
        vq="probeSpec_P31"
        show="probeid_target('check','probeSpec_P31'):$SHOW"
        bg="transparent"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <textmessage loc="0 0" size="80 20"
          style="PlainText"
          vq="probeSpec_P31"
          set="probeid_target('get',probeSpec_P31):$VALUE"
          />
      </group>
      <label loc="5 165" size="220 20"
        style="PlainText"
        label="After calibration:"
	fg="darkRed"
        />
      <check loc="10 185" size="380 20"
        style="PlainText"
        label="Record pw(x) nutation and decoupling profiles"
        vq="profileflg"
        vc="profileflg='y'"
        vc2="profileflg='n'"
        set="$VALUE=(profileflg='y')"
        />
    </group>
    <group loc="5 5" size="620 25"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="Heading3"
        label="Experiment: calibrate   Solvent: cdcl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="Label3"
        label="Observe: H1          Decoupler: C13"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="400 35" size="235 220"
      bg="transparent"
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="5 0" size="135 20"
        style="Label1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="35 25" size="150 20"
        style="PlainText"
        label="Print probefile?"
        vq="prtmod"
        vc="prtmod='y'"
        vc2="prtmod='n'"
        set="$VALUE=(prtmod='y')"
        />
      <check loc="35 45" size="150 20"
        style="PlainText"
        label="Plot results?"
        vq="prtmod pltmod pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <group loc="5 90" size="220 130"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 25"
          style="PlainText"
          label="HCN5mm_calib"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 5" size="130 20"
          style="Label3"
          label="Before calibration:"
          vq="pslabel"
          set="$VALUE='Before calibration:'"
          />
        <check loc="140 5" size="75 20"
          style="PlainText"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="Label1"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Heading4"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label3"
          label="start of Q lock/shim?"
          />
      </group>
    </group>
  </group>
</template>
