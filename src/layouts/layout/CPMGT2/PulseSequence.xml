<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="550 215"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 25" size="335 180"
      style="Heading1"
      label="Excitation"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Excitation"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <radio loc="165 105" size="45 20"
        style="Label1"
        label="no"
        vq="wbs"
        vc="wbs=''"
        vc2="strstr(wbs,'testsn'):$ret1 if ($ret1=0) then wbs='testsn' else wbs=wbs endif"
        set="$ret=0 strstr(wbs,'testsn'):$ret $VALUE=($ret=0)"
        />
      <radio loc="120 105" size="45 20"
        style="Label1"
        label="yes"
        vq="wbs"
        vc="strstr(wbs,'testsn'):$ret1 if ($ret1=0) then wbs='testsn' else wbs=wbs endif"
        vc2="wbs=''"
        set="$ret=0 strstr(wbs,'testsn'):$ret $VALUE=($ret&gt;0)"
        />
      <group loc="10 125" size="315 45"
        vq="wbs"
        show="$ret=0 strstr(wbs,'testsn'):$ret $SHOW=($ret&gt;0)"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="testsn"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <entry loc="195 5" size="65 20"
          style="PlainText"
          vq="wbs"
          vc="$v1='' $v2='' tsnspwp('get'):$v1,$v2 tsnspwp('set',$v1,$VALUE)"
          set="$v3='' tsnspwp('get'):$v3,$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="265 5" size="40 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <entry loc="90 5" size="70 20"
          style="PlainText"
          vq="wbs"
          vc="$v1='' $v2='' tsnspwp('get'):$v1,$v2 tsnspwp('set',$VALUE,$v2)"
          set="tsnspwp('get'):$VALUE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="160 5" size="35 20"
          style="Label1"
          label="and"
          justify="Center"
          />
        <entry loc="265 25" size="45 20"
          style="PlainText"
          vq="sn"
          vc="sn=$VALUE"
          set="$VALUE=sn"
          digits="0"
          disable="Grayed out"
          />
        <label loc="160 25" size="105 20"
          style="Label1"
          label="scans &amp; stop at:"
          justify="Center"
          />
        <menu loc="90 25" size="70 20"
          style="PlainText"
          label="menu"
          vq="bs"
          vc="bs=$VALUE"
          set="$VALUE=bs"
          editable="No"
          >
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
        </menu>
        <label loc="30 5" size="60 20"
          style="Label1"
          label="between"
          justify="Left"
          />
        <label loc="30 25" size="60 20"
          style="Label1"
          label="at every"
          justify="Left"
          />
      </group>
      <label loc="10 105" size="75 20"
        style="Label1"
        label="Check S/N?"
        justify="Left"
        />
      <entry loc="120 65" size="60 20"
        style="PlainText"
        vq="tau"
        vc="vnmrunits('set','tau',$VALUE)"
        set="vnmrunits('get','tau'):$VALUE"
        show="$ENABLE=(tau[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <menu loc="180 65" size="60 20"
        style="PlainText"
        vq="tau"
        vc="parunits('set','tau','$VALUE')"
        set="parunits('get','tau'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 45" size="60 20"
        style="PlainText"
        label="menu"
        vq="p1"
        vc="parunits('set','p1','$VALUE')"
        set="parunits('get','p1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <label loc="10 65" size="110 20"
        style="Label1"
        label="Inter-pulse delay"
        justify="Left"
        />
      <label loc="10 85" size="110 20"
        style="Label1"
        label="Observe Pulse"
        justify="Left"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="First pulse"
        justify="Left"
        />
      <menu loc="180 25" size="60 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <menu loc="180 85" size="60 20"
        style="PlainText"
        label="menu"
        vq="pw"
        vc="parunits('set','pw','$VALUE')"
        set="parunits('get','pw'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="µs"
          chval="us"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
      </menu>
      <entry loc="120 85" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="$ENABLE=(pw[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 45" size="60 20"
        style="PlainText"
        vq="p1"
        vc="vnmrunits('set','p1',$VALUE)"
        set="vnmrunits('get','p1'):$VALUE"
        show="$ENABLE=(p1[1]&gt;0)"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="120 25" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="$ENABLE=(d1[1]&gt;0)"
        digits="3"
        disable="Grayed out"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
    </group>
    <textmessage loc="10 0" size="170 25"
      style="PlainText"
      label="CPMGT2"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
