<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="550 213"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="215 25" size="170 130"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="205 130"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="EDggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="180 60" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="180 100" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="10 80" size="80 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <textmessage loc="10 20" size="180 20"
        style="PlainText"
        label="Encode Grad (8.0 G/cm)"
        vq="gzlvlE invgcal probe"
        set="$gcm='' fixgrdR(gzlvlE):$ret,$gcm if ($gcm&lt;&gt;'') then $gcm='('+$gcm+' G/cm)' endif $VALUE='Encode Grad '+$gcm"
        />
      <entry loc="115 60" size="60 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="115 100" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="25 60" size="70 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="25 40" size="70 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <entry loc="115 40" size="60 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="115 80" size="60 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="4"
        disable="Grayed out"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="gDQCOSY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
