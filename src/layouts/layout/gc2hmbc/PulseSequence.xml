        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <label loc="25 65" size="65 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="25 45" size="65 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <entry loc="105 105" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
