      style="Heading1"
      label="Multiple-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="160 45" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="160 25" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <textmessage loc="105 45" size="50 20"
        style="PlainText"
        vq="jnxh"
        set="$VALUE=500/jnxh"
        digits="1"
        />
      <entry loc="105 25" size="50 20"
        style="PlainText"
        vq="jnxh"
        vc="jnxh=$VALUE"
        set="$VALUE=jnxh"
