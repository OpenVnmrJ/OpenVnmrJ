            style="Label1"
            label="Width"
            justify="Left"
            />
          <label loc="0 0" size="45 20"
            style="Label1"
            label="Shape"
            justify="Left"
            />
        </group>
        <check loc="55 20" size="45 20"
          style="Label1"
          label="BIP"
          vq="bipflg"
          vc="$bip2='' substr(bipflg,2,1):$bip2 bipflg='y'+$bip2"
          vc2="$bip2='' substr(bipflg,2,1):$bip2 bipflg='n'+$bip2"
