<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="705 270"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="455 25" size="195 90"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <label loc="165 60" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="G- time"
        justify="Left"
        />
      <label loc="10 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
    </group>
    <group loc="5 115" size="190 125"
      style="Heading1"
      label="ZQ Filter"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ZQfilterN"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="160 80" size="30 20"
        style="Heading1"
        label="dB"
        />
      <entry loc="95 100" size="60 20"
        style="PlainText"
        vq="gzlvlzq1 Gzqfilt"
        vc="gzlvlzq1=$VALUE"
        set="$VALUE=gzlvlzq1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="95 80" size="60 20"
        style="PlainText"
        vq="zqfpwr1 Gzqfilt"
        vc="zqfpwr1=$VALUE"
        set="$VALUE=zqfpwr1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="zqfpw1 Gzqfilt"
        vc="zqfpw1=$VALUE*1000"
        set="$VALUE=zqfpw1/1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="1"
        />
      <label loc="160 60" size="25 20"
        style="Label1"
        label="ms"
        />
      <check loc="10 20" size="115 20"
        style="Label1"
        label="ZQ Filter"
        vq="Gzqfilt"
        vc="Gzqfilt='y'"
        vc2="Gzqfilt='n'"
        set="$VALUE=(Gzqfilt='y')"
        />
      <entry loc="65 40" size="115 20"
        style="PlainText"
        vq="zqfpat1 Gzqfilt"
        vc="zqfpat1='$VALUE'"
        set="$VALUE=zqfpat1"
        show="$ENABLE=(Gzqfilt='y')"
        />
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="10 60" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <label loc="10 80" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 100" size="55 20"
        style="Label1"
        label="G- level"
        justify="Left"
        />
    </group>
    <group loc="200 25" size="250 215"
      style="Heading1"
      label="Double PFG spinecho"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="dpfgsegroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 185" size="195 20"
        style="Label1"
        label="Homonuclear Doupling (F1)"
        vq="homodec"
        vc="homodec='y'"
        vc2="homodec='n'"
        set="$VALUE=(homodec='y')"
        />
      <button loc="85 160" size="135 20"
        style="Heading2"
        label="make shapes"
        vq="selfrqc selfrqppm"
        vc="suselfrq"
        show="on('selfrqc'):$s1 on('selfrqppm'):$s2 $ENABLE=(($s1&gt;0) or ($s2&gt;0))"
        bg=""
        decor1=""
        />
      <label loc="10 140" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 120" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <entry loc="85 140" size="60 20"
        style="PlainText"
        vq="gtA"
        vc="gtA=$VALUE/1000"
        set="$VALUE=gtA*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 140" size="60 20"
        style="PlainText"
        vq="gtB"
        vc="gtB=$VALUE/1000"
        set="$VALUE=gtB*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="160 120" size="60 20"
        style="PlainText"
        vq="gzlvlB"
        vc="gzlvlB=$VALUE"
        set="$VALUE=gzlvlB"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="85 120" size="60 20"
        style="PlainText"
        vq="gzlvlA"
        vc="gzlvlA=$VALUE"
        set="$VALUE=gzlvlA"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="105 100" size="115 20"
        style="PlainText"
        vq="selshapeB"
        vc="selshapeB='$VALUE'"
        set="$VALUE=selshapeB"
        disable="Grayed out"
        />
      <label loc="20 80" size="55 20"
        style="Label1"
        label="shape"
        justify="Left"
        />
      <entry loc="85 80" size="115 20"
        style="PlainText"
        vq="selshapeA"
        vc="selshapeA='$VALUE'"
        set="$VALUE=selshapeA"
        disable="Grayed out"
        />
      <label loc="20 60" size="55 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <label loc="20 40" size="55 20"
        style="Label1"
        label="width"
        justify="Left"
        />
      <entry loc="85 40" size="60 20"
        style="PlainText"
        vq="selpwA"
        vc="selpwA=$VALUE*1000"
        set="$VALUE=selpwA/1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="85 60" size="60 20"
        style="PlainText"
        vq="selpwrA"
        vc="selpwrA=$VALUE"
        set="$VALUE=selpwrA"
        disable="Grayed out"
        />
      <entry loc="160 60" size="60 20"
        style="PlainText"
        vq="selpwrB"
        vc="selpwrB=$VALUE"
        set="$VALUE=selpwrB"
        disable="Grayed out"
        />
      <label loc="225 60" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="225 40" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="160 40" size="60 20"
        style="PlainText"
        vq="selpwB"
        vc="selpwB=$VALUE*1000"
        set="$VALUE=selpwB/1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="160 20" size="60 20"
        style="Label1"
        label="Echo2"
        justify="Center"
        />
      <label loc="85 20" size="60 20"
        style="Label1"
        label="Echo1"
        justify="Center"
        />
      <label loc="10 20" size="55 20"
        style="Label1"
        label="Pulse"
        justify="Left"
        />
      <label loc="225 145" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="190 90"
      style="Heading1"
      label="NOESY"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="NOESY"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="160 60" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        digits="2"
        disable="Grayed out"
        />
      <label loc="10 60" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 40" size="80 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
      <label loc="10 20" size="75 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
      <entry loc="95 20" size="60 20"
        style="PlainText"
        vq="mixN"
        vc="mixN=$VALUE/1000"
        set="$VALUE=mixN*1000"
        disable="Grayed out"
        />
      <label loc="160 20" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="bashdNOESY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
