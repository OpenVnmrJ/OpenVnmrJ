<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="AcquisitionHom2D" element="pages" type="acquisition" >
  <group size="700 240"
    style="Heading1"
    label="Acquisition"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="AcquisitionHom2D"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="315 25" size="335 210"
      style="Heading1"
      label="Acquisition in F1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitioninF1"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <button loc="215 50" size="110 20"
        style="Heading2"
        label="Set LP1+Weight"
        vc="setLP1andWindow"
        bg=""
        decor1=""
        />
      <textmessage loc="150 30" size="185 20"
        style="PlainText"
        label="446.0 Hz (0.89 ppm)"
        vq="selshapeA selfrqc selfrqppm selfrqw sw1"
        set="bashdbw('setsw1'):$sw1 $s='' format($sw1,0,1):$s $s2='' format($sw1/reffrq,0,2):$s2 $VALUE=$s+' Hz ('+$s2+' ppm)'"
        />
      <textmessage loc="150 70" size="185 20"
        style="PlainText"
        label="0.6 ppm; center: 5.39 ppm"
        vq="selshapeA selfrqc selfrqppm selfrqw"
        set="bashdbw('getbw'):$v1,$v2 $VALUE=$v1+' ppm; center: '+$v2+' ppm'"
        />
      <entry loc="150 50" size="65 20"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        />
      <label loc="10 70" size="130 20"
        style="Label1"
        label="Selection band width:"
        justify="Left"
        />
      <group loc="10 90" size="270 50"
        style="Heading1"
        vq="ni"
        show="if ni&gt;1 then $VALUE=1 else $VALUE=0 endif"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <textmessage loc="140 0" size="70 20"
          style="PlainText"
          label="0.143498"
          vq="ni sw1 selfrqc selfrqppm selfrqw selshapeA"
          set="if ni&gt;1 then bashdbw('setsw1'):$sw1 $VALUE=ni/$sw1 else $VALUE=0 endif"
          digits="6"
          />
        <textmessage loc="95 20" size="30 20"
          style="PlainText"
          label="7.0"
          vq="ni sw1"
          set="bashdbw('setsw1'):$sw1 if ni&gt;1 then $VALUE=$sw1/ni else $VALUE=0 endif"
          digits="1"
          />
        <textmessage loc="175 20" size="40 20"
          style="PlainText"
          label="0.014"
          vq="ni sw1 reffrq1"
          set="bashdbw('setsw1'):$sw1 if ni&gt;1 then $VALUE=($sw1/ni )/reffrq1 else $VALUE=0 endif"
          digits="3"
          />
        <label loc="130 20" size="45 20"
          style="Label1"
          label="Hz, or"
          justify="Left"
          />
        <label loc="215 0" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="215 20" size="40 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <label loc="20 20" size="70 20"
          style="Label1"
          label="Resolution:"
          justify="Left"
          />
        <label loc="0 0" size="140 20"
          style="Label1"
          label="Acquisition time (max.)"
          justify="Left"
          />
      </group>
      <label loc="10 50" size="110 20"
        style="Label1"
        label="Increments in t1"
        justify="Left"
        />
      <group loc="10 170" size="270 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <radio loc="180 0" size="75 20"
          style="Label1"
          label="Full 2D"
          vq="phase"
          vc="phase=1,2 if ni&lt;1.5 then ni=128 endif"
          set="$VALUE=0 $phase=size('phase') if $phase&gt;1.5 then $VALUE=((phase[1]=1) and (phase[2]=2)) endif"
          />
        <radio loc="30 0" size="130 20"
          style="Label1"
          label="First Increment"
          vq="phase"
          vc="phase=1 ni=1"
          set="if phase=1 then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <label loc="130 150" size="120 20"
        style="Label1"
        label="Hypercomplex 2D"
        />
      <label loc="10 150" size="120 20"
        style="Label1"
        label="Acquisition Mode:"
        justify="Left"
        />
      <label loc="10 30" size="100 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="305 210"
      style="Heading1"
      label="Acquisition in F2"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitioninF2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <menu loc="230 20" size="65 20"
        style="PlainText"
        vq="sw sw1"
        vc="parunits('set','sw','$VALUE') $v='$VALUE' if ($v='ppm1') then parunits('set','sw1','ppm2') else parunits('set','sw1','Hz') endif"
        set="parunits('get','sw'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <menu loc="230 160" size="65 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="µs"
          chval="us"
          />
      </menu>
      <label loc="10 180" size="155 20"
        style="Label1"
        label="Observe Pulse / at power"
        justify="Left"
        />
      <label loc="20 60" size="95 20"
        style="Label1"
        label="complex points"
        justify="Left"
        />
      <label loc="10 40" size="100 20"
        style="Label1"
        label="Acquisition time"
        justify="Left"
        />
      <entry loc="170 180" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="if (pw=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="230 180" size="65 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        disable="Grayed out"
        />
      <menu loc="230 40" size="65 20"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="µs"
          chval="us"
          />
      </menu>
      <entry loc="255 110" size="40 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="if ss=0 then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        />
      <button loc="230 140" size="65 20"
        style="Heading2"
        label="Find gain"
        vc="gain2Dproc"
        bg=""
        decor1=""
        />
      <label loc="60 110" size="70 20"
        style="Label1"
        label="Completed"
        justify="Left"
        />
      <textmessage loc="130 110" size="40 20"
        style="PlainText"
        label="2"
        vq="ct"
        set="$VALUE=ct"
        />
      <label loc="60 90" size="70 20"
        style="Label1"
        label="Requested"
        justify="Left"
        />
      <label loc="10 90" size="50 20"
        style="Label1"
        label="Scans:"
        justify="Left"
        />
      <entry loc="170 160" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="if (d1=0) then $VALUE=0 else $VALUE=1 endif"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="170 140" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        disable="Grayed out"
        />
      <label loc="10 160" size="110 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
      <label loc="10 140" size="90 20"
        style="Label1"
        label="Receiver Gain"
        justify="Left"
        />
      <label loc="170 110" size="80 20"
        style="Label1"
        label="Steady-State"
        justify="Left"
        />
      <entry loc="170 90" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        disable="Grayed out"
        />
      <entry loc="170 60" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        disable="Grayed out"
        />
      <entry loc="170 40" size="60 20"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="170 20" size="60 20"
        style="PlainText"
        vq="sw sw1"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="bashdNOESY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
