<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="610 255"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="205 155" size="185 70"
      style="Heading1"
      label="C12-H1 suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tango"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 25" size="140 20"
        style="Label1"
        label="TANGO-Gradient "
        vq="nullflg"
        vc="nullflg='y'"
        vc2="nullflg='n'"
        set="$VALUE=(nullflg='y')"
        />
    </group>
    <group loc="205 25" size="185 130"
      style="Heading1"
      label="Constant Time (BigT)"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="bigT"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="155 65" size="25 20"
        style="Label1"
        label=" dB"
        justify="Left"
        />
      <check loc="10 45" size="155 20"
        style="Label1"
        label="X-decoupling (garp1)"
        vq="dmct"
        vc="dmct='y'"
        vc2="dmct='n'"
        set="$VALUE=(dmct='y')"
        />
      <entry loc="105 85" size="50 20"
        style="PlainText"
        vq="dmfct dmct"
        vc="if (dmct='y') then dmfct=$VALUE else dmfct=dmfct endif"
        set="$VALUE=dmfct"
        show="$ENABLE=(dmct='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="155 25" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <entry loc="105 25" size="50 20"
        style="PlainText"
        vq="BigT"
        vc="BigT=$VALUE/1000"
        set="$VALUE=BigT*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="105 65" size="50 20"
        style="PlainText"
        vq="dpwrct dmct"
        vc="if (dmct='y') then dpwrct=$VALUE else dpwrct=dpwrct endif"
        set="$VALUE=dpwrct"
        show="$ENABLE=(dmct='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="50 85" size="45 20"
        style="Label1"
        label="dmfct"
        />
      <label loc="10 65" size="95 20"
        style="Label1"
        label="Power (dpwrct)"
        />
      <label loc="10 25" size="65 20"
        style="Label1"
        label="Duration"
        justify="Left"
        />
    </group>
    <group loc="395 25" size="170 110"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="195 130"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="EDggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="165 105" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <label loc="165 65" size="30 20"
        style="Label1"
        label=" ms"
        justify="Left"
        />
      <entry loc="115 45" size="50 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="115 65" size="50 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
        disable="Grayed out"
        />
      <entry loc="115 85" size="50 20"
        style="PlainText"
        vq="EDratio"
        vc="EDratio=$VALUE"
        set="$VALUE=EDratio"
        digits="4"
        disable="Grayed out"
        />
      <entry loc="115 105" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 85" size="80 20"
        style="Label1"
        label="E/D ratio"
        justify="Left"
        />
      <label loc="25 65" size="65 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="10 105" size="90 20"
        style="Label1"
        label="Recovery time"
        justify="Left"
        />
      <label loc="25 45" size="65 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <textmessage loc="10 25" size="170 20"
        style="PlainText"
        vq="gzlvlE invgcal probe"
        set="$gcm='' fixgrdR(gzlvlE):$ret,$gcm if ($gcm&lt;&gt;'') then $gcm='('+$gcm+' G/cm)' endif $VALUE='Encode Grad '+$gcm"
        />
    </group>
    <group loc="5 155" size="195 70"
      style="Heading1"
      label="One-Bond Suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="onebondsuppress1"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="165 45" size="30 20"
        style="Label1"
        label=" Hz"
        justify="Left"
        />
      <entry loc="115 45" size="50 20"
        style="PlainText"
        vq="j1min jfilter"
        vc="j1min=$VALUE"
        set="$VALUE=j1min"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 45" size="105 20"
        style="Label1"
        label="Coupling 2 (low)"
        justify="Left"
        />
      <label loc="165 25" size="30 20"
        style="Label1"
        label=" Hz"
        justify="Left"
        />
      <entry loc="115 25" size="50 20"
        style="PlainText"
        vq="j1max jfilter"
        vc="j1max=$VALUE"
        set="$VALUE=j1max"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 25" size="105 20"
        style="Label1"
        label="Coupling 1 (high)"
        justify="Left"
        />
    </group>
  </group>
</template>
