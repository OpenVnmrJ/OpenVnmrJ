<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="tune" element="pages" type="sample" >
  <group size="690 205"
    style="Heading1"
    label="Probe tune"
    digital="2"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    count="1"
    enable="no"
    reference="tune"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="0 0" size="680 195"
      show="$SHOW=(Console='mercury')"
      bg=""
      border="None"
      tab="no"
      count="1"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <group loc="5 0" size="670 190"
        style="Heading1"
        label="Tune Parameters"
        bg=""
        border="Etched"
        tab="no"
        enable="no"
        reference="TuneParameters"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="10 130" size="100 20"
          style="Heading1"
          label="Vertical scale"
          justify="Left"
          />
        <label loc="230 120" size="50 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="230 140" size="50 20"
          style="Label1"
          label="Gain"
          justify="Left"
          />
        <label loc="360 120" size="30 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <entry loc="280 120" size="80 20"
          style="PlainText"
          vq="tupwr"
          vc="tupwr=$VALUE if (acqmode='tune') then setvalue('tupwr',tupwr,'processed'):$e halt('  ') endif"
          set="$VALUE=tupwr"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="280 140" size="80 20"
          style="PlainText"
          vq="tugain"
          vc="tugain=$VALUE gain=tugain if (acqmode='tune') then setvalue('gain',gain,'processed') halt('  ') endif"
          set="$VALUE=tugain"
          digits="0"
          disable="Grayed out"
          />
        <label loc="230 160" size="50 20"
          style="Label1"
          label="# Points"
          justify="Left"
          />
        <menu loc="280 160" size="80 20"
          style="PlainText"
          label="# Points"
          vc="np=$VALUE if (acqmode='tune') then setvalue('np',np,'processed'):$e halt('  ') endif"
          set="$VALUE=np"
          editable="No"
          >
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
          <mlabel 
            label="2048"
            chval="2048"
            />
        </menu>
        <label loc="10 160" size="100 20"
          style="Label1"
          label="Vertical position"
          justify="Left"
          />
        <label loc="10 50" size="60 20"
          style="Label1"
          label="Marker 1"
          />
        <label loc="10 70" size="60 20"
          style="Label1"
          label="Marker 2"
          />
        <filemenu loc="110 50" size="100 20"
          style="PlainText"
          vq="marker1"
          vc="marker1='$VALUE' mtune('changemarker1')"
          set="$VALUE=marker1"
          file="$VALUE=userdir+'/persistence/mtune_nuc'"
          type="file"
          bg=""
          editable="No"
          statshow="yes"
          />
        <filemenu loc="110 70" size="100 20"
          style="PlainText"
          vq="marker2"
          vc="marker2='$VALUE' mtune('changemarker2')"
          set="$VALUE=marker2"
          file="$VALUE=userdir+'/persistence/mtune_nuc'"
          type="file"
          bg=""
          editable="No"
          statshow="yes"
          />
        <button loc="10 90" size="200 20"
          style="Heading2"
          label="Set frequency between markers"
          vc="mtune('setbetweenmarkers')"
          decor1=""
          />
        <button loc="110 120" size="100 20"
          style="Heading2"
          label="Auto Scale"
          vc="vsadj"
          decor1=""
          />
        <button loc="110 140" size="50 20"
          style="Heading2"
          label="* 2"
          vc="vs=vs*2"
          decor1=""
          />
        <button loc="160 140" size="50 20"
          style="Heading2"
          label="/ 2"
          vc="vs=vs/2"
          decor1=""
          />
        <button loc="160 160" size="50 20"
          style="Heading2"
          label="- 20"
          vc="vp=vp-20"
          decor1=""
          />
        <button loc="110 160" size="50 20"
          style="Heading2"
          label="+ 20"
          vc="vp=vp+20"
          decor1=""
          />
        <entry loc="280 20" size="80 20"
          style="PlainText"
          vq="sfrq"
          vc="sfrq=$VALUE if (acqmode='tune') then setvalue('sfrq',sfrq,'processed') halt('  ') endif"
          set="$VALUE=sfrq"
          digits="3"
          disable="Grayed out"
          />
        <label loc="10 20" size="100 20"
          style="Label1"
          label="Tune frequency"
          />
        <filemenu loc="110 20" size="100 20"
          style="PlainText"
          vq="tn"
          vc="tn='$VALUE' if (acqmode='tune') then setvalue('tn',tn,'processed') halt('  ') endif"
          set="$VALUE=tn"
          file="$VALUE=userdir+'/persistence/mtune_nuc'"
          type="file"
          bg=""
          editable="No"
          statshow="yes"
          />
        <button loc="400 100" size="130 40"
          style="Heading2"
          label="Quit"
          vc="mtune('quit')"
          decor1=""
          />
        <button loc="400 20" size="130 40"
          style="Heading2"
          label="Start QTune"
          vc="shell('sleep 1') mtune('start')"
          decor1=""
          />
        <button loc="400 60" size="130 40"
          style="Heading2"
          label="Stop QTune"
          vc="aa('Tuning Complete') acqmode=''"
          decor1=""
          />
        <entry loc="280 40" size="80 20"
          style="PlainText"
          vq="tunesw"
          vc="tunesw=$VALUE*1e6 if (acqmode='tune') then setvalue('tunesw',tunesw,'processed'):$e halt('  ') endif"
          set="$VALUE=tunesw*1e-6"
          digits="1"
          disable="Grayed out"
          />
        <label loc="230 40" size="40 20"
          style="Label1"
          label="Span"
          justify="Left"
          />
        <textmessage loc="230 20" size="40 20"
          style="PlainText"
          label="H1"
          vq="tn"
          set="$VALUE=tn"
          />
        <label loc="360 20" size="30 20"
          style="Label1"
          label="MHz"
          justify="Left"
          />
        <label loc="360 40" size="30 20"
          style="Label1"
          label="MHz"
          justify="Left"
          />
      </group>
