<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="620 195"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="395 25" size="170 150"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="200 25" size="190 150"
      style="Heading1"
      label="ZQ Filter"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ZQfilter"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="130 80" size="30 20"
        style="Label1"
        label="dB"
        />
      <label loc="130 60" size="30 20"
        style="Label1"
        label="ms"
        />
      <entry loc="65 40" size="115 20"
        style="PlainText"
        vq="zqfpat1 Gzqfilt"
        vc="zqfpat1='$VALUE'"
        set="$VALUE=zqfpat1"
        show="$ENABLE=(Gzqfilt='y')"
        />
      <entry loc="65 60" size="60 20"
        style="PlainText"
        vq="zqfpw1 Gzqfilt"
        vc="zqfpw1=$VALUE*1000"
        set="$VALUE=zqfpw1/1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="1"
        />
      <entry loc="65 80" size="60 20"
        style="PlainText"
        vq="zqfpwr1 Gzqfilt"
        vc="zqfpwr1=$VALUE"
        set="$VALUE=zqfpwr1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <label loc="10 40" size="45 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="10 60" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <label loc="10 80" size="50 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 100" size="55 20"
        style="Label1"
        label="G- level"
        justify="Left"
        />
      <entry loc="65 100" size="60 20"
        style="PlainText"
        vq="gzlvlzq1 Gzqfilt"
        vc="gzlvlzq1=$VALUE"
        set="$VALUE=gzlvlzq1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <check loc="10 20" size="115 20"
        style="Label1"
        label="ZQ Filter"
        vq="Gzqfilt"
        vc="Gzqfilt='y'"
        vc2="Gzqfilt='n'"
        set="$VALUE=(Gzqfilt='y')"
        />
    </group>
    <group loc="5 25" size="190 150"
      style="Heading1"
      label="z-COSY"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="z-COSY"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="10 120" size="140 20"
        style="Label1"
        label="Alternating gradients"
        vq="alt_grd"
        vc="alt_grd='y'"
        vc2="alt_grd='n'"
        set="$VALUE=(alt_grd='y')"
        />
      <label loc="160 80" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="160 60" size="30 20"
        style="Label1"
        label="deg."
        justify="Left"
        />
      <label loc="160 40" size="30 20"
        style="Label1"
        label="deg."
        justify="Left"
        />
      <entry loc="95 100" size="60 20"
        style="PlainText"
        vq="gzlvl1"
        vc="gzlvl1=$VALUE"
        set="$VALUE=gzlvl1"
        digits="0"
        />
      <entry loc="95 80" size="60 20"
        style="PlainText"
        vq="gt1"
        vc="gt1=$VALUE/1000"
        set="$VALUE=gt1*1000"
        digits="2"
        disable="Grayed out"
        />
      <entry loc="95 60" size="60 20"
        style="PlainText"
        vq="flip2"
        vc="flip2=$VALUE"
        set="$VALUE=flip2"
        disable="Grayed out"
        />
      <label loc="10 100" size="55 20"
        style="Label1"
        label="G- level"
        justify="Left"
        />
      <label loc="10 80" size="80 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="10 60" size="80 20"
        style="Label1"
        label="Flip angle 2"
        justify="Left"
        />
      <label loc="10 40" size="80 20"
        style="Label1"
        label="Flip angle 1"
        justify="Left"
        />
      <entry loc="95 40" size="60 20"
        style="PlainText"
        vq="flip1"
        vc="flip1=$VALUE"
        set="$VALUE=flip1"
        disable="Grayed out"
        />
      <check loc="10 20" size="115 20"
        style="Label1"
        label="Anti-z COSY"
        vq="antiz_flg"
        vc="antiz_flg='y'"
        vc2="antiz_flg='n'"
        set="$VALUE=(antiz_flg='y')"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="zCOSY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
