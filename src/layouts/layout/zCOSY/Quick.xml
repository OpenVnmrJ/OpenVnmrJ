<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="670 260"
    style="Heading1"
    label="Defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 205"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <group loc="50 30" size="335 130"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="NOESYacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="5 45" size="145 25"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="satdlyd1group"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <group loc="10 0" size="125 20"
            vq="satmode"
            show="$satmode1='' substr(satmode,1,1):$satmode if ($satmode='y') then $SHOW=1 else $SHOW=0 endif"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
            <label loc="0 0" size="115 20"
              style="Label1"
              label="Presaturation time"
              justify="Left"
              />
          </group>
          <group loc="0 0" size="110 20"
            vq="satmode"
            show="$satmode1='' substr(satmode,1,1):$satmode if ($satmode='n') then $SHOW=1 else $SHOW=0 endif"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Untitled"
            >
            <label loc="10 0" size="100 20"
              style="Label1"
              label="Relaxation time"
              justify="Left"
              />
          </group>
        </group>
        <group loc="0 65" size="320 45"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="MixXselect"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <check loc="160 0" size="105 20"
            style="Label1"
            label="Anti-z COSY"
            vq="antiz_flg"
            vc="antiz_flg='y'"
            vc2="antiz_flg='n'"
            set="$VALUE=(antiz_flg='y')"
            />
          <label loc="15 0" size="105 20"
            style="Label1"
            label="Anti-z COSY"
            justify="Left"
            />
        </group>
        <menu loc="160 25" size="90 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          bg=""
          editable="No"
          >
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="400"
            chval="400"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <menu loc="160 45" size="90 20"
          style="PlainText"
          label="menu"
          vq="d1 satmode satdly"
          vc="satdly=$VALUE d1=$VALUE"
          set="$satmode='n' substr(satmode,1,1):$satmode if ($satmode='y') then $VALUE=satdly else $VALUE=d1 endif"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="1.5"
            chval="1.5"
            />
          <mlabel 
            label="3"
            chval="3"
            />
        </menu>
        <menu loc="160 5" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
        </menu>
        <label loc="15 25" size="135 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="15 5" size="145 20"
          style="Label1"
          label="Scans per t1 increment"
          justify="Left"
          />
        <label loc="255 45" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="homcordisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="345 25"
        style="PlainText"
        label="Experiment: zCOSY   Solvent: cdcl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="360 0" size="255 25"
        style="PlainText"
        label="H1-H1 Correlation     [ Decoupler: C13 ]"
        vq="tn dn"
        set="$VALUE=tn+'-'+tn+' Correlation     [ Decoupler: '+dn+' ]'"
        />
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 25" size="215 180"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <check loc="125 25" size="85 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <textmessage loc="10 105" size="190 20"
          style="PlainText"
          label=" "
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 155" size="215 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <check loc="125 5" size="85 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="5 5" size="125 20"
          style="PlainText"
          label="Before zCOSY acq :"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq :'"
          />
        <textmessage loc="5 25" size="115 20"
          style="PlainText"
          label="After zCOSY acq :"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq :'"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label="wet1D PRESAT PROTON"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        label="30"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
  </group>
</template>
