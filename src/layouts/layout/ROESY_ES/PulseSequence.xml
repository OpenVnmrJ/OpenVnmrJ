<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="650 215"
    style="PlainText"
    label="Pulse Sequence"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 25" size="195 130"
      style="Heading2"
      label="SpinLock"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SpinlockR"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="170 80" size="25 20"
        style="PlainText"
        label="dB"
        justify="Left"
        />
      <label loc="170 100" size="25 20"
        style="PlainText"
        label="Âµs"
        justify="Left"
        />
      <entry loc="110 100" size="55 20"
        style="PlainText"
        vq="slpwR"
        vc="slpwR=$VALUE"
        set="$VALUE=slpwR"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="110 80" size="55 20"
        style="PlainText"
        vq="slpwrR"
        vc="getpower($VALUE,tn):spinlockR setpower(spinlockR,tn):slpwrR,slpwR spinlockR=spinlockR slpwR=slpwR"
        set="$VALUE=slpwrR"
        disable="Grayed out"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 40" size="65 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <label loc="170 60" size="25 20"
        style="PlainText"
        label="Hz"
        justify="Left"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="spinlockR"
        vc="setpower($VALUE,tn):slpwrR,slpwR getpower(slpwrR,tn):spinlockR spinlockR=spinlockR slpwR=slpwR"
        set="$VALUE=spinlockR"
        disable="Grayed out"
        />
      <menu loc="75 40" size="90 20"
        style="PlainText"
        label="menu"
        vq="slpatR"
        vc="slpatR='$VALUE'"
        set="$VALUE=slpatR"
        bg="VJBackground"
        editable="No"
        >
        <mlabel 
          label="cw"
          chval="cw"
          />
        <mlabel 
          label="troesy"
          chval="troesy"
          />
      </menu>
      <button loc="75 60" size="35 20"
        style="Label3"
        label="Set"
        vc="setslgh2"
        bg="transparent"
        decor1="yes"
        />
      <label loc="170 20" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <entry loc="110 20" size="55 20"
        style="PlainText"
        vq="mixR"
        vc="mixR=$VALUE/1000"
        set="$VALUE=mixR*1000"
        disable="Grayed out"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
    </group>
    <group loc="405 25" size="170 130"
      style="Heading2"
      label="Steady State"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="145 80" size="25 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <check loc="10 100" size="150 20"
        style="PlainText"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        show="$ENABLE=(PFGflg='n')*2-1"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="PlainText"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="PlainText"
        label="strength"
        justify="Left"
        />
      <check loc="10 20" size="150 20"
        style="PlainText"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="10 40" size="70 20"
        style="PlainText"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg="transparent"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="Heading3"
        label="ROESY_ES"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="205 25" size="195 190"
      style="Heading2"
      label="Z-filter"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="zfilter"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="165 60" size="30 20"
        style="PlainText"
        label="ms"
        justify="Left"
        />
      <check loc="10 20" size="130 20"
        style="Label1"
        label="Z-filter"
        vq="zfilt PFGflg"
        vc="zfilt='y'"
        vc2="zfilt='n'"
        set="$VALUE=(zfilt='y' and PFGflg='y')"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="gtz zfilt PFGflg"
        vc="gtz=$VALUE/1000"
        set="$VALUE=gtz*1000"
        show="$ENABLE=(zfilt='y' and PFGflg='y')"
        disable="Grayed out"
        />
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="gzlvlz zfilt PFGflg"
        vc="gzlvlz=$VALUE"
        set="$VALUE=gzlvlz"
        show="$ENABLE=(zfilt='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 60" size="75 20"
        style="PlainText"
        label="G- duration"
        justify="Left"
        />
      <label loc="20 40" size="75 20"
        style="PlainText"
        label="G- strength"
        justify="Left"
        />
    </group>
  </group>
</template>
