<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="780 230"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="270 30" size="260 70"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <check loc="10 20" size="110 20"
        style="Label1"
        label="Presaturation"
        vq="satmode"
        vc="satmode='y'"
        vc2="satmode='n'"
        set="substr(satmode,1,1):$m $VALUE=($m='y')"
        />
      <parameter loc="10 40" size="240 20"
        style="PlainText"
        label="Spin"
        vq="spin"
        bg="transparent"
        tab="no"
        vq2="spin"
        enabled="vnmrinfo('get','spinExpControl'):$ENABLE"
        checkvalue="$VALUE=0 on('spin'):$e if (spin&gt;0 and $e&gt;0.5) then $VALUE=1 endif"
        checkcmd="on('spin') if spin=0 then spin=20 endif"
        checkcmd2="spin=0"
        entryvalue="$VALUE=spin"
        entrycmd="spin=$VALUE"
        entrysize="90"
        unitsenabled="Label"
        unitssize="20"
        unitslabel="Hz"
        enable="no"
        reference="parameter"
        useref="no"
        disable="Grayed out"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Spin"
          vq="spin spin"
          subtype="parameter"
          />
        <entry loc="130 0" size="90 20"
          style="PlainText"
          vq="spin spin"
          vc="spin=$VALUE"
          set="$VALUE=spin"
          show="vnmrinfo('get','spinExpControl'):$ENABLE"
          disable="Grayed out"
          subtype="parameter"
          />
        <check loc="110 0" size="20 20"
          style="Label1"
          vq="spin spin"
          vc="on('spin') if spin=0 then spin=20 endif"
          vc2="spin=0"
          set="$VALUE=0 on('spin'):$e if (spin&gt;0 and $e&gt;0.5) then $VALUE=1 endif"
          show="vnmrinfo('get','spinExpControl'):$ENABLE"
          subtype="parameter"
          />
        <label loc="220 0" size="20 20"
          style="Label1"
          label="Hz"
          vq="spin spin"
          key="units"
          keyval="Hz"
          subtype="parameter"
          />
      </parameter>
      <label loc="10 0" size="110 20"
        style="Label1"
        label="Decoupling Mode:"
        justify="Left"
        />
      <menu loc="120 0" size="130 20"
        style="PlainText"
        label="menu"
        vq="dm"
        vc="dm='$VALUE'"
        set="$VALUE=dm"
        editable="No"
        >
        <mlabel 
          label="Decoupled+NOE"
          chval="yyy"
          />
        <mlabel 
          label="Decoupled-NOE"
          chval="nny"
          />
        <mlabel 
          label="Coupled+NOE"
          chval="ynn"
          />
        <mlabel 
          label="Coupled-NOE"
          chval="nnn"
          />
      </menu>
    </group>
    <group loc="270 100" size="260 70"
      bg=""
      border="None"
      tab="no"
      enable="no"
      useref="no"
      subtype="Convenience"
      >
      <entry loc="120 50" size="110 20"
        style="PlainText"
        vq="T2exptime"
        vc="T2exptime=$VALUE"
        set="$VALUE=T2exptime"
        />
      <entry loc="120 30" size="110 20"
        style="PlainText"
        vq="T2max"
        vc="T2max=$VALUE"
        set="$VALUE=T2max"
        />
      <entry loc="120 10" size="110 20"
        style="PlainText"
        vq="T2min"
        vc="T2min=$VALUE"
        set="$VALUE=T2min"
        />
      <label loc="230 10" size="20 20"
        style="Label1"
        label=" s"
        />
      <label loc="230 30" size="20 20"
        style="Label1"
        label=" s"
        />
      <label loc="230 50" size="20 20"
        style="Label1"
        label=" h"
        />
      <label loc="10 50" size="100 20"
        style="Label1"
        label="Total Exp Time"
        />
      <label loc="10 30" size="90 20"
        style="Label1"
        label="Max T2"
        />
      <label loc="10 10" size="90 20"
        style="Label1"
        label="Min T2"
        />
    </group>
    <group loc="530 190" size="240 20"
      vq="atune panellevel"
      show="$SHOW=(atune='y' and panellevel&gt;29)"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="10 0" size="90 20"
        style="Heading1"
        label="Tune method   "
        justify="Left"
        />
      <filemenu loc="110 0" size="130 20"
        style="PlainText"
        vq="tunemethod"
        vc="$v='$VALUE' if $v='none' then tunemethod='' else tunemethod=$v endif xmtune('filemenu')"
        set="$n='' $e=0 xmtune('methodname'):$n,$e if $e=1 then $VALUE=$n else $VALUE='none' endif"
        file="$VALUE=userdir+'/persistence/wtune'"
        type="file"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
    </group>
    <group loc="10 160" size="260 60"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="10 20" size="250 40"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="150 20" size="50 20"
          style="PlainText"
          vq="sn wbs"
          vc="sn=$VALUE"
          set="$VALUE=sn"
          show="$ENABLE=(wbs='testsn')*2-1"
          disable="Grayed out"
          />
        <menu loc="150 0" size="50 20"
          style="PlainText"
          label="menu"
          vq="bs wbs"
          vc="bs=$VALUE"
          set="$VALUE=bs"
          editable="No"
          >
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
        </menu>
        <label loc="200 0" size="50 20"
          style="Label1"
          label=" scans"
          justify="Left"
          />
        <label loc="0 20" size="110 20"
          style="Label1"
          label="Stop when S/N is "
          justify="Left"
          />
        <label loc="0 0" size="80 20"
          style="Label1"
          label="Block Size is "
          justify="Left"
          />
      </group>
      <check loc="0 0" size="210 20"
        style="Label1"
        label="Test for S/N at every Block Size"
        vq="wbs"
        vc="wbs='testsn'"
        vc2="wbs=''"
        set="$VALUE=(wbs='testsn')"
        />
    </group>
    <button loc="280 190" size="240 30"
      style="Heading2"
      label="Array Relaxation Delay"
      vc="T2setup(T2min,T2max,T2exptime)"
      decor1=""
      />
    <group loc="530 80" size="243 103"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <textmessage loc="0 0" size="120 20"
        style="PlainText"
        label="Plot when done"
        vq="pltmod"
        set="if pltmod='off' then $VALUE='Plotting turned off' else $VALUE='Plot when done' endif"
        />
      <group loc="10 20" size="230 80"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <menu loc="110 60" size="120 20"
          style="PlainText"
          label="Integral"
          vq="intmod pltmod"
          vc="intmod='$VALUE'"
          set="$VALUE=intmod"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="Partial"
            chval="partial"
            />
          <mlabel 
            label="Full"
            chval="full"
            />
          <mlabel 
            label="Off"
            chval="off"
            />
        </menu>
        <menu loc="110 40" size="120 20"
          style="PlainText"
          label="Peak Pick"
          vq="pkpick pltmod"
          vc="pkpick='$VALUE'"
          set="$VALUE=pkpick"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="On Spectrum"
            chval="ppf"
            />
          <mlabel 
            label="List, at Left"
            chval="pll"
            />
          <mlabel 
            label="List, at Right"
            chval="pll(wc-50)"
            />
          <mlabel 
            label="None"
            />
        </menu>
        <label loc="0 40" size="110 20"
          style="Label1"
          label="Peak Values: "
          justify="Left"
          />
        <label loc="0 60" size="100 20"
          style="Label1"
          label="Integrals: "
          justify="Left"
          />
        <menu loc="110 20" size="120 20"
          style="PlainText"
          label="menu"
          vq="parstyle pltmod"
          vc="parstyle='$VALUE'"
          set="$VALUE=parstyle"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="Full, Top Left"
            chval="pap"
            />
          <mlabel 
            label="Full, Top Right"
            chval="pap(wc-50)"
            />
          <mlabel 
            label="Basic, Top Left"
            chval="ppa"
            />
          <mlabel 
            label="Basic, Top Right"
            chval="ppa(wc-50)"
            />
          <mlabel 
            label="Boxed"
            chval="bpa"
            />
          <mlabel 
            label="Text Only"
            chval="pltext"
            />
          <mlabel 
            label="None"
            />
        </menu>
        <menu loc="110 0" size="120 20"
          style="PlainText"
          label="Spectra Width"
          vq="pltmod"
          vc="pltmod='$VALUE'"
          set="$VALUE=pltmod"
          show="$ENABLE=(pltmod&lt;&gt;'off')"
          editable="No"
          >
          <mlabel 
            label="As Displayed"
            chval="fixed"
            />
          <mlabel 
            label="Full"
            chval="full"
            />
          <mlabel 
            label="From Region"
            chval="variable"
            />
          <mlabel 
            label="Off"
            chval="off"
            />
        </menu>
        <label loc="0 20" size="110 20"
          style="Label1"
          label="Parameters:"
          justify="Left"
          />
        <label loc="0 0" size="110 20"
          style="Label1"
          label="Spectrum:"
          justify="Left"
          />
      </group>
    </group>
    <group loc="530 30" size="240 40"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="120 20" size="120 20"
        style="PlainText"
        label="menu"
        vq="lb"
        vc="lb=$VALUE"
        set="on('lb'):$on if ($on=0) then $VALUE='n' elseif (lb=1/at) then $VALUE='1/at' else $VALUE=lb endif"
        editable="No"
        >
        <mlabel 
          label="None"
          chval="n"
          />
        <mlabel 
          label="1/at"
          chval="1/at"
          />
        <mlabel 
          label="0.1"
          chval="0.1"
          />
        <mlabel 
          label="0.2"
          chval="0.2"
          />
        <mlabel 
          label="0.5"
          chval="0.5"
          />
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="5"
          chval="5"
          />
        <mlabel 
          label="10"
          chval="10"
          />
      </menu>
      <menu loc="120 0" size="120 20"
        style="PlainText"
        label="menu"
        vq="fn"
        vc="format($VALUE,'isreal'):$r if $r then fn=$VALUE*2 else fn=$VALUE endif"
        set="on('fn'):$on if($on&gt;0.5) then $VALUE=fn/2 else $VALUE='n' endif"
        editable="No"
        >
        <mlabel 
          label="Not used"
          chval="n"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
        <mlabel 
          label="32k"
          chval="32768"
          />
        <mlabel 
          label="64k"
          chval="65536"
          />
        <mlabel 
          label="128k"
          chval="131072"
          />
      </menu>
      <label loc="0 20" size="110 20"
        style="Label1"
        label="Line Broadening"
        justify="Left"
        />
      <label loc="0 0" size="100 20"
        style="Label1"
        label="Transform size"
        justify="Left"
        />
    </group>
    <group loc="10 30" size="260 120"
      style="Heading1"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="0 80" size="160 20"
        style="Heading1"
        label="Relaxation Delay [sec]"
        justify="Left"
        />
      <label loc="0 0" size="160 20"
        style="Label1"
        label="Spectral Width [ppm]"
        justify="Left"
        />
      <label loc="0 40" size="150 20"
        style="Label1"
        label="Pulse Width [degrees]"
        justify="Left"
        />
      <menu loc="160 100" size="90 20"
        style="PlainText"
        label="menu"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        editable="No"
        >
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="4"
          chval="4"
          />
        <mlabel 
          label="8"
          chval="8"
          />
        <mlabel 
          label="16"
          chval="16"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1000"
          chval="1000"
          />
        <mlabel 
          label="2000"
          chval="2000"
          />
        <mlabel 
          label="5000"
          chval="5000"
          />
      </menu>
      <menu loc="160 80" size="90 20"
        style="PlainText"
        label="menu"
        vq="d1"
        vc="d1=$VALUE"
        set="$VALUE=d1"
        editable="No"
        >
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="3"
          chval="3"
          />
        <mlabel 
          label="5"
          chval="5"
          />
        <mlabel 
          label="10"
          chval="10"
          />
        <mlabel 
          label="20"
          chval="20"
          />
        <mlabel 
          label="25"
          chval="25"
          />
        <mlabel 
          label="40"
          chval="40"
          />
      </menu>
      <menu loc="160 40" size="90 20"
        style="PlainText"
        label="menu"
        vq="pw pw90"
        vc="pw=($VALUE/90)*pw90"
        set="$VALUE=trunc(90*pw/pw90+0.1)"
        show="$ENABLE=(pw90 &gt; 0)*2-1"
        editable="No"
        >
        <mlabel 
          label="15"
          chval="15"
          />
        <mlabel 
          label="30"
          chval="30"
          />
        <mlabel 
          label="45"
          chval="45"
          />
        <mlabel 
          label="60"
          chval="60"
          />
        <mlabel 
          label="90"
          chval="90"
          />
      </menu>
      <group loc="0 60" size="230 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="10 0" size="120 20"
          style="Heading1"
          label="Enter pulse angle  "
          justify="Right"
          />
        <entry loc="180 0" size="50 20"
          style="PlainText"
          vq="pw pw90"
          vc="pw($VALUE)"
          set="$VALUE=90*pw/pw90"
          show="$ENABLE=(pw90 &gt; 0)*2-1"
          digits="0"
          disable="Grayed out"
          />
      </group>
      <group loc="0 20" size="240 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="swstartend"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="10 0" size="70 20"
          style="Heading1"
          label="Downfield "
          justify="Right"
          />
        <entry loc="80 0" size="50 20"
          style="PlainText"
          vq="sw tof reffrq rfl rfp"
          vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
          set="invsetsw:$f1,$f2 $VALUE=$f1"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="180 0" size="50 20"
          style="PlainText"
          vq="sw tof reffrq rfl rfp"
          vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
          set="invsetsw:$f1,$f2 $VALUE=$f2"
          digits="0"
          disable="Grayed out"
          />
        <label loc="120 0" size="60 20"
          style="Label1"
          label="Upfield "
          justify="Right"
          />
      </group>
      <label loc="0 100" size="120 20"
        style="Label1"
        label="Number of Scans"
        justify="Left"
        />
    </group>
    <textmessage loc="10 0" size="200 20"
      style="PlainText"
      label="T2 measure"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
