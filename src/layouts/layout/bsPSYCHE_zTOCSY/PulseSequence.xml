<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="800 270"
    style="Heading1"
    label="Pulse Sequence"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="5 25" size="270 220"
      style="Heading1"
      label="PSYCHE"
      border="Etched"
      tab="no"
      enable="no"
      reference="PSYCHEgrp"
      useref="no"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      />
    <group loc="515 25" size="270 170"
      style="Heading1"
      label="DPFGSE"
      border="Etched"
      tab="no"
      enable="no"
      reference="DPFGSEgrp"
      useref="no"
      subtype="Titled"
      expanded="no"
      rows="1"
      columns="1"
      />
    <group loc="515 200" size="270 45"
      style="Heading1"
      label="Steady State (G-90-G)"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="5 20" size="20 20"
        style="Label1"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="30 20" size="60 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <entry loc="90 20" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="155 20" size="60 20"
        style="Label1"
        label="time (ms)"
        justify="Left"
        />
      <entry loc="215 20" size="50 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
    </group>

    <group loc="280 25" size="230 125"
      style="Heading1"
      label="SpinLock"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MLEV17c"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <button loc="90 60" size="40 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        halignment="Center"
        valignment="Center"
        />
      <entry loc="130 20" size="50 20"
        style="PlainText"
        vq="mixT"
        vc="mixT=$VALUE/1000"
        set="$VALUE=mixT*1000"
        disable="Grayed out"
        />
      <entry loc="130 60" size="50 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <entry loc="130 80" size="50 20"
        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <entry loc="130 100" size="50 20"
        style="PlainText"
        vq="slpwT"
        vc="slpwT=$VALUE"
        set="$VALUE=slpwT"
	digits="2"
        disable="Grayed out"
        />
      <menu loc="90 40" size="90 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        editable="No"
        >
        <mlabel 
          label="DIPSI2"
          chval="dipsi2"
          />
        <mlabel 
          label="DIPSI3"
          chval="dipsi3"
          />
      </menu>
      <label loc="185 20" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="185 60" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="185 80" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="185 100" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <label loc="10 60" size="55 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 40" size="65 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
    </group>
    <group loc="280 155" size="230 90"
      style="Heading1"
      label="ZQ Filter"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="ZQfilter2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="5 20" size="20 20"
        style="Label1"
        vq="Gzqfilt"
        vc="Gzqfilt='y'"
        vc2="Gzqfilt='n'"
        set="$VALUE=(Gzqfilt='y')"
        />
      <label loc="35 20" size="75 20"
        style="Label1"
        label="Widths (ms)"
        justify="Left"
        />
      <entry loc="110 20" size="50 20"
        style="PlainText"
        vq="zqfpw1 Gzqfilt"
        vc="zqfpw1=$VALUE*1000"
        set="$VALUE=zqfpw1/1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="1"
        />
      <entry loc="165 20" size="50 20"
        style="PlainText"
        vq="zqfpw2 Gzqfilt"
        vc="zqfpw2=$VALUE*1000"
        set="$VALUE=zqfpw2/1000"
        show="$ENABLE=(Gzqfilt='y')"
        digits="1"
        />
      <label loc="35 40" size="75 20"
        style="Label1"
        label="Powers (dB)"
        justify="Left"
        />
      <entry loc="110 40" size="50 20"
        style="PlainText"
        vq="zqfpwr1 Gzqfilt"
        vc="zqfpwr1=$VALUE"
        set="$VALUE=zqfpwr1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="165 40" size="50 20"
        style="PlainText"
        vq="zqfpwr2 Gzqfilt"
        vc="zqfpwr2=$VALUE"
        set="$VALUE=zqfpwr2"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <label loc="35 60" size="75 20"
        style="Label1"
        label="G-levels"
        justify="Left"
        />
      <entry loc="110 60" size="50 20"
        style="PlainText"
        vq="gzlvlzq1 Gzqfilt"
        vc="gzlvlzq1=$VALUE"
        set="$VALUE=gzlvlzq1"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
      <entry loc="165 60" size="50 20"
        style="PlainText"
        vq="gzlvlzq2 Gzqfilt"
        vc="gzlvlzq2=$VALUE"
        set="$VALUE=gzlvlzq2"
        show="$ENABLE=(Gzqfilt='y')"
        digits="0"
        />
    </group>

    <group loc="10 0" size="390 25"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="PSYCHE"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
