<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="700 235"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 25" size="195 135"
      style="Heading1"
      label="General"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="General"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <menu loc="110 20" size="55 20"
        style="PlainText"
        vq="pwx1 pwx2 pwx3"
        vc="$v=$VALUE if $v=1 then pwx1=1 pwx2=0 pwx3=0 elseif $v=2 then pwx1=0 pwx2=1 pwx3=0 else pwx1=0 pwx2=0 pwx3=1 endif"
        set="if pwx2=0 and pwx3=0 then $v=1 elseif pwx1=0 and pwx3=0 then $v=2 else $v=3 endif $VALUE=$v"
        bg=""
        editable="No"
        >
        <mlabel 
          label="1"
          chval="1"
          />
        <mlabel 
          label="2"
          chval="2"
          />
        <mlabel 
          label="3"
          chval="3"
          />
      </menu>
      <menu loc="110 40" size="55 20"
        style="PlainText"
        vq="jname"
        vc="jname='$VALUE'"
        set="$VALUE=jname"
        bg=""
        editable="No"
        >
        <mlabel 
          label="C13"
          chval="C13"
          />
        <mlabel 
          label="P31"
          chval="P31"
          />
        <mlabel 
          label="N15"
          chval="N15"
          />
      </menu>
      <label loc="10 20" size="100 20"
        style="Label1"
        label="Channel"
        justify="Left"
        />
      <label loc="10 40" size="100 20"
        style="Label1"
        label="Nucleus"
        justify="Left"
        />
      <label loc="170 60" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="170 80" size="25 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <label loc="170 100" size="25 20"
        style="Label1"
        label="µs"
        justify="Left"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="jC13 jP31 jN15 jname"
        vc="$j='j'+jname {$j}=$VALUE"
        set="$j='j'+jname $VALUE={$j}"
        disable="Grayed out"
        />
      <label loc="10 60" size="100 20"
        style="Label1"
        label="J1 coupling"
        justify="Left"
        />
      <label loc="10 80" size="100 20"
        style="Label1"
        label="Relaxation Time"
        justify="Left"
        />
      <entry loc="110 80" size="55 20"
        style="PlainText"
        vq="d1"
        vc="d1=$VALUE"
        set="$VALUE=d1"
        disable="Grayed out"
        />
      <entry loc="110 100" size="55 20"
        style="PlainText"
        vq="pw"
        vc="pw=$VALUE"
        set="$VALUE=pw"
        disable="Grayed out"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Xmtr pulse"
        justify="Left"
        />
    </group>
    <group loc="205 25" size="470 135"
      style="Heading1"
      label="Decoupler Pulse Calibration"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Excitation"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <label loc="440 75" size="25 20"
        style="Heading1"
        label="dB"
        justify="Left"
        />
      <label loc="440 50" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="385 50" size="50 20"
        style="PlainText"
        vq="pwxlvl2 pwx2"
        vc="pwxlvl2=$VALUE"
        set="$VALUE=pwxlvl2"
        show="$ENABLE=(pwx2&gt;0)*2-1"
        disable="Grayed out"
        />
      <entry loc="385 75" size="50 20"
        style="PlainText"
        vq="pwxlvl3 pwx3"
        vc="pwxlvl3=$VALUE"
        set="$VALUE=pwxlvl3"
        show="$ENABLE=(pwx3&gt;0)*2-1"
        disable="Grayed out"
        />
      <label loc="285 50" size="100 20"
        style="Label1"
        label="Power (pwxlvl2)"
        justify="Left"
        />
      <label loc="255 50" size="25 20"
        style="Label1"
        label="µs"
        justify="Left"
        />
      <label loc="255 75" size="25 20"
        style="Label1"
        label="µs"
        justify="Left"
        />
      <label loc="285 75" size="100 20"
        style="Label1"
        label="Power (pwxlvl3)"
        justify="Left"
        />
      <label loc="10 50" size="100 20"
        style="Label1"
        label="Decoupler 2:"
        justify="Left"
        />
      <label loc="10 75" size="100 20"
        style="Label1"
        label="Decoupler 3:"
        justify="Left"
        />
      <label loc="115 50" size="85 20"
        style="Label1"
        label="Pulse (pwx2)"
        justify="Left"
        />
      <label loc="115 75" size="85 20"
        style="Label1"
        label="Pulse (pwx3)"
        justify="Left"
        />
      <entry loc="200 50" size="50 20"
        style="PlainText"
        vq="pwx2"
        vc="pwx2=$VALUE"
        set="$VALUE=pwx2"
        show="$ENABLE=(pwx2&gt;0)*2-1"
        disable="Grayed out"
        />
      <entry loc="200 75" size="50 20"
        style="PlainText"
        vq="pwx3"
        vc="pwx3=$VALUE"
        set="$VALUE=pwx3"
        show="$ENABLE=(pwx3&gt;0)*2-1"
        disable="Grayed out"
        />
      <button loc="200 100" size="50 20"
        style="Heading2"
        label="Array"
        vc="vnmrjcmd('array','popup')"
        bg=""
        decor1=""
        />
      <label loc="440 25" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="385 25" size="50 20"
        style="PlainText"
        vq="pwxlvl1 pwx1"
        vc="pwxlvl1=$VALUE"
        set="$VALUE=pwxlvl1"
        show="$ENABLE=(pwx1&gt;0)*2-1"
        disable="Grayed out"
        />
      <label loc="285 25" size="100 20"
        style="Label1"
        label="Power (pwxlvl1)"
        justify="Left"
        />
      <label loc="255 25" size="25 20"
        style="Label1"
        label="µs"
        justify="Left"
        />
      <entry loc="200 25" size="50 20"
        style="PlainText"
        vq="pwx1"
        vc="pwx1=$VALUE"
        set="$VALUE=pwx1"
        show="$ENABLE=(pwx1&gt;0)*2-1"
        disable="Grayed out"
        />
      <label loc="115 25" size="85 20"
        style="Label1"
        label="Pulse (pwx1)"
        justify="Left"
        />
      <label loc="10 25" size="100 20"
        style="Label1"
        label="Decoupler 1:"
        justify="Left"
        />
    </group>
    <textmessage loc="10 0" size="170 25"
      style="PlainText"
      label="pwxcal"
      vq="pslabel"
      set="$VALUE=pslabel"
      />
  </group>
</template>
