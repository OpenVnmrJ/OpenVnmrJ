        vq="gstabwg"
        vc="gstabwg=$VALUE/1000"
        set="$VALUE=gstabwg*1000"
        digits="1"
        disable="Grayed out"
        tooltip="Gradient recovery delay in the WG block"
        />
      <entry loc="140 40" size="60 20"
        style="PlainText"
        vq="gtwg"
        vc="gtwg=$VALUE/1000"
        set="$VALUE=gtwg*1000"
        digits="1"
