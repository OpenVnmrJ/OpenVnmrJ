<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Standard" element="pages" type="sample" >
  <group size="840 305"
    style="Heading1"
    label="Standard"
    vq="customflag"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Standard"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="380 70" size="210 70"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="5 25" size="50 20"
        style="Heading1"
        label="plotter"
        justify="Left"
        />
      <filemenu loc="60 25" size="140 20"
        style="PlainText"
        vq="userplotter userdir"
        vc="userplotter='$VALUE'"
        set="$VALUE=userplotter"
        file="$VALUE=userdir+'/persistence/plotters'"
        type="file"
        editable="No"
        vq2="userplotter"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <group loc="0 45" size="200 20"
        vq="emailflag"
        show="$SHOW=(emailflag='y')"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="BeforeFirstExperiment"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <check loc="90 0" size="90 20"
          style="Label1"
          label="e-message"
          vq="emessage"
          vc="emessage='y'"
          vc2="emessage='n'"
          set="$VALUE=(emessage='y')"
          />
        <label loc="5 0" size="80 20"
          style="Label1"
          label="After Queue"
          justify="Left"
          />
      </group>
      <check loc="90 5" size="70 20"
        style="Label1"
        label="Autoplot"
        vq="pltopt"
        vc="pltopt='y'"
        vc2="pltopt='n'"
        set="$VALUE=(pltopt='y')"
        />
      <label loc="5 5" size="80 20"
        style="Label1"
        label="After EXP"
        justify="Left"
        />
    </group>
    <group loc="380 30" size="210 40"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <toggle loc="15 5" size="70 30"
        style="Heading2"
        label="Insert"
        vc="insert"
        statpar="air"
        statval="insert"
        radiobutton="yes"
        />
      <toggle loc="115 5" size="70 30"
        style="Heading2"
        label="Eject"
        vc="eject"
        statpar="air"
        statval="eject"
        radiobutton="yes"
        />
    </group>
    <group loc="45 255" size="685 30"
      vq="sqflag appmode"
      show="$SHOW=((appmode='walkup') and (sqflag='n'))"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampqinfo2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="530 5" size="145 20"
        style="Heading2"
        label="Turn ON studyQ !"
        vq="panellevel"
        vc="sqflag='y' xmhaha('refreshSQ') seqfil=seqfil"
        show="$ENABLE=(panellevel&gt;29)"
        bg=""
        decor1=""
        />
      <label loc="10 5" size="525 20"
        style="Label1"
        label="Use StudyQ Action menu (Refresh Queue) to populate StudyQ ... OR"
        justify="Left"
        />
    </group>
    <group loc="380 140" size="210 70"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="50 25" size="140 20"
        style="PlainText"
        label="menu"
        vq="alock"
        vc="$alock='$VALUE' if (Console='mercury') and ($alock='u') then $alock='n' endif alock=$alock"
        set="if (alock&lt;&gt;'n') and (alock&lt;&gt;'u') then $VALUE='s' else $VALUE=alock endif"
        editable="No"
        >
        <mlabel 
          label="Yes (alock&lt;&gt;n)"
          chval="s"
          />
        <mlabel 
          label="No (alock=n)"
          chval="n"
          />
        <mlabel 
          label="Unlocked (alock=u)"
          chval="u"
          />
      </menu>
      <label loc="5 25" size="45 20"
        style="Label1"
        label="Lock?"
        justify="Left"
        />
      <label loc="25 5" size="160 20"
        style="Label1"
        label="Before First Exp"
        justify="Center"
        />
      <check loc="30 46" size="60 20"
        style="Label1"
        label="Shim"
        vq="wshim"
        vc="wshim='e'"
        vc2="wshim='n'"
        set="$VALUE=(wshim&lt;&gt;'n')"
        />
      <check loc="120 46" size="60 20"
        style="Label1"
        label="Tune"
        vq="ptuneatsample wtune atune tunemethod"
        vc="ptuneatsample='yes'"
        vc2="ptuneatsample='no'"
        set="$VALUE=(ptuneatsample='yes')"
        show="$ok='no' checkprotune('global'):$ok $ENABLE=($ok='yes')*2-1"
        />
    </group>
    <group loc="595 150" size="210 50"
      vq="probetype"
      show="$SHOW=(probetype='nano')"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="nanosrate"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="30 25" size="70 20"
        style="Heading1"
        label="spin rate:"
        justify="Right"
        />
      <label loc="165 25" size="20 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="105 25" size="60 20"
        style="PlainText"
        vq="srate spin"
        vc="srate=$VALUE setsrate($VALUE)"
        set="$VALUE=srate"
        show="on('spin'):$ENABLE"
        digits="0"
        disable="Grayed out"
        />
      <textmessage loc="5 5" size="190 20"
        style="PlainText"
        vq="probetype"
        set="$VALUE='Probe type:  '+probetype"
        />
    </group>
    <group loc="595 30" size="210 115"
      style="Heading1"
      vq="system"
      show="is_spectrometer('value'):$SHOW"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampstat"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="60 5" size="85 20"
        style="Heading1"
        label="Sample"
        justify="Center"
        />
      <group loc="5 30" size="195 20"
        vq="spintype"
        show="$SHOW=(spintype&lt;&gt;'mas')"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <entry loc="95 0" size="35 20"
          style="PlainText"
          vq="spin"
          vc="spin=$VALUE"
          set="$VALUE=spin"
          show="on('spin'):$ENABLE"
          digits="0"
          disable="Grayed out"
          />
        <button loc="0 0" size="65 20"
          style="Heading2"
          label="Spin"
          vc="spin"
          bg=""
          decor1=""
          />
        <label loc="70 0" size="20 20"
          style="Label1"
          label="at"
          justify="Center"
          />
        <label loc="135 0" size="20 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <check loc="160 0" size="20 20"
          style="Label1"
          vq="spin"
          vc="on('spin')"
          vc2="off('spin')"
          set="on('spin'):$VALUE"
          />
      </group>
      <group loc="5 50" size="195 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <button loc="0 0" size="65 20"
          style="Heading2"
          label="Temp"
          vc="su"
          bg=""
          decor1=""
          />
        <label loc="70 0" size="20 20"
          style="Label1"
          label="at"
          justify="Center"
          />
        <entry loc="95 0" size="35 20"
          style="PlainText"
          vq="temp tin"
          vc="temp=$VALUE tin='y'"
          set="$VALUE=temp"
          show="on('temp'):$ENABLE"
          digits="1"
          disable="Grayed out"
          />
        <label loc="135 0" size="10 20"
          style="Label1"
          label="C"
          justify="Left"
          />
        <check loc="160 0" size="20 20"
          style="Label1"
          vq="temp tin"
          vc="on('temp') tin='y'"
          vc2="off('temp') tin='n'"
          set="on('temp'):$VALUE"
          />
      </group>
      <label loc="5 70" size="45 20"
        style="Label1"
        label="Spin:"
        justify="Left"
        />
      <textmessage loc="50 70" size="80 20"
        style="PlainText"
        label="Off"
        statpar="spin"
        />
      <label loc="135 70" size="40 20"
        style="Label1"
        label="Rate:"
        justify="Left"
        />
      <textmessage loc="175 70" size="30 20"
        style="PlainText"
        statpar="spinval"
        />
      <textmessage loc="175 90" size="30 20"
        style="PlainText"
        statpar="lklvl"
        />
      <label loc="135 90" size="40 20"
        style="Label1"
        label="Level:"
        justify="Left"
        />
      <textmessage loc="50 90" size="80 20"
        style="PlainText"
        label="Off"
        statpar="lock"
        />
      <label loc="5 90" size="45 20"
        style="Label1"
        label="Lock:"
        justify="Left"
        />
    </group>
    <group loc="380 215" size="445 30"
      vq="wshim"
      show="$SHOW=(wshim&lt;&gt;'n')"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="BeforeFirstExperiment"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <filemenu loc="130 5" size="295 20"
        style="PlainText"
        vq="Gmap Gmapname operator"
        vc="Gmaplist('set','$VALUE')"
        set="if (Gmapname='') then $VALUE=Gmap else $VALUE='custom_'+Gmapname endif"
        file="$VALUE=userdir+'/persistence/'+operator+'_Gmaplist'"
        type="file"
        editable="No"
        vq2="Gmap Gmapname"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 5" size="115 20"
        style="Label1"
        label="Select shimmap:"
        />
    </group>
    <textmessage loc="35 5" size="200 20"
      style="PlainText"
      label="Operator : cpwuser"
      vq="operator"
      set="$VALUE='     Operator : '+operator"
      />
    <group loc="5 30" size="370 215"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampinfo"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <filemenu loc="85 85" size="125 20"
        style="PlainText"
        vq="solvent"
        vc="solvent='$VALUE' setref"
        set="format(solvent,'lower'):$VALUE"
        file="/vnmr/solventlist"
        type="file"
        editable="No"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <textfile loc="5 145" size="360 65"
        style="PlainText"
        vq="curexp"
        set="$VALUE=curexp+'/text'"
        editable="yes"
        wrap="yes"
        units="char"
        />
      <entry loc="255 65" size="105 20"
        style="PlainText"
        vq="emailaddr "
        vc="emailaddr='$VALUE'"
        set="$VALUE=emailaddr"
        disable="Grayed out"
        />
      <label loc="215 65" size="45 20"
        style="Label1"
        label="Eaddr"
        justify="Left"
        />
      <entry loc="255 45" size="105 20"
        style="PlainText"
        vq=" page"
        vc=" page='$VALUE'"
        set="$VALUE=page"
        disable="Grayed out"
        />
      <label loc="215 45" size="40 20"
        style="Label1"
        label="Page:"
        justify="Left"
        />
      <group loc="235 5" size="130 20"
        vq="userremembrance"
        show="$SHOW=(userremembrance='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="userrem"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <button loc="2 0" size="120 20"
          style="Heading2"
          label="Use My Prefs"
          vc="loaduserprefs"
          show="isnewexp:$isnew $ENABLE=($isnew&gt;0)*2-1"
          bg=""
          decor1=""
          />
      </group>
      <entry loc="100 5" size="130 20"
        style="PlainText"
        vq=" samplename"
        vc=" samplename='$VALUE' setsamplename"
        set="$VALUE=samplename"
        disable="Grayed out"
        />
      <entry loc="100 45" size="110 20"
        style="PlainText"
        vq="lot"
        vc="lot='$VALUE'"
        set="$VALUE=lot"
        disable="Grayed out"
        />
      <label loc="10 45" size="90 20"
        style="Label1"
        label="Lot Number:"
        justify="Left"
        />
      <label loc="10 25" size="90 20"
        style="Label1"
        label="SampleDir:"
        justify="Left"
        />
      <textmessage loc="100 25" size="260 20"
        style="PlainText"
        label="(_20080415_01)"
        vq="sample sdirtmplt adirtmplt samplename"
        set="if (sample='') then $as='' $s='' Svfname(adirtmplt+'/'+sdirtmplt,''):$as shell('basename '+$as):$s $VALUE='('+$s+')' else $VALUE=sample endif"
        />
      <label loc="10 65" size="90 20"
        style="Label1"
        label="Notebook:"
        justify="Left"
        />
      <entry loc="100 65" size="110 20"
        style="PlainText"
        vq=" notebook"
        vc=" notebook='$VALUE'"
        set="$VALUE=notebook"
        disable="Grayed out"
        />
      <toggle loc="295 105" size="70 20"
        style="Heading2"
        label="Other"
        vq="solvent"
        vc="solvent=solvent"
        set="format(solvent,'lower'):$solv $VALUE=(($solv&lt;&gt;'d2o') and ($solv&lt;&gt;'dmso') and ($solv&lt;&gt;'cdcl3'))"
        radiobutton="yes"
        />
      <toggle loc="225 105" size="70 20"
        style="Heading2"
        label="D2O"
        vq="solvent"
        vc="solvent='d2o' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='d2o')"
        radiobutton="yes"
        />
      <toggle loc="155 105" size="70 20"
        style="Heading2"
        label="CDCl3"
        vq="solvent"
        vc="solvent='cdcl3' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='cdcl3')"
        radiobutton="yes"
        />
      <toggle loc="85 105" size="70 20"
        style="Heading2"
        label="DMSO"
        vq="solvent"
        vc="solvent='dmso' setref"
        set="format(solvent,'lower'):$solv $VALUE=($solv='dmso')"
        radiobutton="yes"
        />
      <label loc="5 95" size="80 20"
        style="Label1"
        label="Solvent"
        justify="Left"
        />
      <label loc="5 125" size="100 20"
        style="Label1"
        label="Comment"
        justify="Left"
        />
      <label loc="10 5" size="90 20"
        style="Label1"
        label="Sample Name:"
        justify="Left"
        />
    </group>
  </group>
</template>
