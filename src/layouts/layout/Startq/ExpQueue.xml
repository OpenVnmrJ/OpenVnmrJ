<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="ExpQueue" element="pages" type="sample" >
  <group size="665 270"
    style="Heading1"
    label="ExpQueue"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="ExpQueue"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="315 50" size="315 75"
      vq="appmode sqflag"
      show="$SHOW=((appmode&lt;&gt;'walkup') or (sqflag='n'))"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sampqinfo3"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="40 35" size="110 30"
        style="Heading2"
        label="Customize"
        vq="DEname"
        vc="walkupQ('DelEdit','edit')"
        show="$ENABLE=((DEname&lt;&gt;'') and (DEname&lt;&gt;'NULL'))*2-1"
        bg=""
        decor1=""
        />
      <button loc="175 35" size="110 30"
        style="Heading2"
        label="Delete"
        vq="DEname"
        vc="walkupQ('DelEdit','delete')"
        show="$ENABLE=((DEname&lt;&gt;'') and (DEname&lt;&gt;'NULL'))*2-1"
        bg=""
        decor1=""
        />
      <filemenu loc="170 5" size="130 20"
        style="PlainText"
        vq="DEname cursqexp curexp"
        vc="DEname='$VALUE'"
        set="$VALUE=DEname"
        file="$VALUE=cursqexp+'/EXPdeledit'"
        type="file"
        editable="No"
        vq2="cursqexp"
        statshow="yes"
        elastic="no"
        display="yes"
        />
      <label loc="5 5" size="160 20"
        style="Label1"
        label="Customize/Delete:"
        />
    </group>
    <group loc="40 5" size="230 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="expqueue"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <textfile loc="5 25" size="220 190"
        style="PlainText"
        vq="cursqexp curexp"
        set="$VALUE=cursqexp+'/EXPTIME'"
        show="$ENABLE=0"
        editable="no"
        wrap="no"
        units="char"
        />
      <group loc="5 5" size="220 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="daynight"
        useref="no"
        subtype="Convenience"
        expanded="yes"
        >
        <label loc="15 0" size="195 20"
          style="Heading1"
          label="Experiment Queue"
          justify="Center"
          />
      </group>
    </group>
  </group>
</template>
