<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="630 255"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="gHMBCAD"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="10 155" size="200 90"
      style="Heading1"
      label="Multiple-Bond Coupling"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="j1xhgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="175 45" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="175 25" size="25 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="10 45" size="95 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
      <textmessage loc="120 45" size="50 20"
        style="PlainText"
        label="62.5"
        vq="jnxh"
        set="$VALUE=500/jnxh"
        digits="1"
        />
      <label loc="10 25" size="110 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
      <entry loc="120 25" size="50 20"
        style="PlainText"
        vq="jnxh"
        vc="jnxh=$VALUE"
        set="$VALUE=jnxh"
        disable="Grayed out"
        />
    </group>
    <group loc="215 25" size="205 130"
      style="Heading1"
      label="One-Bond suppression"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="onebondsuppress"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 20" size="190 20"
        style="Label1"
        label="One-Bond suppression"
        vq="jfilter"
        vc="jfilter='y'"
        vc2="jfilter='n'"
        set="$VALUE=(jfilter='y')"
        />
      <group loc="5 40" size="195 85"
        vq="jfilter"
        show="$SHOW=(jfilter='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="170 65" size="25 20"
          style="Heading1"
          label="ms"
          justify="Left"
          />
        <label loc="170 20" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <label loc="170 0" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <entry loc="115 65" size="50 20"
          style="PlainText"
          vc="gt0=$VALUE/1000"
          set="$VALUE=gt0*1000"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="115 45" size="50 20"
          style="PlainText"
          vq="gzlvl0 jfilter"
          vc="gzlvl0=$VALUE"
          set="$VALUE=gzlvl0"
          digits="0"
          disable="Grayed out"
          />
        <label loc="5 65" size="60 20"
          style="Label1"
          label="G- time"
          justify="Left"
          />
        <label loc="5 45" size="80 20"
          style="Label1"
          label="G- strength"
          justify="Left"
          />
        <entry loc="115 20" size="50 20"
          style="PlainText"
          vq="j1min jfilter"
          vc="j1min=$VALUE"
          set="$VALUE=j1min"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="115 0" size="50 20"
          style="PlainText"
          vq="j1max jfilter"
          vc="j1max=$VALUE"
          set="$VALUE=j1max"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 20" size="110 20"
          style="Label1"
          label="Coupling 2 (low)"
          justify="Left"
          />
        <label loc="5 0" size="110 20"
          style="Label1"
          label="Coupling 1 (high)"
          justify="Left"
          />
      </group>
    </group>
    <group loc="425 25" size="170 130"
