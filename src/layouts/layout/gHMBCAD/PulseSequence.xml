        style="PlainText"
        vq="pwx180ad"
        vc="pwx180ad='$VALUE'"
        set="$VALUE=pwx180ad"
        disable="Grayed out"
        />
      <label loc="10 25" size="45 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <entry loc="120 65" size="50 20"
        style="PlainText"
        vq="pwxlvl180"
        vc="pwxlvl180=$VALUE"
        set="$VALUE=pwxlvl180"
        disable="Grayed out"
        />
      <entry loc="120 45" size="50 20"
        style="PlainText"
        vq="pwx180"
        vc="pwx180=$VALUE"
        set="$VALUE=pwx180"
        disable="Grayed out"
        />
      <label loc="10 65" size="50 20"
        style="Label1"
        label="power"
        justify="Left"
        />
      <label loc="10 45" size="50 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
    </group>
    <group loc="10 25" size="200 130"
      style="Heading1"
      label="Gradient Selection"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="EDggroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="175 105" size="25 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="175 65" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="120 45" size="50 20"
        style="PlainText"
        vq="gzlvlE"
        vc="gzlvlE=$VALUE"
        set="$VALUE=gzlvlE"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="120 65" size="50 20"
        style="PlainText"
        vq="gtE"
        vc="gtE=$VALUE/1000"
        set="$VALUE=gtE*1000"
        digits="1"
