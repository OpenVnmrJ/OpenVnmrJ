<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="650 280"
    style="Heading1"
    label="Defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="380 25"
        style="PlainText"
        label="Experiment: stepNOESY1D   Solvent: d2o"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="395 0" size="220 25"
        style="PlainText"
        label="Observe: H1       Decoupler: C13"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       Decoupler: '+dn"
        />
    </group>
    <group loc="400 35" size="235 240"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 25" size="220 215"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        label="30"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
    <group loc="5 35" size="395 240"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="NOESY1Doption"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <check loc="70 150" size="155 20"
        style="Label1"
        label="NOE  ON/OFF"
        vq="NOE"
        vc="NOE='y'"
        vc2="NOE='n'"
        set="$VALUE=(NOE='y')"
        />
      <menu loc="190 25" size="90 20"
        style="PlainText"
        label="menu"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        bg=""
        editable="No"
        >
        <mlabel 
          label="8"
          chval="8"
          />
        <mlabel 
          label="16"
          chval="16"
          />
        <mlabel 
          label="32"
          chval="32"
          />
        <mlabel 
          label="64"
          chval="64"
          />
        <mlabel 
          label="128"
          chval="128"
          />
        <mlabel 
          label="256"
          chval="256"
          />
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1024"
          chval="1024"
          />
      </menu>
      <label loc="70 25" size="105 20"
        style="Label1"
        label="Number of scans"
        justify="Left"
        />
      <group loc="5 45" size="380 105"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="65 0" size="155 20"
          style="Label1"
          label="TOCSY STEP  ON/OFF"
          vq="STEP"
          vc="STEP='y'"
          vc2="STEP='n'"
          set="$VALUE=(STEP='y')"
          />
        <group loc="0 20" size="380 85"
          vq="STEP"
          show="$SHOW=(STEP='y')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="STEPquick"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <menu loc="185 0" size="90 20"
            style="PlainText"
            label="menu"
            vq="stepmix"
            vc="stepmix=$VALUE"
            set="$VALUE=stepmix"
            bg=""
            editable="No"
            >
            <mlabel 
              label="30"
              chval="0.03"
              />
            <mlabel 
              label="50"
              chval="0.05"
              />
            <mlabel 
              label="70"
              chval="0.07"
              />
            <mlabel 
              label="80"
              chval="0.08"
              />
            <mlabel 
              label="100"
              chval="0.1"
              />
            <mlabel 
              label="150"
              chval="0.15"
              />
          </menu>
          <button loc="260 50" size="85 20"
            style="Heading2"
            label="makeshape"
            vc="setupstep('makeshape')"
            bg=""
            decor1=""
            />
          <entry loc="185 60" size="65 20"
            style="PlainText"
            vq="stepwidth"
            vc="stepwidth=$VALUE "
            set="$VALUE=stepwidth"
            digits="1"
            />
          <entry loc="185 40" size="65 20"
            style="PlainText"
            vq="stepppm"
            vc="stepppm=$VALUE"
            set="$VALUE=stepppm"
            digits="3"
            disable="Grayed out"
            />
          <label loc="280 0" size="30 20"
            style="Label1"
            label="ms"
            justify="Left"
            />
          <button loc="185 20" size="160 20"
            style="Heading2"
            label="Select and makeshape"
            vq="procdim"
            vc="setupstep('frq') setupstep('makeshape')"
            show="$ENABLE=(procdim=1)*2-1"
            bg=""
            decor1=""
            />
          <label loc="5 20" size="180 20"
            style="Label1"
            label="Set STEP band using cursors..."
            justify="Left"
            />
          <label loc="25 40" size="30 20"
            style="Label1"
            label="OR"
            justify="Left"
            />
          <label loc="65 60" size="105 20"
            style="Label1"
            label="STEP width (Hz)"
            justify="Left"
            />
          <label loc="65 0" size="105 20"
            style="Label1"
            label="STEP mixing time"
            justify="Left"
            />
          <label loc="65 40" size="110 20"
            style="Label1"
            label="STEP peak (ppm)"
            justify="Left"
            />
        </group>
      </group>
      <group loc="5 170" size="380 65"
        vq="NOE"
        show="$SHOW=(NOE='y')"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="STEPquick"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <textmessage loc="65 40" size="280 20"
          style="PlainText"
          label="NOE peak at 4.50 ppm (width: 0.0 Hz)"
          vq="selshapeA"
          set="$ppm=0 $width=0 selfrqinfo(selshapeA+'.RF'):$ppm,$width $sp='' $sw='' format($ppm,0,2):$sp format($width,0,1):$sw $VALUE='NOE peak at '+$sp+' ppm (width: '+$sw+' Hz)'"
          />
        <label loc="65 0" size="105 20"
          style="Label1"
          label="NOE Mixing time"
          justify="Left"
          />
        <label loc="5 20" size="180 20"
          style="Label1"
          label="Set NOE band using cursors..."
          justify="Left"
          />
        <menu loc="185 0" size="90 20"
          style="PlainText"
          label="menu"
          vq="mixN"
          vc="mixN=$VALUE"
          set="$VALUE=mixN"
          bg=""
          editable="No"
          >
          <mlabel 
            label="150"
            chval="0.15"
            />
          <mlabel 
            label="250"
            chval="0.25"
            />
          <mlabel 
            label="350"
            chval="0.35"
            />
          <mlabel 
            label="500"
            chval="0.5"
            />
          <mlabel 
            label="600"
            chval="0.6"
            />
          <mlabel 
            label="700"
            chval="0.7"
            />
          <mlabel 
            label="800"
            chval="0.8"
            />
          <mlabel 
            label="1000"
            chval="1.0"
            />
        </menu>
        <label loc="280 0" size="30 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <button loc="185 20" size="160 20"
          style="Heading2"
          label="Select and makeshape"
          vq="procdim"
          vc="setselfrqc('clear') setselfrqc suselfrq"
          show="$ENABLE=(procdim=1)*2-1"
          bg=""
          decor1=""
          />
      </group>
    </group>
  </group>
</template>
