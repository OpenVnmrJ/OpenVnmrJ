<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="680 230"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <entry loc="330 0" size="50 20"
      style="PlainText"
      vq="r7"
      vc="r7=$VALUE importHT(r7)"
      set="$VALUE=r7"
      digits="0"
      />
    <button loc="120 0" size="210 20"
      style="Heading2"
      label="Import HT frequency list from exp"
      vc="importHT(r7)"
      bg=""
      decor1=""
      />
    <group loc="10 0" size="100 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="90 20"
        style="PlainText"
        label="roesyHT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <button loc="390 0" size="130 20"
      style="Heading2"
      label="Make Frequency List"
      vc="$cmd=seqfil lb=20 wft(1) peak:$a th=$a/6.0 htll lb='n' wft(1) {$cmd} "
      bg=""
      decor1=""
      toolTip="Set the threshold and push this button to make the required frequency list"
      />
    <button loc="530 0" size="140 20"
      style="Heading2"
      label="Display Pulse Sequence"
      vc="dps"
      bg=""
      decor1=""
      />
    <group loc="435 25" size="245 190"
      style="Heading1"
      label="Gradients"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="5 125" size="60 20"
        style="Label1"
        label="sspul"
        vq="sspul"
        vc="sspul='y' "
        vc2="sspul='n'"
        set="if (sspul='y') then $VALUE=1 else $VALUE=0 endif"
        toolTip="Suppress steady-state magnetization"
        />
      <label loc="5 100" size="45 20"
        style="Label1"
        label="gzlvlzq"
        />
      <entry loc="50 100" size="60 20"
        style="PlainText"
        vq="gzlvlzq"
        vc="gzlvlzq=$VALUE"
        set="$VALUE=gzlvlzq"
        />
      <label loc="110 100" size="40 20"
        style="Label1"
        label="gstab"
        />
      <entry loc="150 100" size="50 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        digits="3"
        />
      <entry loc="150 80" size="50 20"
        style="PlainText"
        vq="gt3"
        vc="gt3=$VALUE/1000"
        set="$VALUE=gt3*1000"
        digits="3"
        />
      <label loc="125 80" size="25 20"
        style="Label1"
        label="gt3"
        />
      <entry loc="50 80" size="60 20"
        style="PlainText"
        vq="gzlvl3"
        vc="gzlvl3=$VALUE"
        set="$VALUE=gzlvl3"
        />
      <label loc="5 80" size="45 20"
        style="Label1"
        label="gzlvl3"
        />
      <entry loc="150 60" size="50 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        digits="3"
        />
      <label loc="125 60" size="25 20"
        style="Label1"
        label="gt2"
        />
      <entry loc="50 60" size="60 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        />
      <label loc="5 60" size="40 20"
        style="Label1"
        label="gzlvl2"
        />
      <label loc="205 100" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="205 80" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="205 60" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="205 40" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="50 20" size="50 20"
        style="Label1"
        label="Level"
        />
      <label loc="150 20" size="50 20"
        style="Label1"
        label="Delay"
        />
      <entry loc="150 40" size="50 20"
        style="PlainText"
        vq="gt0"
        vc="gt0=$VALUE/1000"
        set="$VALUE=gt0*1000"
        digits="3"
        />
      <label loc="125 40" size="25 20"
        style="Label1"
        label="gt0"
        />
      <entry loc="50 40" size="60 20"
        style="PlainText"
        vq="gzlvl0"
        vc="gzlvl0=$VALUE"
        set="$VALUE=gzlvl0"
        />
      <label loc="5 40" size="40 20"
        style="Label1"
        label="gzlvl0"
        />
    </group>
    <group loc="225 95" size="210 120"
      style="Heading1"
      label="Solvent Suppression"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventSuppression"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="10 20" size="60 20"
        style="Label1"
        label="wet"
        vq="wet satmode"
        vc="wet='y' satmode='nnn'"
        vc2="wet='n'"
        set="$VALUE=(wet='y') "
        toolTip="Solvent presaturation ON / OFF status"
        />
      <check loc="100 20" size="60 20"
        style="Label1"
        label="presat"
        vq="satmode wet"
        vc="satmode='ynn' wet='n'"
        vc2="satmode='nnn'"
        set="$s='' substr(satmode,1,1):$s $VALUE=($s='y')"
        toolTip="Solvent presaturation ON / OFF status"
        />
      <label loc="160 80" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="160 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="160 40" size="30 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <entry loc="100 80" size="60 20"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="satfrq satmode"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Frequency"
        justify="Left"
        />
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        disable="Grayed out"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="220 190"
      style="Heading1"
      label="HT Excitation"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="HTExcitation"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <entry loc="135 65" size="40 20"
        style="PlainText"
        vq="htofs1"
        vc="htofs1=$VALUE"
        set="$VALUE=htofs1"
        />
      <entry loc="135 40" size="40 20"
        style="PlainText"
        vq="htbw1"
        vc="htbw1=$VALUE"
        set="$VALUE=htbw1"
        digits="0"
        />
      <check loc="10 110" size="190 20"
        style="Label1"
        label="Show Pbox Reports"
        vq="repflg"
        vc="repflg='y' "
        vc2="repflg='h'"
        set="if( repflg = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 90" size="190 20"
        style="Label1"
        label="Bloch-Seigert Correction"
        vq="bscor"
        vc="bscor='y' "
        vc2="bscor='n'"
        set="if( bscor = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="10 65" size="120 20"
        style="Label1"
        label="Skip First increments"
        />
      <button loc="30 150" size="140 20"
        style="Heading2"
        label="Edit HT Frequencies"
        vc="editht('show')"
        bg=""
        decor1=""
        />
      <label loc="180 40" size="30 20"
        style="Label1"
        label="Hz"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Bandwidth"
        />
      <menu loc="115 20" size="100 20"
        style="PlainText"
        label="menu"
        vq="pshape"
        vc="pshape='$VALUE'"
        set="$VALUE=pshape"
        editable="No"
        >
        <mlabel 
          label="gaus180"
          chval="gaus180"
          />
        <mlabel 
          label="sinc180"
          chval="sinc180"
          />
        <mlabel 
          label="isnob2"
          chval="isnob2"
          />
        <mlabel 
          label="iburp2"
          chval="iburp2"
          />
      </menu>
      <label loc="10 20" size="110 20"
        style="Label1"
        label="HT Waveform"
        />
    </group>
    <group loc="225 25" size="210 70"
      style="Heading1"
      label="Mixing"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SpinLock"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <menu loc="110 40" size="80 20"
        style="PlainText"
        label="menu"
        vq="ad180"
        vc="ad180='$VALUE' "
        set="$VALUE=ad180"
        editable="No"
        >
        <mlabel 
          label="ZQ filter"
          chval="z"
          />
        <mlabel 
          label="inversion"
          chval="y"
          />
        <mlabel 
          label="none"
          chval="n"
          />
      </menu>
      <label loc="170 20" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="110 20" size="60 20"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 40" size="100 20"
        style="Label1"
        label="Adiabatic 180"
        justify="Left"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Mixing Time"
        justify="Left"
        />
    </group>
  </group>
</template>
