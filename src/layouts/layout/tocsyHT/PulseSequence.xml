<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="710 240"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="440 30" size="250 190"
      style="Heading1"
      label="Gradients"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Gradients"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="200 120" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 120" size="60 20"
        style="PlainText"
        vq="gstab"
        vc="gstab=$VALUE/1000"
        set="$VALUE=gstab*1000"
        />
      <label loc="60 120" size="60 20"
        style="Label1"
        label="gstab"
        />
      <label loc="200 100" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 100" size="60 20"
        style="PlainText"
        vq="gt2"
        vc="gt2=$VALUE/1000"
        set="$VALUE=gt2*1000"
        />
      <label loc="60 100" size="50 20"
        style="Label1"
        label="gt2"
        />
      <entry loc="140 40" size="60 20"
        style="PlainText"
        vq="gzlvl2"
        vc="gzlvl2=$VALUE"
        set="$VALUE=gzlvl2"
        />
      <label loc="60 40" size="60 20"
        style="Label1"
        label="gzlvl2"
        />
      <check loc="10 165" size="65 20"
        style="Label1"
        label="sspul"
        vq="sspul"
        vc="sspul='y' "
        vc2="sspul='n'"
        set="$s='' substr(sspul,1,1):$s $VALUE=($s='y')"
        toolTip="Suppress steady state magnetization"
        />
      <label loc="200 80" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <entry loc="140 80" size="60 20"
        style="PlainText"
        vq="gt0"
        vc="gt0=$VALUE/1000"
        set="$VALUE=gt0*1000"
        />
      <entry loc="140 20" size="60 20"
        style="PlainText"
        vq="gzlvl0"
        vc="gzlvl0=$VALUE"
        set="$VALUE=gzlvl0"
        />
      <label loc="60 80" size="50 20"
        style="Label1"
        label="gt0"
        />
      <label loc="10 80" size="50 20"
        style="Label1"
        label="Delay"
        />
      <label loc="10 20" size="50 20"
        style="Label1"
        label="Level"
        />
      <label loc="60 20" size="60 20"
        style="Label1"
        label="gzlvl0"
        />
    </group>
    <group loc="225 150" size="210 70"
      style="Heading1"
      label="Mixing"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SpinLock"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <label loc="10 20" size="100 20"
        style="Heading1"
        label="Waveform"
        justify="Left"
        />
      <label loc="10 40" size="80 20"
        style="Label1"
        label="Time"
        justify="Left"
        />
      <entry loc="110 40" size="60 20"
        style="PlainText"
        vq="mix"
        vc="mix=$VALUE/1000"
        set="$VALUE=mix*1000"
        digits="0"
        disable="Grayed out"
        />
      <label loc="170 40" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <menu loc="110 20" size="90 20"
        style="PlainText"
        label="menu"
        vq="mixpat"
        vc="mixpat='$VALUE'"
        set="$VALUE=mixpat"
        editable="No"
        >
        <mlabel 
          label="DIPSI2"
          chval="DIPSI2"
          />
        <mlabel 
          label="MLEV16"
          chval="MLEV16"
          />
        <mlabel 
          label="FLOPSY8"
          chval="FLOPSY8"
          />
      </menu>
    </group>
    <button loc="385 0" size="140 20"
      style="Heading2"
      label="Make Frequency List"
      vc="$cmd=seqfil lb=20 wft(1) peak:$a th=$a/6.0 htll lb='n' wft(1) {$cmd} "
      bg=""
      decor1=""
      />
    <entry loc="320 0" size="50 20"
      style="PlainText"
      vq="r7"
      vc="r7=$VALUE importHT(r7)"
      set="$VALUE=r7"
      />
    <button loc="120 0" size="200 20"
      style="Heading2"
      label="Import HT frequencies from exp"
      vq="r7"
      vc="importHT(r7)"
      bg=""
      decor1=""
      />
    <group loc="10 0" size="100 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="90 20"
        style="PlainText"
        label="tocsyHT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <button loc="535 0" size="155 20"
      style="Heading2"
      label="Display Pulse Sequence"
      vc="dps"
      bg=""
      decor1=""
      />
    <group loc="225 30" size="210 120"
      style="Heading1"
      label="Solvent Suppression"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SolventSuppression"
      useref="no"
      subtype="Major"
      expanded="yes"
      >
      <check loc="0 100" size="160 20"
        style="Label1"
        label="WET ON / OFF Status"
        vq="satmode wet"
        vc="wet='y' satmode='nnn'"
        vc2="wet='n'"
        set="$s='' substr(wet,1,1):$s $VALUE=($s='y')"
        toolTip="WET solvent suppression on/off"
        />
      <label loc="160 80" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="160 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="160 40" size="30 20"
        style="Label1"
        label="s"
        justify="Left"
        />
      <check loc="0 20" size="160 20"
        style="Label1"
        label="Presat ON / OFF Status"
        vq="satmode wet"
        vc="satmode='ynn' wet='n'"
        vc2="satmode='nnn'"
        set="$s='' substr(satmode,1,1):$s $VALUE=($s='y')"
        />
      <entry loc="100 80" size="60 20"
        style="PlainText"
        vq="satpwr satmode"
        vc="satpwr=$VALUE"
        set="$VALUE=satpwr"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="0"
        disable="Grayed out"
        />
      <label loc="10 80" size="70 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="satfrq satmode"
        vc="satfrq=$VALUE"
        set="$VALUE=satfrq"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Frequency"
        justify="Left"
        />
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="satdly satmode"
        vc="satdly=$VALUE"
        set="$VALUE=satdly"
        show="$s='' substr(satmode,1,1):$s $ENABLE=($s='y')"
        disable="Grayed out"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Delay"
        justify="Left"
        />
    </group>
    <group loc="0 30" size="220 190"
      style="Heading1"
      label="HT Excitation"
      bg=""
      border="etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Major"
      >
      <menu loc="120 20" size="100 20"
        style="PlainText"
        label="menu"
        vq="httype pshape gt1 gt2 gzlvl1 gzlvl2"
        vc="httype='$VALUE' HTchoice"
        set="$VALUE=httype"
        editable="No"
        >
        <mlabel 
          label="Inversion"
          chval="i"
          />
        <mlabel 
          label="Refocusing"
          chval="r"
          />
        <mlabel 
          label="Excitation"
          chval="e"
          />
        <mlabel 
          label="DPFGSE"
          chval="d"
          />
      </menu>
      <button loc="35 165" size="140 20"
        style="Heading2"
        label="Edit HT Frequencies"
        vc="editht('show')"
        bg=""
        decor1=""
        toolTip="Edit Hadamard encoding frequency list"
        />
      <check loc="10 140" size="190 20"
        style="Label1"
        label="Show Pbox reports"
        vq="repflg"
        vc="repflg='y' "
        vc2="repflg='n'"
        set="if( repflg = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="130 100" size="50 20"
        style="PlainText"
        vq="htofs1"
        vc="htofs1=$VALUE"
        set="$VALUE=htofs1"
        />
      <entry loc="130 80" size="90 20"
        style="PlainText"
        vq="compH"
        vc="compH=$VALUE"
        set="$VALUE=compH"
        digits="3"
        />
      <entry loc="130 60" size="50 20"
        style="PlainText"
        vq="htbw1"
        vc="htbw1=$VALUE"
        set="$VALUE=htbw1"
        digits="0"
        />
      <label loc="180 60" size="30 20"
        style="Label1"
        label="Hz"
        />
      <entry loc="130 40" size="90 20"
        style="PlainText"
        vq="pshape ptype"
        vc="pshape='$VALUE'"
        set="$VALUE=pshape"
        />
      <check loc="10 120" size="190 20"
        style="Label1"
        label="Bloch-Seigert Correction"
        vq="bscor"
        vc="bscor='y' "
        vc2="bscor='n'"
        set="if( bscor = 'y') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="10 100" size="120 20"
        style="Label1"
        label="Skip First increments"
        />
      <label loc="10 80" size="120 20"
        style="Label1"
        label="H1 Amp Compres."
        />
      <label loc="10 60" size="70 20"
        style="Label1"
        label="Bandwidth"
        />
      <label loc="10 40" size="110 20"
        style="Label1"
        label="HT Waveform"
        />
      <label loc="10 20" size="110 20"
        style="Label1"
        label="Selection Method"
        />
    </group>
  </group>
</template>
