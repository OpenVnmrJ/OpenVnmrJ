<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="650 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="430 25" size="170 130"
      style="Heading1"
      label="Steady State"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <check loc="10 20" size="155 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        show="$ENABLE=(PFGflg='y')*2-1"
        />
      <label loc="145 80" size="25 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <check loc="10 100" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        show="$ENABLE=(PFGflg='n')*2-1"
        />
      <entry loc="80 80" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        disable="Grayed out"
        />
      <entry loc="80 60" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')*2-1"
        digits="0"
        disable="Grayed out"
        />
      <label loc="20 80" size="60 20"
        style="Label1"
        label="time"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <label loc="10 40" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
    </group>
    <group loc="230 25" size="195 190"
      style="Heading1"
      label="Z-filter"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="zfilter"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="165 60" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <check loc="10 20" size="130 20"
        style="Label1"
        label="Z-filter"
        vq="zfilt PFGflg"
        vc="zfilt='y'"
        vc2="zfilt='n'"
        set="$VALUE=(zfilt='y' and PFGflg='y')"
        />
      <entry loc="100 60" size="60 20"
        style="PlainText"
        vq="gtz zfilt PFGflg"
        vc="gtz=$VALUE/1000"
        set="$VALUE=gtz*1000"
        show="$ENABLE=(zfilt='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="100 40" size="60 20"
        style="PlainText"
        vq="gzlvlz zfilt PFGflg"
        vc="gzlvlz=$VALUE"
        set="$VALUE=gzlvlz"
        show="$ENABLE=(zfilt='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <group loc="5 80" size="190 105"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Convenience"
        >
        <label loc="160 85" size="30 20"
          style="Heading1"
          label="deg"
          justify="Left"
          />
        <label loc="160 65" size="30 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <label loc="160 45" size="30 20"
          style="Label1"
          label="dB"
          justify="Left"
          />
        <check loc="5 5" size="130 20"
          style="Label1"
          label="Flipback pulse"
          vq="flipback zfilt"
          vc="flipback='y'"
          vc2="flipback='n'"
          set="$VALUE=(flipback='y')"
          show="$ENABLE=(zfilt='y')*2-1"
          />
        <entry loc="95 85" size="60 20"
          style="PlainText"
          vq="zfphinc flipback"
          vc="zfphinc=$VALUE"
          set="$VALUE=zfphinc"
          show="$ENABLE=(flipback='y')*2-1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="95 65" size="60 20"
          style="PlainText"
          vq="fbpw flipback"
          vc="fbpw=$VALUE*1000"
          set="$VALUE=fbpw/1000"
          show="$ENABLE=(flipback='y')*2-1"
          digits="3"
          disable="Grayed out"
          />
        <entry loc="95 45" size="60 20"
          style="PlainText"
          vq="fbpwr flipback"
          vc="fbpwr=$VALUE"
          set="$VALUE=fbpwr"
          show="$ENABLE=(flipback='y')*2-1"
          disable="Grayed out"
          />
        <entry loc="95 25" size="60 20"
          style="PlainText"
          vq="fbshp flipback"
          vc="fbshp='$VALUE'"
          set="$VALUE=fbshp"
          show="$ENABLE=(flipback='y')*2-1"
          disable="Grayed out"
          />
        <label loc="15 25" size="75 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
        <label loc="15 85" size="75 20"
          style="Label1"
          label="Phaseshift"
          justify="Left"
          />
        <label loc="15 65" size="75 20"
          style="Label1"
          label="Duration"
          justify="Left"
          />
        <label loc="15 45" size="75 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
      </group>
      <label loc="20 60" size="75 20"
        style="Label1"
        label="G- duration"
        justify="Left"
        />
      <label loc="20 40" size="75 20"
        style="Label1"
        label="G- strength"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="220 190"
      style="Heading1"
      label="SpinLock"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="MLEV17c"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="10 120" size="90 20"
        style="Heading1"
        label="Trim pulse"
        justify="Left"
        />
      <entry loc="110 120" size="55 20"
        style="PlainText"
        vq="trim"
        vc="trim=$VALUE/1000"
        set="$VALUE=trim*1000"
        disable="Grayed out"
        />
      <label loc="170 120" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <label loc="170 100" size="25 20"
        style="Label1"
        label="Âµs"
        justify="Left"
        />
      <entry loc="110 100" size="55 20"
        style="PlainText"
        vq="slpwT"
        vc="slpwT=$VALUE"
        set="$VALUE=slpwT"
        digits="3"
        disable="Grayed out"
        />
      <label loc="10 100" size="100 20"
        style="Label1"
        label="Pi/2 pulse width"
        justify="Left"
        />
      <label loc="10 80" size="95 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
      <entry loc="110 80" size="55 20"
        style="PlainText"
        vq="slpwrT"
        vc="getpower($VALUE,tn):spinlockT setpower(spinlockT,tn):slpwrT,slpwT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=slpwrT"
        disable="Grayed out"
        />
      <label loc="170 80" size="30 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <label loc="170 60" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <entry loc="110 60" size="55 20"
        style="PlainText"
        vq="spinlockT"
        vc="setpower($VALUE,tn):slpwrT,slpwT getpower(slpwrT,tn):spinlockT spinlockT=spinlockT slpwT=slpwT"
        set="$VALUE=spinlockT"
        disable="Grayed out"
        />
      <button loc="70 60" size="40 20"
        style="Heading2"
        label="Set"
        vc="setslgh2"
        bg=""
        decor1=""
        />
      <label loc="10 60" size="55 20"
        style="Label1"
        label="Strength"
        justify="Left"
        />
      <label loc="10 40" size="60 20"
        style="Label1"
        label="Pattern"
        justify="Left"
        />
      <menu loc="70 40" size="135 20"
        style="PlainText"
        label="menu"
        vq="slpatT"
        vc="slpatT='$VALUE'"
        set="$VALUE=slpatT"
        editable="No"
        >
        <mlabel 
          label="MLEV17"
          chval="mlev17"
          />
        <mlabel 
          label="clean MLEV17"
          chval="mlev17c"
          />
        <mlabel 
          label="DIPSI2"
          chval="dipsi2"
          />
        <mlabel 
          label="DIPSI3"
          chval="dipsi3"
          />
      </menu>
      <label loc="10 20" size="80 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
      <entry loc="110 20" size="55 20"
        style="PlainText"
        vq="mixT"
        vc="mixT=$VALUE/1000"
        set="$VALUE=mixT*1000"
        disable="Grayed out"
        />
      <label loc="170 20" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="TOCSY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
