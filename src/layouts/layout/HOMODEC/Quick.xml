<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="665 280"
    style="Heading1"
    label="Defaults"
    vq="panellevel"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="5 35" size="395 240"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      >
      <group loc="10 110" size="380 120"
        vq="customflag procdim"
        show="expactive:$active $SHOW=(($active&lt;1) and (procdim&lt;1))"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="customizepar2-sppm2"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <group loc="5 40" size="370 70"
          vq="wds"
          show="strstr(wds,'wds'):$is if ($is&lt;1) then $SHOW=1 else $SHOW=0 endif"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="customizepar-sppm"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="10 0" size="225 20"
            style="Heading1"
            label="No. of Selective Freqencies (max: 5):"
            />
          <entry loc="235 0" size="80 20"
            style="PlainText"
            vq="Array"
            vc="mkpar('integer','Array',0) Array=$VALUE if (Array &gt; 5) then Array=5 endif  selppm2frq('init',Array)"
            set="$VALUE=Array"
            />
          <label loc="335 45" size="30 20"
            style="Label1"
            label="Hz"
            />
          <entry loc="290 45" size="40 20"
            style="PlainText"
            vq="selfrqw findfrqflg"
            vc="if ((Array &gt; 4) and (findfrqflg&lt;&gt;'y')) then selfrqw[5]=$VALUE  endif"
            set="$sps=size('selfrqw') if ($sps &gt; 4) then $VALUE=selfrqw[5] else $VALUE=0 endif"
            show="$ENABLE=((Array &gt; 4) and (findfrqflg&lt;&gt;'y'))"
            digits="1"
            />
          <entry loc="245 45" size="40 20"
            style="PlainText"
            vq="selfrqw findfrqflg"
            vc="if ((Array &gt; 3) and (findfrqflg&lt;&gt;'y')) then selfrqw[4]=$VALUE  endif"
            set="$sps=size('selfrqw') if ($sps &gt; 3) then $VALUE=selfrqw[4] else $VALUE=0 endif"
            show="$ENABLE=((Array &gt; 3) and (findfrqflg&lt;&gt;'y'))"
            digits="1"
            />
          <entry loc="200 45" size="40 20"
            style="PlainText"
            vq="selfrqw findfrqflg"
            vc="if ((Array &gt; 2) and (findfrqflg&lt;&gt;'y'))  then selfrqw[3]=$VALUE  endif"
            set="$sps=size('selfrqw') if ($sps &gt; 2) then $VALUE=selfrqw[3] else $VALUE=0 endif"
            show="$ENABLE=((Array &gt; 2) and (findfrqflg&lt;&gt;'y'))"
            digits="1"
            />
          <entry loc="155 45" size="40 20"
            style="PlainText"
            vq="selfrqw Array findfrqflg"
            vc="if ((Array &gt; 1) and (findfrqflg&lt;&gt;'y')) then selfrqw[2]=$VALUE  endif"
            set="$sps=size('selfrqw') if ($sps &gt; 1) then $VALUE=selfrqw[2] else $VALUE=0 endif"
            show="$ENABLE=((Array &gt; 1) and (findfrqflg&lt;&gt;'y'))"
            digits="1"
            />
          <entry loc="110 45" size="40 20"
            style="PlainText"
            vq="selfrqw Array findfrqflg"
            vc="if ((Array &gt; 0) and (findfrqflg&lt;&gt;'y')) then selfrqw[1]=$VALUE  endif"
            set="$sps=size('selfrqw') if ($sps &gt; 0) then $VALUE=selfrqw[1] else $VALUE=0 endif"
            show="$ENABLE=((Array &gt; 0) and (findfrqflg&lt;&gt;'y'))"
            digits="1"
            />
          <label loc="10 45" size="100 20"
            style="Label1"
            label="Selective width:"
            />
          <label loc="335 25" size="30 20"
            style="Label1"
            label="ppm"
            />
          <entry loc="290 25" size="40 20"
            style="PlainText"
            vq="selfrqppm "
            vc="if (Array &gt; 4) then selfrqppm[5]=$VALUE  endif"
            set="$sps=size('selfrqppm') if ($sps &gt; 4) then $VALUE=selfrqppm[5] else $VALUE=0 endif"
            show="$ENABLE=(Array &gt; 4)"
            digits="2"
            />
          <entry loc="245 25" size="40 20"
            style="PlainText"
            vq="selfrqppm "
            vc="if (Array &gt; 3) then selfrqppm[4]=$VALUE  endif"
            set="$sps=size('selfrqppm') if ($sps &gt; 3) then $VALUE=selfrqppm[4] else $VALUE=0 endif"
            show="$ENABLE=(Array &gt; 3)"
            digits="2"
            />
          <entry loc="200 25" size="40 20"
            style="PlainText"
            vq="selfrqppm "
            vc="if (Array &gt; 2) then selfrqppm[3]=$VALUE  endif"
            set="$sps=size('selfrqppm') if ($sps &gt; 2) then $VALUE=selfrqppm[3] else $VALUE=0 endif"
            show="$ENABLE=(Array &gt; 2)"
            digits="2"
            />
          <entry loc="155 25" size="40 20"
            style="PlainText"
            vq="selfrqppm "
            vc="if (Array &gt; 1) then selfrqppm[2]=$VALUE  endif"
            set="$sps=size('selfrqppm') if ($sps &gt; 1) then $VALUE=selfrqppm[2] else $VALUE=0 endif"
            show="$ENABLE=(Array &gt; 1)"
            digits="2"
            />
          <entry loc="110 25" size="40 20"
            style="PlainText"
            vq="selfrqppm "
            vc="if (Array &gt; 0) then selfrqppm[1]=$VALUE  endif"
            set="$sps=size('selfrqppm') if ($sps &gt; 0) then $VALUE=selfrqppm[1] else $VALUE=0 endif"
            show="$ENABLE=(Array &gt; 0)"
            digits="2"
            />
          <label loc="10 25" size="100 20"
            style="Label1"
            label="Selective Freq:"
            />
        </group>
        <group loc="15 0" size="335 40"
          vq="customflag"
          show="$SHOW=(customflag='y')"
          bg=""
          border="None"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          >
          <label loc="25 0" size="95 20"
            style="Heading1"
            label="Selection Band:"
            />
          <check loc="125 20" size="95 20"
            style="Label1"
            label="Define now"
            vq="wds"
            vc="mkpar('integer','Array',0) selppm2frq('init',Array) wds='wstart=`suselfrq`'"
            vc2="rmpar('Array') selppm2frq('init',0) wds=''"
            set="strstr(wds,'wstart'):$is if ($is&gt;0) then $VALUE=1 else $VALUE=0 endif"
            />
          <check loc="125 0" size="170 20"
            style="Label1"
            label="Select interactively later"
            vq="wds"
            vc="rmpar('Array') selppm2frq('init',0) wds='wds=`suselfrq`'"
            vc2="wds=''"
            set="strstr(wds,'wds'):$is if ($is&gt;0) then $VALUE=1 else $VALUE=0 endif"
            />
        </group>
      </group>
      <group loc="40 20" size="300 200"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="selexcitacq"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <label loc="10 70" size="160 20"
          style="Heading1"
          label="Multifrequency decoupling"
          justify="Left"
          />
        <label loc="10 10" size="140 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <label loc="10 30" size="140 20"
          style="Label1"
          label="Presaturate?"
          justify="Left"
          />
        <label loc="10 50" size="140 20"
          style="Label1"
          label="Decoupling Duty Cycle"
          justify="Left"
          />
        <group loc="10 95" size="275 85"
          vq="procdim wds"
          vc="crmode='b' ds"
          vc2="clear(2)"
          show="if (procdim=1) and (wds&lt;&gt;'') then $SHOW=1 else $SHOW=0 endif"
          bg=""
          border="LoweredBevel"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="ds_selfrq"
          useref="no"
          subtype="Untitled"
          expanded="yes"
          >
          <label loc="15 9" size="240 20"
            style="Heading1"
            label="Select Decoupling Bands using cursors"
            justify="Center"
            />
          <button loc="185 34" size="80 20"
            style="Heading2"
            label="Select"
            vc="if crmode='c' then write('error','Use 2 cursors') else setselfrqc endif ds('restart')"
            bg=""
            decor1=""
            />
          <button loc="100 34" size="80 20"
            style="Heading2"
            label="Expand/Full"
            vc="ds('expand')"
            bg=""
            decor1=""
            />
          <button loc="10 34" size="80 20"
            style="Heading2"
            label="Box/Cursor"
            vc="ds('toggle')"
            bg=""
            decor1=""
            />
          <button loc="55 59" size="80 20"
            style="Heading2"
            label="CLEAR"
            vc="selfrqc=0 selfrqw=0 selshapeA=selshapeA[1] off('selfrqc') clear(2) ds"
            bg=""
            decor1=""
            />
          <button loc="145 59" size="80 20"
            style="Heading2"
            label="ShowList"
            vq="selfrqc"
            vc="showfreq('selfrqc')"
            show="on('selfrqc'):$ENABLE"
            bg=""
            decor1=""
            />
        </group>
        <check loc="175 70" size="30 20"
          style="Label1"
          vq="mfslp"
          vc="mkpar('flag','mfslp','y') mfslp='y'"
          vc2="mkpar('flag','mfslp','n') mfslp='n'"
          set="exists('mfslp','parameter'):$parex if ($parex) then $VALUE=(mfslp='y') else $VALUE=0 endif"
          />
        <entry loc="175 50" size="40 20"
          style="PlainText"
          vq="dutyc"
          vc="dutyc=$VALUE/100"
          set="$VALUE=(dutyc*100)"
          digits="0"
          />
        <label loc="220 50" size="30 20"
          style="Label1"
          label="%"
          />
        <check loc="175 30" size="85 20"
          style="Label1"
          vq="hdpsmode"
          vc="hdpsmode='y'"
          vc2="hdpsmode='n'"
          set="$VALUE=(hdpsmode='y')"
          />
        <menu loc="175 10" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
        </menu>
      </group>
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <group loc="5 60" size="215 65"
        vq="customflag file wds pslabel seqfil procdim panellevel"
        show="HOMODECproc('iscphomodec','processed'):$ishomo $SHOW=((customflag='n') and ($ishomo) and (procdim=1) and (panellevel&gt;29))"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <button loc="10 35" size="195 20"
          style="Heading2"
          label="Ignore newly selected peaks"
          vq="wds wetfrqs"
          vc="wds='' wft resetselfrq"
          show="$ENABLE=(wds&lt;&gt;'')*2-1"
          bg=""
          decor1=""
          />
        <button loc="10 10" size="195 20"
          style="Heading2"
          label="Select more peaks to decouple"
          vq="wds wetfrqs"
          vc="HOMODECproc('continue') ds('restart')"
          show="$ENABLE=(wds='')*2-1"
          bg=""
          decor1=""
          />
      </group>
      <group loc="10 25" size="215 180"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Titled"
        expanded="yes"
        >
        <label loc="0 155" size="215 20"
          style="Heading1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          label="---"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <textmessage loc="15 105" size="190 20"
          style="PlainText"
          label=" "
          vq="samplename"
          set="$VALUE=samplename"
          />
        <check loc="125 25" size="85 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="125 5" size="85 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="5 5" size="125 20"
          style="PlainText"
          label="Before HOMODEC acq :"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq :'"
          />
        <textmessage loc="5 25" size="115 20"
          style="PlainText"
          label="After HOMODEC acq :"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq :'"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          label="wet1D PRESAT PROTON"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        label="30"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Titled"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 23"
        style="PlainText"
        label="Experiment: HOMODEC   Solvent: cdcl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 23"
        style="PlainText"
        label="Observe: H1          Decoupler: C13"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
  </group>
</template>
