<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Process1D" element="pages" type="processing" >
  <group size="550 240"
    style="Heading1"
    label="Weighting"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Process1D"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <button loc="230 210" size="160 20"
      style="Heading2"
      label="Interactive Weighting"
      vc="wti"
      bg=""
      decor1=""
      />
    <group loc="215 60" size="190 140"
      style="Heading1"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <label loc="0 0" size="180 20"
        style="Heading1"
        label="Weight Parameters"
        justify="Center"
        />
      <check loc="160 20" size="20 20"
        style="Label1"
        vq="lb"
        vc="lb='y'"
        vc2="lb='n'"
        set="on('lb'):$VALUE"
        />
      <check loc="160 40" size="20 20"
        style="Label1"
        vq="sb sbs"
        vc="sb='y'"
        vc2="sb='n'"
        set="on('sb'):$VALUE"
        />
      <check loc="160 60" size="20 20"
        style="Label1"
        vq="sbs"
        vc="sbs='y'"
        vc2="sbs='n'"
        set="on('sbs'):$VALUE"
        />
      <check loc="160 80" size="20 20"
        style="Label1"
        vq="gf gfs"
        vc="gf='y'"
        vc2="gf='n'"
        set="on('gf'):$VALUE"
        />
      <check loc="160 100" size="20 20"
        style="Label1"
        vq="gfs"
        vc="gfs='y'"
        vc2="gfs='n'"
        set="on('gfs'):$VALUE"
        />
      <check loc="160 120" size="20 20"
        style="Label1"
        vq="awc"
        vc="awc='y'"
        vc2="awc='n'"
        set="on('awc'):$VALUE"
        />
      <entry loc="120 80" size="40 20"
        style="PlainText"
        vq="gf"
        vc="gf=$VALUE"
        set="$VALUE=gf"
        show="on('gf'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="120 120" size="40 20"
        style="PlainText"
        vq="awc"
        vc="awc=$VALUE"
        set="$VALUE=awc"
        show="on('awc'):$VALUE"
        digits="e"
        disable="Grayed out"
        />
      <entry loc="120 100" size="40 20"
        style="PlainText"
        vq="gf gfs"
        vc="gfs=$VALUE"
        set="$VALUE=gfs"
        show="onparams('gf','gfs'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="120 60" size="40 20"
        style="PlainText"
        vq="sb sbs"
        vc="sbs=$VALUE"
        set="$VALUE=sbs"
        show="onparams('sb','sbs'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="120 40" size="40 20"
        style="PlainText"
        vq="sb"
        vc="sb=$VALUE"
        set="$VALUE=sb"
        show="on('sb'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="120 20" size="40 20"
        style="PlainText"
        vq="lb"
        vc="lb=$VALUE"
        set="$VALUE=lb"
        show="on('lb'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 120" size="90 20"
        style="Label1"
        label="additive Offset"
        justify="Left"
        />
      <label loc="20 100" size="60 20"
        style="Label1"
        label="shift"
        justify="Left"
        />
      <label loc="10 80" size="80 20"
        style="Label1"
        label="gaussian"
        justify="Left"
        />
      <label loc="20 60" size="60 20"
        style="Label1"
        label="shift"
        justify="Left"
        />
      <label loc="10 40" size="80 20"
        style="Label1"
        label="sinebell"
        justify="Left"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="line broadening"
        justify="Left"
        />
    </group>
    <group loc="0 0" size="215 65"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="DEPTprocess"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <button loc="105 20" size="100 20"
        style="Heading2"
        label="CH only edit"
        vq="qphase arraydim"
        vc="process_DEPT('CHn')"
        show="$qsize=size('qphase') $ENABLE=($qsize&gt;1)*2-1"
        bg=""
        decor1=""
        />
      <button loc="105 40" size="100 20"
        style="Heading2"
        label="Quat. Carbons"
        vq="qphase arraydim"
        vc="process_DEPT('Cq')"
        show="$qsize=size('qphase') $ENABLE=($qsize&gt;1)*2-1"
        bg=""
        decor1=""
        />
      <button loc="5 40" size="100 20"
        style="Heading2"
        label="Full Edit"
        vq="qphase mult"
        vc="process_DEPT"
        show="$qs=size('qphase') $ms=size('mult') $ENABLE=(($qs&gt;1) or ($ms=4))"
        bg=""
        decor1=""
        />
      <button loc="5 20" size="100 20"
        style="Heading2"
        label="Transform all"
        vc="wft"
        bg=""
        decor1=""
        />
      <label loc="47 0" size="130 20"
        style="Label1"
        label="DEPT process"
        justify="Center"
        />
    </group>
    <group loc="225 10" size="180 40"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <menu loc="120 0" size="60 20"
        style="PlainText"
        label="menu"
        vq="fn"
        vc="fn=$VALUE*2"
        set="$VALUE=fn/2"
        show="on('fn'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="512"
          chval="512"
          />
        <mlabel 
          label="1k"
          chval="1024"
          />
        <mlabel 
          label="2k"
          chval="2048"
          />
        <mlabel 
          label="4k"
          chval="4096"
          />
        <mlabel 
          label="8k"
          chval="8192"
          />
        <mlabel 
          label="16k"
          chval="16384"
          />
        <mlabel 
          label="32k"
          chval="32768"
          />
        <mlabel 
          label="64k"
          chval="65536"
          />
        <mlabel 
          label="128k"
          chval="131072"
          />
      </menu>
      <check loc="0 0" size="120 20"
        style="Label1"
        label="Transform Size"
        vq="fn"
        vc="on('fn')"
        vc2="off('fn')"
        set="on('fn'):$VALUE"
        />
      <textmessage loc="120 20" size="60 20"
        style="PlainText"
        label="32,768"
        vq="np"
        set="$VALUE=np/2"
        digits="0"
        />
      <label loc="20 20" size="100 20"
        style="Label1"
        label="Acquired Points"
        justify="Left"
        />
    </group>
    <group loc="415 0" size="120 180"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="Adjust"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <button loc="0 160" size="120 20"
        style="Heading2"
        label="Display text"
        vc="dtext"
        decor1=""
        />
      <button loc="0 140" size="120 20"
        style="Heading2"
        label="Display linewidth"
        vc="nl dres"
        decor1=""
        />
      <button loc="0 120" size="120 20"
        style="Heading2"
        label="Find nearest line"
        vc="nl"
        decor1=""
        />
      <button loc="0 80" size="120 20"
        style="Heading2"
        label="Autophase Zero"
        vc="aph0"
        decor1=""
        />
      <button loc="0 60" size="120 20"
        style="Heading2"
        label="Autophase Full"
        vc="aph"
        decor1=""
        />
      <button loc="0 20" size="120 20"
        style="Heading2"
        label="Autoscale"
        vc="vsadj ds"
        decor1=""
        />
      <label loc="0 0" size="120 20"
        style="Label1"
        label="Adjust  "
        justify="Center"
        />
    </group>
    <entry loc="120 65" size="50 20"
      style="PlainText"
      vq="ftproc"
      vc="wft($VALUE)"
      set="select:$VALUE"
      digits="0"
      disable="Grayed out"
      />
    <label loc="10 65" size="110 20"
      style="Label1"
      label="Transform FID #"
      justify="Center"
      />
    <group loc="0 90" size="180 140"
      style="Heading1"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <button loc="0 100" size="90 20"
        style="Heading2"
        label="pi/3 ssqsine"
        vc="pi3ssbsq('acq')"
        decor1=""
        />
      <button loc="0 80" size="90 20"
        style="Heading2"
        label="pseudo"
        vc="pseudoecho('acq')"
        decor1=""
        />
      <button loc="90 100" size="90 20"
        style="Heading2"
        label="pi/4 ssqsine"
        vc="pi4ssbsq('acq')"
        decor1=""
        />
      <button loc="40 120" size="100 20"
        style="Heading2"
        label="none"
        vc="lb='n' gf='n' gfs='n' sb='n' sbs='n' awc='n'"
        decor1=""
        />
      <label loc="0 0" size="180 20"
        style="Label1"
        label="AutoSelect Weighting"
        justify="Center"
        />
      <button loc="90 80" size="90 20"
        style="Heading2"
        label="res-enhance"
        vc="resenhance('acq')"
        decor1=""
        />
      <button loc="90 60" size="90 20"
        style="Heading2"
        label="sq-cosine"
        vc="sqcosine('acq')"
        decor1=""
        />
      <button loc="90 40" size="90 20"
        style="Heading2"
        label="cosine"
        vc="cosine('acq')"
        decor1=""
        />
      <button loc="90 20" size="90 20"
        style="Heading2"
        label="gaussian"
        vc="gaussian('acq')"
        decor1=""
        />
      <button loc="0 60" size="90 20"
        style="Heading2"
        label="sq-sine"
        vc="sqsinebell('acq')"
        decor1=""
        />
      <button loc="0 40" size="90 20"
        style="Heading2"
        label="sine"
        vc="sinebell('acq')"
        decor1=""
        />
      <button loc="0 20" size="90 20"
        style="Heading2"
        label="exponential"
        vc="expfilt('acq')"
        decor1=""
        />
    </group>
  </group>
</template>
