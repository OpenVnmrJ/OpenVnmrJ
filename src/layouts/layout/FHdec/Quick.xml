<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="655 260"
    style="Heading1"
    label="Defaults"
    vq="pslabel tn"
    show="$SHOW=((pslabel&lt;&gt;'PROTON') and (pslabel&lt;&gt;'CARBON') and (pslabel&lt;&gt;'PHOSPHORUS') and (pslabel&lt;&gt;'FLUORINE'))"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        label="Experiment: FHdec   Solvent: CDCl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        label="Observe: H1          Decoupler: F19"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="35 120" size="325 55"
        style="Heading1"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="ppm"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="215 5" size="35 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <textmessage loc="0 5" size="30 20"
          style="PlainText"
          label="F19"
          vq="dn"
          set="$VALUE=dn"
          />
        <entry loc="140 5" size="70 20"
          style="PlainText"
          vq="dofppm"
          vc="dofppm=$VALUE setoffset(dofppm,dn):dof"
          set="$VALUE=dofppm"
          digits="2"
          disable="Grayed out"
          />
        <label loc="30 5" size="105 20"
          style="Label1"
          label="Decoupler offset"
          justify="Left"
          />
      </group>
      <group loc="35 30" size="325 90"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="FLUORINEacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="30 65" size="75 20"
          style="Label1"
          label="Pulse Angle"
          justify="Left"
          />
        <label loc="0 5" size="135 20"
          style="Label1"
          label="Spectral width - from"
          justify="Left"
          />
        <menu loc="140 65" size="70 20"
          style="PlainText"
          label="menu"
          vq="pw pw90"
          vc="pw($VALUE)"
          set="$VALUE=pw*90/pw90"
          bg=""
          editable="No"
          >
          <mlabel 
            label="45"
            chval="45"
            />
          <mlabel 
            label="30"
            chval="30"
            />
          <mlabel 
            label="60"
            chval="60"
            />
          <mlabel 
            label="90"
            chval="90"
            />
        </menu>
        <menu loc="140 45" size="70 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="0"
            chval="0"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <label loc="30 45" size="105 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
        <label loc="30 25" size="110 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <label loc="190 5" size="20 20"
          style="Label1"
          label="to"
          justify="Center"
          />
        <label loc="265 5" size="50 20"
          style="Label1"
          label="ppm"
          justify="Left"
          />
        <label loc="215 65" size="60 20"
          style="Label1"
          label="degrees"
          justify="Left"
          />
        <label loc="215 45" size="60 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <menu loc="140 25" size="70 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1024"
            chval="1024"
            />
          <mlabel 
            label="4096"
            chval="4096"
            />
        </menu>
        <entry loc="140 5" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq"
          vc="invsetsw:$f1,$f2 setsw($f1,$VALUE) delacq"
          set="invsetsw:$f1,$f2 $VALUE=$f2"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="210 5" size="50 20"
          style="PlainText"
          vq="rfl rfp reffrq sw"
          vc="invsetsw:$f1,$f2 setsw($VALUE,$f2) delacq"
          set="invsetsw:$f1,$f2 $VALUE=$f1"
          digits="1"
          disable="Grayed out"
          />
      </group>
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <label loc="5 0" size="135 20"
        style="Label1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <check loc="145 20" size="80 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 45" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
  </group>
</template>
