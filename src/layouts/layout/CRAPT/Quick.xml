<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="675 275"
    style="Heading1"
    label="Defaults"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="tndndisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="355 25"
        style="PlainText"
        label="Experiment: APT   Solvent: cdcl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        label="Observe: C13          Decoupler: H1"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'          Decoupler: '+dn"
        />
    </group>
    <group loc="5 35" size="395 220"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="15 30" size="370 180"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="CARBONacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="235 65" size="60 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <menu loc="150 65" size="80 20"
          style="PlainText"
          label="menu"
          vq="d1"
          vc="d1=$VALUE"
          set="$VALUE=d1"
          editable="No"
          >
          <mlabel 
            label="1"
            chval="1"
            />
          <mlabel 
            label="0"
            chval="0"
            />
          <mlabel 
            label="2"
            chval="2"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="25"
            chval="25"
            />
        </menu>
        <menu loc="150 45" size="80 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="64"
            chval="64"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="512"
            chval="512"
            />
          <mlabel 
            label="1000"
            chval="1000"
            />
          <mlabel 
            label="2000"
            chval="2000"
            />
          <mlabel 
            label="5000"
            chval="5000"
            />
          <mlabel 
            label="10000"
            chval="10000"
            />
        </menu>
        <group loc="55 110" size="310 45"
          vq="wbs"
          show="$ret=0 strstr(wbs,'testsn'):$ret $SHOW=($ret&gt;0)"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="atevery"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <label loc="255 5" size="40 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <entry loc="195 5" size="55 20"
            style="PlainText"
            vq="wbs"
            vc="$v1='' $v2='' tsnspwp('get'):$v1,$v2 tsnspwp('set',$v1,$VALUE)"
            set="$v3='' tsnspwp('get'):$v3,$VALUE"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="95 5" size="65 20"
            style="PlainText"
            vq="wbs"
            vc="$v1='' $v2='' tsnspwp('get'):$v1,$v2 tsnspwp('set',$VALUE,$v2)"
            set="tsnspwp('get'):$VALUE"
            digits="1"
            disable="Grayed out"
            />
          <label loc="160 5" size="35 20"
            style="Label1"
            label="and"
            justify="Center"
            />
          <menu loc="95 25" size="65 20"
            style="PlainText"
            label="menu"
            vq="bs"
            vc="bs=$VALUE"
            set="$VALUE=bs"
            editable="No"
            >
            <mlabel 
              label="64"
              chval="64"
              />
            <mlabel 
              label="128"
              chval="128"
              />
            <mlabel 
              label="256"
              chval="256"
              />
            <mlabel 
              label="512"
              chval="512"
              />
            <mlabel 
              label="1024"
              chval="1024"
              />
          </menu>
          <label loc="160 25" size="105 20"
            style="Label1"
            label="scans &amp; Stop at"
            justify="Center"
            />
          <entry loc="265 25" size="45 20"
            style="PlainText"
            vq="sn"
            vc="sn=$VALUE"
            set="$VALUE=sn"
            digits="0"
            disable="Grayed out"
            />
          <label loc="30 5" size="60 20"
            style="Label1"
            label="between"
            justify="Left"
            />
          <label loc="30 25" size="55 20"
            style="Label1"
            label="at every"
            justify="Left"
            />
        </group>
        <label loc="35 90" size="75 20"
          style="Label1"
          label="Check S/N?"
          justify="Left"
          />
        <label loc="35 45" size="115 20"
          style="Label1"
          label="Number of scans"
          justify="Left"
          />
        <radio loc="195 90" size="45 20"
          style="Label1"
          label="no"
          vq="wbs"
          vc="wbs=''"
          vc2="strstr(wbs,'testsn'):$ret1 if ($ret1=0) then wbs='testsn' else wbs=wbs endif"
          set="$ret=0 strstr(wbs,'testsn'):$ret $VALUE=($ret=0)"
          />
        <radio loc="150 90" size="45 20"
          style="Label1"
          label="yes"
          vq="wbs"
          vc="strstr(wbs,'testsn'):$ret1 if ($ret1=0) then wbs='testsn' else wbs=wbs endif"
          vc2="wbs=''"
          set="$ret=0 strstr(wbs,'testsn'):$ret $VALUE=($ret&gt;0)"
          />
        <group loc="10 5" size="300 40"
          bg=""
          border="None"
          tab="no"
          enable="no"
          reference="swentry"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <label loc="5 20" size="130 20"
            style="Label1"
            label="(...or enter):"
            justify="Right"
            />
          <label loc="0 0" size="135 20"
            style="Label1"
            label="Spectral Width (select)"
            justify="Right"
            />
          <menu loc="140 0" size="120 20"
            style="PlainText"
            label="menu"
            vq="obsSW"
            vc="exec('$VALUE')"
            set="$VALUE=''"
            editable="No"
            >
            <mlabel 
              label="-15  -&gt;  235"
              chval="setsw(235,-15)"
              />
            <mlabel 
              label="-10  -&gt;  200"
              chval="setsw(200,-10)"
              />
            <mlabel 
              label="-25  -&gt;  250"
              chval="setsw(250,-25)"
              />
          </menu>
          <entry loc="140 20" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq"
            vc="invsetsw:$f1,$f2 setsw($f1,$VALUE)"
            set="invsetsw:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
          <label loc="265 20" size="35 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <label loc="265 0" size="35 20"
            style="Label1"
            label="ppm"
            justify="Left"
            />
          <entry loc="210 20" size="50 20"
            style="PlainText"
            vq="rfl rfp reffrq sw"
            vc="invsetsw:$f1,$f2 setsw($VALUE,$f2)"
            set="invsetsw:$f1,$f2 $VALUE=$f1"
            digits="1"
            disable="Grayed out"
            />
          <label loc="190 20" size="20 20"
            style="Label1"
            label="to"
            justify="Center"
            />
        </group>
        <label loc="34 65" size="115 20"
          style="Label1"
          label="Relaxation Delay"
          justify="Left"
          />
      </group>
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5a"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <entry loc="145 0" size="50 20"
        style="PlainText"
        vq="gain"
        vc="on('gain'):$gainon if ($gainon) then gain=$VALUE else setvalue('gain',$VALUE) endif"
        set="$VALUE=gain"
        show="on('gain'):$ENABLE"
        digits="0"
        />
      <label loc="5 0" size="135 20"
        style="Label1"
        label="Receiver Gain (dB)"
        justify="Right"
        />
      <check loc="145 20" size="80 20"
        style="Label1"
        label="Autogain"
        vq="gain arraydim"
        vc="if (arraydim=1) then off('gain') else write('line3','arraydim greater than 1') on('gain') endif"
        vc2="on('gain')"
        set="on('gain'):$gainon if ($gainon=0) then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 45" size="220 175"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="0 150" size="220 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="10 105" size="200 25"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
    </group>
  </group>
</template>
