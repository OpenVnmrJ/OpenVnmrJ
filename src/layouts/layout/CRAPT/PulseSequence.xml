<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="550 250"
    style="Heading1"
    label="Pulse Sequence"
    vq="personaType"
    show="$S=1 PersonaManager('showInPanel','acquire','pulsesequence'):$S $SHOW=$S"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="180 25" size="185 105"
      style="Heading1"
      label="HiBand-Inversion"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AD-Xinversion"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 40" size="175 60"
        vq="bipflg"
        show="$bip='' substr(bipflg,2,1):$bip $SHOW=($bip='y')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="130 40" size="25 20"
          style="Heading1"
          label="dB"
          justify="Left"
          />
        <label loc="130 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <entry loc="45 0" size="120 20"
          style="PlainText"
          vq="dnbipshp"
          vc="dnbipshp='$VALUE'"
          set="$VALUE=dnbipshp"
          disable="Grayed out"
          />
        <entry loc="45 20" size="80 20"
          style="PlainText"
          vq="dnbippw"
          vc="dnbippw=$VALUE"
          set="$VALUE=dnbippw"
          disable="Grayed out"
          />
        <entry loc="45 40" size="80 20"
          style="PlainText"
          vq="dnbippwr"
          vc="dnbippwr=$VALUE"
          set="$VALUE=dnbippwr"
          disable="Grayed out"
          />
        <label loc="0 40" size="45 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="0 20" size="45 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
        <label loc="0 0" size="45 20"
          style="Label1"
          label="Shape"
          justify="Left"
          />
      </group>
      <check loc="55 20" size="45 20"
        style="Label1"
        label="BIP"
        vq="bipflg"
        vc="bipflg='ny'"
        vc2="bipflg='nn'"
        set="$bip='' substr(bipflg,2,1):$bip $VALUE=($bip='y')"
        />
      <textmessage loc="10 20" size="45 20"
        style="PlainText"
        label="H1"
        vq="dn"
        set="$VALUE=dn"
        />
      <group loc="5 40" size="175 60"
        vq="bipflg"
        show="$bip='' substr(bipflg,2,1):$bip $SHOW=($bip='n')"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <entry loc="45 20" size="80 20"
          style="PlainText"
          vq="pp"
          vc="pp=$VALUE/2"
          set="$VALUE=pp*2"
          disable="Grayed out"
          />
        <entry loc="45 40" size="80 20"
          style="PlainText"
          vq="pplvl"
          vc="pplvl=$VALUE"
          set="$VALUE=pplvl"
          disable="Grayed out"
          />
        <label loc="130 40" size="25 20"
          style="Heading1"
          label="dB"
          justify="Left"
          />
        <label loc="130 20" size="25 20"
          style="Label1"
          label="µs"
          justify="Left"
          />
        <label loc="0 40" size="45 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="5 20" size="45 20"
          style="Label1"
          label="Width"
          justify="Left"
          />
      </group>
    </group>
    <group loc="5 135" size="225 75"
      style="Heading1"
      label="One-Bond XH Coupling"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Titled"
      rows="1"
      columns="1"
      >
      <label loc="190 45" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="190 25" size="30 20"
        style="Label1"
        label="Hz"
        justify="Left"
        />
      <label loc="10 45" size="115 20"
        style="Label1"
        label="Transfer delay"
        justify="Left"
        />
      <textmessage loc="125 45" size="60 20"
        style="PlainText"
        label="3.425"
        vq="j1xh"
        set="$VALUE=500/j1xh"
        digits="3"
        />
      <entry loc="125 25" size="60 20"
        style="PlainText"
        vq="j1xh"
        vc="j1xh=$VALUE"
        set="$VALUE=j1xh"
        disable="Grayed out"
        />
      <label loc="10 25" size="115 20"
        style="Label1"
        label="Coupling constant"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="170 105"
      style="Heading1"
      label="CRISIS"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="CRISIS"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="145 45" size="25 20"
        style="Heading1"
        label="µs"
        justify="Left"
        />
      <label loc="145 65" size="25 20"
        style="Label1"
        label="dB"
        justify="Left"
        />
      <entry loc="90 45" size="50 20"
        style="PlainText"
        vq="pw180r"
        vc="pw180r=$VALUE"
        set="$VALUE=pw180r"
        disable="Grayed out"
        />
      <entry loc="90 65" size="50 20"
        style="PlainText"
        vq="tpwr180r"
        vc="tpwr180r=$VALUE"
        set="$VALUE=tpwr180r"
        disable="Grayed out"
        />
      <entry loc="55 25" size="110 20"
        style="PlainText"
        vq="pw180ref"
        vc="pw180ref='$VALUE'"
        set="$VALUE=pw180ref"
        disable="Grayed out"
        />
      <label loc="10 25" size="45 20"
        style="Label1"
        label="Shape"
        justify="Left"
        />
      <label loc="10 45" size="45 20"
        style="Label1"
        label="Width"
        justify="Left"
        />
      <label loc="10 65" size="45 20"
        style="Label1"
        label="Power"
        justify="Left"
        />
    </group>
    <group loc="5 0" size="390 25"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        label="CRAPT"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
