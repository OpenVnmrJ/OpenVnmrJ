<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="660 260"
    style="Heading1"
    label="Defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="5 35" size="395 205"
      style="Heading1"
      label="Acquisition Options"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitionOptions"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 25" size="375 170"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="gHMBCacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <entry loc="190 140" size="65 20"
          style="PlainText"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          digits="0"
          disable="Grayed out"
          />
        <textmessage loc="190 120" size="65 20"
          style="PlainText"
          label="123"
          vq="BigT j1min j1max gtE gstab pwx180r sw1"
          set="$VALUE = trunc((BigT/2-1/(j1min+j1max)-2*gtE-2*gstab-pwx180r*1e-6)*2*sw1-2)"
          digits="0"
          />
        <menu loc="190 100" size="65 20"
          style="PlainText"
          label="menu"
          vq="jnxh"
          vc="jnxh=$VALUE"
          set="$VALUE=jnxh"
          bg=""
          editable="No"
          >
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="5"
            chval="5"
            />
          <mlabel 
            label="3"
            chval="3"
            />
          <mlabel 
            label="10"
            chval="10"
            />
          <mlabel 
            label="12"
            chval="12"
            />
        </menu>
        <menu loc="190 60" size="65 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          bg=""
          editable="No"
          >
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
          <mlabel 
            label="64"
            chval="64"
            />
        </menu>
        <label loc="25 140" size="110 20"
          style="Label1"
          label="t1 Increments"
          justify="Left"
          />
        <label loc="25 120" size="165 20"
          style="Label1"
          label="Maximum number of Inc."
          justify="Left"
          />
        <label loc="265 100" size="25 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <textmessage loc="190 80" size="65 20"
          style="PlainText"
          label="YES"
          vq="jfilter"
          set="if (jfilter='y') then $VALUE='YES' else $VALUE='NO' endif"
          />
        <label loc="25 100" size="125 20"
          style="Label1"
          label="Multiple-Bond Jnxh"
          justify="Left"
          />
        <label loc="25 80" size="115 20"
          style="Label1"
          label="2-step J1xh filter"
          justify="Left"
          />
        <label loc="25 60" size="160 20"
          style="Label1"
          label="Scans per t1 Increment"
          justify="Left"
          />
        <textmessage loc="25 40" size="165 20"
          style="PlainText"
          label="C13 Spectral Width (ppm)"
          vq="dn"
          set="$VALUE=dn+' Spectral Width (ppm)'"
          />
        <group loc="250 10" size="120 20"
          vq="dn"
          show="$SHOW=(dn='N15')"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Convenience"
          expanded="yes"
          >
          <textmessage loc="5 0" size="110 20"
            style="PlainText"
            vq="dn"
            set="$ref='' N15ref:$ref if ($ref='') then $ref='CH3NO2' endif $VALUE='['+$ref+' (0 ppm)]'"
            />
        </group>
        <group loc="25 5" size="220 30"
          bg=""
          border="Etched"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          reference="F1Nucleus:"
          useref="no"
          subtype="Basic"
          expanded="yes"
          >
          <menu loc="75 5" size="85 20"
            style="PlainText"
            label="menu"
            vq="dn"
            vc="$dn='$VALUE' if ($dn = 'other') then dn='P31' else dn=$dn endif setdecpars('update') reff1:$ret if (dn='C13') then jfilter='y' else jfilter='n' endif"
            set="$VALUE=dn"
            bg=""
            editable="No"
            >
            <mlabel 
              label="C13"
              chval="C13"
              />
            <mlabel 
              label="N15"
              chval="N15"
              />
            <mlabel 
              label="Other"
              chval="other"
              />
          </menu>
          <label loc="5 5" size="75 20"
            style="Label1"
            label="F1 Nucleus"
            justify="Left"
            />
          <group loc="165 0" size="50 25"
            vq="dn"
            show="if (dn='C13') or (dn='N15') then $SHOW=0 else $SHOW=1 endif"
            bg=""
            border="None"
            side="Top"
            justify="Left"
            tab="no"
            enable="no"
            useref="no"
            subtype="Basic"
            >
            <entry loc="0 5" size="50 20"
              style="PlainText"
              vq="dn"
              vc="dn='$VALUE' setdecpars('update') reff1:$ret"
              set="$VALUE=dn"
              disable="Grayed out"
              />
          </group>
        </group>
        <group loc="170 40" size="185 20"
          vq="dn"
          bg=""
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Basic"
          >
          <entry loc="85 0" size="45 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$VALUE,$f2)"
            set="invsetsw1:$f1,$f2 $VALUE=$f1"
            digits="0"
            disable="Grayed out"
            />
          <label loc="65 0" size="20 20"
            style="Label1"
            label="to"
            justify="Center"
            />
          <entry loc="20 0" size="45 20"
            style="PlainText"
            vq="dn"
            vc="invsetsw1:$f1,$f2 setsw1(dn,$f1,$VALUE)"
            set="invsetsw1:$f1,$f2 $VALUE=$f2"
            digits="1"
            disable="Grayed out"
            />
        </group>
      </group>
    </group>
    <group loc="5 5" size="620 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="hetcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="10 0" size="365 25"
        style="PlainText"
        label="Experiment: gc2h2bcme   Solvent: CDCl3"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
      <textmessage loc="380 0" size="235 25"
        style="PlainText"
        label="Observe: H1       H1-C13 Correlation"
        vq="tn dn"
        set="$VALUE='Observe: '+tn+'       '+tn+'-'+dn+' Correlation'"
        />
    </group>
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="5 25" size="220 180"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <textmessage loc="10 105" size="200 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <textmessage loc="5 25" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq:'"
          />
        <textmessage loc="5 5" size="130 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq:'"
          />
        <check loc="140 25" size="75 20"
          style="Label1"
          label="Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="140 5" size="75 20"
          style="Label1"
          label="Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <label loc="0 155" size="215 20"
          style="Label1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        label="30"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
  </group>
</template>
