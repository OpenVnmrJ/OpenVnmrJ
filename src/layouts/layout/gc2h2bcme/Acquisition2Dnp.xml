<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Acquisition2Dnp" element="pages" type="acquisition" >
  <group size="640 245"
    style="Heading1"
    label="Acquisition"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Acquisition2Dnp"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="320 25" size="295 210"
      style="Heading1"
      label="Acquisition in F1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitioninF1"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <group loc="10 110" size="270 50"
        style="Heading1"
        vq="ni phase sw1"
        show="if ni&gt;1 then $VALUE=1 else $VALUE=0 endif"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="215 20" size="40 20"
          style="Heading1"
          label="ppm"
          justify="Left"
          />
        <label loc="215 0" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <label loc="130 20" size="45 20"
          style="Label1"
          label="Hz, or"
          justify="Left"
          />
        <textmessage loc="175 20" size="35 20"
          style="PlainText"
          vq="ni sw1 phase reffrq1"
          set="if ni&gt;1 then $VALUE=(sw1/ni )/reffrq1 else $VALUE=0 endif"
          digits="2"
          />
        <textmessage loc="95 20" size="35 20"
          style="PlainText"
          vq="ni sw1 phase"
          set="if ni&gt;1 then $VALUE=sw1/ni else $VALUE=0 endif"
          digits="0"
          />
        <label loc="20 20" size="70 20"
          style="Label1"
          label="Resolution:"
          justify="Left"
          />
        <textmessage loc="140 0" size="70 20"
          style="PlainText"
          vq="ni sw1"
          set="if ni&gt;1 then $VALUE=ni/sw1 else $VALUE=0 endif"
          digits="6"
          />
        <label loc="0 0" size="140 20"
          style="Label1"
          label="Acquisition time (max.)"
          justify="Left"
          />
      </group>
      <button loc="150 80" size="135 20"
        style="Heading2"
        label="Set LP1 and Weighting"
        vc="setLP1andWindow"
        bg=""
        decor1=""
        />
      <label loc="10 160" size="120 20"
        style="Label1"
        label="Acquisition Mode:"
        justify="Left"
        />
      <group loc="120 150" size="120 30"
        vq="phase"
        show="$SHOW=(phase[1]&lt;0.5)"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="10 10" size="110 20"
          style="Heading1"
          label="Absolute Value"
          justify="Left"
          />
      </group>
      <group loc="130 160" size="140 20"
        vq="phase"
        show="$SHOW=(phase[1]&gt;=0.5)"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <label loc="0 0" size="130 20"
          style="Heading1"
          label="Hypercomplex 2D"
          justify="Left"
          />
      </group>
      <group loc="10 180" size="270 20"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Basic"
        >
        <radio loc="180 0" size="80 20"
          style="Label1"
          label="Full 2D"
          vq="ni"
          vc="if phase[1]&gt;0.5 then phase=1,5 endif if ni&lt;1.5 then ni=128 endif"
          set="$VALUE=(ni&gt;1.5)"
          />
        <radio loc="30 0" size="140 20"
          style="Label1"
          label="First Increment"
          vq="ni"
          vc="ni=1 phase=phase[1]"
          set="$VALUE=(ni&lt;1.5)"
          />
      </group>
      <menu loc="220 20" size="65 20"
        style="PlainText"
        label="menu"
        vq="sw1 ni"
        vc="parunits('set','sw1','$VALUE')"
        set="parunits('get','sw1'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm2"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <entry loc="150 60" size="70 20"
        style="PlainText"
        vq="ni"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        />
      <label loc="10 60" size="110 20"
        style="Label1"
        label="Increments in t1"
        justify="Left"
        />
      <label loc="10 40" size="140 20"
        style="Label1"
        label="Maximum # of  t1 inc.:"
        justify="Left"
        />
      <textmessage loc="150 40" size="70 20"
        style="PlainText"
        vq="BigT gstab gtE j1min j1max pwx180 sw1"
        set="$VALUE = trunc((BigT/2-1/(j1min+j1max)-2*gtE-2*gstab-pwx180*1e-6)*2*sw1-2)"
        digits="0"
        />
      <entry loc="150 20" size="70 20"
        style="PlainText"
        vq="sw1 ni"
        vc="vnmrunits('set','sw1',$VALUE)"
        set="vnmrunits('get','sw1'):$VALUE"
        show="if (ni=0) or (ni=1) then $VALUE=0 else $VALUE=1 endif"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
    </group>
    <group loc="5 25" size="310 210"
      style="Heading1"
      label="Acquisition in F2"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="AcquisitioninF2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <menu loc="230 20" size="65 20"
        style="PlainText"
        vq="sw"
        vc="parunits('set','sw','$VALUE')"
        set="parunits('get','sw'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
        <mlabel 
          label="Hz"
          chval="Hz"
          />
      </menu>
      <menu loc="230 40" size="65 20"
        style="PlainText"
        label="menu"
        vq="at"
        vc="parunits('set','at','$VALUE')"
        set="parunits('get','at'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <entry loc="250 110" size="45 20"
        style="PlainText"
        vq="ss"
        vc="ss=$VALUE"
        set="$VALUE=ss"
        show="if ss=0 then $VALUE=0 else $VALUE=1 endif"
        disable="Grayed out"
        />
      <entry loc="230 180" size="65 20"
        style="PlainText"
        vq="tpwr"
        vc="tpwr=$VALUE"
        set="$VALUE=tpwr"
        disable="Grayed out"
        />
      <menu loc="230 160" size="65 20"
        style="PlainText"
        vq="d1"
        vc="parunits('set','d1','$VALUE')"
        set="parunits('get','d1'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="s"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <button loc="230 140" size="65 20"
        style="Heading2"
        label="Find gain"
        vc="gain2Dproc"
        bg=""
        decor1=""
        />
      <label loc="10 180" size="155 20"
        style="Label1"
        label="Observe Pulse / at power"
        justify="Left"
        />
      <label loc="20 60" size="100 20"
        style="Label1"
        label="complex points"
        justify="Left"
        />
      <entry loc="170 180" size="60 20"
        style="PlainText"
        vq="pw"
        vc="vnmrunits('set','pw',$VALUE)"
        set="vnmrunits('get','pw'):$VALUE"
        show="if (pw=0) then $VALUE=0 else $VALUE=1 endif"
        digits="2"
        disable="Grayed out"
        />
      <label loc="60 110" size="70 20"
        style="Label1"
        label="Completed"
        justify="Left"
        />
      <textmessage loc="130 110" size="40 20"
        style="PlainText"
        vq="ct"
        set="$VALUE=ct"
        />
      <label loc="60 90" size="70 20"
        style="Label1"
        label="Requested"
        justify="Left"
        />
      <label loc="10 90" size="50 20"
        style="Label1"
        label="Scans:"
        justify="Left"
        />
      <entry loc="170 160" size="60 20"
        style="PlainText"
        vq="d1"
        vc="vnmrunits('set','d1',$VALUE)"
        set="vnmrunits('get','d1'):$VALUE"
        show="if (d1=0) then $VALUE=0 else $VALUE=1 endif"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="170 140" size="60 20"
        style="PlainText"
        vq="gain"
        vc="gain=$VALUE"
        set="$VALUE=gain"
        show="on('gain'):$VALUE"
        disable="Grayed out"
        />
      <label loc="10 160" size="110 20"
        style="Label1"
        label="Relaxation delay"
        justify="Left"
        />
      <label loc="10 140" size="90 20"
        style="Label1"
        label="Receiver Gain"
        justify="Left"
        />
      <label loc="170 110" size="80 20"
        style="Label1"
        label="Steady-State"
        justify="Left"
        />
      <entry loc="170 90" size="60 20"
        style="PlainText"
        vq="nt"
        vc="nt=$VALUE"
        set="$VALUE=nt"
        disable="Grayed out"
        />
      <entry loc="170 60" size="60 20"
        style="PlainText"
        vq="np"
        vc="np=$VALUE*2"
        set="$VALUE=( np / 2)"
        disable="Grayed out"
        />
      <entry loc="170 40" size="60 20"
        style="PlainText"
        vq="at"
        vc="vnmrunits('set','at',$VALUE)"
        set="vnmrunits('get','at'):$VALUE"
        digits="3"
        disable="Grayed out"
        />
      <entry loc="170 20" size="60 20"
        style="PlainText"
        vq="sw"
        vc="vnmrunits('set','sw',$VALUE)"
        set="vnmrunits('get','sw'):$VALUE"
        digits="1"
        disable="Grayed out"
        />
      <label loc="10 40" size="100 20"
        style="Label1"
        label="Acquisition time"
        justify="Left"
        />
      <label loc="10 20" size="100 20"
        style="Label1"
        label="Spectral width"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="390 25"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 25"
        style="PlainText"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
  </group>
</template>
