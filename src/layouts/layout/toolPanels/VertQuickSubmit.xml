<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="QuickSubmit" element="pages" type="processing" >
  <group size="310 730"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    label="QuickSubmit"
    tab="no"
    enable="no"
    reference="QuickSubmit"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
<!-- Common display stuff -->
    <textmessage loc="20 5" size="290 25"
      style="Info"
      vq="QSpar"
      set="if (QSpar[16]='autodir') then if (QSpar[7]='random') then $VALUE='Next available location: '+QSpar[4] else $VALUE='Submit to location: '+QSpar[4] endif elseif (QSpar[16]='bgauto') then $VALUE='Submission to background' else $VALUE='Submission to acquisition' endif"
      />
    <textmessage loc="20 30" size="280 20"
      style="Info"
      vq="operator QSpar"
       set="$VALUE='Operator: '+operator"
      />
    <textmessage loc="20 50" size="290 20"
      style="Info"
      vq="QSpar"
      set="$VALUE='Active study: '+QSpar[22]"
      />
    <button loc="20 75" size="135 25"
      style="Heading2"
      vq="QSpar"
      label="New study"
      vc="QuickSubmit('NewStudy')"
      decor1="no"
      />
      <button loc="160 75" size="135 25"
        style="Heading2"
        vq="QSpar file archivedir sample autodir seqfil opentray"
        label="Continue study"
        vc="QuickSubmit('ContinueStudy')"
        show="if (opentray[1]='close') then is_cpsample:$ret $ENABLE=($ret&gt;0)*2-1 else $ENABLE=1 endif"
        decor1="no"
        />
<!-- Parameter group - Displayed only if NOT in admin mode -->
    <!-- reqpar greater than 1 -->
    <group loc="5 115" size="300 135"
      vq="QSpar owner operator"
      show="substr(QSpar[1],'wc'):$ok $SHOW=((QSpar[19]&lt;&gt;'admin') and (QSpar[18]&lt;&gt;'y') and ($ok&gt;1))"
      label="Parameters"
      style="Heading1"
