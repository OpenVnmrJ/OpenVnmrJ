        set="$VALUE='' QuickSubmit('get','solvent'):$VALUE"
        show="$ENABLE=((QSpar[15]='') and ((QSpar[23]='') or (QSpar[23]='NewStudy')))*2-1"
        file="$VALUE=userdir+'/persistence/QuickSubmit/'+operator+'/QSmenu2'"
