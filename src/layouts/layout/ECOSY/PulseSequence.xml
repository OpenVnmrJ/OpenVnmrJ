<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="670 220"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="25 30" size="170 125"
      style="Heading1"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sspulgroup"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <label loc="136 85" size="30 20"
        style="Heading1"
        label="ms"
        justify="Left"
        />
      <label loc="11 1" size="130 20"
        style="Label1"
        label="Steady State"
        justify="Left"
        />
      <check loc="11 23" size="150 20"
        style="Label1"
        label="Grad-90-Grad option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='y')"
        />
      <check loc="11 103" size="150 20"
        style="Label1"
        label="trim(x)-trim(y) option"
        vq="sspul PFGflg"
        vc="sspul='y'"
        vc2="sspul='n'"
        set="$VALUE=(sspul='y' and PFGflg='n')"
        />
      <label loc="21 81" size="60 20"
        style="Label1"
        label=" time"
        justify="Left"
        />
      <label loc="21 61" size="60 20"
        style="Label1"
        label="strength"
        justify="Left"
        />
      <label loc="11 41" size="70 20"
        style="Label1"
        label="Gradient -"
        justify="Left"
        />
      <entry loc="81 81" size="60 20"
        style="PlainText"
        vq="hsgt sspul PFGflg"
        vc="hsgt=$VALUE/1000"
        set="$VALUE=hsgt*1000"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
      <entry loc="81 61" size="60 20"
        style="PlainText"
        vq="hsglvl sspul PFGflg"
        vc="hsglvl=$VALUE"
        set="$VALUE=hsglvl"
        show="$ENABLE=(sspul='y' and PFGflg='y')"
        digits="0"
        disable="Grayed out"
        />
    </group>
    <button loc="440 0" size="90 20"
      style="Heading2"
      label="Arrays"
      vc="vnmrjcmd('array','popup')"
      decor1=""
      />
    <group loc="10 0" size="390 20"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="0 0" size="240 20"
        style="PlainText"
        label="COSY"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
      <button loc="250 0" size="130 20"
        style="Heading2"
        label="Display Sequence"
        vc="dps"
        decor1=""
        />
    </group>
  </group>
</template>
