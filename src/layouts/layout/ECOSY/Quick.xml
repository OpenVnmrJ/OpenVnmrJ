<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="670 260"
    style="Heading1"
    label="Defaults"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Basic"
    expanded="yes"
    >
    <group loc="400 35" size="235 220"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="custompar5"
      useref="no"
      subtype="Convenience"
      expanded="yes"
      >
      <group loc="10 25" size="215 180"
        vq="customflag"
        show="$SHOW=(customflag='y')"
        bg=""
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="customizepar2"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <label loc="0 155" size="215 20"
          style="Heading1"
          label="More Options: PlotProcAdv page"
          justify="Center"
          />
        <textmessage loc="140 125" size="65 20"
          style="PlainText"
          vq="SQalock SQwshim"
          set="$VALUE=SQalock+' / '+SQwshim"
          />
        <label loc="5 85" size="125 20"
          style="Label1"
          label="Samplename:"
          />
        <label loc="5 125" size="130 20"
          style="Label1"
          label="start of Q lock/shim?"
          />
        <textmessage loc="15 105" size="190 20"
          style="PlainText"
          vq="samplename"
          set="$VALUE=samplename"
          />
        <check loc="125 25" size="85 20"
          style="Label1"
          label=" Autoplot"
          vq="pltopt"
          vc="pltopt='y'"
          vc2="pltopt='n'"
          set="$VALUE=(pltopt='y')"
          />
        <check loc="125 5" size="85 20"
          style="Label1"
          label=" Re-shim"
          vq="wshim"
          vc="wshim='e'"
          vc2="wshim='n'"
          set="if (wshim='n') then $VALUE=0 else $VALUE=1 endif"
          />
        <textmessage loc="5 5" size="125 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='Before '+pslabel+' acq :'"
          />
        <textmessage loc="5 25" size="115 20"
          style="PlainText"
          vq="pslabel"
          set="$VALUE='After '+pslabel+' acq :'"
          />
        <label loc="5 45" size="85 20"
          style="Label1"
          label="Starting with:"
          />
        <textmessage loc="10 65" size="200 20"
          style="PlainText"
          vq="strtpar REQexp chreqexp"
          set="if (strtpar='') or (strtpar='other') or (strtpar='default') then $VALUE=REQexp[1] else $VALUE=strtpar endif"
          />
      </group>
      <label loc="39 0" size="125 20"
        style="Label1"
        label="Receiver Gain (dB):"
        justify="Left"
        />
      <textmessage loc="169 0" size="35 20"
        style="PlainText"
        vq="gain"
        set="$VALUE=gain"
        digits="0"
        />
    </group>
    <group loc="5 35" size="395 220"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      useref="no"
      subtype="Basic"
      >
      <group loc="55 40" size="285 50"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        reference="ECOSYacq"
        useref="no"
        subtype="Basic"
        expanded="yes"
        >
        <menu loc="180 25" size="90 20"
          style="PlainText"
          label="menu"
          vq="ni"
          vc="ni=$VALUE setLP1andWindow"
          set="$VALUE=ni"
          editable="No"
          >
          <mlabel 
            label="200"
            chval="200"
            />
          <mlabel 
            label="128"
            chval="128"
            />
          <mlabel 
            label="256"
            chval="256"
            />
          <mlabel 
            label="512"
            chval="512"
            />
        </menu>
        <label loc="10 5" size="165 20"
          style="Label1"
          label="Number of scans per t1 inc.:"
          justify="Left"
          />
        <label loc="65 25" size="105 20"
          style="Label1"
          label="Number of  t1 inc.:"
          justify="Left"
          />
        <menu loc="180 5" size="90 20"
          style="PlainText"
          label="menu"
          vq="nt"
          vc="nt=$VALUE"
          set="$VALUE=nt"
          editable="No"
          >
          <mlabel 
            label="8"
            chval="8"
            />
          <mlabel 
            label="4"
            chval="4"
            />
          <mlabel 
            label="16"
            chval="16"
            />
          <mlabel 
            label="32"
            chval="32"
            />
        </menu>
      </group>
      <label loc="115 0" size="140 20"
        style="Label1"
        label="Acquisition Options"
        justify="Center"
        />
    </group>
    <group loc="5 5" size="610 25"
      bg=""
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="homcordisplay"
      useref="no"
      subtype="Basic"
      expanded="yes"
      >
      <textmessage loc="330 0" size="270 25"
        style="PlainText"
        label="H1--H1 Correlation         [ Decoupler: C13 ]"
        vq="tn dn"
        set="$VALUE=tn+'--'+tn+' Correlation         [ Decoupler: '+dn+' ]'"
        />
      <textmessage loc="5 0" size="310 25"
        style="PlainText"
        label="Experiment: ECOSY   Solvent: dmso"
        vq="pslabel"
        set="$VALUE='Experiment: '+pslabel+'   Solvent: '+solvent"
        />
    </group>
  </group>
</template>
