<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="0 0" size="660 290"
    style="Heading1"
    label="SolvSuppM"
    vq="panellevel"
    show="$SHOW=((panellevel&gt;29) and (Console='mercury'))"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="SolventSuppress"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="420 30" size="220 110"
      style="PlainText"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="presatgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="5 5" size="160 20"
        style="Heading1"
        label="Presaturation"
        vq="satmode"
        vc="satmode='y'"
        vc2="satmode='n'"
        set="$VALUE=(satmode='y')"
        />
