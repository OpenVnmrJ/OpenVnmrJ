          style="PlainText"
          vq="satpwr satmode"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          show="$ENABLE=(satmode='y')"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="110 20" size="60 20"
          style="PlainText"
          vq="satfrq satmode"
          vc="satfrq=$VALUE"
          set="$VALUE=satfrq"
          show="$ENABLE=(satmode='y')"
