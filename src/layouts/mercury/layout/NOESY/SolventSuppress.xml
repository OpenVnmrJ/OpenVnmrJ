<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="0 0" size="695 280"
    style="Heading1"
    label="SolvSupp"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="SolventSuppress"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="15 35" size="410 240"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="wetgroup2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="10 30" size="385 205"
        vq="wet"
        show="if (wet='y') then $SHOW=1 else $SHOW=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="5 90" size="95 20"
          style="Label1"
          label="Delay after WET"
          justify="Left"
          />
        <entry loc="105 90" size="60 20"
          style="PlainText"
          vq="dz "
          vc="dz=$VALUE/1e6"
          set="$VALUE=dz*1e6"
          disable="Grayed out"
          />
        <label loc="170 90" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <label loc="225 90" size="70 20"
          style="Label1"
          label="Gradient -"
          justify="Left"
          />
        <label loc="240 110" size="55 20"
          style="Label1"
          label="Strength"
          justify="Left"
          />
        <entry loc="295 110" size="50 20"
          style="PlainText"
          vq="gzlvlw"
          vc="gzlvlw=$VALUE"
          set="$VALUE=gzlvlw"
          digits="0"
          disable="Grayed out"
          />
        <label loc="240 130" size="45 20"
          style="Label1"
          label="Time"
          justify="Left"
          />
        <entry loc="295 130" size="50 20"
          style="PlainText"
          vq="gtw wet"
          vc="gtw=$VALUE/1000"
          set="$VALUE=gtw*1000"
          digits="0"
          disable="Grayed out"
          />
        <label loc="350 130" size="25 20"
          style="Label1"
          label="ms"
          justify="Left"
          />
        <textmessage loc="40 145" size="170 20"
          style="PlainText"
          label="C13  decoupling during WET?"
          vq="dn"
          set="$VALUE=dn+'  decoupling during WET?'"
          />
        <check loc="20 145" size="25 20"
          style="PlainText"
          vq="c13wet"
          vc="c13wet='y'"
          vc2="c13wet='n'"
          set="if (c13wet='y') then $VALUE=1 else $VALUE=0 endif"
          />
        <group loc="15 165" size="330 40"
          vq="c13wet"
          show="if (c13wet='y') then $SHOW=1 else $SHOW=0 endif"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <entry loc="245 20" size="60 20"
            style="PlainText"
            vq="decwavewet "
            vc="decwavewet='$VALUE'"
            set="$VALUE=decwavewet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="245 0" size="60 20"
            style="PlainText"
            vq="dmfwet "
            vc="dmfwet=$VALUE/1e6"
            set="$VALUE=1e6/dmfwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="310 0" size="20 20"
            style="Label1"
            label="us"
            justify="Left"
            />
          <label loc="145 0" size="95 20"
            style="Label1"
            label="pi/2 pulse width"
            justify="Left"
            />
          <label loc="145 20" size="90 20"
            style="Label1"
            label="Modulation"
            justify="Left"
            />
          <entry loc="80 20" size="60 20"
            style="PlainText"
            vq="dpwrwet "
            vc="dpwrwet=$VALUE"
            set="$VALUE=dpwrwet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="80 0" size="60 20"
            style="PlainText"
            vq="dofwet "
            vc="dofwet=$VALUE"
            set="$VALUE=dofwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="5 20" size="75 20"
            style="Label1"
            label="Power"
            justify="Left"
            />
          <label loc="5 0" size="75 20"
            style="Label1"
            label="Offset"
            justify="Left"
            />
        </group>
        <entry loc="75 60" size="70 20"
          style="PlainText"
          vq="wetpwr1"
          vc="wetpwr1=$VALUE"
          set="$VALUE=wetpwr1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="75 40" size="70 20"
          style="PlainText"
          vq="pwwet1"
          vc="pwwet1=$VALUE"
          set="$VALUE=pwwet1"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="145 60" size="65 20"
          style="PlainText"
          vq="wetpwr2"
          vc="wetpwr2=$VALUE"
          set="$VALUE=wetpwr2"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="145 40" size="65 20"
          style="PlainText"
          vq="pwwet2"
          vc="pwwet2=$VALUE"
          set="$VALUE=pwwet2"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="210 60" size="70 20"
          style="PlainText"
          vq="wetpwr3"
          vc="wetpwr3=$VALUE"
          set="$VALUE=wetpwr3"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="210 40" size="70 20"
          style="PlainText"
          vq="pwwet3"
          vc="pwwet3=$VALUE"
          set="$VALUE=pwwet3"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="280 60" size="65 20"
          style="PlainText"
          vq="wetpwr4"
          vc="wetpwr4=$VALUE"
          set="$VALUE=wetpwr4"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="280 40" size="65 20"
          style="PlainText"
          vq="pwwet4"
          vc="pwwet4=$VALUE"
          set="$VALUE=pwwet4"
          digits="1"
          disable="Grayed out"
          />
        <label loc="350 40" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="210 20" size="135 20"
          style="PlainText"
          vq="wetshape4"
          vc="wetshape4='$VALUE'"
          set="$VALUE=wetshape4"
          disable="Grayed out"
          />
        <entry loc="75 20" size="135 20"
          style="PlainText"
          vq="wetshape3"
          vc="wetshape3='$VALUE'"
          set="$VALUE=wetshape3"
          disable="Grayed out"
          />
        <entry loc="210 0" size="135 20"
          style="PlainText"
          vq="wetshape2"
          vc="wetshape2='$VALUE'"
          set="$VALUE=wetshape2"
          disable="Grayed out"
          />
        <entry loc="75 0" size="135 20"
          style="PlainText"
          vq="wetshape 1"
          vc="wetshape1='$VALUE'"
          set="$VALUE=wetshape1"
          disable="Grayed out"
          />
        <label loc="5 40" size="55 20"
          style="Label1"
          label="Widths"
          justify="Left"
          />
        <label loc="5 60" size="60 20"
          style="Label1"
          label="Powers"
          justify="Left"
          />
        <label loc="4 1" size="60 20"
          style="Label1"
          label="Shapes"
          justify="Left"
          />
      </group>
      <check loc="195 5" size="155 20"
        style="PlainText"
        label="After evolution/mixing"
        vq="wet"
        vc="$wet1='n' substr(wet,1,1):$wet1 wet=$wet1+'y'"
        vc2="$wet1='n' substr(wet,1,1):$wet1 wet=$wet1+'n'"
        set="$wet3='' substr(wet,2,1):$wet3 if ($wet3='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="55 5" size="135 20"
        style="PlainText"
        label="Before preparation"
        vq="wet"
        vc="$wet3='n' substr(wet,2,1):$wet3 wet='y'+$wet3"
        vc2="$wet3='n' substr(wet,2,1):$wet3 wet='n'+$wet3"
        set="$wet1='' substr(wet,1,1):$wet1 if ($wet1='y') then $VALUE=1 else $VALUE=0 endif"
        />
      <label loc="10 5" size="45 20"
        style="Heading1"
        label="WET:"
        justify="Left"
        />
    </group>
    <group loc="10 0" size="425 25"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="240 25"
        style="Info"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="440 30" size="240 180"
      style="Heading1"
      label="Presaturation"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="presatgroup2"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <label loc="30 125" size="70 20"
        style="Label1"
        label="Mixing time"
        justify="Left"
        />
      <label loc="190 125" size="30 20"
        style="Label1"
        label="ms"
        justify="Left"
        />
      <check loc="10 150" size="200 20"
        style="PlainText"
        label="During relaxation and mixing"
        vq="satmode"
        vc="satmode='yy'"
        vc2="satmode='nn'"
        set="if (satmode='yy') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 105" size="200 20"
        style="PlainText"
        label="During mixing only"
        vq="satmode"
        vc="satmode='ny'"
        vc2="satmode='nn'"
        set="if (satmode='ny') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="25 40" size="205 65"
        vq="satmode"
        show="if (satmode='n') or (satmode='nn') then $SHOW=0 else $SHOW=1 endif"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="165 20" size="30 20"
          style="Label1"
          label="Hz"
          justify="Left"
          />
        <label loc="165 0" size="30 20"
          style="Label1"
          label="s"
          justify="Left"
          />
        <entry loc="100 40" size="60 20"
          style="PlainText"
          vq="satpwr satmode"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          show="if (satmode='y') or (satmode='ny') or (satmode='yy') or (satmode='yn')  then  $ENABLE=1 else $ENABLE=0 endif"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="100 20" size="60 20"
          style="PlainText"
          vq="satfrq satmode"
          vc="satfrq=$VALUE"
          set="$VALUE=satfrq"
          show="if (satmode='y') or (satmode='ny') or (satmode='yy') or (satmode='yn')  then  $ENABLE=1 else $ENABLE=0 endif"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="100 0" size="60 20"
          style="PlainText"
          vq="satdly satmode"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          show="if (satmode='y') or (satmode='yn') or (satmode='yy') then  $ENABLE=1 else $ENABLE=0 endif"
          digits="0"
          disable="Grayed out"
          />
        <label loc="5 40" size="70 20"
          style="Label1"
          label="Power"
          justify="Left"
          />
        <label loc="5 20" size="70 20"
          style="Label1"
          label="Frequency"
          justify="Left"
          />
        <label loc="5 0" size="70 20"
          style="Label1"
          label="Delay"
          justify="Left"
          />
      </group>
      <check loc="10 20" size="210 20"
        style="PlainText"
        label="During relaxation only"
        vq="satmode"
        vc="satmode='yn'"
        vc2="satmode='nn'"
        set="if (satmode='y') or (satmode='yn') then $VALUE=1 else $VALUE=0 endif"
        />
      <textmessage loc="120 125" size="65 20"
        style="Info"
        vq="mixN"
        set="$VALUE=mixN*1000"
        digits="1"
        />
    </group>
  </group>
</template>
