<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template element="panels" type="acquisition" >
  <group loc="0 0" size="635 240"
    style="Heading1"
    label="SolvSupp"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="SolventSuppress"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <group loc="400 35" size="220 190"
      style="Heading1"
      label="Presaturation"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="presatgrouphsqc"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <check loc="10 160" size="200 20"
        style="PlainText"
        label="During BIRDnull + relaxation"
        vq="satmode"
        vc="satmode='yy'"
        vc2="satmode='nn'"
        set="if (satmode='yy') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 110" size="200 20"
        style="PlainText"
        label="During BIRDnull only"
        vq="satmode"
        vc="satmode='ny'"
        vc2="satmode='nn'"
        set="if (satmode='ny') then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 15" size="175 20"
        style="PlainText"
        label="During relaxation only"
        vq="satmode"
        vc="satmode='yn'"
        vc2="satmode='nn'"
        set="if (satmode='y') or (satmode='yn') then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="5 40" size="205 65"
        vq="satmode"
        show="if (satmode='n') or (satmode='nn') then $SHOW=0 else $SHOW=1 endif"
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <label loc="175 25" size="25 20"
          style="PlainText"
          label="Hz"
          justify="Left"
          />
        <label loc="175 5" size="25 20"
          style="PlainText"
          label="s"
          justify="Left"
          />
        <entry loc="110 45" size="60 20"
          style="PlainText"
          vq="satpwr satmode"
          vc="satpwr=$VALUE"
          set="$VALUE=satpwr"
          digits="0"
          disable="Grayed out"
          />
        <label loc="10 45" size="70 20"
          style="PlainText"
          label="Power"
          justify="Left"
          />
        <label loc="10 25" size="70 20"
          style="PlainText"
          label="Frequency"
          justify="Left"
          />
        <entry loc="110 25" size="60 20"
          style="PlainText"
          vq="satfrq satmode"
          vc="satfrq=$VALUE"
          set="$VALUE=satfrq"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="110 5" size="60 20"
          style="PlainText"
          vq="satdly satmode"
          vc="satdly=$VALUE"
          set="$VALUE=satdly"
          show="if (satmode='y') or (satmode='yn') or (satmode='yy') then  $ENABLE=1 else $ENABLE=0 endif"
          digits="0"
          disable="Grayed out"
          />
        <label loc="10 5" size="70 20"
          style="PlainText"
          label="Delay"
          justify="Left"
          />
      </group>
      <label loc="55 130" size="65 20"
        style="PlainText"
        label="Null delay"
        justify="Left"
        />
      <label loc="190 130" size="25 20"
        style="PlainText"
        label="s"
        justify="Left"
        />
      <textmessage loc="120 130" size="65 20"
        style="PlainText"
        vq="null"
        set="$VALUE=null"
        show="if (null &gt; 0) then $ENABLE=1 else $ENABLE=0 endif"
        digits="3"
        />
    </group>
    <group loc="10 5" size="390 25"
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="SeqDisplay2"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <textmessage loc="0 0" size="240 25"
        style="Info"
        vq="pslabel"
        set="$VALUE=pslabel"
        />
    </group>
    <group loc="5 35" size="385 190"
      style="Heading1"
      border="RaisedBevel"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="wetgroup"
      useref="no"
      subtype="Titled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <group loc="5 20" size="365 165"
        vq="wet"
        show="if (wet='y') then $SHOW=1 else $SHOW=0 endif"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <group loc="15 115" size="330 40"
          vq="c13wet"
          show="if (c13wet='y') then $SHOW=1 else $SHOW=0 endif"
          border="None"
          side="Top"
          justify="Left"
          tab="no"
          enable="no"
          useref="no"
          subtype="Untitled"
          rows="1"
          columns="1"
          >
          <label loc="310 0" size="20 20"
            style="Label1"
            label="us"
            justify="Left"
            />
          <entry loc="250 0" size="60 20"
            style="PlainText"
            vq="dmfwet "
            vc="dmfwet=$VALUE/1e6"
            set="$VALUE=1e6/dmfwet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="251 19" size="60 20"
            style="PlainText"
            vq="decwavewet "
            vc="decwavewet='$VALUE'"
            set="$VALUE=decwavewet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="145 0" size="95 20"
            style="Label1"
            label="pi/2 pulse width"
            justify="Left"
            />
          <label loc="145 20" size="90 20"
            style="Label1"
            label="Modulation"
            justify="Left"
            />
          <entry loc="80 20" size="60 20"
            style="PlainText"
            vq="dpwrwet "
            vc="dpwrwet=$VALUE"
            set="$VALUE=dpwrwet"
            digits="1"
            disable="Grayed out"
            />
          <entry loc="80 0" size="60 20"
            style="PlainText"
            vq="dofwet "
            vc="dofwet=$VALUE"
            set="$VALUE=dofwet"
            digits="1"
            disable="Grayed out"
            />
          <label loc="5 20" size="75 20"
            style="Label1"
            label="Power"
            justify="Left"
            />
          <label loc="5 0" size="75 20"
            style="Label1"
            label="Offset"
            justify="Left"
            />
        </group>
        <textmessage loc="30 90" size="170 20"
          style="PlainText"
          label="C13  decoupling during WET?"
          vq="dn"
          set="$VALUE=dn+'  decoupling during WET?'"
          />
        <check loc="15 90" size="25 20"
          style="PlainText"
          vq="c13wet"
          vc="c13wet='y'"
          vc2="c13wet='n'"
          set="if (c13wet='y') then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="329 61" size="25 20"
          style="PlainText"
          label="ms"
          justify="Left"
          />
        <entry loc="104 1" size="225 20"
          style="PlainText"
          vq="wetshape "
          vc="wetshape='$VALUE'"
          set="$VALUE=wetshape"
          disable="Grayed out"
          />
        <label loc="4 1" size="100 20"
          style="Label1"
          label="WET pulse shape"
          justify="Left"
          />
        <entry loc="275 60" size="50 20"
          style="PlainText"
          vq="gtw wet"
          vc="gtw=$VALUE/1000"
          set="$VALUE=gtw*1000"
          digits="0"
          disable="Grayed out"
          />
        <entry loc="275 40" size="50 20"
          style="PlainText"
          vq="gzlvlw"
          vc="gzlvlw=$VALUE"
          set="$VALUE=gzlvlw"
          digits="0"
          disable="Grayed out"
          />
        <label loc="215 60" size="60 20"
          style="Label1"
          label="Time"
          justify="Left"
          />
        <label loc="215 40" size="60 20"
          style="Label1"
          label="Strength"
          justify="Left"
          />
        <label loc="205 20" size="70 20"
          style="Label1"
          label="Gradient -"
          justify="Left"
          />
        <label loc="170 60" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="105 60" size="60 20"
          style="PlainText"
          vq="dz "
          vc="dz=$VALUE/1e6"
          set="$VALUE=dz*1e6"
          disable="Grayed out"
          />
        <label loc="5 60" size="95 20"
          style="Label1"
          label="Delay after WET"
          justify="Left"
          />
        <label loc="5 40" size="75 20"
          style="Label1"
          label="Pulse power"
          justify="Left"
          />
        <entry loc="105 40" size="60 20"
          style="PlainText"
          vq="wetpwr "
          vc="wetpwr=$VALUE"
          set="$VALUE=wetpwr"
          digits="1"
          disable="Grayed out"
          />
        <label loc="170 20" size="20 20"
          style="Label1"
          label="us"
          justify="Left"
          />
        <entry loc="105 20" size="60 20"
          style="PlainText"
          vq="pwwet "
          vc="pwwet=$VALUE"
          set="$VALUE=pwwet"
          digits="1"
          disable="Grayed out"
          />
        <label loc="5 20" size="75 20"
          style="Label1"
          label="Pulse width"
          justify="Left"
          />
      </group>
      <check loc="10 0" size="120 20"
        style="Heading1"
        label="WET"
        vq="wet"
        vc="wet='y'"
        vc2="wet='n'"
        set="if (wet='y') then $VALUE=1 else $VALUE=0 endif"
        />
    </group>
  </group>
</template>
