<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="NusSampling" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="NUS Sampling"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="NusSampling"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="10 20" size="360 170"
      vq="SPARSE"
      show="if(SPARSE='y') then $SHOW=1 else $SHOW=0 endif BPchkseq:$v if $v=1 then $SHOW=0 endif"
      bg=""
      border="None"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="sparse_acquire"
      useref="no"
      subtype="Minor"
      expanded="yes"
      >
      <button loc="10 150" size="80 20"
        style="Heading2"
        label="manual"
        vc="man('SPARSE')"
        bg=""
        decor1=""
        />
      <label loc="130 150" size="220 20"
        style="Label1"
        label="NLS Software by Eriks Kupce, Agilent"
        />
      <radio loc="300 100" size="60 20"
        style="Label1"
        label="Other"
        vq="stype"
        vc="stype='n'"
        vc2="stype='a'"
        />
      <radio loc="240 100" size="50 20"
        style="Label1"
        label="Auto"
        vq="stype"
        vc="stype='a'"
        vc2="stype='n'"
        set="if stype='a' then $VALUE=1 else $VALUE=0 endif"
        />
      <group loc="300 10" size="50 90"
        vq="ni3"
        show="exists('ni3','parameter'):$SHOW"
        bg=""
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 70" size="50 20"
          style="Label1"
          vq="phase3"
          vc="phase3=1,2"
          vc2="phase3=1"
          set="$s=size('phase3') if($s&gt;1) then $VALUE=1 else $VALUE=0 endif"
          />
        <entry loc="0 50" size="50 20"
          style="PlainText"
          vq="ni3max phase3 CSdensity"
          vc="ni3max=$VALUE phase3=1,2 CSdensity:CSdensity"
          set="$VALUE=ni3max"
          disable="Grayed out"
          />
        <entry loc="0 30" size="50 20"
          style="PlainText"
          vq="ni3"
          vc="ni3=$VALUE"
          set="$VALUE=ni3"
          disable="Grayed out"
          />
        <label loc="0 8" size="50 20"
          style="Label1"
          label="ni3 (t3)"
          justify="Left"
          />
      </group>
      <group loc="240 10" size="50 90"
        vq="ni2"
        show="exists('ni2','parameter'):$SHOW"
        bg=""
        border="None"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        >
        <check loc="0 70" size="50 20"
          style="Label1"
          vq="phase2"
          vc="phase2=1,2"
          vc2="phase2=1"
          set="$s=size('phase2') if($s&gt;1) then $VALUE=1 else $VALUE=0 endif"
          />
        <entry loc="0 50" size="50 20"
          style="PlainText"
          vq="ni2max phase2 CSdensity"
          vc="ni2max=$VALUE phase2=1,2 CSdensity:CSdensity"
          set="$VALUE=ni2max"
          disable="Grayed out"
          />
        <entry loc="0 30" size="50 20"
          style="PlainText"
          vq="ni2"
          vc="ni2=$VALUE"
          set="$VALUE=ni2"
          disable="Grayed out"
          />
        <label loc="0 8" size="50 20"
          style="Label1"
          label="ni2 (t2)"
          justify="Left"
          />
      </group>
      <check loc="270 120" size="90 20"
        style="Label1"
        label="Sequential"
        vq="skey stype"
        vc="skey=-skey"
        vc2="skey=-skey"
        set="if(skey&gt;0) then $VALUE=1 else $VALUE=0 endif"
        show="if(stype='a') or (stype='y') then $SHOW = 1 else $SHOW = 0 endif"
        />
      <label loc="230 120" size="30 20"
        style="Label1"
        label="lines"
        justify="Left"
        />
      <entry loc="190 120" size="30 20"
        style="PlainText"
        vq="CSlines"
        vc="CSlines=$VALUE"
        set="$VALUE=CSlines"
        show="exists(curexp+'/sampling.sch','file'):$SHOW"
        disable="Grayed out"
        />
      <button loc="130 120" size="50 20"
        style="Heading2"
        label="Show"
        vq="CSlines"
        vc="clear print_sch(CSlines) "
        show="exists(curexp+'/sampling.sch','file'):$SHOW"
        bg=""
        decor1=""
        />
      <label loc="190 100" size="20 20"
        style="Label1"
        label="%"
        />
      <textmessage loc="130 100" size="50 20"
        style="PlainText"
        label="100.00"
        vq="CSdensity"
        set="CSdensity:$VALUE"
        digits="2"
        />
      <label loc="10 120" size="120 20"
        style="Label1"
        label="Sampling Schedule:"
        />
      <label loc="10 100" size="120 20"
        style="Label1"
        label="Sampling Density:"
        />
      <label loc="10 0" size="210 20"
        style="Label1"
        label="Set Up Sampling Schedule:"
        />
      <label loc="10 40" size="170 20"
        style="Label1"
        label="Number of Sampling Points:"
        justify="Left"
        />
      <label loc="10 60" size="160 20"
        style="Label1"
        label="Final Data Size:"
        justify="Left"
        />
      <label loc="10 80" size="160 20"
        style="Label1"
        label="Hypercomplex:"
        justify="Left"
        />
      <check loc="180 80" size="50 20"
        style="Label1"
        vq="phase"
        vc="phase=1,2"
        vc2="phase=1"
        set="$s=size('phase') if($s&gt;1) then $VALUE=1 else $VALUE=0 endif"
        />
      <entry loc="180 60" size="50 20"
        style="PlainText"
        vq="nimax phase CSdensity"
        vc="nimax=$VALUE phase=1,2 CSdensity:CSdensity"
        set="$VALUE=nimax"
        disable="Grayed out"
        />
      <entry loc="180 40" size="50 20"
        style="PlainText"
        vq="ni phase"
        vc="ni=$VALUE"
        set="$VALUE=ni"
        disable="Grayed out"
        />
      <label loc="180 20" size="40 20"
        style="Label1"
        label="ni (t1)"
        justify="Left"
        />
    </group>
    <selmenu loc="10 0" size="360 20"
      style="Heading4"
      label="Select Indirect Dimension Sampling"
      vq="SPARSE"
      vc="exec('$VALUE')"
      bg="VJBackground"
      editable="No"
      >
      <mlabel 
        label="Sparse (Kupce) NLS Sampling"
        chval="SPARSE='y'"
        />
      <mlabel 
        label="Linear (Normal) Sampling"
        chval="SPARSE='n'"
        />
    </selmenu>
  </group>
</template>
