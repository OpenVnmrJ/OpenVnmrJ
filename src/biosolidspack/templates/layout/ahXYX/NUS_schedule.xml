          vc="exists('nimax','parameter'):$e if ($e=1) then nimax=$VALUE else exists('ni','parameter'):$e if ($e=1) then ni=$VALUE endif endif"
          set="$VALUE=0 on('nimax'):$e if ($e&gt;=0) then $VALUE=nimax else on('ni'):$e  if ($e&gt;=0) then $VALUE=ni endif  endif"
          show="on('ni'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          disable="Grayed out"
          />
        <check loc="8 60" size="50 20"
          style="PlainText"
          vq="phase"
          vc="exists('phase','parameter'):$e if $e=1 then phase=1,2 endif"
          vc2="exists('phase','parameter'):$e if $e=1 then phase=1 endif"
          set="$VALUE=0 exists('phase','parameter'):$e if $e=1 then $s=size('phase') if($s&gt;1) then $VALUE=1 endif endif"
          />
        <check loc="8 0" size="50 20"
          style="PlainText"
          label="ni"
          vq="ni"
          vc="ni='y'"
          vc2="ni='n'"
          set="on('ni'):$e if($e=1) then $VALUE=1 else $VALUE=0 endif"
          />
