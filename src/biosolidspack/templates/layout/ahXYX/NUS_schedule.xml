      <check loc="10 120" size="90 20"
        style="PlainText"
        label="Weighted"
        vq="CSwt CStype CSseed"
        vc="CSwt='y' if (CSseed &lt; 0) then CSseed= -CSseed endif"
        vc2="CSwt='n'"
        set="if  CSwt='y' then $VALUE=1 else $VALUE=0 endif"
        />
      <check loc="10 100" size="90 20"
        style="PlainText"
        label="Random"
        vq="CSseed CSwt"
        vc="CSseed=-CSseed if(CSseed &lt; 0) then CSwt='n' endif "
        vc2="CSseed=-CSseed if(CSseed &lt; 0) then CSwt='n' endif"
        set="$VALUE=0 on('CSseed'):$e if $e &gt;0 then if CSseed &lt;0  then $VALUE=1 else $VALUE=0 endif endif"
        show="$VALUE=0 on('CSseed'):$e if $e &gt;0 then $VALUE=1 else $VALUE = -1 endif"
        />
      <group loc="10 160" size="290 50"
        label="Profile"
        bg="transparent"
        border="Etched"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="no"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <button loc="10 10" size="60 30"
          style="Menu4"
          label="Build"
          vc="CSmksch CSext='n' CStype='i'"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="80 10" size="60 30"
          style="Label2"
          label="Delete"
          vc="CSrmsch"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <button loc="150 10" size="60 30"
          style="PlainText"
          label="Show"
          vc="CSdsch"
          bg="transparent"
          decor1="no"
          halignment="Center"
          valignment="Center"
          />
        <check loc="230 10" size="60 20"
          style="PlainText"
          label="Auto"
          vq="CSshowsch  CStype"
          vc="CSshowsch='y'"
          vc2="CSshowsch='n'"
          set="if CSshowsch='y' then $VALUE=1 else $VALUE=0 endif"
