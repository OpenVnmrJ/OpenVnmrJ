        useref="no"
        subtype="Untitled"
        expanded="yes"
        rows="1"
        columns="1"
        >
        <radio loc="0 20" size="90 20"
          style="PlainText"
          label="Local"
          vq="CStype CSext"
          vc="CStype='i' CSext='n'"
          vc2="CStype='i' CSext='n'"
          set="$VALUE=0 if ((CStype='i') and (CSext='n')) then $VALUE=1 endif"
          show="$ENABLE=1 exists('CStype','parameter'):$CStype if ($CStype &lt; 1) then $ENABLE=-1 endif "
          />
        <radio loc="0 40" size="90 20"
          style="PlainText"
          label="External"
          vq="CStype CSext"
          vc="CStype='i' CSext='y'"
          vc2="CStype='i' CSext='n'"
          set="$VALUE=0 if ((CStype='i') and (CSext='y')) then $VALUE=1 endif"
          show="$ENABLE=1 exists('CSext','parameter'):$CSext if ($CSext &lt; 1) then $ENABLE=-1 endif"
          />
        <radio loc="0 0" size="90 20"
          style="PlainText"
          label="Automatic"
          vq="CStype CSext"
          vc="CStype='a' CSext='n'"
          vc2="CStype='i' CSext='n'"
          set="$VALUE=0 if ((CStype='a') and (CSext='n')) then $VALUE=1 endif"
          show="$ENABLE=1 exists('CStype','parameter'):$CStype if ($CStype &lt; 1) then $ENABLE=-1 endif "
          />
      </group>
      <group loc="240 20" size="60 80"
        vq="ni3"
        show="on('ni3'):$e if $e&gt;=0 then $SHOW=1 else $SHOW=0 endif"
        bg="transparent"
        border="None"
        side="Top"
        justify="Left"
        tab="no"
        enable="no"
        useref="false"
        subtype="Untitled"
        rows="1"
        columns="1"
        >
        <check loc="10 0" size="50 20"
          style="PlainText"
          label="ni3"
          vq="ni3"
          vc="ni3='y'"
          vc2="ni3='n'"
          set="on('ni3'):$e if($e=1) then $VALUE=1 else $VALUE=0 endif"
          />
        <entry loc="0 40" size="50 20"
          style="PlainText"
          vq="ni3 ni3max CSdensity3"
          vc="on('ni3max'):$e if ($e&gt;0) then ni3=ni3max*$VALUE/100.0 else on('CSdensity3'):$e  if ($e&gt;0) then CSdensity3=$VALUE endif endif"
          set="$VALUE=0 on('ni3max'):$e if ($e&gt;0) then $VALUE=100*ni3/ni3max else on('CSdensity3'):$e  if ($e&gt;0) then $VALUE=CSdensity3 endif  endif"
          show="on('ni3'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          digits="1"
          disable="Grayed out"
          />
        <entry loc="0 20" size="50 20"
          style="PlainText"
          vq="ni3 ni3max"
          vc="exists('ni3max','parameter'):$e if ($e=1) then ni3max=$VALUE else exists('ni3','parameter'):$e if ($e=1) then ni3=$VALUE endif endif"
          set="$VALUE=0 on('ni3max'):$e if ($e&gt;=0) then $VALUE=ni3max else on('ni3'):$e  if ($e&gt;=0) then $VALUE=ni3 endif  endif"
          show="on('ni3'):$e if($e=1) then $VALUE=1 else $VALUE=-1 endif"
          disable="Grayed out"
          />
        <check loc="10 60" size="50 20"
          style="PlainText"
          vq="phase3"
          vc="exists('phase3','parameter'):$e if $e=1 then phase3=1,2 endif"
          vc2="exists('phase3','parameter'):$e if $e=1 then phase3=1 endif"
          set="$VALUE=0 exists('phase3','parameter'):$e if $e=1 then $s=size('phase3') if($s&gt;1) then $VALUE=1 else $VALUE=0 endif endif"
          />
