      <label loc="10 80" size="110 20"
        style="Label1"
        label="Hypercomplex"
        />
      <label loc="10 60" size="110 20"
        style="Label1"
        label="Sampling density"
        />
      <label loc="10 40" size="110 20"
        style="PlainText"
        label="Domain size"
        justify="Left"
        />
      <label loc="10 20" size="110 20"
        style="Label1"
        label="Domain"
        justify="Left"
        />
    </group>
    <group loc="650 50" size="140 220"
      style="Heading2"
      label="External Schedule"
      vq="CSextschname CSext"
      show="on('CSextschname'):$CSext if ($CSext=1) then if (CSextschname &lt;&gt; '') then $VALUE=1  else $VALUE=0 endif endif"
      bg="transparent"
      border="Etched"
      side="Top"
      justify="Left"
      tab="no"
      enable="no"
      reference="nus_simulate"
      useref="no"
      subtype="Major"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <button loc="10 60" size="60 30"
        style="Menu4"
        label="Get"
        vq="CSext CStype"
        vc="CSextsch('get') CSext='y' CStype='i' sampling='sparse'"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
      <button loc="10 20" size="60 30"
        style="Menu4"
        label="Set"
        vc="CSextsch('set') CSextschname=CSextschname CSext='y' CStype='i' sampling='sparse'"
        bg="transparent"
        decor1="no"
        halignment="Center"
        valignment="Center"
        />
    </group>
    <label loc="510 10" size="100 20"
      style="Label1"
      label="Schedule name"
      justify="Left"
      />
    <check loc="10 10" size="220 20"
      style="Label1"
      label="Enable non-uniform sampling"
      vq="sampling"
      vc="sampling='sparse'   if (CSdensity=100.0) then CSdensity=50.0 endif if (CSdensity2=100.0) then CSdensity2=50.0 endif"
      vc2="sampling='' CSdensity=100.0 CSdensity2=100.0"
      set="if sampling='sparse' then $VALUE=1 else $VALUE=0 endif"
      />
  </group>
</template>
