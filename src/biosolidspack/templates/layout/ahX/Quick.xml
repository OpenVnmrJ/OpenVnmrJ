<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="Quick" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="Defaults"
    bg="transparent"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="Quick"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    rows="1"
    columns="1"
    >
    <button loc="130 60" size="130 20"
      style="Heading2"
      label="Clear Module"
      vc="exists('SPsamplefilename','parameter','global'):$e if ($e&gt;0.5) then if (SPsamplefilename&lt;&gt;'') then reset_ahX('editmod','delete') else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPsamplefilename','global'):$VALUE"
      bg="transparent"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <button loc="130 40" size="130 20"
      style="Heading2"
      label="Edit Module"
      vc="exists('SPsamplefilename','parameter','global'):$e if ($e&gt;0.5) then if (SPsamplefilename&lt;&gt;'') then reset_ahX('editmod','make') else write('line3','ERROR: Set a Module Name') endif endif"
      show="on('SPsamplefilename','global'):$VALUE"
      bg="transparent"
      decor1="yes"
      halignment="Center"
      valignment="Center"
      />
    <entry loc="130 20" size="130 20"
      style="PlainText"
      vq="SPsamplefilename"
      vc="SPsamplefilename='$VALUE' "
      set="$VALUE=SPsamplefilename"
      show="exists('SPsamplefilename','parameter','global'):$VALUE"
      disable="Grayed out"
      />
    <label loc="10 20" size="120 20"
      style="Label1"
      label="Sample Module:"
      />
    <group loc="270 0" size="260 60"
      border="None"
      tab="no"
      enable="no"
      reference="Htppm"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      rows="1"
      columns="1"
      >
      <menu loc="10 20" size="250 20"
        style="PlainText"
        vq="SPspinrateon"
        vc="SPspinrateon='$VALUE'"
        set="$VALUE = SPspinrateon"
        show="on('SPspinrateon','global'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="No Spinrate Measurement"
          chval="n"
          />
        <mlabel 
          label="Spinrate Measurement On"
          chval="y"
          />
      </menu>
      <label loc="10 0" size="250 20"
        style="Label1"
        label="Gobal SolidsPack Controls:"
        />
    </group>
    <textmessage loc="90 0" size="170 20"
      style="PlainText"
      label="ahX"
      vq="seqfil"
      set="$VALUE=seqfil"
      />
    <label loc="10 0" size="80 20"
      style="Label1"
      label="Sequence:"
      />
  </group>
</template>
