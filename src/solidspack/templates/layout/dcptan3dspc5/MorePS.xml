<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="MorePS" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="More"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="MorePS"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <entry loc="70 120" size="60 20"
      style="PlainText"
      vq="aHmix"
      vc="vnmrunits('set','aHmix',$VALUE)"
      set="vnmrunits('get','aHmix'):$VALUE"
      show="on('aHmix'):$VALUE"
      />
    <entry loc="70 100" size="60 20"
      style="PlainText"
      vq="tZF"
      vc="vnmrunits('set','tZF',$VALUE)"
      set="vnmrunits('get','tZF'):$VALUE"
      show="on('tZF'):$VALUE"
      />
    <label loc="10 120" size="60 20"
      style="Label1"
      label="aHmix:"
      />
    <label loc="10 100" size="60 20"
      style="Label1"
      label="tZF:"
      />
    <group loc="0 0" size="260 100"
      label="SPC5"
      border="None"
      tab="no"
      enable="no"
      reference="spc5X"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="200 40" size="40 20"
        style="Label1"
        label="360"
        />
      <label loc="10 80" size="60 20"
        style="Label1"
        label="ofXspc5:"
        />
      <entry loc="70 80" size="60 20"
        style="PlainText"
        vq="ofXspc5"
        vc="vnmrunits('set','ofXspc5',$VALUE)"
        set="vnmrunits('get','ofXspc5'):$VALUE"
        show="on('ofXspc5'):$VALUE"
        />
      <menu loc="130 80" size="60 20"
        style="PlainText"
        label="menu"
        vq="ofXspc5"
        vc="parunits('set','ofXspc5','$VALUE')"
        set="parunits('get','ofXspc5'):$VALUE"
        show="on('ofXspc5'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="Hz"
          chval="Hz"
          />
        <mlabel 
          label="kHz"
          chval="kHz"
          />
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
      </menu>
      <label loc="10 20" size="60 20"
        style="Label1"
        label="aXspc5:"
        />
      <label loc="10 0" size="120 20"
        style="Label1"
        label="spc5X:"
        />
      <menu loc="130 40" size="60 20"
        style="PlainText"
        vq="pwXspc5"
        vc="parunits('set','pwXspc5','$VALUE')"
        set="parunits('get','pwXspc5'):$VALUE"
        show="on('pwXspc5'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <entry loc="70 60" size="60 20"
        style="PlainText"
        vq="qXspc5"
        vc="vnmrunits('set','qXspc5',$VALUE)"
        set="vnmrunits('get','qXspc5'):$VALUE"
        show="on('qXspc5'):$VALUE"
        />
      <entry loc="70 40" size="60 20"
        style="PlainText"
        vq="pwXspc5"
        vc="vnmrunits('set','pwXspc5',$VALUE)"
        set="vnmrunits('get','pwXspc5'):$VALUE"
        show="on('pwXspc5'):$VALUE"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="qXspc5:"
        />
      <label loc="10 40" size="60 20"
        style="Label1"
        label="pwXspc5:"
        />
      <entry loc="70 20" size="60 20"
        style="PlainText"
        vq="aXspc5"
        vc="vnmrunits('set','aXspc5',$VALUE)"
        set="vnmrunits('get','aXspc5'):$VALUE"
        show="on('aXspc5'):$VALUE"
        />
      <textmessage loc="160 0" size="40 20"
        style="PlainText"
        label="C13"
        vq="tn"
        set="$VALUE = tn"
        />
      <label loc="130 0" size="30 20"
        style="Label1"
        label="On:"
        />
    </group>
  </group>
</template>
