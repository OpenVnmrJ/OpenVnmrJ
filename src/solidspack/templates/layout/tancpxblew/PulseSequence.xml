<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="0 0" size="260 120"
      border="None"
      tab="no"
      enable="no"
      reference="autocpHX"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="210 0" size="40 20"
        style="PlainText"
        vq="tn dn dn2 dn3 toHX"
        set="if (toHX='obs') then $VALUE=tn elseif (toHX='dec') then $VALUE=dn elseif (toHX='dec2') then $VALUE=dn2 elseif (toHX='dec3') then $VALUE=dn3 else $VALUE='no' endif"
        show="on('toHX'):$VALUE"
        />
      <menu loc="190 20" size="60 20"
        style="PlainText"
        vq="chHX"
        vc="chHX='$VALUE'"
        set="$VALUE = chHX"
        show="on('chHX'):$VALUE"
        editable="No"
        >
        <mlabel 
          label="from"
          chval="fr"
          />
        <mlabel 
          label="to"
          chval="to"
          />
      </menu>
      <menu loc="120 80" size="60 20"
        style="PlainText"
        vq="tHX"
        vc="parunits('set','tHX','$VALUE')"
        set="parunits('get','tHX'):$VALUE"
        show="on('tHX'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <menu loc="60 20" size="60 20"
        style="PlainText"
        vq="shHX"
        vc="shHX='$VALUE'"
        set="$VALUE=shHX"
        show="on('shHX'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="linear"
          chval="l"
          />
        <mlabel 
          label="const"
          chval="c"
          />
        <mlabel 
          label="tan"
          chval="t"
          />
      </menu>
      <label loc="180 0" size="30 20"
        style="Label1"
        label="To:"
        />
      <textmessage loc="100 0" size="40 20"
        style="PlainText"
        vq="tn dn dn2 dn3 frHX"
        set="if (frHX='obs') then $VALUE=tn elseif (frHX='dec') then $VALUE=dn elseif (frHX='dec2') then $VALUE=dn2 elseif (frHX='dec3') then $VALUE=dn3 else $VALUE='no' endif"
        show="on('frHX'):$VALUE"
        />
      <label loc="60 0" size="40 20"
        style="Label1"
        label="From:"
        />
      <entry loc="180 60" size="60 20"
        style="PlainText"
        vq="dHX shHX"
        vc="dHX = $VALUE"
        set="$VALUE = dHX"
        show="on('dHX'):$dHXon if (($dHXon = 1) and ((shHX ='l')  or (shHX = 't'))) then $VALUE = 1 else $VALUE = 0 endif"
        />
      <entry loc="180 40" size="60 20"
        style="PlainText"
        vq="aXhx"
        vc="aXhx = $VALUE"
        set="$VALUE = aXhx"
        show="on('aXhx'):$VALUE"
        digits="0"
        />
      <label loc="130 60" size="50 20"
        style="Label1"
        label="dHX:"
        />
      <label loc="130 40" size="50 20"
        style="Label1"
        label="aXhx:"
        />
      <menu loc="120 100" size="60 20"
        style="PlainText"
        vq="ofHX"
        vc="parunits('set','ofHX','$VALUE')"
        set="parunits('get','ofHX'):$VALUE"
        show="on('ofHX'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="Hz"
          chval="Hz"
          />
        <mlabel 
          label="kHz"
          chval="kHz"
          />
      </menu>
      <entry loc="60 100" size="60 20"
        style="PlainText"
        vq="ofHX"
        vc="vnmrunits('set','ofHX',$VALUE)"
        set="vnmrunits('get','ofHX'):$VALUE"
        show="on('ofHX'):$VALUE"
        />
      <label loc="10 100" size="50 20"
        style="Label1"
        label="ofHX:"
        />
      <label loc="10 0" size="50 20"
        style="Label1"
        label="cpHX:"
        />
      <label loc="10 20" size="50 20"
        style="Label1"
        label="Shape:"
        />
      <label loc="10 40" size="50 20"
        style="Label1"
        label="aHhx:"
        />
      <label loc="10 80" size="50 20"
        style="Label1"
        label="tHX:"
        />
      <entry loc="60 40" size="60 20"
        style="PlainText"
        vq="aHhx"
        vc="aHhx = $VALUE"
        set="$VALUE = aHhx"
        show="on('aHhx'):$VALUE"
        disable="Grayed out"
        />
      <entry loc="60 60" size="60 20"
        style="PlainText"
        vq="bHX shHX"
        vc="bHX = $VALUE"
        set="$VALUE = bHX"
        show="on('bHX'):$bHXon if (($bHXon = 1) and (shHX = 't')) then $VALUE = 1 else $VALUE = 0 endif"
        />
      <entry loc="60 80" size="60 20"
        style="PlainText"
        vq="tHX"
        vc="vnmrunits('set','tHX',$VALUE)"
        set="vnmrunits('get','tHX'):$VALUE"
        show="on('tHX'):$VALUE"
        />
      <label loc="12 60" size="50 20"
        style="Label1"
        label="bHX:"
        />
      <label loc="132 20" size="60 20"
        style="Label1"
        label="Channel:"
        />
    </group>
    <group loc="270 0" size="260 100"
      label="DUMBO"
      border="None"
      tab="no"
      enable="no"
      reference="blewH"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <label loc="8 80" size="70 20"
        style="Label1"
        label="ofHblew:"
        />
      <entry loc="78 80" size="60 20"
        style="PlainText"
        vq="ofHblew"
        vc="vnmrunits('set','ofHblew',$VALUE)"
        set="vnmrunits('get','ofHblew'):$VALUE"
        show="on('ofHblew'):$VALUE"
        />
      <menu loc="138 80" size="60 20"
        style="PlainText"
        label="menu"
        vq="ofHblew"
        vc="parunits('set','ofHblew','$VALUE')"
        set="parunits('get','ofHblew'):$VALUE"
        show="on('ofHblew'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="Hz"
          chval="Hz"
          />
        <mlabel 
          label="kHz"
          chval="kHz"
          />
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
      </menu>
      <textmessage loc="160 0" size="40 20"
        style="PlainText"
        label="H1"
        vq="tn"
        set="$VALUE = dn"
        />
      <label loc="130 0" size="30 20"
        style="Label1"
        label="On:"
        />
      <label loc="210 40" size="30 20"
        style="Label1"
        label="360"
        />
      <menu loc="140 40" size="60 20"
        style="PlainText"
        vq="pwHblew"
        vc="parunits('set','pwHblew','$VALUE')"
        set="parunits('get','pwHblew'):$VALUE"
        show="on('pwHblew'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <entry loc="78 60" size="60 20"
        style="PlainText"
        vq="nHblew"
        vc="vnmrunits('set','nHblew',$VALUE)"
        set="vnmrunits('get','nHblew'):$VALUE"
        show="on('nHblew'):$VALUE"
        />
      <entry loc="78 40" size="60 20"
        style="PlainText"
        vq="pwHblew"
        vc="vnmrunits('set','pwHblew',$VALUE)"
        set="vnmrunits('get','pwHblew'):$VALUE"
        show="on('pwHblew'):$VALUE"
        />
      <entry loc="78 20" size="60 20"
        style="PlainText"
        vq="aHblew"
        vc="vnmrunits('set','aHblew',$VALUE)"
        set="vnmrunits('get','aHblew'):$VALUE"
        show="on('aHblew'):$VALUE"
        />
      <label loc="8 60" size="70 20"
        style="Label1"
        label="nHblew:"
        />
      <label loc="8 40" size="70 20"
        style="Label1"
        label="pwHblew:"
        />
      <label loc="8 20" size="70 20"
        style="Label1"
        label="aHblew:"
        />
      <label loc="8 0" size="120 20"
        style="Label1"
        label="blewH:"
        />
    </group>
  </group>
</template>
