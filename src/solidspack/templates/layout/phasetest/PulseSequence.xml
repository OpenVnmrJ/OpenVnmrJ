<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="270 0" size="260 80"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="tilt1H"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <menu loc="130 60" size="60 20"
        style="PlainText"
        label="menu"
        vq="ofX902"
        vc="parunits('set','ofX902','$VALUE')"
        set="parunits('get','ofX902'):$VALUE"
        show="on('ofX902'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        bg=""
        editable="No"
        >
        <mlabel 
          label="Hz"
          chval="Hz"
          />
        <mlabel 
          label="kHz"
          chval="kHz"
          />
        <mlabel 
          label="ppm"
          chval="ppm1"
          />
      </menu>
      <menu loc="130 40" size="60 20"
        style="PlainText"
        label="menu"
        vq="pwX902"
        vc="parunits('set','pwX902','$VALUE')"
        set="parunits('get','pwX902'):$VALUE"
        show="on('pwX902'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        bg=""
        editable="No"
        >
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <textmessage loc="160 0" size="40 20"
        style="PlainText"
        label="H1"
        vq="tn"
        set="$VALUE = tn"
        />
      <label loc="130 0" size="30 20"
        style="Label1"
        label="On:"
        />
      <entry loc="70 60" size="60 20"
        style="PlainText"
        vq="ofX902"
        vc="vnmrunits('set','ofX902',$VALUE)"
        set="vnmrunits('get','ofX902'):$VALUE"
        show="on('ofX902'):$VALUE"
        />
      <label loc="10 60" size="60 20"
        style="Label1"
        label="ofX902:"
        />
      <label loc="10 40" size="60 20"
        style="Label1"
        label="pwX902:"
        />
      <entry loc="70 40" size="60 20"
        style="PlainText"
        vq="pwX902"
        vc="vnmrunits('set','pwX902',$VALUE)"
        set="vnmrunits('get','pwX902'):$VALUE"
        show="on('pwX902'):$VALUE"
        />
      <entry loc="70 20" size="60 20"
        style="PlainText"
        vq="aX902"
        vc="aX902= $VALUE"
        set="$VALUE = aX902"
        show="on('aX902'):$VALUE"
        />
      <label loc="10 20" size="60 20"
        style="Label1"
        label="aX902:"
        />
      <label loc="10 0" size="60 20"
        style="Label1"
        label="902X:"
        />
    </group>
    <group loc="0 0" size="260 80"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="tilt1H"
      useref="no"
      subtype="Untitled"
