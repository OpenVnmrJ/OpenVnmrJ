    <group loc="270 0" size="260 100"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="echselX"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <menu loc="150 80" size="60 20"
        style="PlainText"
        vq="pwXechsel"
        vc="parunits('set','pwXechsel','$VALUE')"
