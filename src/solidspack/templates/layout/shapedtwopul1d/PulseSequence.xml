<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <group loc="540 0" size="260 120"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="autoTppmSpinal"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <textmessage loc="140 0" size="40 20"
        style="PlainText"
        vq="tn dn dn2 dn3 Hseq chHspinal chHtppm"
        set="if (Hseq='tppm') then if (chHtppm='obs') then $VALUE=tn elseif (chHtppm='dec') then $VALUE=dn elseif (chHtppm='dec2') then $VALUE=dn2 elseif (chHtppm='dec3') then $VALUE=dn3 else $VALUE='no' endif elseif (Hseq='spinal') then if (chHspinal='obs') then $VALUE=tn elseif (chHspinal='dec') then $VALUE=dn elseif (chHspinal='dec2') then $VALUE=dn2 elseif (chHspinal='dec3') then $VALUE=dn3 else $VALUE='no' endif else $VALUE='no' endif"
        show="on('Hseq'):$Hseqon if ($Hseqon=1) then if (Hseq='tppm') then on('chHtppm'):$VALUE elseif (Hseq='spinal') then on('chHspinal'):$VALUE else $VALUE=0 endif else $VALUE=0 endif"
        />
      <group loc="0 20" size="260 90"
        label="Hspinal"
        vq="Hseq"
        show="if (Hseq='spinal') then $VALUE=1 else $VALUE=0 endif"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="Hspinal"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <entry loc="90 40" size="60 20"
          style="PlainText"
          vq="aHspinal pwHspinal phHspinal"
          vc="vnmrunits('set','pwHspinal',$VALUE)"
          set="vnmrunits('get','pwHspinal'):$VALUE"
          show="on('pwHspinal'):$pwHspinalon if (($pwHspinalon=1) and (aHspinal&gt;0.0) and (phHspinal&gt;0.0) ) then $VALUE=1 else $VALUE=0 endif"
          />
        <entry loc="90 60" size="60 20"
          style="PlainText"
          vq="aHspinal pwHspinal phHspinal"
          vc="phHspinal= $VALUE"
          set="$VALUE = phHspinal"
          show="on('phHspinal'):$phHspinalon if (($phHspinalon=1) and (aHspinal&gt;0.0) and (phHspinal&gt;0.0) ) then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="10 60" size="80 20"
          style="Label1"
          label="phHspinal:"
          />
        <menu loc="150 40" size="60 20"
          style="PlainText"
          vq="pwHspinal"
          vc="parunits('set','pwHspinal','$VALUE')"
          set="parunits('get','pwHspinal'):$VALUE"
          show="on('pwHspinal'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
          bg=""
          editable="No"
          >
          <mlabel 
            label="sec"
            chval="sec"
            />
          <mlabel 
            label="ms"
            chval="ms"
            />
          <mlabel 
            label="us"
            chval="us"
            />
        </menu>
        <label loc="10 0" size="80 20"
          style="Label1"
          label="Hspinal:"
          />
        <label loc="10 20" size="80 20"
          style="Label1"
          label="aHspinal:"
          />
        <label loc="10 40" size="80 20"
          style="Label1"
          label="pwHspinal:"
          />
        <entry loc="90 20" size="60 20"
          style="PlainText"
          vq="aHspinal"
          vc="aHspinal = $VALUE"
          set="$VALUE = aHspinal"
          show="on('aHspinal'):$aHspinalon if (($aHspinalon=1) and (aHspinal&gt;0.0)) then $VALUE=1 else $VALUE=0 endif"
          />
      </group>
      <group loc="0 20" size="260 80"
        label="Htppm"
        vq="Hseq"
        show="if (Hseq='tppm') then $VALUE=1 else $VALUE=0 endif"
        bg=""
        border="None"
        tab="no"
        enable="no"
        reference="Htppm"
        useref="no"
        subtype="Untitled"
        expanded="yes"
        >
        <entry loc="90 60" size="60 20"
          style="PlainText"
          vq="aHtppm pwHtppm phHtppm"
          vc="phHtppm = $VALUE"
          set="$VALUE = phHtppm"
          show="on('phHtppm'):$phHtppmon if (($phHtppmon=1) and (aHtppm&gt;0.0) and (phHtppm&gt;0.0) ) then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="10 60" size="80 20"
          style="Label1"
          label="phHtppm:"
          />
        <entry loc="90 20" size="60 20"
          style="PlainText"
          vq="aHtppm"
          vc="aHtppm = $VALUE"
          set="$VALUE = aHtppm"
          show="on('aHtppm'):$aHtppmon if (($aHtppmon=1) and (aHtppm&gt;0.0) ) then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="10 0" size="60 20"
          style="Label1"
          label="Htppm:"
          />
        <menu loc="150 40" size="60 20"
          style="PlainText"
          vq="pwHtppm"
          vc="parunits('set','pwHtppm','$VALUE')"
          set="parunits('get','pwHtppm'):$VALUE"
          show="on('pwHtppm'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
          bg=""
          editable="No"
          >
          <mlabel 
            label="sec"
            chval="sec"
            />
          <mlabel 
            label="ms"
            chval="ms"
            />
          <mlabel 
            label="us"
            chval="us"
            />
        </menu>
        <entry loc="90 40" size="60 20"
          style="PlainText"
          vq="aHtppm pwHtppm phHtppm "
          vc="vnmrunits('set','pwHtppm',$VALUE)"
          set="vnmrunits('get','pwHtppm'):$VALUE"
          show="on('pwHtppm'):$pwHtppmon if (($pwHtppmon=1) and (aHtppm&gt;0.0) and (phHtppm&gt;0.0) ) then $VALUE=1 else $VALUE=0 endif"
          />
        <label loc="10 40" size="80 20"
          style="Label1"
          label="pwHtppm:"
          />
        <label loc="10 20" size="80 20"
          style="Label1"
          label="aHtppm:"
          />
      </group>
      <menu loc="180 0" size="80 20"
        style="PlainText"
        label="menu"
        vq="Hseq"
        vc="Hseq='$VALUE'"
        set="$VALUE = Hseq"
        show="on('Hseq'):$VALUE"
        bg=""
        editable="No"
        >
        <mlabel 
          label="tppm"
          chval="tppm"
          />
        <mlabel 
          label="spinal"
          chval="spinal"
          />
      </menu>
      <label loc="110 0" size="30 20"
        style="Label1"
        label="On:"
        />
      <label loc="10 0" size="100 20"
        style="Label1"
        label="Decoupling:"
        />
    </group>
    <group loc="270 0" size="260 120"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="shp2X"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <entry loc="90 100" size="60 20"
        style="PlainText"
        vq="stXshp2"
        vc="stXshp2= $VALUE"
        set="$VALUE = stXshp2"
        show="on('stXshp2'):$VALUE"
        />
      <label loc="10 100" size="80 20"
        style="Label1"
        label="stXshp2:"
        />
      <label loc="10 80" size="80 20"
        style="Label1"
        label="phXshp2:"
        />
      <entry loc="90 80" size="60 20"
        style="PlainText"
        vq="phXshp2"
        vc="phXshp2= $VALUE"
        set="$VALUE = phXshp2"
        show="on('phXshp2'):$VALUE"
        />
      <entry loc="180 0" size="80 20"
        style="PlainText"
        vq="wvXshp2"
        vc="wvXshp2 = '$VALUE'"
        set="$VALUE = wvXshp2"
        show="on('wvXshp2'):$VALUE"
        />
      <entry loc="90 20" size="60 20"
        style="PlainText"
        vq="aXshp2"
        vc="aXshp2 = $VALUE"
        set="$VALUE = aXshp2"
        show="on('aXshp2'):$VALUE"
        />
      <menu loc="150 60" size="60 20"
        style="PlainText"
        vq="pwXshp2"
        vc="parunits('set','pwXshp2','$VALUE')"
        set="parunits('get','pwXshp2'):$VALUE"
        show="on('pwXshp2'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        bg=""
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <label loc="220 60" size="30 20"
        style="Label1"
        label="180"
        />
      <entry loc="90 40" size="60 20"
        style="PlainText"
        vq="dbXshp2"
        vc="dbXshp2 = $VALUE"
        set="$VALUE = dbXshp2"
        show="on('dbXshp2'):$VALUE"
        />
      <label loc="10 40" size="80 20"
        style="Label1"
        label="dbXshp2:"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="aXshp2:"
        />
      <entry loc="90 60" size="60 20"
        style="PlainText"
        vq="pwXshp2"
        vc="vnmrunits('set','pwXshp2',$VALUE)"
        set="vnmrunits('get','pwXshp2'):$VALUE"
        show="on('pwXshp2'):$VALUE"
        />
      <label loc="10 60" size="80 20"
        style="Label1"
        label="pwXshp2:"
        />
      <textmessage loc="120 0" size="50 20"
        style="PlainText"
        label="H1"
        vq="tn"
        set="$VALUE = tn"
        />
      <label loc="90 0" size="30 20"
        style="Label1"
        label="On:"
        />
      <label loc="10 0" size="60 20"
        style="Label1"
        label="shp2X:"
        />
    </group>
    <group loc="0 0" size="260 120"
      bg=""
      border="None"
      tab="no"
      enable="no"
      reference="shp1X"
      useref="no"
      subtype="Untitled"
      expanded="yes"
      >
      <entry loc="90 100" size="60 20"
        style="PlainText"
        vq="stXshp1"
        vc="stXshp1= $VALUE"
        set="$VALUE = stXshp1"
        show="on('stXshp1'):$VALUE"
        />
      <label loc="10 100" size="80 20"
        style="Label1"
        label="stXshp1:"
        />
      <label loc="10 80" size="80 20"
        style="Label1"
        label="phXshp1:"
        />
      <entry loc="90 80" size="60 20"
        style="PlainText"
        vq="phXshp1"
        vc="phXshp1= $VALUE"
        set="$VALUE = phXshp1"
        show="on('phXshp1'):$VALUE"
        />
      <entry loc="180 0" size="80 20"
        style="PlainText"
        vq="wvXshp1"
        vc="wvXshp1 = '$VALUE'"
        set="$VALUE = wvXshp1"
        show="on('wvXshp1'):$VALUE"
        />
      <entry loc="90 20" size="60 20"
        style="PlainText"
        vq="aXshp1"
        vc="aXshp1 = $VALUE"
        set="$VALUE = aXshp1"
        show="on('aXshp1'):$VALUE"
        />
      <menu loc="150 60" size="60 20"
        style="PlainText"
        vq="pwXshp1"
        vc="parunits('set','pwXshp1','$VALUE')"
        set="parunits('get','pwXshp1'):$VALUE"
        show="on('pwXshp1'):$VALUE if ($VALUE=0) then $VALUE=1 endif"
        bg=""
        editable="No"
        >
        <mlabel 
          label="sec"
          chval="sec"
          />
        <mlabel 
          label="ms"
          chval="ms"
          />
        <mlabel 
          label="us"
          chval="us"
          />
      </menu>
      <label loc="220 60" size="30 20"
        style="Label1"
        label="180"
        />
      <entry loc="90 40" size="60 20"
        style="PlainText"
        vq="dbXshp1"
        vc="dbXshp1 = $VALUE"
        set="$VALUE = dbXshp1"
        show="on('dbXshp1'):$VALUE"
        />
      <label loc="10 40" size="80 20"
        style="Label1"
        label="dbXshp1:"
        />
      <label loc="10 20" size="80 20"
        style="Label1"
        label="aXshp1:"
        />
      <entry loc="90 60" size="60 20"
        style="PlainText"
        vq="pwXshp1"
        vc="vnmrunits('set','pwXshp1',$VALUE)"
        set="vnmrunits('get','pwXshp1'):$VALUE"
        show="on('pwXshp1'):$VALUE"
        />
      <label loc="10 60" size="80 20"
        style="Label1"
        label="pwXshp1:"
        />
      <textmessage loc="120 0" size="50 20"
        style="PlainText"
        label="H1"
        vq="tn"
        set="$VALUE = tn"
        />
      <label loc="90 0" size="30 20"
        style="Label1"
        label="On:"
        />
      <label loc="10 0" size="60 20"
        style="Label1"
        label="shp1X:"
        />
    </group>
  </group>
</template>
