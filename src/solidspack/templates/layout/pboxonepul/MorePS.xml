<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="MorePS" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="More"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="MorePS"
