<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<template name="PulseSequence" element="pages" type="acquisition" >
  <group size="800 200"
    style="Heading1"
    label="Pulse Sequence"
    vq="panellevel"
    show="$SHOW=(panellevel&gt;29)"
    bg=""
    border="None"
    side="Top"
    justify="Left"
    tab="yes"
    enable="no"
    reference="PulseSequence"
    useref="no"
    subtype="Untitled"
    expanded="yes"
    >
    <entry loc="90 40" size="60 20"
      style="PlainText"
      vq="xperiods"
      vc="xperiods = $VALUE"
      set="$VALUE = xperiods"
      show="on('xperiods'):$VALUE"
      />
    <label loc="10 40" size="80 20"
      style="Label1"
      label="xperiods:"
      />
    <entry loc="90 20" size="60 20"
      style="PlainText"
      vq="periods"
      vc="periods = $VALUE"
      set="$VALUE = periods"
      show="on('periods'):$VALUE"
      />
    <label loc="10 20" size="80 20"
      style="Label1"
      label="periods:"
      />
    <label loc="10 0" size="140 20"
      style="Label1"
      label="Rotorsync:"
      />
  </group>
</template>
