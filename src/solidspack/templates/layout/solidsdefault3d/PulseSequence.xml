        <label loc="10 40" size="80 20"
          style="Label4"
          label="pwHtppm:"
          />
        <label loc="10 20" size="80 20"
          style="Label4"
          label="aHtppm:"
          />
      </group>
      <menu loc="180 0" size="80 20"
