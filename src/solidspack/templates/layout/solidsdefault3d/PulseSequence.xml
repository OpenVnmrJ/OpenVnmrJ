        set="$VALUE = bYX"
        show="on('bYX'):$bHXon if (($bYXon = 1) and (shYX = 't')) then $VALUE = 1 else $VALUE = 0 endif"
        />
      <entry loc="60 80" size="60 20"
        style="Label4"
        vq="tYX"
        vc="vnmrunits('set','tYX',$VALUE)"
        set="vnmrunits('get','tYX'):$VALUE"
        show="on('tYX'):$VALUE"
        />
