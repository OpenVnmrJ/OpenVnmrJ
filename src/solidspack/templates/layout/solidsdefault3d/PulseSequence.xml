      <entry loc="180 60" size="60 20"
        style="Label4"
        vq="dYX shYX"
        vc="dYX = $VALUE"
        set="$VALUE = dYX"
        show="on('dYX'):$dHXon if (($dYXon = 1) and ((shYX ='l')  or (shYX = 't'))) then $VALUE = 1 else $VALUE = 0 endif"
        />
      <entry loc="180 40" size="60 20"
